        SEQUENCE {
                ciscoEnvMonSupplyStatusIndex    Integer32 (0..2147483647),
                ciscoEnvMonSupplyStatusDescr    DisplayString,
                ciscoEnvMonSupplyState          CiscoEnvMonState,
                ciscoEnvMonSupplySource         INTEGER
        }

