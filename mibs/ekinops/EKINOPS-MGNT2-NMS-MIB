::= { mgnt2EventLogEntry 13 }


mgnt2EventLogChassisId OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "This OID gives the chassis ID "
::= { mgnt2EventLogEntry 14 }

