    {
        hh3cDot11SrvL2AuthenID               Hh3cDot11ServicePolicyIDType,
        hh3cDot11L2AuthenMode                INTEGER,
        hh3cDot11L2IntrusProtectEnable       TruthValue,
        hh3cDot11L2IntrusProtectOpt          INTEGER,
        hh3cDot11TempServiceStopTimer        Integer32,
        hh3cDot11TempBlockMACTimer           Integer32,
        hh3cDot11L2IgnoreAuthorization       TruthValue,
        hh3cDot11L2FailVLAN                  Integer32,
        hh3cDot11L2CriticalVLAN              Integer32,
        hh3cDot11L2AuthorFailOffline         TruthValue,
        hh3cDot11L2AccountFailOffline        TruthValue,
        hh3cDot11Dot1xHSEnable               TruthValue,
        hh3cDot11Dot1xSecureHSEnable         TruthValue,
        hh3cDot11Dot1xReauthenEnable         TruthValue,
        hh3cDot11Dot1xMandatoryDomain        OCTET STRING,
