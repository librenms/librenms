            DEFVAL { 0 }
            ::= { hh3cQoSTrafficPriorityEntry 4 }

        hh3cQoSTrafPrioUserAclRule OBJECT-TYPE
            SYNTAX Integer32 (0..65535)
            MAX-ACCESS read-create
