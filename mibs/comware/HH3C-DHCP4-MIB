        {
            hh3cDhcpServer2PoolGpName              OCTET STRING,
            hh3cDhcpServer2PoolGpVpnName           OCTET STRING,
            hh3cDhcpServer2PoolGpIncUndo           TruthValue,
            hh3cDhcpServer2PoolGpRowSta            RowStatus
        }

