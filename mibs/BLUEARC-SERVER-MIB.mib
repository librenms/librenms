    cronJobNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of cron jobs configured to run on this system.
             NOTE: this table also includes those cron jobs which are 
                   configured to perform snapshot scheduling."
        ::= { cron 1 }

    cronJobTable OBJECT-TYPE
        SYNTAX SEQUENCE OF CronJobEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing a list of all cron jobs configured
             to run on this system.

             The number of entries in this table is given by the value of 
             cronJobNumber."
        ::= { cron 2 }

    cronJobEntry OBJECT-TYPE
        SYNTAX CronJobEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Cron job information."
        INDEX { cronJobIndex }
        ::= { cronJobTable 1 }

    CronJobEntry ::=
        SEQUENCE {
            cronJobIndex
                Integer32,
            cronJobDateSpec
                DisplayString,
            cronJobCommandList
                DisplayString,
            cronJobMailList
                DisplayString,
            cronJobAccessLevel
                DisplayString
        }

    cronJobIndex OBJECT-TYPE
        SYNTAX Integer32 (1..16384)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique value for each cron job entry."
        ::= { cronJobEntry 1 }

    cronJobDateSpec OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The date specification for the cron job."
        ::= { cronJobEntry 2 }

    cronJobCommandList OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A semi-colon separated list of commands to be run as 
             part of the cron job."
        ::= { cronJobEntry 3 }

    cronJobMailList OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A semi-colon separated list of recipients who each are to
             receive a copy of the output generated as a result of running 
             the cron job.

             NOTE: This value will be a zero length string if the output
                   of running the job is not be emailed."
        ::= { cronJobEntry 4 }

    cronJobAccessLevel OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The access level at which the cron job is run."
        ::= { cronJobEntry 5 }

    atJobNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of at jobs configured to run on this system."
        ::= { cron 3 }

    atJobTable OBJECT-TYPE
        SYNTAX SEQUENCE OF AtJobEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing a list of all at jobs configured
             to run on this system.

             The number of entries in this table is given by the value of 
             atJobNumber."
        ::= { cron 4 }

    atJobEntry OBJECT-TYPE
        SYNTAX AtJobEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "At job information."
