    AtJobEntry ::=
        SEQUENCE {
            atJobIndex
                Integer32,
            atJobRunTime
                DisplayString,
            atJobCommandList
                DisplayString,
            atJobMailList
                DisplayString,
            atJobAccessLevel
                DisplayString
        }

    atJobIndex OBJECT-TYPE
        SYNTAX Integer32 (1..16384)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique value for each at job entry."
        ::= { atJobEntry 1 }

    atJobRunTime OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The date at which the at job is scheduled to run expressed
             as a local time in the format YYYY-MM-DD HH:MI (Local)"
        ::= { atJobEntry 2 }

    atJobCommandList OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A semi-colon separated list of commands to be run as 
