       "
    ::= { fsMIY1731PortEntry 6 }
 
fsMIY1731PortTstIn OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "Number of TST PDUs received by this interface.
       "
    ::= { fsMIY1731PortEntry 7 } 

fsMIY1731PortLmmOut OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "Number of LMM PDUs transmitted by this interface.
       "
    ::= { fsMIY1731PortEntry 8 }
 
fsMIY1731PortLmmIn OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "Number of LMM PDUs received by this interface.
       "
    ::= { fsMIY1731PortEntry 9 } 

