ogAlarm OBJECT IDENTIFIER ::= { ogStatus2 23 }

ogAlarmLogTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF AlarmLogEntry
        MAX-ACCESS not-accessible
        STATUS     current
 	DESCRIPTION
            "The alarm history table."
 	::= { ogAlarm 1 }

