		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION "IPv6 Router table table."
		::= { iswIPv6StaticRoute 1 }
	iswIPv6StaticRouteEntry 	OBJECT-TYPE
		SYNTAX IswIPv6StaticRouteEntry
		MAX-ACCESS not-accessible
		STATUS current
