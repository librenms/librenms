	iswSnmpCommunityStatus		OBJECT-TYPE
		SYNTAX		RowStatus
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION	"Row Status."
		::= { iswSnmpCommunityEntry 6 } 




