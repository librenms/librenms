    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
       " A Table of Meter Pool"
    ::= { wwpLeosTrafficProfile 16 } 
    
  wwpLeosTrafficProfileMeterPoolEntry OBJECT-TYPE
     SYNTAX      WwpLeosTrafficProfileMeterPoolEntry
     MAX-ACCESS not-accessible
     STATUS current
     DESCRIPTION
