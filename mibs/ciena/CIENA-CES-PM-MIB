    cienaCesPmBasicPtpStats                  OBJECT IDENTIFIER ::= { cienaCesPmStatistics 13 }
    cienaCesPmBasicOtuStats                  OBJECT IDENTIFIER ::= { cienaCesPmStatistics 14 }
    cienaCesPmBasicOduStats                  OBJECT IDENTIFIER ::= { cienaCesPmStatistics 15 }
