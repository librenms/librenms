     cienaCesPmBasicPtpTotalStatsBinOptAvgOch                  PmInteger32Milli,
     cienaCesPmBasicPtpTotalStatsBinOprOts                     PmInteger32Milli,
     cienaCesPmBasicPtpTotalStatsBinOprnOts                    Integer32,
