     ::= { wwpLeosPbtLocalBridgeNameMacMapEntry 1 }
  
 wwpLeosPbtLocalBridgeNameMacMapBridgeName OBJECT-TYPE
     SYNTAX      DisplayString (SIZE (1..15))
     MAX-ACCESS  read-create
