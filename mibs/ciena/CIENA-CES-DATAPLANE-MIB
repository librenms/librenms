 DpTsAttachType ::= TEXTUAL-CONVENTION
     STATUS       current
     DESCRIPTION  "Logical Interface Attachment Types."
     SYNTAX       INTEGER {
                    port(1),
                    tunnelEncapPbt(2),
                    tunnelDecapPbt(3),
                    tunnelGroupPbt(4),
                    transitPbt(5), 
                    tunnelEncapMpls(6),
                    tunnelDecapMpls(7),
                    tunnelGroupMpls(8),
                    transitMpls(9),
                    subPort(10),
                    qosFlow(11),
                    accessFlow(12),
                    servicePbt(13),
                    servicePbb(14),
                    vcMpls(15),
                    serviceMplsMesh(16),
                    cpuInterface(17),
                    cpuSubInterface(18),
                    ettp(19),
                    lspEncapMpls(20),
                    lspDecapMpls(21),
                    l3Interface(22),
                    l3Adjacency(23),
                    unknown(99)
                  }


