     STATUS       current


     DESCRIPTION  "Private Forward Group Policies."


     SYNTAX       INTEGER {


                        talkToA(1),


                        talkToB(2),


                        talkToC(3),


                        talkToAB(4),


                        talkToAC(5),


                        talkToBC(6),


                        talkToABC(7)


                    }


 


