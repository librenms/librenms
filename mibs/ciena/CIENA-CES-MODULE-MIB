 OBJECTS {        
       			 cienaGlobalSeverity, 
       			 cienaGlobalMacAddress,
       			 cienaCesModuleChassisNotifIndx,
        		 cienaCesModuleShelfNotifIndx,
        		 cienaCesModuleSlotNotifIndx,
	             cienaCesModulePOSTErrorDescription
