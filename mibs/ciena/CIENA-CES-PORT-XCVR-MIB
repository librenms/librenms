    SYNTAX           INTEGER {
                       unknown(1),
		               gbic(2),
		               solderedType(3),
		               sfp(4),
                       xbi(5),
                       xenpak(6),
                       xfp(7),
                       xff(8),
                       xfpe(9),
                       xpak(10),
                       x2(11),
		               reserved(12),
		               vendorSpecific(13),
                       cfp(14),
                       qsfpPlus(15),  
                       qsfp28(16)
                     }
    MAX-ACCESS       read-only
    STATUS           current
    DESCRIPTION
	    "Type for the transceiver."
    ::= { cienaCesPortXcvrEntry 21 }
   
 cienaCesPortXcvrExtIdentiferType OBJECT-TYPE
    SYNTAX           INTEGER 
    MAX-ACCESS       read-only
    STATUS           current
    DESCRIPTION
