         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
                     "System Fan Speen in RPM."
         ::= { bws 10 }

    cpuTemperature OBJECT-TYPE
         SYNTAX      Integer32
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
                     "Cpu Temperature in degree Celsius."
         ::= { bws 11 }

    firmwareStorage OBJECT-TYPE
         SYNTAX      Integer32
