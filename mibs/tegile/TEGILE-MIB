tegileCompliances OBJECT IDENTIFIER ::= { tegileConformance 2 }

basicCompliance MODULE-COMPLIANCE
    STATUS current
    DESCRIPTION
        "TEGILE-MIB MODULE-COMPLIANCE."
    MODULE -- this module
        MANDATORY-GROUPS { currentObjectGroup }
    ::= { tegileCompliances 1 }


