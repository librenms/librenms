tegileArray-notifications           OBJECT IDENTIFIER ::= { tegile 2 }

properties                          OBJECT IDENTIFIER ::= { tegileArray 1 }
globalStatistics                    OBJECT IDENTIFIER ::= { tegileArray 2 }
disks                               OBJECT IDENTIFIER ::= { tegileArray 3 }
