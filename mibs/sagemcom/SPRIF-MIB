		debug3Line              LINE,
		debug3arv				SagemBoolean,
		debug3ato 				SagemBoolean,
		debug3aun 				SagemBoolean,
		debug3ptm   			SagemBoolean,
		debug3mms  				SagemBoolean,
		debug3exr   			SagemBoolean
   }
	          
    debug3Date OBJECT-TYPE
      SYNTAX 	    INTEGER(0..2147483647)
