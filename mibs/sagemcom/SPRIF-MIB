   debug3Table   OBJECT-TYPE
      SYNTAX       SEQUENCE OF Debug3Entry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION 
	""
      ::= { debug3 2 } 

    debug3Entry   OBJECT-TYPE
      SYNTAX       Debug3Entry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION 
	"" 
      INDEX        { debugIndex } 
      ::= { debug3Table 1 } 
   
   Debug3Entry ::= SEQUENCE 
   {
