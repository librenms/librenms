		-- OID : .1.3.6.1.4.1.12551.4.1.5.4.1
		tsiAlarmEntry  OBJECT-TYPE
      SYNTAX       TsiAlarmEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION "The conceptual row referencing the SEQUENCE definition"
      INDEX    {  tsiAlarmIndex  }
    ::=  { tsiAlarmTable 1 }

		TsiAlarmEntry  ::=  SEQUENCE {
      tsiAlarmIndex					Integer32,
			tsiAlarmID						Integer32,
			tsiAlarmType					INTEGER,
			tsiAlarmSource				INTEGER,
			tsiAlarmDescription		DisplayString,
      tsiAlarmTime					DateAndTime
    }

		-- OID : .1.3.6.1.4.1.12551.4.1.5.4.1.1
		tsiAlarmIndex  OBJECT-TYPE 
      SYNTAX         Integer32 (1..50)
      MAX-ACCESS     not-accessible
      STATUS         current
      DESCRIPTION    "The alarm entry identifier"
    ::=  {  tsiAlarmEntry  1  }

		-- OID : .1.3.6.1.4.1.12551.4.1.5.4.1.2
