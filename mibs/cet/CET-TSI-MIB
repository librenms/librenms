      STATUS        current
      DESCRIPTION    "Unique identifier number of the alarm"
    ::=  {  tsiAlarmEntry  2  }

		-- OID : .1.3.6.1.4.1.12551.4.1.5.4.1.3
		tsiAlarmType   OBJECT-TYPE 
