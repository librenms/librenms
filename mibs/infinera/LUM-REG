lumGeneric OBJECT-IDENTITY
    STATUS      current
    DESCRIPTION
        "Sub-tree for common object and event definitions."
    ::= { lumentis 2 }


-- ----------------------------------------------------
-- MIB root oids
-- ----------------------------------------------------
