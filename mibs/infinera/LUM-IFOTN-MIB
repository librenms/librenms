lumIfOtnGroups OBJECT IDENTIFIER ::= { lumIfOtnConfs 1 }
lumIfOtnCompl OBJECT IDENTIFIER ::= { lumIfOtnConfs 2 }


-- ----------------------------------------------------
-- Root for objects in the IFOTN MIB
-- ----------------------------------------------------

lumIfOtnMIBObjects OBJECT IDENTIFIER ::= { lumIfOtnMIB 2 }


