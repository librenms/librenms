    OBJECTS { gigamonSnmpNotifHardWareName,
              gigamonSnmpNotifLevel,
              gigamonSnmpNotifDescription,
              gigamonSnmpNotifFsMountPoint }
    STATUS  current
    DESCRIPTION
            "Free space in one of the filesystems is low"
    ::= { gigamonGigaVUE 18 }

