    OBJECTS     { gigamonSnmpNotifHardWareName,
                  gigamonSnmpNotifLevel,
                  gigamonSnmpNotifDescription}
    STATUS      current
    DESCRIPTION
        "System reset notification triggered by watchdog timer."
    ::= { gigamonGigaVUE 19 }

