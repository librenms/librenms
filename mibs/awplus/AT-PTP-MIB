        ptpClockPortName OBJECT-TYPE
            SYNTAX DisplayString (SIZE  (1..64))
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "This object specifies the PTP clock port name configured on the
                node."
            ::= { ptpClockPortEntry 5 }

        ptpClockPortRole OBJECT-TYPE
            SYNTAX PtpClockRoleType
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
