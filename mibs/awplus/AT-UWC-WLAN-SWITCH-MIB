    intrusionDetection                     OBJECT IDENTIFIER ::= { fastPathWLANSwitch 9 }
    snmpTrapsConfig                        OBJECT IDENTIFIER ::= { fastPathWLANSwitch 10 }
    wsMibInfo                              OBJECT IDENTIFIER ::= { fastPathWLANSwitch 11 }
