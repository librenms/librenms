     DEFVAL { unchanged }
     ::= {wsPowerPlanPerRadioEntry  13 }

 wsPowerPlanAPLastPwrAdjReasonCode  OBJECT-TYPE
     SYNTAX       INTEGER {
                      powerPlanNeverExecuted(0),
