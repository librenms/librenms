LANTRONIX-SLC-MIB DEFINITIONS ::= BEGIN

-- This MIB defines the management information for Lantronix Secure 
-- Console Managers and Advanced Console Managers.  

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, 
    Integer32, Unsigned32, IpAddress, NOTIFICATION-TYPE, TimeTicks
        FROM SNMPv2-SMI
    TEXTUAL-CONVENTION, MacAddress
        FROM SNMPv2-TC
    Ipv6Address
        FROM IPV6-TC
    products
        FROM LANTRONIX-MIB;

slc MODULE-IDENTITY
    LAST-UPDATED "201604170000Z"
    ORGANIZATION "Lantronix, Inc."
    CONTACT-INFO "Lantronix Technical Support
                  167 Technology Drive 
                  Irvine, CA 92618 USA 
                  +1 800 526-8766 
                  snmp@lantronix.com"
    DESCRIPTION  "MIB module for managing Lantronix Secure Console Managers 
                  and Advanced Console Managers"

    -- revision history

