    SYNTAX        Counter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Total uncompressed bytes from cache."
    ::= { vServerStatsPerfGraphScalars 3 }

vServerStatsSubscriberOutBytes OBJECT-TYPE
    SYNTAX        Counter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Total bytes sent to subscriber."
    ::= { vServerStatsPerfGraphScalars 4 }

