    STATUS        current
    DESCRIPTION
        "Total number of bytes received from Venturi clients."
    ::= { vServerTransportEntry 7 }

vServerTransportBytesRetransmitted OBJECT-TYPE
    SYNTAX        Counter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Total number of bytes retransmitted to Venturi clients."
