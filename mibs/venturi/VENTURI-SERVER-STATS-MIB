    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Round trip time"
    ::= { vServerClientInfoEntry 10 }

vServerClientInfoNSub OBJECT-TYPE
    SYNTAX        Counter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        ""
    ::= { vServerClientInfoEntry 11 }

