-- Reproduction of this document is authorized on condition that this
-- copyright notice is included.


--Versions history:
--1.0.0 (28/11/02) - Initial version
--1.0.4 (18/02/03) - Name of PSU index changed; Name of slot index changed; Remote device overheat added; Remote device TP limk up/down added
--1.0.8 (10/04/03) - GBE link/SD traps added
--1.1.0 (21/05/03) - Vars indications added
--1.2.1 (08/12/03) - Chassis and link indexes added; Module index name changed
--1.2.2 (14/01/04) - Variables change; Remote LX traps removed
--1.2.3 (17/02/04) - MCM100-RL traps added
--1.2.5 (10/06/04) - Signal detected/lost trap added; Remote device redundant link traps added
--1.3.0b(25/07/04) - E1 traps added
--1.4.0b(01/12/04) - Descriptions rephrased; OFC traps added
--1.4.1d(28/03/05) - Variables change
--1.5.0b(08/09/05) - Temperature normal traps added; Remote UNI FO link traps added
-- not released: 1.5.2b() - SFP traps added
--1.6.0b(21/06/06) - Remote device FO signal detected/lost added; SFP traps changed and re-assigned
--1.6.0 (03/07/06) - Variables added
--1.6.2 (27/03/08) - Added trap severity object and variables. Traps descriptions revised. Traps version applicability added. Added MCM1000X-RL traps.
--1.7.0 (24/05/10) - Cannot manage remote device using old MA version trap added


FIBROLAN-MIB-MS-TRAPS   DEFINITIONS ::= BEGIN

	 IMPORTS
		OBJECT-TYPE, enterprises	FROM SNMPv2-SMI
		TRAP-TYPE			FROM RFC-1215
		flMsChassisModuleNumber, flMsChassisChannelNumber, flMsChassisPsuIndex	FROM FIBROLAN-MIB-METRO-STAR
		flMcm1xxChannelExtMvPortDescription	FROM FIBROLAN-MIB-MCM1xx
		flLta41ma-V2PortNumber			FROM FIBROLAN-MIB-LTA41MA
		flMcm1000ChannelExtMvPortDescription	FROM FIBROLAN-MIB-GBE-MCM1000
		flMsMcm100xE1PortIndex, flMsMcm100xE1PortDescription	FROM FIBROLAN-MIB-MCM100-xE1		
		flSfpDevicePortIndex	FROM FIBROLAN-MIB-SFP;


fibrolan             OBJECT IDENTIFIER ::= { enterprises       4467 }

fibrolanSNMP         OBJECT IDENTIFIER ::= { fibrolan           100 }

flTrapSeverity	OBJECT-TYPE
	SYNTAX	INTEGER {
				off	(0),
				info	(1),
				warning	(2),
				error	(3),
				fatal	(4)
			}
         MAX-ACCESS    read-only
         STATUS    current
         DESCRIPTION
         "Trap severity"
	 ::= { fibrolanSNMP  1 }

-- *******************************************************
-- chassis level traps
-- *******************************************************

	-- module traps

	-- this trap is applicable from SW version 1.0.0 (initial version)
	moduleInstalled TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES	{ flTrapSeverity, flMsChassisModuleNumber }
		DESCRIPTION
		"New module installed.
		Vars: trap severity, slot number"
		::= 5000

	-- this trap is applicable from SW version 1.0.0 (initial version)
	moduleRemoved TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES   { flTrapSeverity, flMsChassisModuleNumber }
		DESCRIPTION
		"Module removed.
		Vars: trap severity, slot number"
		::= 5001

	-- PSU traps

	-- this trap is applicable from SW version 1.0.0 (initial version)
	psuInstalled TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES   { flTrapSeverity, flMsChassisPsuIndex }
		DESCRIPTION
		"PSU module installed.
		Vars: trap severity, PSU number"
		::= 5010

	-- this trap is applicable from SW version 1.0.0 (initial version)
	psuRemoved TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES   { flTrapSeverity, flMsChassisPsuIndex }
		DESCRIPTION
		"PSU module removed.
		Vars: trap severity, PSU number"
		::= 5011

	-- this trap is applicable from SW version 1.0.0 (initial version)
	psuUp TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES   { flTrapSeverity, flMsChassisPsuIndex }
		DESCRIPTION
		"PSU module is up.
		Vars: trap severity, PSU number"
		::= 5012

	-- this trap is applicable from SW version 1.0.0 (initial version)
	psuDown TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES   { flTrapSeverity, flMsChassisPsuIndex }
		DESCRIPTION
		"PSU module is down.
		Vars: trap severity, PSU number"
		::= 5013

	-- temperature trap

	-- this trap is applicable from SW version 1.0.0 (initial version)
	overHeat TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES   { flTrapSeverity }
		DESCRIPTION
		"Temperature higher than 65 degrees, for more than 1 minute.
		Vars: trap severity"
		::= 5020

	-- this trap is applicable from SW version 1.5.0
	temperatureNormal TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES   { flTrapSeverity }
		DESCRIPTION
		"Chassis temperature back to normal for more than 1 minute (clears over-heat alarm).
		Vars: trap severity"
		::= 5021






-- *******************************************************
-- module level traps
-- *******************************************************

	-- remote device connection traps

	-- this trap is applicable from SW version 1.0.0 (initial version)
	remoteDeviceConnected TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES	{ flTrapSeverity,
				flMsChassisModuleNumber, 
				flMsChassisChannelNumber,
				flMcm1xxChannelExtMvPortDescription }
		DESCRIPTION
		"A new remote MA device has been connected and detected.
		Vars: trap severity, slot number, channel number, channel description"
		::= 5030

	-- this trap is applicable from SW version 1.0.0 (initial version)
	remoteDeviceDisconnected TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES	{ flTrapSeverity, 
				flMsChassisModuleNumber, 
				flMsChassisChannelNumber,
				flMcm1xxChannelExtMvPortDescription }
		DESCRIPTION
		"A remote MA device has been disconnected.
		Vars: trap severity, slot number, channel number, channel description"
		::= 5031

	-- this trap is applicable from SW version 1.0.0 (initial version)
	remoteDevicePowerFail TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES	{ flTrapSeverity, 
				flMsChassisModuleNumber, 
				flMsChassisChannelNumber,
				flMcm1xxChannelExtMvPortDescription }
		DESCRIPTION
		"A remote MA device had a power failure.
		Vars: trap severity, slot number, channel number, channel description"
		::= 5032

	-- this trap is applicable from SW version 1.0.4
	remoteDeviceOverHeat TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES	{ flTrapSeverity, 
				flMsChassisModuleNumber, 
				flMsChassisChannelNumber }
		DESCRIPTION
		"Temperature in remote device is higher than 65 degrees, for more than 1 minute.
		Vars: trap severity, slot number, channel number"
		::= 5033

	-- this trap is applicable from SW version 1.5.0
	remoteDeviceTemperatureNormal TRAP-TYPE
		ENTERPRISE  fibrolanSNMP
		VARIABLES	{ flTrapSeverity, 
				flMsChassisModuleNumber, 
				flMsChassisChannelNumber }
		DESCRIPTION
		"Remote device temperature back to normal for more than 1 minute (clears over-heat alarm).
  		Vars: trap severity, slot number, channel number"
		::= 5034

	-- local link state traps

	-- this trap is applicable from SW version 1.0.0 (initial version)
	tpLinkDown TRAP-TYPE 
