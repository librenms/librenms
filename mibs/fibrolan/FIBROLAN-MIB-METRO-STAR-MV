-- ************************************************************
     flMsChassisMvRebootSystem        OBJECT-TYPE
		 SYNTAX    INTEGER
						{
							normal   (1),
							reboot   (2)
						}
         MAX-ACCESS     read-write
         STATUS    current
         DESCRIPTION
         "Rebooting the system"
	 ::= { flMsChassisMv 1001 }



