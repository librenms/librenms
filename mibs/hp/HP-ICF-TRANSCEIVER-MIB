-- *******************************************************************
-- Transceiver Channel Status
-- *******************************************************************

    hpicfXcvrChannelInfoTable OBJECT-TYPE
          SYNTAX      SEQUENCE OF HpicfXcvrChannelInfoEntry
          MAX-ACCESS  not-accessible
          STATUS      current
          DESCRIPTION
                 "A table of objects that displays the transceiver
                  channel characteristics."
          ::= { hpicfXcvrInfo 2 }

    hpicfXcvrChannelInfoEntry OBJECT-TYPE
          SYNTAX      HpicfXcvrChannelInfoEntry
          MAX-ACCESS  not-accessible
          STATUS      current
          DESCRIPTION
                 "A set of objects that displays the information of
                  a transceiver channel."
          INDEX { ifIndex,
                  hpicfXcvrChannel }
          ::= { hpicfXcvrChannelInfoTable 1 }

    HpicfXcvrChannelInfoEntry ::=
          SEQUENCE {
             hpicfXcvrChannel               Unsigned32,
             hpicfXcvrChannelTxBias         Unsigned32,
             hpicfXcvrChannelTxPower        Integer32,
             hpicfXcvrChannelRxPower        Integer32,
             hpicfXcvrChannelAlarms         BITS,
             hpicfXcvrChannelErrors         BITS
          }

    hpicfXcvrChannel OBJECT-TYPE
          SYNTAX      Unsigned32
          MAX-ACCESS  not-accessible
          STATUS      current
          DESCRIPTION
                 "The channel number for this entry."
          ::= { hpicfXcvrChannelInfoEntry 1 }

    hpicfXcvrChannelTxBias OBJECT-TYPE
          SYNTAX      Unsigned32 (0..65535)
          UNITS       "microamps"
          MAX-ACCESS  read-only
          STATUS      current
          DESCRIPTION
                 "Tx bias current in microamps.

                  The value of this object is valid when the value of
                  the hpicfXcvrDiagnostics object is DOM."
          ::= { hpicfXcvrChannelInfoEntry 2 }

    hpicfXcvrChannelTxPower OBJECT-TYPE
          SYNTAX      Integer32
          UNITS       "thousandths of dBm"
          MAX-ACCESS  read-only
          STATUS      current
          DESCRIPTION
                 "This is transmit output power
                  in thousandths of dBm.

                  As an example: -5840 is -5.840dBm.

                  Negative infinity dBm (0 microwatts) is reported as
                  -99999999.

                  The value of this object is valid when the value of 
                  the hpicfXcvrDiagnostics object is DOM."
          ::= { hpicfXcvrChannelInfoEntry 3 }

    hpicfXcvrChannelRxPower OBJECT-TYPE
          SYNTAX      Integer32
          UNITS       "thousandths of dBm"
          MAX-ACCESS  read-only
          STATUS      current
          DESCRIPTION
                 "Received optical power in thousandths of dBm.

                  As an example: -5840 is -5.840dBm.

                  Negative infinity dBm (0 microwatts) is reported as
                  -99999999.

                  The value of this object is valid when the value of
                  the hpicfXcvrDiagnostics object is DOM."
          ::= { hpicfXcvrChannelInfoEntry 4 }

    hpicfXcvrChannelAlarms OBJECT-TYPE
          SYNTAX      BITS {
                           rxPowerLowWarning(0),
                           rxPowerHighWarning(1),
                           txPowerLowWarning(2),
                           txPowerHighWarning(3),
                           txBiasLowWarning(4),
                           txBiasHighWarning(5),
                           rxPowerLowAlarm(6),
                           rxPowerHighAlarm(7),
                           txPowerLowAlarm(8),
                           txPowerHighAlarm(9),
                           txBiasLowAlarm(10),
                           txBiasHighAlarm(11)
                           }
          MAX-ACCESS    read-only
          STATUS        current
          DESCRIPTION
                 "Bitmask indicating transceiver channel alarms,
                  Rx power low warning(0)
                  Rx power high warning(1)
                  Tx power low warning(2)
                  Tx power high warning(3)
                  Tx bias low warning(4)
                  Tx bias high warning(5)
                  Rx power low alarm(6)
                  Rx power high alarm(7)
                  Tx power low alarm(8)
                  Tx power high alarm(9)
                  Tx bias low alarm(10)
                  Tx bias high alarm(11)

                  Unused(12-31)

                  The value of this object is valid when the value of 
                  the hpicfXcvrDiagnostics object is DOM."
          ::= { hpicfXcvrChannelInfoEntry 5 }

    hpicfXcvrChannelErrors OBJECT-TYPE
          SYNTAX      BITS {
                           txFault(0),
                           txLossOfSignal(1),
                           rxLossOfSignal(2)
                           }
          MAX-ACCESS    read-only
          STATUS        current
          DESCRIPTION
                 "TX fault(0)
                  TX loss of signal(1)
                  RX loss of signal(2)

                  Unused(3-31)

                  The value of this object is valid when the value of 
                  the hpicfXcvrDiagnostics object is DOM."
          ::= { hpicfXcvrChannelInfoEntry 6 }
 
