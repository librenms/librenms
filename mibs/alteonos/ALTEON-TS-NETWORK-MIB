    SYNTAX  INTEGER {
        enabled(2),      
        disabled(3) 
	}
    ACCESS  read-only 
    STATUS  mandatory  
    DESCRIPTION 
        "Enable or disable listening to default routes." 
    ::= { ripConfig 5 }

ripNewCfgDefListen OBJECT-TYPE
    SYNTAX  INTEGER {
        enabled(2),
        disabled(3)
	}
