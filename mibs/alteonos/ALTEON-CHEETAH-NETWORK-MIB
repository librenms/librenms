    INDEX   { haSwitchGrpTrkIndex }
    ::= { haSwitchGrpTrkTable 1 }

HaSwitchGrpTrkEntry ::= SEQUENCE {
    haSwitchGrpTrkIndex  DisplayString
}

haSwitchGrpTrkIndex OBJECT-TYPE
    SYNTAX DisplayString (SIZE(1..255))
    MAX-ACCESS  read-only
    STATUS current
    DESCRIPTION
        "The Group ID in alphanumeric which is configured for tracking in switch 
        HA failover trigger.
	Note:This mib is not supported for VX instance of virtualization."
    ::= { haSwitchGrpTrkEntry 1 }

-- Table to list the Layer 4 groups with group's internal index as key

haSwitchGrpIdxTable OBJECT-TYPE
    SYNTAX SEQUENCE OF HaSwitchGrpIdxTableEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The table of Layer 4 groups configured in Alteon. This table lists group's internal
         index and its alphanumeric name.
         Note: This MIB object is not supported for VX instance of virtualization."
    ::= { haTrigger 10 }

