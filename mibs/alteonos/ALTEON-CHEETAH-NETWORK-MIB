    SYNTAX INTEGER { 
        enabled(1),
        disabled(2),
        unsupported(2147483647)
	}
    MAX-ACCESS  read-only
    STATUS  current  
    DESCRIPTION
	"Enable or disable the network filter."
    ::= { ipCurCfgNwfEntry 4 }

