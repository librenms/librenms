 HaServiceTriggerVipServIdxTableEntry ::= SEQUENCE {
     haServiceTriggerVipServIdxCfgIndex     DisplayString,
     haServiceTriggerVipServIdxTableIndex   Integer32,
     haServiceTriggerVipServIdxName         DisplayString,
     haServiceTriggerVipServIdxVport        INTEGER,
     haServiceTriggerVipServIdxTrackType    INTEGER
 }
 
   haServiceTriggerVipServIdxCfgIndex OBJECT-TYPE
    SYNTAX DisplayString (SIZE(1..32))
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The HA service ID number in alphanumeric.
         Note:This mib is not supported for VX instance of virtualization."
    ::= { haServiceTriggerVipServIdxTableEntry 1 }
    
   haServiceTriggerVipServIdxTableIndex OBJECT-TYPE
     SYNTAX Integer32
     MAX-ACCESS  read-only
     STATUS  current
     DESCRIPTION
        "The internal index of a VIP and service for service ha.
         Note: This MIB object is not supported for VX instance of virtualization."
     ::= { haServiceTriggerVipServIdxTableEntry 2 }

