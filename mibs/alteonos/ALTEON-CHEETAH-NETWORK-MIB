    ::= { haSwitchGrpIdxTableEntry 2 }

-- Table to list the Real Server with its internal index, alphanumeric ID and Real IP 

haSwitchRealIdxTable OBJECT-TYPE
    SYNTAX SEQUENCE OF HaSwitchRealIdxTableEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table lists the alphanumeric ID of Real servers with its internal index
         and Real IP address.
         Note: This MIB object is not supported for VX instance of virtualization."
    ::= { haTrigger 11 }

haSwitchRealIdxTableEntry OBJECT-TYPE
    SYNTAX HaSwitchRealIdxTableEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Tabular entry to list Real server with its internal index, alphanumeric ID and
         Real server IP address.
         Note: This MIB object is not supported for VX instance of virtualization."
    INDEX { haSwitchRealIndex, haSwitchRealName }
    ::= { haSwitchRealIdxTable 1 }

HaSwitchRealIdxTableEntry ::= SEQUENCE {
    haSwitchRealIndex       Integer32,
    haSwitchRealName        DisplayString,
    haSwitchRealIpVer       INTEGER,
    haSwitchRealIpAddr      IpAddress,
    haSwitchRealIpv6Addr    DisplayString
}

haSwitchRealIndex  OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The internal index of a real server.
         Note: This MIB object is not supported for VX instance of virtualization."
    ::= { haSwitchRealIdxTableEntry 1 }

haSwitchRealName OBJECT-TYPE
    SYNTAX DisplayString(SIZE(1..255))
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The alphanumeric ID of a real server.
         Note: This MIB object is not supported for VX instance of virtualization."
    ::= { haSwitchRealIdxTableEntry 2 }

haSwitchRealIpVer OBJECT-TYPE
    SYNTAX INTEGER{
        ipv4(1),
        ipv6(2),
        unsupported(2147483647)
    }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Version of the real server IP address.
         Note: This MIB object is not supported for VX instance of virtualization."
    ::= { haSwitchRealIdxTableEntry 3 }

haSwitchRealIpAddr OBJECT-TYPE
    SYNTAX IpAddress 
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "IP address of the Real server.
         Note: This MIB object is not supported for VX instance of virtualization."
    ::= { haSwitchRealIdxTableEntry 4 }

haSwitchRealIpv6Addr OBJECT-TYPE
    SYNTAX DisplayString(SIZE(0..40))
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "IPV6 address of the real server. Address should be 4-byte hexadecimal 
         colon notation. Valid IPv6 address should be in any of the following forms
         xxxx:xxxx:xxxx:xxxx:xxxx:xxxx:xxxx:xxxx or xxxx::xxxx:xxxx:xxxx:xxxx or 
         ::xxxx.
         Note: This MIB object is not supported for VX instance of virtualization."
     ::= { haSwitchRealIdxTableEntry 5 }

