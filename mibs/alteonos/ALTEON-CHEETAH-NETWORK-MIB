    SYNTAX INTEGER {
        always(1),
        candidate(2),
        unsupported(2147483647)
        }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Translator role.
        Note:This mib is not supported for VX instance of virtualization."
    ::= { ospfv3CurCfgAreaEntry 8 }

