    SYNTAX INTEGER {
        v4(0),
        v6(1),
        unsupported(2147483647)
        }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The type of IP address Version"
    ::= { ipCurCfgNwfEntry 5 }

