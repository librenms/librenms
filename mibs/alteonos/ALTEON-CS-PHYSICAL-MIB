-- Link Layer Discovery Protocol (LLDP) 

lldpCurTxState OBJECT-TYPE
    SYNTAX  INTEGER {
        enabled(1),
        disabled(2),
        unsupported(2147483647)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Enable or disable LLDP transmission.
        Note:This mib is not supported in vADC instance on Virtualization."
    ::= { lldp 1 }

lldpNewTxState OBJECT-TYPE
    SYNTAX  INTEGER {
        enabled(1),
        disabled(2),
        unsupported(2147483647)
        }
    MAX-ACCESS read-create
    STATUS  current
    DESCRIPTION
        "Enable or disable LLDP transmission.
        Note:This mib is not supported in vADC instance on Virtualization."
    ::= { lldp 2 }

lldpCurTxInterval OBJECT-TYPE
    SYNTAX      INTEGER (2..600)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The interval at which LLDP frames are transmitted on
            behalf of this LLDP agent.

            The default value for lldpCurTxInterval object is
            30 seconds.
            Note:This mib is not supported in vADC instance on Virtualization."
    ::= { lldp 3 }

lldpNewTxInterval OBJECT-TYPE
    SYNTAX      INTEGER (2..600)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The interval at which LLDP frames are transmitted on
            behalf of this LLDP agent.

            The default value for lldpCurTxInterval object is
            30 seconds.
            Note:This mib is not supported in vADC instance on Virtualization."
    ::= { lldp 4 }

lldpCurTxHold OBJECT-TYPE
    SYNTAX      INTEGER (2..20)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "TX hold multiplier. 
	    The time-to-live value expressed as a multiple of the
	    lldpCurTxInterval object.
        Note:This mib is not supported in vADC instance on Virtualization."
    ::= { lldp 5 }

lldpNewTxHold OBJECT-TYPE
    SYNTAX      INTEGER (2..20)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "TX hold multiplier.
            The time-to-live value expressed as a multiple of the
            lldpCurTxInterval object.
            Note:This mib is not supported in vADC instance on Virtualization."
    ::= { lldp 6 }

lldpCurVendtlv OBJECT-TYPE
    SYNTAX  INTEGER {
        enabled(1),
        disabled(2),
        unsupported(2147483647)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Enable or disable publishing vendor specific TLVs.
        Note:This mib is not supported in vADC instance on Virtualization."
    ::= { lldp 7 }

