			dhcpSnoopingIndex       Integer32,
			dhcpSnoopingMac					DisplayString,
			dhcpSnoopingIp	        IpAddress,
			dhcpSnoopingLeaseTime   INTEGER,
			dhcpSnoopingVlanId			Integer32,
			dhcpSnoopingPort        Integer32
		}

		dhcpSnoopingIndex	OBJECT-TYPE
