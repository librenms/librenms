-- management.lldp    
 	lldpEnabled OBJECT-TYPE
		SYNTAX      INTEGER {
		 	enable(1),
		 	disable(2)
		}
		MAX-ACCESS  read-write
		STATUS      current
		DESCRIPTION	"Enable/Disable LLDP protocol on this switch"
