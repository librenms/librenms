			vlanIndex		Integer32,
			vlanId			Integer32,
			vlanNamePrefix	DisplayString,
			vlanType		INTEGER,
			vlanRowStatus	INTEGER
		}		                  
		vlanIndex	OBJECT-TYPE
			SYNTAX		Integer32 (1..4094)
			MAX-ACCESS	read-write
			STATUS		current
			DESCRIPTION "The index of VLAN"
			::= { vlanEntry 1 }			
