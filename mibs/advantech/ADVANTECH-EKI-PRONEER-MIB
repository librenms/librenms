-- maintenance.backupManager
	backupMethod OBJECT-TYPE
		SYNTAX      INTEGER {
		 	tftp(1)
		}
		MAX-ACCESS  read-write
		STATUS      current
		DESCRIPTION	"The method of backup file"
		::= { backupManager 1 }
