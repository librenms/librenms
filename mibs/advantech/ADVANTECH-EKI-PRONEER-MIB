			sessionId			Integer32,
			monitorSessionState	INTEGER,
			destinationPort		Integer32,
			ingressState		INTEGER,
			sourceTxPort		BITS,
			sourceRxPort		BITS
		}		
		
		sessionId	OBJECT-TYPE
			SYNTAX		Integer32 (1..4)
			MAX-ACCESS	read-only
			STATUS		current
			DESCRIPTION "Mirror session ID"
			::= { mirrorEntry 1 }	 
