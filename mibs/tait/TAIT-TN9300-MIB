-- the compliance specifications
tn9300ComplianceV1 MODULE-COMPLIANCE
   STATUS current
   DESCRIPTION "The implementation requirements for the Tait TN9300 MIB"
   MODULE -- This Module
      MANDATORY-GROUPS {
         tn9300StatusGroup,
         tn9300SiteGroup,
         tn9300ChannelGroup,
         tn9300SipLineGroup,
         tn9300DipLineGroup,
         tn9300Mpt1327Group,
         tn9300Mpt1327ChGroup,
         tn9300RemoteNodeGroup,
         tn9300EventGroup,
         tn9300MobileIpGroup,
         tn9300UnitGroup,
         tn9300EventObjectGroup,
         tn9300EventGroupV2,
         tn9300NetworkGwGroup
      }
      ::= { tn9300Compl 1}

