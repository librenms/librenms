      tn9300RemoteNodeState RemoteNodeState,
      tn9300RemoteNodeCallSw Gauge32,
      tn9300RemoteNodeConnectSw Gauge32,
      tn9300RemoteNodeSynced RemoteNodeSyncState
   }

