	                              ::= { mstpPortEntry 18 }
	
	mstpPortTxTcnBpduCounter      OBJECT-TYPE
	                              SYNTAX      BpduCounter
	                              MAX-ACCESS  read-only
	                              STATUS      current
	                              DESCRIPTION
	                                "Transmitted Topology Change Notification (TCN) Messages."
	                              ::= { mstpPortEntry 19 }
	
	mstpPortTxCfgBpduCounter      OBJECT-TYPE
	                              SYNTAX      BpduCounter
	                              MAX-ACCESS  read-only
	                              STATUS      current
	                              DESCRIPTION
