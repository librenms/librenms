            f3NetFlowPointIndex, f3NetFpQosPolicerIndex }
    ::= { f3NetFpQosPolicerTable 1 }

F3NetFpQosPolicerEntry ::= SEQUENCE {
    f3NetFpQosPolicerIndex                 Integer32,

    f3NetFpQosPolicerAdminState            AdminState,
