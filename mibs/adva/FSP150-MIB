    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Enable CP'S dasaswap"
    ::= { fsp150IfConfigEntry 7 }

fsp150IfConfigTerminalLoopback OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Enable CP'S terminalloopback"
    ::= { fsp150IfConfigEntry 8 }

