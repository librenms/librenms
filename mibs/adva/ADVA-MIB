  DESCRIPTION
        "OTU Trace Identifier Mismatch (TIM) detection/action for TIM defect."
  SYNTAX    INTEGER
        {
            undefined(0),
            disabled(1),
            enableAisDisabled(2),
            enableAisEnabled(3)
        }

