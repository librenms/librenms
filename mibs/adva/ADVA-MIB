  STATUS        current
  DESCRIPTION
        "OTU Trace Identifier Mismatch (TIM) detection/action for TIM defect."
  SYNTAX    BITS
        {
            capUndefined(0),
            capDisabled(1),
            capEnableAisDisabled(2),
            capEnableAisEnabled(3)
        }

