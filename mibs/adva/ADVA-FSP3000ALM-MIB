                        highResolution (5),
                        cabinetSensorShort (6),
                        cabinetSensorLong (7),
                        dpaCustom (8),
                        dpaFast (9),
                        dpaBalanced (10) ,
                        dpaHighSensitivity (11)
      }
      MAX-ACCESS    read-only
      STATUS        current
      DESCRIPTION
