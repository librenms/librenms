    SYNTAX      INTEGER {
        disabled (1),  -- ssh server password interactive disabled
        enabled (2)  -- default value, ssh server password interactive  enabled
    }
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
         "Ssh server configuration for password interactive."
::= { access 3 }

