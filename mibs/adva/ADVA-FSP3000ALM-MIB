    SYNTAX    INTEGER {
        local0(0),
        local1(1),
        local2(2),
        local3(3),
        local4(4),
        local5(5),
        local6(6),
        local7(7)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This value is the default facility ALM labels its syslog messages."
    ::= { eventLogs 8 }

