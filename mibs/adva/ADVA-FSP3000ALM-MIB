    SYNTAX      INTEGER {
        disabled (1),  -- netconf server disabled
        enabled (2)  -- default value, netconf server enabled
    }
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Netconf server configuration."
::= { access 2 }

