    SYNTAX      INTEGER { v1(1), v2(2), v3(3) }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The default is v2(2)."
    ::=  { ipv6TrapsinkEntry 6 }

