        f3NtpAccPortFlowPointThresholdInterval CmPmIntervalType,
        f3NtpAccPortFlowPointThresholdVariable VariablePointer,
        f3NtpAccPortFlowPointThresholdValueLo Unsigned32,
        f3NtpAccPortFlowPointThresholdValueHi Unsigned32,
        f3NtpAccPortFlowPointThresholdMonValue PerfCounter64
    }


