              f3Gps10MhzPortGroup, neTMParamsGroup,
              
              cmEthernetTrafficPortJdsuExtGroup, f3FpQosShaperGroup,
              f3FpQosPolicerGroup, cpdV2TrafficPortGroup,
              
              f3AclRuleGroup
        }
    ::= { cmFacilityCompliances 1 }

