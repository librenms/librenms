	--1.3.6.1.4.1.40297.1.2.1.2
	::= { rptRealTimeInfo 2 }

rptVoltage OBJECT-TYPE
	SYNTAX  OCTET STRING(SIZE(4))
	MAX-ACCESS read-only
	STATUS  mandatory
	DESCRIPTION
		"The voltage.
		 It should be changed to float format.
