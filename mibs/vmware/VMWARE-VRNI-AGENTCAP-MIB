               vmwNetworkInsightNotificationGroup7,
               vmwNetworkInsightNotificationGroup8,
               vmwNetworkInsightNotificationGroup9,
               vmwNetworkInsightNotificationGroup10
            }
    ::= { vmwVRNICapability 16 }

