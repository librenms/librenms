	shdslPort		OBJECT IDENTIFIER ::= { port 3 }
	pvc				OBJECT IDENTIFIER ::= { port 4 }
	ppvc			OBJECT IDENTIFIER ::= { port 5 }
	portOperation	OBJECT IDENTIFIER ::= { port 6 }
