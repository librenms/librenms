--      ::= { adslPortBatchSet 21 }
--
--      adslL0toL2RateForBatchSet  OBJECT-TYPE
--      SYNTAX INTEGER
--	UNITS "kbps"
--      MAX-ACCESS read-write
--      STATUS current
--      DESCRIPTION
--	"configured transition L0 to L2 rate.
--	 (<=minl2rate/2 and >= 16 Kbps, For CTLM solution, 0 for system assign; for BCM solution, default:16.)"
--      ::= { adslPortBatchSet 22 }

