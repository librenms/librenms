        		utc_plus_0500(18),
        		utc_plus_0600(19),
        		utc_plus_0700(20),
        		utc_plus_0800(21),
        		utc_plus_0900(22),
        		utc_plus_1000(23),
        		utc_plus_1100(24),
        		utc_plus_1200(25),
        		utc_minus_0330(26),
        		utc_plus_0330(27),
        		utc_plus_0430(28),
        		utc_plus_0530(29),
        		utc_plus_0545(30),
        		utc_plus_0630(31),
        		utc_plus_0930(32),
        		utc_plus_1300(33)
		}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"The system time zone."
	::= { timeSetup 5 }

