	::= { igmpStats 11 }
	
	igmpQueryDropCnt OBJECT-TYPE
	SYNTAX Counter32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"The total number of IGMP query messages dropped."
	::= { igmpStats 12 }
	
	igmpReportDropCnt OBJECT-TYPE
