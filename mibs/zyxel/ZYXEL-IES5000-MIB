			e1000BaseT(2),
			e1000BaseLX(3),
			e1000BaseSX(4),
			e100BaseFX(5), 
			e100BaseTX(6),
			e1000BaseGBIC(7),
			e10000BaseGBIC(8)			
		}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
