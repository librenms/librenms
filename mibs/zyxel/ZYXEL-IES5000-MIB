        ::= { h248ProfileEntry 11 } 
        
        h248ProfileSoftswitch OBJECT-TYPE
        SYNTAX INTEGER {
        		defval(1),
        		zxss10_ss(2),
        		nortel_cs1500(3),
        		nortel_cs2000(4),
        		huawei_soft3000(5),
        		alu_5020(6),
        		nsn(7),
        		iskratel(8),
        		alu_5060(9)
        	}
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
