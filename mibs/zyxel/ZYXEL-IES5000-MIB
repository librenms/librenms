	::= { adslLinePerfEntry 7 }
	
	adslLinePerfAturFecInterleave OBJECT-TYPE
	SYNTAX Counter32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"The total number of FEC errors in interleave mode."
	::= { adslLinePerfEntry 8 }
					
        adslLinePerfAtucEs OBJECT-TYPE
