        MAX-ACCESS      read-write
        STATUS          obsolete
        DESCRIPTION
            "Alternative busy tone timing tolerance (%)."
        ::= { potsIfFxoEntry 20 }

    potsIfFxoDialOutMode OBJECT-TYPE
        SYNTAX          INTEGER { pulse(1), dtmf(2) }
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "Dial-out mode (1: pulse, 2: DTMF)."
        DEFVAL          { dtmf }
        ::= { potsIfFxoEntry 21 }

    potsIfFxoDialOutDelay OBJECT-TYPE
        SYNTAX          INTEGER
        UNITS           "ms"
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
