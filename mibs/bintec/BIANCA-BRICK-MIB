		timeZoneIndex
	    }
        ::= { timeZoneTable 1 }

	TimeZoneEntry ::=
	    SEQUENCE {
		timeZoneIndex			INTEGER,
		timeZoneName			DisplayString,
		timeZoneAlias			DisplayString,
		timeZoneOffset			INTEGER
	    }

