wlcPresenceProfileTable OBJECT-TYPE
    SYNTAX SEQUENCE OF WlcPresenceProfileEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Contains the global configuration of the Presence Analytics daemon."
    ::= { wlc 39 }

    wlcPresenceProfileEntry OBJECT-TYPE
        SYNTAX WlcPresenceProfileEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
        ""
        INDEX { wlcPresenceProfileIndex }
    ::= { wlcPresenceProfileTable 1 } 

    WlcPresenceProfileEntry ::= 
        SEQUENCE {
            wlcPresenceProfileIndex			INTEGER,
            wlcPresenceProfileAdminStatus		INTEGER,
            wlcPresenceProfileOrgSecret			DisplayString,
            wlcPresenceProfileCollectionInterval	INTEGER,
            wlcPresenceProfileMaxElem			INTEGER,
            wlcPresenceProfileDebugLevel		INTEGER
        }

    wlcPresenceProfileIndex OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Unique index."
    ::= { wlcPresenceProfileEntry 1 }

    wlcPresenceProfileAdminStatus OBJECT-TYPE
        SYNTAX INTEGER { enabled(1), disabled(2), delete(3) }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Activates or deactivates Presence Analytics daemon. Note that
	     Presence Analytics must be enabled per-interface by means
             of 'wlcWlanIfPresence' field."
        DEFVAL { disabled }
    ::= { wlcPresenceProfileEntry 2 }

    wlcPresenceProfileOrgSecret OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Customer-specific secret. This secret is used to anonymize
             the client MAC addresses as part of the cryptographic
             mechanism"
    ::= { wlcPresenceProfileEntry 3 }

    wlcPresenceProfileCollectionInterval OBJECT-TYPE
        SYNTAX INTEGER (5..86400)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "The interval in seconds to contact the collection servers."
        DEFVAL { 30 }
    ::= { wlcPresenceProfileEntry 4 }

    wlcPresenceProfileMaxElem OBJECT-TYPE
        SYNTAX INTEGER (256..65535)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Maximum number of probes kept in memory. When the buffer
             is full, the oldest entry is deleted."
        DEFVAL { 512 }
    ::= { wlcPresenceProfileEntry 5 }

    wlcPresenceProfileDebugLevel OBJECT-TYPE
        SYNTAX INTEGER { quiet (1), normal(2), verbose(3) }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Defines the verbosity of the debugging utility for this daemon.
             Level 'verbose' shows the maximum amount of debugging information
	     that can be generated by the daemon."
        DEFVAL { normal }
    ::= { wlcPresenceProfileEntry 6 } 

