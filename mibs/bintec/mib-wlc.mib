-- **********************************************************************

wlcPresenceServerTable OBJECT-TYPE
    SYNTAX SEQUENCE OF WlcPresenceServerEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Contains all collection servers defined by the user."
    ::= { wlc 40 }

    wlcPresenceServerEntry OBJECT-TYPE
        SYNTAX WlcPresenceServerEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
        ""
        INDEX { wlcPresenceServerIndex }
    ::= { wlcPresenceServerTable 1 }

    WlcPresenceServerEntry ::=
        SEQUENCE {
            wlcPresenceServerIndex	INTEGER,
            wlcPresenceServerProfIndex	INTEGER,
            wlcPresenceServerAddress	DisplayString,
            wlcPresenceServerSecret	DisplayString,
            wlcPresenceServerValidator	DisplayString,
            wlcPresenceServerRowStatus	RowStatus
        }

    wlcPresenceServerIndex OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Unique index of the server."
    ::= { wlcPresenceServerEntry 1 }

    wlcPresenceServerProfIndex OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Index of the 'wlcPresenceProfile' entry used for this server."
    ::= { wlcPresenceServerEntry 2 }

    wlcPresenceServerAddress OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Collection server address. The server address starts with
             'http' or 'https', providing this last option security
             mechanisms, followed by an IP address or a domain name with
             an optional port number separated by a colon. An aditional
             resource address can be appended at the end of this field."
    ::= { wlcPresenceServerEntry 3 }

    wlcPresenceServerSecret OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Secret string used by the collection server for accepting
             incoming messages."
    ::= { wlcPresenceServerEntry 4 }

    wlcPresenceServerValidator OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Organization-specific validator string. The one provided
             by the collection server must match this one."
    ::= { wlcPresenceServerEntry 5 }

    wlcPresenceServerRowStatus OBJECT-TYPE
        SYNTAX RowStatus
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "The row-status of a server entry."
        DEFVAL { createAndWait }
    ::= { wlcPresenceServerEntry 6 }

-- **********************************************************************
-- *    wlcRateControlTable  TABLE
-- **********************************************************************

    wlcRateControlTable OBJECT-TYPE
        SYNTAX SEQUENCE OF WlcRateControlEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Allows to define a 'Rate Control' that applies to a VSS 
            profile"
    ::= { wlc 41 }

    wlcRateControlEntry OBJECT-TYPE
        SYNTAX WlcRateControlEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
        ""
        INDEX { wlcRateControlIndex }
    ::= { wlcRateControlTable 1 }

    WlcRateControlEntry ::=
        SEQUENCE {
            wlcRateControlIndex INTEGER,
            wlcRateControlSupported24Ghz BITS,
            wlcRateControlBasic24Ghz BITS,
            wlcRateControlSupported5Ghz BITS,
            wlcRateControlBasic5Ghz BITS,
            wlcRateControlRowStatus RowStatus
        }

    wlcRateControlIndex OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "Reference to the associated VSS profile entry."
    ::= { wlcRateControlEntry 1 }

    wlcRateControlSupported24Ghz OBJECT-TYPE
        SYNTAX BITS {
            s1mbit(0),
            s2mbit(1),
            s5mbit(2),
            s6mbit(3),
            s9mbit(4),
            s11mbit(5),
            s12mbit(6),
            s18mbit(7),
            s24mbit(8),
            s36mbit(9),
            s48mbit(10),
            s54mbit(11)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
             "Defines the list in Mb/s of non-HT data rates at which the
              stations may transmit data in the 2,4 Ghz band."
        DEFVAL { {
            s1mbit,
            s2mbit,
            s5mbit,
            s6mbit,
            s9mbit,
            s11mbit,
            s12mbit,
            s18mbit,
            s24mbit,
            s36mbit,
            s48mbit,
            s54mbit
        } }
    ::= { wlcRateControlEntry 2 }

    wlcRateControlBasic24Ghz OBJECT-TYPE
        SYNTAX BITS {
            s1mbit(0),
            s2mbit(1),
            s5mbit(2),
            s6mbit(3),
            s9mbit(4),
            s11mbit(5),
            s12mbit(6),
            s18mbit(7),
            s24mbit(8),
            s36mbit(9),
            s48mbit(10),
            s54mbit(11)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "Defines the subset of the basic rates that are announced by
            the AP in the 2,4 Ghz band. For each basic rate, the
            corresponding supported rate must be present. The lowest
            value of this list will be used as the multicast and broadcast 
            frame rates."
        DEFVAL { {
            s1mbit,
            s2mbit,
            s5mbit,
            s11mbit
        } }
    ::= { wlcRateControlEntry 3 }

    wlcRateControlSupported5Ghz OBJECT-TYPE
        SYNTAX BITS {
            s6mbit(0),
            s9mbit(1),
            s12mbit(2),
            s18mbit(3),
            s24mbit(4),
            s36mbit(5),
            s48mbit(6),
            s54mbit(7)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
             "Defines the list in Mb/s of non-HT data rates at which the
              stations may transmit data in the 5 Ghz band."
        DEFVAL { {
            s6mbit,
            s9mbit,
            s12mbit,
            s18mbit,
            s24mbit,
            s36mbit,
            s48mbit,
            s54mbit
        } }
    ::= { wlcRateControlEntry 4 }

    wlcRateControlBasic5Ghz OBJECT-TYPE
        SYNTAX BITS {
            s6mbit(0),
            s9mbit(1),
            s12mbit(2),
            s18mbit(3),
            s24mbit(4),
            s36mbit(5),
            s48mbit(6),
            s54mbit(7)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "Defines the subset of the basic rates that are announced by
            the AP in the 5 Ghz band. For each basic rate, the
            corresponding supported rate must be present. The lowest
            value of this list will be used as the multicast and broadcast 
            frame rates."
        DEFVAL { {
            s6mbit,
            s12mbit,
            s24mbit
        } }
    ::= { wlcRateControlEntry 5 }

    wlcRateControlRowStatus OBJECT-TYPE
        SYNTAX RowStatus
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "The current status of row."
        DEFVAL { createAndWait }
    ::= { wlcRateControlEntry 6 }

-- **********************************************************************
-- *    End of wlcRateControlTable  TABLE
-- **********************************************************************

END
