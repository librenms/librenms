    ::= { wlcWTPEntry 3 }
     
    wlcWTPMacAddress OBJECT-TYPE
        SYNTAX MacAddress 
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
        "First ethernet MAC address of the WTP.
	"
    ::= { wlcWTPEntry 4 }
    
   wlcWTPDescription OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..63))
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Description of the requested WTP.
	    "
