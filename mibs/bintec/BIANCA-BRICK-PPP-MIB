		ms-stac(3), 
		mppc(4) 
	    } 
	    ACCESS  read-only
	    STATUS  mandatory

	    DESCRIPTION
		"The negotiated CCP compression mode."
	::= { pppSessionEntry 6 }

	pppSessionEncryption OBJECT-TYPE
	    SYNTAX  INTEGER { 
		none(1), 
		mppe-40(2), 
		mppe-128(3),
		des-56(4),
		triple-des-168(5),
		blowfish-168(6),
		mppe-56(7),
		mppev2-40(8),
		mppev2-56(9),
		mppev2-128(10),
		blowfish-56(11),
		mppev2-40-rfc3078(12),
		mppev2-56-rfc3078(13),
		mppev2-128-rfc3078(14)
	    } 
	    ACCESS  read-only
	    STATUS  mandatory

	    DESCRIPTION
		"The negotiated CCP encryption mode."
	::= { pppSessionEntry 7 }

	pppSessionCbcpMode OBJECT-TYPE
