	biboPPPLinkL1Protocol OBJECT-TYPE
	    SYNTAX INTEGER {
		data-64k(1),
		data-56k(2),
		modem(3),
		dovb(4),
		v110-1200(5),
		v110-2400(6),
		v110-4800(7),
		v110-9600(8),
		v110-14400(9),
		v110-19200(10),
		v110-38400(11),
                modem-profile-1(12),
