	ipSifStatTotalTcpSessions OBJECT-TYPE
	    SYNTAX  Counter
	    ACCESS  read-only
	    STATUS  mandatory
	    DESCRIPTION
	    	"Total number of monitored TCP sessions"
	::= { ipSifStat 7 } 

	ipSifStatTotalOtherSessions OBJECT-TYPE
	    SYNTAX  Counter
	    ACCESS  read-only
	    STATUS  mandatory
	    DESCRIPTION
	    	"Total number of monitored non-TCP/UDP sessions"
	::= { ipSifStat 8 } 

	ipSifStatTotalExpectedSessions OBJECT-TYPE
	    SYNTAX  Counter
	    ACCESS  read-only
	    STATUS  mandatory
	    DESCRIPTION
	    	"Total number of monitored non-TCP/UDP sessions"
	::= { ipSifStat 9 } 


END
