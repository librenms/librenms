mpsCallerNumberTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF MpsCallerNumberEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Blacklist for incoming calls / Call distribution by caller number."
    ::= { mpsNumber 7 }

    mpsCallerNumberEntry OBJECT-TYPE
    SYNTAX          MpsCallerNumberEntry 
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            ""
        INDEX { mpsCallerNumberIndex }
        ::= { mpsCallerNumberTable 1 }

    MpsCallerNumberEntry ::=
        SEQUENCE {
            mpsCallerNumberIndex                 Unsigned32,
            mpsCallerNumberFilter                DisplayString,
            mpsCallerNumberFilterDescr           DisplayString,
            mpsCallerNumberFilterType            INTEGER,
            mpsCallerNumberTarget                INTEGER,
            mpsCallerNumberTargetIndex           INTEGER,
            mpsCallerNumberAnnFileDir            DisplayString,
            mpsCallerNumberAdminStatus           INTEGER
        }

    mpsCallerNumberIndex OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "A unique value for each caller number entry."
        ::= { mpsCallerNumberEntry 1 }

    mpsCallerNumberFilter OBJECT-TYPE
        SYNTAX          DisplayString (SIZE(0..24))
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "Caller number filter (leading) digits.
             Ignored if FilterType is set to 'anonymous'."
        ::= { mpsCallerNumberEntry 2 }

    mpsCallerNumberFilterDescr OBJECT-TYPE
        SYNTAX          DisplayString
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "Description of this caller number filter."
        ::= { mpsCallerNumberEntry 3 }

    mpsCallerNumberFilterType OBJECT-TYPE
        SYNTAX          INTEGER { normal(1),
                                  anonymous(2)
                                }
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "Filter type of this caller number entry:
             normal:           verify 'normal provided' numbers.
             anonymous:        verify 'presentation not allowed' numbers."
        DEFVAL { normal }
        ::= { mpsCallerNumberEntry 4 }

    mpsCallerNumberTarget OBJECT-TYPE
        SYNTAX          INTEGER { none(0),
                                  group(1),
                                  extension(2),
                                  announcement(3)
                                }
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "The caller number call can be distributed to an internal target:
             none:             no target, reject the incoming call.
             group:            a group of internal extensions.
             extension:        one internal extension.
             announcement:     direct routing to an announcement."
        DEFVAL { none }
        ::= { mpsCallerNumberEntry 5 }

    mpsCallerNumberTargetIndex OBJECT-TYPE
        SYNTAX          INTEGER
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "This variable contains index of the target table, if
             mpsCallerNumberTarget=group       : index = mpsGroupIndex.
             mpsCallerNumberTarget=extension   : index = mpsExtensionIndex.
             For all other target types this variable is ignored."
        ::= { mpsCallerNumberEntry 6 }

    mpsCallerNumberAnnFileDir OBJECT-TYPE
        SYNTAX          DisplayString
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "Name of directory (on flash medium) which contains file to be used
			 for announcement.
             This variable references the entry in mpsWaveFile table where
			 variable mpsWaveFileDirectory has identical content.
			 This entry is only valid if variable mpsCallerNumberTarget is set
			 to value announcement."
        DEFVAL { "" }
        ::= { mpsCallerNumberEntry 7 }

    mpsCallerNumberAdminStatus OBJECT-TYPE
        SYNTAX          INTEGER { enabled(1),
                                  delete(2)
                                }
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "Just for MIB entry deletion."
        DEFVAL { enabled }
        ::= { mpsCallerNumberEntry 8 }

