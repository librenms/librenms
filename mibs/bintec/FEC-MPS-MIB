        ::= { mpsExtensionABEntry 1 }

    mpsExtensionABMode OBJECT-TYPE
        SYNTAX          INTEGER {
                            none(1),
                            telephone(2),
                            fax(3),
                            answering-machine(4),
                            modem(5),
                            multi-port(6),
                            moh(7),
                            tfe-123d12(8),
                            tfe-other(9),
                            tfe-doorline(10),
                            announcement(11),
                            voice-mail-system(12),
                            alarm-line(13),
                            tfe-adapter(14),
                            moh-anouncement(15),
