        DEFVAL { 300 }
        ::= { mpsVoIPConfig 5 }

    mpsVoIPConfigSipPort OBJECT-TYPE
        SYNTAX          INTEGER 
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "Standard value of the SIP port."
        DEFVAL { 5060 }
        ::= { mpsVoIPConfig 6 }

    mpsVoIPSupportedCodecs OBJECT-TYPE
        SYNTAX          BitValue
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "Bitmask for all codecs possibly supported by the system:
             ulaw       (1)
             alaw       (2)
             g729       (4)
             g726_24    (8)
             g726_16   (16)
             g726_32   (32)
             g726_40   (64)
             g728     (128)
             g723_63  (256)
             g723_53  (512)
             g729_e  (1024)
             gsm     (2048)
             dtmf    (4096)
             g722    (8192)
             ilbc_13(16384)
             ilbc_15(32768)
             data   (65536) 

             System default: G.711ulaw, G.711alaw, G.729, G.726-32, G.722 
             and DTMF outband."
        DEFVAL { 12327 }
        ::= { mpsVoIPConfig 7 }

    mpsVoIPConfigSipNatPort OBJECT-TYPE
        SYNTAX          INTEGER 
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "The external port mapped by NAT. For security reasons this might be
             different than the internal port (mpsVoIPConfigSipPort)."
        DEFVAL { 5060 }
        ::= { mpsVoIPConfig 8 }

