	    s36mbit(10),
	    s48mbit(11),
	    s54mbit(12),

	    sMCS0-HT(13),
	    sMCS1-HT(14),
	    sMCS2-HT(15),
	    sMCS3-HT(16),
	    sMCS4-HT(17),
	    sMCS5-HT(18),
	    sMCS6-HT(19),
	    sMCS7-HT(20),
	    sMCS8-HT(21),
	    sMCS9-HT(22),
	    sMCS10-HT(23),
	    sMCS11-HT(24),
	    sMCS12-HT(25),
            sMCS13-HT(26),
            sMCS14-HT(27),
            sMCS15-HT(28),

	    auto(31)
