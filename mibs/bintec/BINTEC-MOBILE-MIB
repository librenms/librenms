    mobileStatAction OBJECT-TYPE
        SYNTAX      INTEGER {
            idle(1),
            reset(2),
            qmi(3),
            dip(4),
            dhcprelay(5)
            }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
