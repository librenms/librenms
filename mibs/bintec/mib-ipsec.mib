    ::= { ipsecGlobals 45 }

ipsecGlobIkev2Enabled OBJECT-TYPE
    SYNTAX      INTEGER {
        true(1),        -- IKEv2 enabled
        false(2)        -- IKEv2 disabled
    }
    MAX-ACCESS  read-write
    STATUS      current
