    DEFVAL      { false }
    ::= { ipsecGlobals 31 }

ipsecGlobNoCrls OBJECT-TYPE
    SYNTAX      INTEGER {
        true(1),        -- do not send certificate revocation lists
        false(2)        -- send certificate revocation lists
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object specifies whether IKE should send certificate
