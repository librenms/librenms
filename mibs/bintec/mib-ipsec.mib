    SYNTAX      SEQUENCE OF IpsecInternalSubnetEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Lists to-be-reported attributes of type INTERNAL_IP4_SUBNET or
         INTERNAL_IP6_SUBNET (see section 3.15.2 of RFC-7296).
         Traffic-selector-negotiation or child-SA-allocation are NOT
         affected by these entries.
         Entries are relevant for IKEv2 only."
    ::= { ipsec 34 }

ipsecInternalSubnetEntry OBJECT-TYPE
    SYNTAX      IpsecInternalSubnetEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        ""
    INDEX {
        ipsecInternalSubnetPeerIndex,
        ipsecInternalSubnetAddressType,
        ipsecInternalSubnetAddress,
        ipsecInternalSubnetPrefixLength
    }
    ::= { ipsecInternalSubnetTable 1 }

IpsecInternalSubnetEntry ::=
    SEQUENCE {
        ipsecInternalSubnetPeerIndex        INTEGER,
        ipsecInternalSubnetDescription      DisplayString,
        ipsecInternalSubnetAddressType      InetAddressType,
        ipsecInternalSubnetAddress          InetAddress,
        ipsecInternalSubnetPrefixLength     InetAddressPrefixLength,
        ipsecInternalSubnetAdminStatus      INTEGER
    }

ipsecInternalSubnetPeerIndex OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Index of ipsecPeer that shall be informed about this subnet."
    ::= { ipsecInternalSubnetEntry 1 }

ipsecInternalSubnetDescription OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Optional human-readable description for this entry."
    ::= { ipsecInternalSubnetEntry 2 }

ipsecInternalSubnetAddressType OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Fields (AddressType, Address, PrefixLength) specify subnet for this
         entry. At present, following address-types are allowed:
           ipv4  -- Entry specifies INTERNAL_IP4_SUBNET attribute.
                    Allowed PrefixLength range is [0..32].
           ipv6  -- Entry specifies INTERNAL_IP6_SUBNET attribute.
                    Allowed PrefixLength range is [0..128].
         Any other values implicitly invalidate this entry."
    ::= { ipsecInternalSubnetEntry 3 }

ipsecInternalSubnetAddress OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "See description of AddressType field."
    ::= { ipsecInternalSubnetEntry 4 }

ipsecInternalSubnetPrefixLength OBJECT-TYPE
    SYNTAX      InetAddressPrefixLength
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "See description of AddressType field."
    ::= { ipsecInternalSubnetEntry 5 }

ipsecInternalSubnetAdminStatus OBJECT-TYPE
    SYNTAX      INTEGER {
        enabled(1),
        disabled(2),
        delete(3)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Activate/deactivate/delete this entry."
    DEFVAL      { enabled }
    ::= { ipsecInternalSubnetEntry 6 }

END
