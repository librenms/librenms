					INTEGER,
				sleEponOnuSipGlobalConfigIp
					IpAddress,
				sleEponOnuSipGlobalConfigNetmask
					IpAddress,
				sleEponOnuSipGlobalConfigGateway
					IpAddress,
				sleEponOnuSipGlobalUserName
					OCTET STRING,
				sleEponOnuSipGlobalPasswd
					OCTET STRING,
