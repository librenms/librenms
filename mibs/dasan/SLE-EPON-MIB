				sfu(4),
				niu10ge2plus(5),
				siuGe24(7),
				siuFe48(8),
				niu10ge2(9),
				siuGe48(10),
				iu10ge48(11),
				iu10ge8(12),
				iuGepon8(13),
				iu10gepon8(14),
				iu10ge44(15)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"slot type"
			::= { sleEponOltSlotEntry 2 }

		
