				{
				enable(1),
				disable(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Fault Managment for IETF shows enable or disable"
			DEFVAL { disable }
			::= { sleMplsTpOamFmInfoEntry 3 }

		
