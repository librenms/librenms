        rptrPortFrameSz64To127
                Counter,
        rptrPortFrameSz128To255
                Counter,
        rptrPortFrameSz256To511
                Counter,
        rptrPortFrameSz512To1023
                Counter,
        rptrPortFrameSz1024To1518
                Counter,
        rptrPortFrameSzGiant
                Counter
  }

