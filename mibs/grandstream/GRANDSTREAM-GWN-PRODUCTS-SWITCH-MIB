ErrDisableFeatureID ::= TEXTUAL-CONVENTION
    STATUS          current
    DESCRIPTION
        "An integer-value assigned to various features/events
		that can error disable a system entity"
    SYNTAX          INTEGER  {
						bpdu(1),
                        udld(2),
						selfLoop(3),
						bcastFlood(4),
						mcastFlood(5),
						ucastFlood(6),
						acl(7),
						portSec(8),
						dhcpRate(9),
						arpInspect(10),
						dhcpv6Rate(11),
						selfloopDetect(12)
                    }

