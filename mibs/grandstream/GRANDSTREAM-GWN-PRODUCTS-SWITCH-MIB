    gwnProducts
	FROM GRANDSTREAM-GWN-ROOT-MIB
	MODULE-IDENTITY,
	NOTIFICATION-TYPE,
	OBJECT-TYPE,
	Integer32,
	Counter32,
	Unsigned32
			FROM SNMPv2-SMI
	TEXTUAL-CONVENTION,
	DisplayString,
	TimeStamp,
	TruthValue,
	RowStatus,MacAddress
			FROM SNMPv2-TC
	MODULE-COMPLIANCE,
	OBJECT-GROUP,
	NOTIFICATION-GROUP
			FROM SNMPv2-CONF
	SnmpAdminString
			FROM SNMP-FRAMEWORK-MIB;


gwnSwitchMIB MODULE-IDENTITY
        LAST-UPDATED    "202312231201Z"
