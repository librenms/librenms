                         SYNTAX PositiveInteger
                         UNITS  "Minutes"
                         MAX-ACCESS read-only
                         STATUS current
                         DESCRIPTION
                         "BDT duration at the time
                          this test was run.
                         "
                         REFERENCE " "
                         ::= {vpwrBDTHistEntry 3}

     vpwrBDTHistAlarmVoltage OBJECT-TYPE
                         SYNTAX PositiveInteger 
                         UNITS  " *.01 Volts"
                         MAX-ACCESS read-write
                         STATUS current
                         DESCRIPTION
                         "BDT Alarm Voltage setpoint when
                          this test was run.
                         "
                         REFERENCE " "
                         ::= {vpwrBDTHistEntry 4}

       vpwrBDTHistAbortVoltage OBJECT-TYPE
                         SYNTAX PositiveInteger
                         UNITS  " *.01 Volts"
