ELTEK-BC2000-DC-POWER-MIB DEFINITIONS ::= BEGIN

    IMPORTS
         MODULE-IDENTITY 
       , OBJECT-TYPE 
       , NOTIFICATION-TYPE
       , OBJECT-IDENTITY 
       , Counter32
       , Gauge32 
       , Integer32 
       , enterprises 
       , IpAddress 
       -- , Gauge
            FROM SNMPv2-SMI 
           
         Gauge 
            FROM RFC1155-SMI

         DisplayString 
       , TimeStamp 
--        , TimeInterval 
--        , TestAndIncr
       , AutonomousType 
       , TEXTUAL-CONVENTION
            FROM SNMPv2-TC
           
