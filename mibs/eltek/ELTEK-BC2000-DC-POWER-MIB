BDTStartSourceTC ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION
      "The defines how was BDT started."
    SYNTAX       INTEGER {
                    bdtInactive(0),
                    bdtManualStart(1),
                    bdtSched01Start1(2),
                    bdtSched02Start2(3),
                    bdtSched03Start3(4),
                    bdtSched04Start4(5),
                    bdtSched05Start5(6),
                    bdtSched06Start6(7),
                    bdtSched07Start7(8),
                    bdtSched08Start8(9),
                    bdtSched09Start9(10),
                    bdtSched10Start10(11),
                    bdtSched11Start11(12),
                    bdtSched12Start12(13),
                    bdtACFailStart(14)
                  }
BDTResultTC ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION
      "The defines how was BDT started."
    SYNTAX       INTEGER {
                    bdtResultCleared(0),
                    bdtResultPassed(1),
                    bdtResultFailed(2),
                    bdtResultStopped(3),
                    bdtResultAborted(4),
                    bdtResultInProgress(5)
                  }


