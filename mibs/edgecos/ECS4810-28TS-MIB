    ecs4810-28tsTraps  OBJECT IDENTIFIER ::= { ecs4810-28tsNotifications 1 }
    ecs4810-28tsTrapsPrefix OBJECT IDENTIFIER ::= { ecs4810-28tsTraps 0 }

--      swPowerStatusChangeTrap NOTIFICATION-TYPE
