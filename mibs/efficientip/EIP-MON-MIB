eipSvcStatus OBJECT-GROUP
    OBJECTS { eipSvcSyslogStatus,
              eipSvcSshStatus,
              eipSvcApacheStatus,
              eipSvcIpmServerStatus,
              eipSvcDatabaseStatus,
              eipSvcDhcpStatus,
              eipSvcDhcpMsStatus,
              eipSvcDnsStatus,
              eipSvcGuardianStatus,
              eipSvcQuaggaStatus,
              eipSvcNtpStatus,
              eipSvcTftpStatus,
              eipSvcSnmpStatus,
              eipSvcSendmailStatus }
    STATUS  current
    DESCRIPTION
            "A collection of objects providing status info per service.
            0 : service is disabled
            1 : service is running
            2 : service is misconfigured (running but disabled)
            3 : service execution failed (enabled but not running)"
    ::= { eipSvc 201 }

eipSvcCpu OBJECT-GROUP
    OBJECTS { eipSvcSyslogCpu,
              eipSvcSshCpu,
              eipSvcApacheCpu,
              eipSvcIpmServerCpu,
              eipSvcDatabaseCpu,
              eipSvcDhcpCpu,
              eipSvcDhcpMsCpu,
              eipSvcDnsCpu,
              eipSvcGuardianCpu,
              eipSvcQuaggaCpu,
              eipSvcNtpCpu,
              eipSvcTftpCpu,
              eipSvcSnmpCpu,
              eipSvcSendmailCpu }
    STATUS  current
    DESCRIPTION
            "A collection of objects providing
            CPU usage per service (%)."
    ::= { eipSvc 202 }

eipSvcMem OBJECT-GROUP
    OBJECTS { eipSvcSyslogMem,
              eipSvcSshMem,
