        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Number of failed disks."
        ::= { eipHwRaid 5 }

eipHwRaidBbuStatus OBJECT-TYPE
        SYNTAX INTEGER { disabled(0), ok(1), degraded(2) }
        MAX-ACCESS read-only
        STATUS current
