    	SYNTAX          MplsLabel
    	MAX-ACCESS      read-only
    	STATUS          current
   		DESCRIPTION
        	""      
       	::= { ubiPwEntry 12 }       	   	
       	   
   	ubiPwActiveState OBJECT-TYPE
    	SYNTAX          INTEGER { 
    		none(0),  
    		primaryUp(1),
    		backupUp(2),
    		primaryActive(3),
    		backupActive(4)
    	}
    	MAX-ACCESS      read-only
    	STATUS          current
   		DESCRIPTION
        	""      
       	::= { ubiPwEntry 13 }

  	ubiPwRowStatus OBJECT-TYPE
    	SYNTAX          RowStatus
    	MAX-ACCESS      read-create
    	STATUS          current
   		DESCRIPTION
        	""      
       	::= { ubiPwEntry 14 }     
                         
    
                                     
