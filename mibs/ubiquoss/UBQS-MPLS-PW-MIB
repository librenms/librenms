-- ubiPwGlobal
-- ***********************************************************
 	ubiPwPlatformNodeType OBJECT-TYPE
    	SYNTAX          INTEGER {   
    		none(0),
    		cot(1),
    		int(2),
    		rt(3)
    	}
    	MAX-ACCESS      read-write
    	STATUS          current    
    	DESCRIPTION
        	""
	   	::= { ubiPwGlobal 1 }   

