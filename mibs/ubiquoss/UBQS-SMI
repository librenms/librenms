	MODULE-IDENTITY,
	OBJECT-IDENTITY,
	enterprises
		FROM SNMPv2-SMI;

ubiquoss MODULE-IDENTITY
	LAST-UPDATED "0607130000Z"
	ORGANIZATION "Ubiquoss Corp."
	CONTACT-INFO
		"	Ubiquoss
			Customer Service

		Postal: 24F Milennium B/D,
