	ubiOntSwitchMIBNotificationPrefix OBJECT IDENTIFIER ::= { ubiOntSwitchMIB 2 }
	ubiOntSwitchMIBNotifications OBJECT IDENTIFIER ::= { ubiOntSwitchMIBNotificationPrefix 0 }
                                    
