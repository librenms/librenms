-- ***********************************************************
-- igmpVlanTable
-- ***********************************************************
		igmpVlanTable OBJECT-TYPE
			SYNTAX SEQUENCE OF IgmpVlanEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Description."
			::= { ubiPonMacMIBObjects 7 }
		
		igmpVlanEntry OBJECT-TYPE
			SYNTAX IgmpVlanEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Description."
			INDEX { ubiPortIndex }            -- TODO 
			::= { igmpVlanTable 1 }
		
		IgmpVlanEntry ::=
			SEQUENCE { 
				fcfsPoolSize
					Integer32
