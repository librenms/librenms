ltp8xObjectGroup OBJECT-GROUP
			OBJECTS 
			{
				ltp8xPONChannelSlot,
				ltp8xPONChannelID,
				ltp8xPONChannelState,
				ltp8xPONChannelONTCount,
				ltp8xPONChannelEnabled,
				
				ltp8xONTSlot,
				ltp8xONTSerial,
				ltp8xONTStateChannel,
