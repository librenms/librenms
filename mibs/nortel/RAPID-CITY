        rcKhiForwardingRofIfpForwIndex          Integer32,
        rcKhiForwardingRofIfpForwName           DisplayString,
        rcKhiForwardingRofIfpForwStatsPorts     DisplayString,
        rcKhiForwardingRofIfpForwStatsCnt       Counter64
     }


