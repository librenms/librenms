        SYNTAX        INTEGER { 
                        none(1),
                        msdp(2),
                        static(3)
                      }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "msdp -  if the owner is MSDP;
                        static - otherwise"
    ::= { rcIpSpbPimGwGatewayForeignSrcEntry 9 }

