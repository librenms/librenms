        SYNTAX        INTEGER {
                          none(1),
                          ipv4Offset(2),
                          ipv6Offset(3)
                      }
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION   "Using this parameter, Ipv4 + TCP/UDP header or Ipv6 + 
                       TCP/UDP header can be Offset from MACSec Encryption"
        DEFVAL        { none }
        ::= { rcMACSecIfConfigEntry 2 }

