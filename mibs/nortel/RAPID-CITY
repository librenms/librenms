        SYNTAX        INTEGER { active(1),
	                        inactive(2),
				fail(3) }
	MAX-ACCESS    read-only
	STATUS        current
        DESCRIPTION   "State of IPv6 static route entry."
	::= { rcNlsMgmtIpv6StaticRouteEntry 8 }

