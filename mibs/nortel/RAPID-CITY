        SYNTAX        RowStatus
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Used to create/delete IPv6 static routes in the Table."
        ::= { rcNlsMgmtIpv6StaticRouteEntry 4 }

