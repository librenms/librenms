        DESCRIPTION   "pre notification interval 1."
        ::= { rcCli 39 }

rcCliPasswordPreNotificationInterval2 OBJECT-TYPE
        SYNTAX        Integer32 (1..99)
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION   "pre notification interval 2."
        ::= { rcCli 40 }

