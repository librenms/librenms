                          oob1(1),
                          oob2(2),
                          clip(3),
                          vlan(4)
                      }
	MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Instance id of IPv6 neighbor entry."
	::= { rcNlsMgmtIpv6NeighborEntry 2 }

