    SYNTAX        INTEGER {
                      ifpPt(1),
                      ifpPcv(2)
                  }
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION   "Type of table IFP PT and IFP PCV for ROF cards"
    ::= { rcKhiForwardingRofIfpForwEntry 4 }

