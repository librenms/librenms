        SYNTAX        DisplayString (SIZE (0..10))
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Mgmt IP interface name, mgmt-<instance-id> 
                       of IPv6 neighbor entry."
        ::= { rcNlsMgmtIpv6NeighborEntry 3 }

