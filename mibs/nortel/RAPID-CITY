      SYNTAX        Integer32 (1..2)
      MAX-ACCESS    not-accessible
      STATUS        current
      DESCRIPTION   "The Index of USB"
      ::= { rcVossUsbEntry 1 }
rcVossUsbStatus OBJECT-TYPE
      SYNTAX        INTEGER {
                              present(1),
                              notPresent(2)
                            }
      MAX-ACCESS    read-only
      STATUS        current
      DESCRIPTION   "The present status of the USB storage"
      ::= { rcVossUsbEntry 2 }

