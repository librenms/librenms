        SYNTAX        OCTET STRING (SIZE (6))
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "System ID of the node selected as the gateway for this
                       foreing source."
    ::= { rcIpSpbPimGwGatewayForeignSrcEntry 5 }

