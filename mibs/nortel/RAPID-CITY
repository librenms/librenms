        SEQUENCE {
	  rcDvrL3vsnVrfId      Integer32, 
	  rcDvrL3vsnIsid       Integer32,
	  rcDvrL3vsnVrfName    DisplayString,
	  rcDvrL3vsnInjectDefaultRouteDisable TruthValue
	}

