        SYNTAX        DisplayString (SIZE (5..15))
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION   "Name of Connectivity Association attached to the port/interface"
        ::= { rcMACSecIfConfigEntry 1 }

