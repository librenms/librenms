	rcKhiForwardingRofZagPmmEgrUcPageCnt			Counter32,
	rcKhiForwardingRofZagPmmEgrUcPeakPageCnt		Counter32,
	rcKhiForwardingRofZagPmmEgrUcMaxLmt			Counter32,
