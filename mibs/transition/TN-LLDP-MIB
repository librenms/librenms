        "Action"
    ::= { tnLldpConfigMedPolicyRowEditor 100 }

tnLldpStatus OBJECT IDENTIFIER
    ::= { tnLldpMibObjects 3 }

tnLldpStatusStatistics OBJECT IDENTIFIER
    ::= { tnLldpStatus 1 }

tnLldpStatusStatisticsGlobalCounters OBJECT IDENTIFIER
    ::= { tnLldpStatusStatistics 1 }

