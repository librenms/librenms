                INTEGER,
            staLrmPFrame
                INTEGER,
            staLrmPLos
                OkValue,
            staLrmPAis
                OkValue,
            staLrmPLof
                OkValue,
            staLrmPBpv
                OkValue,
            staLrmPSlip
                OkValue,
            staLrmPSlips
                Counter32,
            staLrmPLosSide2
                OkValue,
            staLrmPBpvTestTime
                Counter32,
            staLrmPBpv60SRate
                DisplayString,
            staLrmPBpv24HRate
                DisplayString,
            staLrmPBpvErrSecs
                Counter32,
            staLrmPBpvSevErrSecs
                Counter32,
            staLrmPBpvSevErrRatio
                DisplayString
        }

