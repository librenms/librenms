    ::= { setJccoTable 1 }

    SetJccoEntry ::=
        SEQUENCE {
            setJccoChassis
                INTEGER,    
            setJccoSlot
                INTEGER,    
            setJccoBypass
                OnValue,    
            setJcco400Hz
                OnValue,    
            setJccoEnable
                OCTET STRING,   
            setJccoComp
                OCTET STRING    
        }

    setJccoChassis OBJECT-TYPE
        SYNTAX  INTEGER (1..5)
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
