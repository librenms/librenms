                OCTET STRING,   
            setE12048oSquelch
                OCTET STRING,   
            setE12048oFrame
                INTEGER,    
            setE12048oCrc
                OnValue,    
            setE12048oSsm
                OnValue,    
            setE12048oSsmBit
                INTEGER 
        }

    setE12048oChassis OBJECT-TYPE
        SYNTAX  INTEGER (1..5)
        MAX-ACCESS  not-accessible
        STATUS  current
