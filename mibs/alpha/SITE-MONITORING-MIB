              ::= { es1dc1ConfigTable 1 }
Es1dc1ConfigEntry ::=
             SEQUENCE{
             es1dc1ConfigIndex       Unsigned32,
             es1dc1ConfigName        DisplayString,
             es1dc1ConfigValue       DisplayString,
              es1dc1ConfigEntryStatus  RowStatus
             }
