        ENTERPRISE      upsTrapGroup
        VARIABLES {
        upsBatteryGroupTemperature,
        upsConfigGroupOverTemperatureSetPoint
        }
        DESCRIPTION
                "SEVERE: The UPS temperature is too high."
        ::= 22

