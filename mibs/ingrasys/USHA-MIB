        ENTERPRISE      upsTrapGroup
        VARIABLES {
        upsOutputGroupPercentLoad,
        upsConfigGroupOverLoadSetPoint
        }
        DESCRIPTION
                "SEVERE: The UPS is overload."
        ::= 24

