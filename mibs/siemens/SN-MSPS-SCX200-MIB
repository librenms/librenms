        DEFVAL  {disable }
          ::= { snMspsEthernetIp 20 } 

       snMspsEtherNetIPDlrDscpTagStatus OBJECT-TYPE
   		SYNTAX       INTEGER {
         	    	enable(1),
            	    disable(2)
               }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
           "This object informs whether the entry is active or inactive"
        DEFVAL  {disable }
          ::= { snMspsEthernetIp 22 } 
          
       snMspsEtherNetIPDlrDscpUrgent OBJECT-TYPE
