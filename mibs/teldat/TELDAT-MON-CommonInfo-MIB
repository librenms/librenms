                INTEGER,
            telProdNpMonInterfGeneralVect
                INTEGER,
            telProdNpMonInterfGeneralTestvalid
                Counter,
            telProdNpMonInterfGeneralTestfailure
                Counter,
            telProdNpMonInterfGeneralMaintenFailure
                Counter
            }

