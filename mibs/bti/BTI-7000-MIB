-----------------------------------------------------------------------
-- Objects used to retrieve current provisioned circuit pack
-- Performance Monitoring statistics
-----------------------------------------------------------------------


cpCrntPMTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF CpCrntPMEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "A table of current performance monitoring statistics collected
                  for the circuit packs on the NE."
    ::= { performance 49 }


cpCrntPMEntry OBJECT-TYPE
    SYNTAX       CpCrntPMEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The row entry object of the equipment current
                  performance monitoring table. Each row of this table contains
                  a full set of PM statistics and related data for a current
                  measurement interval for a given circuit pack."
    INDEX        { cpCrntPMCpTypeIdx,
                   cpCrntPMShelfIdx,
                   cpCrntPMSlotIdx,
                   cpCrntPMIntervalTypeIdx }
    ::= { cpCrntPMTable 1 }


CpCrntPMEntry ::= SEQUENCE { cpCrntPMCpTypeIdx               CpType,
                             cpCrntPMShelfIdx                INTEGER,
                             cpCrntPMSlotIdx                 INTEGER,
                             cpCrntPMIntervalTypeIdx         PMIntervalType,
                             cpCrntPMTempValue               INTEGER,
                             cpCrntPMTempTimeStamp           DateAndTime,
                             cpCrntPMTempValidity            PMValidity }


cpCrntPMCpTypeIdx OBJECT-TYPE
    SYNTAX       CpType
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The type of the circuit pack targeted for PM retrieval."
    ::= { cpCrntPMEntry 1 }


cpCrntPMShelfIdx OBJECT-TYPE
    SYNTAX       INTEGER (1|11|21|31)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The number of the shelf on which the circuit pack targeted for
                  PMs is provisioned."
    ::= { cpCrntPMEntry 2 }

cpCrntPMSlotIdx OBJECT-TYPE
    SYNTAX       INTEGER (1..20)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The number of the slot within which the circuit pack
                  targeted for PMs is provisioned."
    ::= { cpCrntPMEntry 3 }


cpCrntPMIntervalTypeIdx OBJECT-TYPE
    SYNTAX       PMIntervalType
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The type of PM bin according to interval length."
    ::= { cpCrntPMEntry 4 }


cpCrntPMTempValue OBJECT-TYPE
    SYNTAX       INTEGER (-32768..32767)
    UNITS        "degrees Celsius"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "The circuit pack temperature."
    ::= { cpCrntPMEntry 5 }


cpCrntPMTempTimeStamp OBJECT-TYPE
    SYNTAX       DateAndTime
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "The date and time stamp of the temperature value."
    ::= { cpCrntPMEntry 6 }


cpCrntPMTempValidity OBJECT-TYPE
    SYNTAX       PMValidity
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "Indicates the validity of the provided temperature
                  value."
    ::= { cpCrntPMEntry 7 }


-----------------------------------------------------------------------
-- Objects used to retrieve historical provisioned circuit pack
-- Performance Monitoring statistics
-----------------------------------------------------------------------


cpHistPMTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF CpHistPMEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "A table of historical performance monitoring statistics
                  collected for the circuit packs on the NE."
    ::= { performance 50 }


cpHistPMEntry OBJECT-TYPE
    SYNTAX       CpHistPMEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The row entry object of the circuit pack historical
                  performance monitoring table. Each row of this table contains
                  a full set of PM statistics and related data for a historical
                  measurement interval for a given circuit pack."
    INDEX        { cpHistPMCpTypeIdx,
                   cpHistPMShelfIdx,
                   cpHistPMSlotIdx,
                   cpHistPMIntervalTypeIdx,
                   cpHistPMIntervalIdx }
    ::= { cpHistPMTable 1 }


CpHistPMEntry ::= SEQUENCE { cpHistPMCpTypeIdx             CpType,
                             cpHistPMShelfIdx              INTEGER,
                             cpHistPMSlotIdx               INTEGER,
                             cpHistPMIntervalTypeIdx       PMIntervalType,
                             cpHistPMIntervalIdx           INTEGER,
                             cpHistPMTempValue             INTEGER,
                             cpHistPMTempTimeStamp         DateAndTime,
                             cpHistPMTempValidity          PMValidity }


cpHistPMCpTypeIdx OBJECT-TYPE
    SYNTAX       CpType
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The type of the circuit pack targeted for PM retrieval."
    ::= { cpHistPMEntry 1 }


cpHistPMShelfIdx OBJECT-TYPE
    SYNTAX       INTEGER (1|11|21|31)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The number of the shelf on which the circuit pack targeted for
                  PMs is provisioned."
    ::= { cpHistPMEntry 2 }


cpHistPMSlotIdx OBJECT-TYPE
    SYNTAX       INTEGER (1..20)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The number of the slot within which the circuit pack
                  targeted for PMs is provisioned."
    ::= { cpHistPMEntry 3 }


cpHistPMIntervalTypeIdx OBJECT-TYPE
    SYNTAX       PMIntervalType
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The type of PM bin according to interval length."
    ::= { cpHistPMEntry 4 }


cpHistPMIntervalIdx OBJECT-TYPE
    SYNTAX       INTEGER (1..96)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The specific PM bin number. The range is 1..96 for 15-minute
                  bins, and for 1-day historical bins, the value is 1."
    ::= { cpHistPMEntry 5 }


cpHistPMTempValue OBJECT-TYPE
    SYNTAX       INTEGER (-32768..32767)
    UNITS        "degrees Celsius"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "The circuit pack temperature."
    ::= { cpHistPMEntry 6 }


cpHistPMTempTimeStamp OBJECT-TYPE
    SYNTAX       DateAndTime
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "The date and time stamp of the temperature value."
    ::= { cpHistPMEntry 7 }


cpHistPMTempValidity OBJECT-TYPE
    SYNTAX       PMValidity
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "Indicates the validity of the provided temperature
                  value."
    ::= { cpHistPMEntry 8 }


-----------------------------------------------------------------------
-- Objects used to retrieve current GSeries Optical Amplifier
-- Performance Monitoring statistics
-- 
-- A listing of current GSeries optical amplifier PM values are contained in
-- the oaGSeriesCrntPMTable of the BTI 7000 Series MIB. When retrieved, this
-- table provides one row for each set of current GSeries optical amplifier
-- PM data. The following table lists the objects in each entry of the
-- oaGSeriesCrntPMTable.
-----------------------------------------------------------------------


oaGSeriesCrntPMTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF OaGSeriesCrntPMEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "A table of current performance monitoring statistics collected
                  for the GSeries optical amplifier facilities on the NE."
    ::= { performance 51 }


oaGSeriesCrntPMEntry OBJECT-TYPE
    SYNTAX       OaGSeriesCrntPMEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The row entry object of the GSeries optical amplifier current
                  performance monitoring table. Each row of this table contains
                  a full set of PM statistics and related data for a current
                  measurement interval for a given optical amplifier."
    INDEX        { oaGSeriesCrntPMOaTypeIdx,
                   oaGSeriesCrntPMShelfIdx,
                   oaGSeriesCrntPMSlotIdx,
                   oaGSeriesCrntPMOaIdx,
                   oaGSeriesCrntPMIntervalTypeIdx }
    ::= { oaGSeriesCrntPMTable 1 }


OaGSeriesCrntPMEntry ::= SEQUENCE { oaGSeriesCrntPMOaTypeIdx        OaType,
                             oaGSeriesCrntPMShelfIdx                INTEGER,
                             oaGSeriesCrntPMSlotIdx                 INTEGER,
                             oaGSeriesCrntPMOaIdx                   INTEGER,
                             oaGSeriesCrntPMIntervalTypeIdx         PMIntervalType,
                             oaGSeriesCrntPMOPRValue                FixedX10,
                             oaGSeriesCrntPMOPRTimeStamp            DateAndTime,
                             oaGSeriesCrntPMOPRValidity             PMValidity,
                             oaGSeriesCrntPMSSIOPRValue             FixedX10,
                             oaGSeriesCrntPMSSIOPRTimeStamp         DateAndTime,
                             oaGSeriesCrntPMSSIOPRValidity          PMValidity,
                             oaGSeriesCrntPMOPTValue                FixedX10,
                             oaGSeriesCrntPMOPTTimeStamp            DateAndTime,
                             oaGSeriesCrntPMOPTValidity             PMValidity,
                             oaGSeriesCrntPMMSInsLossValue          FixedX10,
                             oaGSeriesCrntPMMSInsLossTimeStamp      DateAndTime,
                             oaGSeriesCrntPMMSInsLossValidity       PMValidity,
                             oaGSeriesCrntPMEffectiveGainValue      FixedX10,
                             oaGSeriesCrntPMEffectiveGainTimeStamp  DateAndTime,
                             oaGSeriesCrntPMEffectiveGainValidity   PMValidity,
                             oaGSeriesCrntPMFSOOPTValue             FixedX10,
                             oaGSeriesCrntPMFSOOPTTimeStamp         DateAndTime,
                             oaGSeriesCrntPMFSOOPTValidity          PMValidity,
                             oaGSeriesCrntPMOBRValue                FixedX10,
                             oaGSeriesCrntPMOBRTimeStamp            DateAndTime,
                             oaGSeriesCrntPMOBRValidity             PMValidity,
                             oaGSeriesCrntPMOPRMinValue             FixedX10,
                             oaGSeriesCrntPMOPRMinTimeStamp         DateAndTime,
                             oaGSeriesCrntPMOPRMinValidity          PMValidity,
                             oaGSeriesCrntPMOPRMinInitialize        InitializeCmd,
                             oaGSeriesCrntPMOPRMaxValue             FixedX10,
                             oaGSeriesCrntPMOPRMaxTimeStamp         DateAndTime,
                             oaGSeriesCrntPMOPRMaxValidity          PMValidity,
                             oaGSeriesCrntPMOPRMaxInitialize        InitializeCmd,
                             oaGSeriesCrntPMOPRAvgValue             FixedX10,
                             oaGSeriesCrntPMOPRAvgTimeStamp         DateAndTime,
                             oaGSeriesCrntPMOPRAvgValidity          PMValidity,
                             oaGSeriesCrntPMOPRAvgInitialize        InitializeCmd,
                             oaGSeriesCrntPMOPTMinValue             FixedX10,
                             oaGSeriesCrntPMOPTMinTimeStamp         DateAndTime,
                             oaGSeriesCrntPMOPTMinValidity          PMValidity,
                             oaGSeriesCrntPMOPTMinInitialize        InitializeCmd,
                             oaGSeriesCrntPMOPTMaxValue             FixedX10,
                             oaGSeriesCrntPMOPTMaxTimeStamp         DateAndTime,
                             oaGSeriesCrntPMOPTMaxValidity          PMValidity,
                             oaGSeriesCrntPMOPTMaxInitialize        InitializeCmd,
                             oaGSeriesCrntPMOPTAvgValue             FixedX10,
                             oaGSeriesCrntPMOPTAvgTimeStamp         DateAndTime,
                             oaGSeriesCrntPMOPTAvgValidity          PMValidity,
                             oaGSeriesCrntPMOPTAvgInitialize        InitializeCmd,
                             oaGSeriesCrntPMTiltAchValue            FixedX10,
                             oaGSeriesCrntPMTiltAchTimeStamp        DateAndTime,
                             oaGSeriesCrntPMTiltAchValidity         PMValidity}


oaGSeriesCrntPMOaTypeIdx OBJECT-TYPE
    SYNTAX       OaType
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The type of the optical amplifier targeted for PM retrieval."
    ::= { oaGSeriesCrntPMEntry 1 }


oaGSeriesCrntPMShelfIdx OBJECT-TYPE
    SYNTAX       INTEGER (1|11|21|31)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The number of the shelf on which the amplifier targeted for
                  PMs is provisioned."
    ::= { oaGSeriesCrntPMEntry 2 }

oaGSeriesCrntPMSlotIdx OBJECT-TYPE
    SYNTAX       INTEGER (1..20)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The number of the slot within which the optical amplifier
                  targeted for PMs is provisioned."
    ::= { oaGSeriesCrntPMEntry 3 }


oaGSeriesCrntPMOaIdx OBJECT-TYPE
    SYNTAX       INTEGER (1..1)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The number of the amplifier port on which the circuit pack for
                  PMs is targeted."
    ::= { oaGSeriesCrntPMEntry 4 }


oaGSeriesCrntPMIntervalTypeIdx OBJECT-TYPE
    SYNTAX       PMIntervalType
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The type of PM bin according to interval length."
    ::= { oaGSeriesCrntPMEntry 5 }


oaGSeriesCrntPMOPRValue OBJECT-TYPE
    SYNTAX       FixedX10
    UNITS        "dBm/10"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "The optical power received on the input of the amplifer."
    ::= { oaGSeriesCrntPMEntry 6 }


