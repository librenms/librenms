        ::=                 { cmcIIISetupModbus 3 }

    cmcIIIModbusTable       OBJECT-TYPE
        SYNTAX              SEQUENCE OF CmcIIIModbusEntry
        MAX-ACCESS          not-accessible
        STATUS              current
        DESCRIPTION         "A list of Modbus entries."
        ::=                 { cmcIIISetupModbus 4 }

    cmcIIIModbusEntry       OBJECT-TYPE
        SYNTAX              CmcIIIModbusEntry
        MAX-ACCESS          not-accessible
