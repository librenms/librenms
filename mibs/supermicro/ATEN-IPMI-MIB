            -0230
            -0200
            -0100
            +0000
            +0100
            +0200
            +0300
            +0330
            +0400
            +0430
            +0500
            +0530
            +0545
            +0600
            +0630
            +0700
            +0800
            +0900
            +0930
            +1000
            +1030
            +1100
            +1130
            +1200
            +1300
