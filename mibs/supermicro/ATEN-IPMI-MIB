    SYNTAX      INTEGER { stateless(0) , stateful(1) }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "DHCPv6 State"
    ::= { network 5 }

hostname  OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(63))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "BMC hostname"
    ::= { network 6 }

