    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "the current Lan interface"
    ::= { network 10 }

rmcpPort  OBJECT-TYPE
    SYNTAX      Integer32 (1..65535)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "IPMI port number"
    ::= { network 11 }

activeLanInterface  OBJECT-TYPE
