    SEQUENCE {
        selEid                      Integer32,
        selTimeStamp                DateAndTime,
        selSensorName               OCTET STRING,
        selSensorType               OCTET STRING,
        selDescription              OCTET STRING,
        selRawData                  OCTET STRING
    }

