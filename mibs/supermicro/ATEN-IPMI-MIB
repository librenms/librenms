    SYNTAX      INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "VLan enabled"
    ::= { network 8 }

vlanID  OBJECT-TYPE
    SYNTAX      Integer32 (1..4095)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "Vlan ID"
    ::= { network 9 }

