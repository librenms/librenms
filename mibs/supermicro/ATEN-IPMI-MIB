    ::= { userInfo 4 }

uid OBJECT-TYPE
    SYNTAX      Integer32 (0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "Get and set the UID status.
                Get case:
                0 : off
                1 : on
                Set case:
                0 : off
                1 : on"
    ::= { ipmi 10 }



