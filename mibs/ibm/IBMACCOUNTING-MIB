      ::=  { ibmappnIsAcTimeEntry 8 }


ibmappnIsAcTimeAgtUtcMonths OBJECT-TYPE
      SYNTAX INTEGER {
                     january(0),
                     february(1),
                     march(2),
                     april(3),
                     may(4),
                     june(5),
                     july(6),
