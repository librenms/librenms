	tacIndex
		INTEGER,
	tacacsPlusServerIPAddr
		IpAddress
	}

tacIndex  OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"Fixed index for the four TACACS+ Server entries. Valid values are [1,2,3,4] only."
	::= { tacacsPlusServerIPEntry 1 }

