        SYNTAX  INTEGER {
		enable (1),
		disable (2)
        }
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
                "Specifies if SSL is enabled for insightix ldap server. 
                Default: 1, enable"
        ::= { insightixCfgGrp 5 }

