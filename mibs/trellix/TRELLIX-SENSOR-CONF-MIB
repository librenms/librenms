    SYNTAX  INTEGER {
        optical(1),
        electrical (2)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "Sets the media of the connector the user desired for the port.
        Default: optical"
    ::= { intfPhysicalPortEntry 12 }
intfPhysicalPortMonPortIpAddress OBJECT-TYPE
    SYNTAX  IpAddress
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
