	SYNTAX 	INTEGER
	{
		notApplicable(0),
		managedHost(1),
		unmanagedHost(2)
	}
	MAX-ACCESS  read-only
	STATUS	current
	DESCRIPTION 
		"This objects returns the message returned by the MPE server."
	::= { hostQBulkFilterV4Entry 7 }

