        SYNTAX  INTEGER {
		enable (1),
		disable (2)
        }
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
                "This object is used to enable or disable the TCP Connection with
                 the NTBA server 
                Default: 2, disable"
        ::= { ntbaChannelCfgGrp 5 }

