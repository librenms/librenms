        SYNTAX  INTEGER {
                    other (0),
        certAbsent (1),
        defaultCert (2),
        selfsignedCert (3),
        casignedCert (4)
        }
  MAX-ACCESS  read-only
  STATUS  current
  DESCRIPTION
    "This object indicates the cert status on the sensor.
    Default: 0"
  ::= { sensorCertificateConfigGrp 2 }

