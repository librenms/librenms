        STATUS  current
        DESCRIPTION
        "This object specifies the IPv4 Address of the Backup RADIUS server"
        ::= { radiusAuthGrp 11 }

radiusBackupServerIPv6Addr OBJECT-TYPE
        SYNTAX  Ipv6Address
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
                "This object specifies the IPv6 Address of the Backup RADIUS Server"
        ::= { radiusAuthGrp 12 }

