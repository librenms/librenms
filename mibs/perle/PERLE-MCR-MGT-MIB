        DESCRIPTION
            "The recurring summer time end hour"
        ::= { customTimeZoneGroup 15 }

    timeSummerRecEndMin OBJECT-TYPE
        SYNTAX      Integer32(0..59)
        MAX-ACCESS  read-write
