    STATUS        current
    DESCRIPTION
        "LDAP user attribute.
           e.g.
           uid                (OpenLDAP)
           sAMAccountName     (Active Directory)

           The default is 'uid'."
    ::= {ldapGroup 7}

ldapPasswordEncryption OBJECT-TYPE
    SYNTAX      INTEGER {
                        none(1),
                        md5(2)
                    }
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "LDAP password encryption method."
    ::= {ldapGroup 8}

