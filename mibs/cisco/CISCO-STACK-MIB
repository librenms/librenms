                                       -- Distributed forwarding card
                                       -- XXL Ultra
                                c6880xledfc(241)
                                       -- Distributed forwarding card
                                       -- Lite

                                }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The type of daughterboard attached to this
                      module."
        ::= { moduleEntry 16 }

