   SYNTAX      VlanList1
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION "A list of staticaly created vlans from 1 to 1024."
   DEFVAL {'00'H}
   ::= { rlVlanUsageForbiddenListEntry 2}

