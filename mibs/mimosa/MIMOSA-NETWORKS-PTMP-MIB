    OBJECTS {
            mimosaPtmpSsidName,
            mimosaPtmpSsidType,
            mimosaPtmpSsidEnabled,
            mimosaPtmpSsidBroadcastEnabled,
            mimosaPtmpSsidIsolationEnabled
    }
    STATUS      current
    DESCRIPTION
        "A collection of objects for the SSID Table."
    ::= { mimosaPtmpGroups 1 }

