		Counter32,
	mwrPortStatsOutOctets32Bit
		Counter32,
	mwrPortStatsOutGoodPkts32Bit
		Counter32,
	mwrPortStatsOutErrPkts32Bit
		Counter32,
	mwrPortStatsOutDiscardPkts32Bit
		Counter32
