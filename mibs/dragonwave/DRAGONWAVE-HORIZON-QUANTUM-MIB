		DisplayString,
	snmpV3TrapHostAuthProtocol
		INTEGER,
	snmpV3TrapHostAuthPassword
		DisplayString,
	snmpV3TrapHostPrivProtocol
		INTEGER,
	snmpV3TrapHostPrivPassword
		DisplayString,
	snmpV3TrapHostActivated
		INTEGER
}

