    vtssDhcpServerStatusBindingPoolName     VTSSDisplayString,
    vtssDhcpServerStatusBindingServerId     IpAddress,
    vtssDhcpServerStatusBindingVlanId   VTSSUnsigned16,
    vtssDhcpServerStatusBindingSubnetMask   IpAddress,
    vtssDhcpServerStatusBindingClientIdentifierType     VTSSDhcpServerClientIdentifierEnum,
    vtssDhcpServerStatusBindingClientIdentifierFqdn     VTSSDisplayString,
    vtssDhcpServerStatusBindingClientIdentifierMac      MacAddress,
