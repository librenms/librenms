VTSS-DHCP-SERVER-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP
        FROM SNMPv2-CONF
    MODULE-IDENTITY, OBJECT-TYPE
        FROM SNMPv2-SMI
    TEXTUAL-CONVENTION
        FROM SNMPv2-TC
    vtssSwitchMgmt
        FROM VTSS-SMI
    Integer32 FROM SNMPv2-SMI
    IpAddress FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    MacAddress FROM SNMPv2-TC
    TruthValue FROM SNMPv2-TC
    VTSSDisplayString FROM VTSS-TC
    VTSSInterfaceIndex FROM VTSS-TC
    VTSSRowEditorState FROM VTSS-TC
    VTSSUnsigned16 FROM VTSS-TC
    ;

vtssDhcpServerMib MODULE-IDENTITY
    LAST-UPDATED "201411270000Z"
    ORGANIZATION
        "Vitesse Semiconductor Corporation"
    CONTACT-INFO
        "Application support <application_support@vitesse.com>"
    DESCRIPTION
        "This is a private version of DhcpServer"
    REVISION    "201411270000Z"
    DESCRIPTION
        "revise descriptions by Palle"
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { vtssSwitchMgmt 109 }


VTSSDhcpServerBindingEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of binding."
    SYNTAX      INTEGER { none(0), automatic(1), manual(2), expired(3) }

VTSSDhcpServerBindingStateEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the state of binding."
    SYNTAX      INTEGER { none(0), allocated(1), committed(2), expired(3) }

VTSSDhcpServerClientIdentifierEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of client identifier."
    SYNTAX      INTEGER { none(0), fqdn(1), mac(2) }

VTSSDhcpServerNetbiosNodeEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of NetBIOS node."
    SYNTAX      INTEGER { nodeNone(0), nodeB(1), nodeP(2), nodeM(3), nodeH(4) }

VTSSDhcpServerPoolEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of DHCP pool."
    SYNTAX      INTEGER { none(0), network(1), host(2) }

vtssDhcpServerMibObjects OBJECT IDENTIFIER
    ::= { vtssDhcpServerMib 1 }

vtssDhcpServerConfig OBJECT IDENTIFIER
    ::= { vtssDhcpServerMibObjects 2 }

vtssDhcpServerConfigGlobals OBJECT IDENTIFIER
    ::= { vtssDhcpServerConfig 1 }

vtssDhcpServerConfigGlobalsMode OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Global mode of DHCP server. true is to enable the functions of DHCP
         server and false is to disable it."
    ::= { vtssDhcpServerConfigGlobals 1 }

vtssDhcpServerConfigVlanTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSDhcpServerConfigVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is the table of DHCP server VLAN configuration. The index is VLAN
         ID."
    ::= { vtssDhcpServerConfig 2 }

vtssDhcpServerConfigVlanEntry OBJECT-TYPE
    SYNTAX      VTSSDhcpServerConfigVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each VLAN has a set of parameters"
    INDEX       { vtssDhcpServerConfigVlanIfIndex }
    ::= { vtssDhcpServerConfigVlanTable 1 }

VTSSDhcpServerConfigVlanEntry ::= SEQUENCE {
    vtssDhcpServerConfigVlanIfIndex     VTSSInterfaceIndex,
    vtssDhcpServerConfigVlanMode    TruthValue
}

vtssDhcpServerConfigVlanIfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Logical interface number of VLAN."
    ::= { vtssDhcpServerConfigVlanEntry 1 }

vtssDhcpServerConfigVlanMode OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "VLAN mode of DHCP server. true is to enable DHCP server per VLAN and
         false is to disable it per VLAN."
    ::= { vtssDhcpServerConfigVlanEntry 2 }

vtssDhcpServerConfigExcludedTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSDhcpServerConfigExcludedEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The table is DHCP server excluded IP onfiguration table. The indexes
         are low IP and high IP address."
    ::= { vtssDhcpServerConfig 3 }

vtssDhcpServerConfigExcludedEntry OBJECT-TYPE
    SYNTAX      VTSSDhcpServerConfigExcludedEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry has a set of parameters"
    INDEX       { vtssDhcpServerConfigExcludedLowIpAddress, vtssDhcpServerConfigExcludedHighIpAddress }
    ::= { vtssDhcpServerConfigExcludedTable 1 }

VTSSDhcpServerConfigExcludedEntry ::= SEQUENCE {
    vtssDhcpServerConfigExcludedLowIpAddress    IpAddress,
    vtssDhcpServerConfigExcludedHighIpAddress   IpAddress,
    vtssDhcpServerConfigExcludedAction      VTSSRowEditorState
}

vtssDhcpServerConfigExcludedLowIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Low IP address."
    ::= { vtssDhcpServerConfigExcludedEntry 1 }

vtssDhcpServerConfigExcludedHighIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "High IP address."
    ::= { vtssDhcpServerConfigExcludedEntry 2 }

vtssDhcpServerConfigExcludedAction OBJECT-TYPE
    SYNTAX      VTSSRowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Action"
    ::= { vtssDhcpServerConfigExcludedEntry 100 }

vtssDhcpServerConfigExcludedIpTableRowEditor OBJECT IDENTIFIER
    ::= { vtssDhcpServerConfig 4 }

vtssDhcpServerConfigExcludedIpTableRowEditorLowIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Low IP address."
    ::= { vtssDhcpServerConfigExcludedIpTableRowEditor 1 }

vtssDhcpServerConfigExcludedIpTableRowEditorHighIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "High IP address."
    ::= { vtssDhcpServerConfigExcludedIpTableRowEditor 2 }

vtssDhcpServerConfigExcludedIpTableRowEditorAction OBJECT-TYPE
    SYNTAX      VTSSRowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Action"
    ::= { vtssDhcpServerConfigExcludedIpTableRowEditor 100 }

vtssDhcpServerConfigPoolTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSDhcpServerConfigPoolEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The table is DHCP server pool onfiguration table. The indexe is pool
         name."
    ::= { vtssDhcpServerConfig 5 }

vtssDhcpServerConfigPoolEntry OBJECT-TYPE
    SYNTAX      VTSSDhcpServerConfigPoolEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry has a set of parameters"
    INDEX       { vtssDhcpServerConfigPoolPoolName }
    ::= { vtssDhcpServerConfigPoolTable 1 }

VTSSDhcpServerConfigPoolEntry ::= SEQUENCE {
    vtssDhcpServerConfigPoolPoolName    VTSSDisplayString,
    vtssDhcpServerConfigPoolPoolType    VTSSDhcpServerPoolEnum,
    vtssDhcpServerConfigPoolIpv4Address     IpAddress,
    vtssDhcpServerConfigPoolSubnetMask      IpAddress,
    vtssDhcpServerConfigPoolSubnetBroadcast     IpAddress,
    vtssDhcpServerConfigPoolLeaseDay    Unsigned32,
    vtssDhcpServerConfigPoolLeaseHour   Unsigned32,
    vtssDhcpServerConfigPoolLeaseMinute     Unsigned32,
    vtssDhcpServerConfigPoolDomainName      VTSSDisplayString,
    vtssDhcpServerConfigPoolDefaultRouter1      IpAddress,
    vtssDhcpServerConfigPoolDefaultRouter2      IpAddress,
    vtssDhcpServerConfigPoolDefaultRouter3      IpAddress,
    vtssDhcpServerConfigPoolDefaultRouter4      IpAddress,
    vtssDhcpServerConfigPoolDnsServer1      IpAddress,
    vtssDhcpServerConfigPoolDnsServer2      IpAddress,
    vtssDhcpServerConfigPoolDnsServer3      IpAddress,
    vtssDhcpServerConfigPoolDnsServer4      IpAddress,
    vtssDhcpServerConfigPoolNtpServer1      IpAddress,
    vtssDhcpServerConfigPoolNtpServer2      IpAddress,
    vtssDhcpServerConfigPoolNtpServer3      IpAddress,
    vtssDhcpServerConfigPoolNtpServer4      IpAddress,
    vtssDhcpServerConfigPoolNetbiosNodeType     VTSSDhcpServerNetbiosNodeEnum,
    vtssDhcpServerConfigPoolNetbiosScope    VTSSDisplayString,
    vtssDhcpServerConfigPoolNetbiosNameServer1      IpAddress,
    vtssDhcpServerConfigPoolNetbiosNameServer2      IpAddress,
    vtssDhcpServerConfigPoolNetbiosNameServer3      IpAddress,
    vtssDhcpServerConfigPoolNetbiosNameServer4      IpAddress,
    vtssDhcpServerConfigPoolNisDomainName   VTSSDisplayString,
    vtssDhcpServerConfigPoolNisServer1      IpAddress,
    vtssDhcpServerConfigPoolNisServer2      IpAddress,
    vtssDhcpServerConfigPoolNisServer3      IpAddress,
    vtssDhcpServerConfigPoolNisServer4      IpAddress,
    vtssDhcpServerConfigPoolClientIdentifierType    VTSSDhcpServerClientIdentifierEnum,
    vtssDhcpServerConfigPoolClientIdentifierFqdn    VTSSDisplayString,
    vtssDhcpServerConfigPoolClientIdentifierMac     MacAddress,
    vtssDhcpServerConfigPoolClientHardwareAddress   MacAddress,
    vtssDhcpServerConfigPoolClientName      VTSSDisplayString,
    vtssDhcpServerConfigPoolVendorClassId1      VTSSDisplayString,
    vtssDhcpServerConfigPoolVendorSpecificInfo1     VTSSDisplayString,
    vtssDhcpServerConfigPoolVendorClassId2      VTSSDisplayString,
    vtssDhcpServerConfigPoolVendorSpecificInfo2     VTSSDisplayString,
    vtssDhcpServerConfigPoolVendorClassId3      VTSSDisplayString,
    vtssDhcpServerConfigPoolVendorSpecificInfo3     VTSSDisplayString,
    vtssDhcpServerConfigPoolVendorClassId4      VTSSDisplayString,
    vtssDhcpServerConfigPoolVendorSpecificInfo4     VTSSDisplayString,
    vtssDhcpServerConfigPoolAction      VTSSRowEditorState
}

vtssDhcpServerConfigPoolPoolName OBJECT-TYPE
    SYNTAX      VTSSDisplayString (SIZE(0..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Name of DHCP pool."
    ::= { vtssDhcpServerConfigPoolEntry 1 }

vtssDhcpServerConfigPoolPoolType OBJECT-TYPE
    SYNTAX      VTSSDhcpServerPoolEnum
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Type of pool. none(0) means the pool type is not defined yet.
         network(1) means the pool defines a pool of IP addresses to service
         more than one DHCP client. host(2) means the pool services for a
         specific DHCP client identified by client identifier or hardware
         address."
    ::= { vtssDhcpServerConfigPoolEntry 2 }

vtssDhcpServerConfigPoolIpv4Address OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Network number of the subnet. If the pool type is of network, the IP
         address can be any general IP address. If the pool type is of host, the
         IP address must be a unicast IP address."
    ::= { vtssDhcpServerConfigPoolEntry 3 }

vtssDhcpServerConfigPoolSubnetMask OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Subnet Mask. DHCP option 1. Specify subnet mask of the DHCP address
         pool, excluding 0.0.0.0 and 255.255.255.255."
    ::= { vtssDhcpServerConfigPoolEntry 4 }

vtssDhcpServerConfigPoolSubnetBroadcast OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Broadcast IP address in the subnet. DHCP option 28. Specify the
         broadcast address in use on the client's subnet."
    ::= { vtssDhcpServerConfigPoolEntry 5 }

vtssDhcpServerConfigPoolLeaseDay OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Number of days of lease time. DHCP option 51, 58 and 59. The value
         range is 0-365. Specify lease time that allows the client to request a
         lease time for the IP address. If all of LeaseDay, LeaseHour and
         LeaseMinute are 0's, then it means the lease time is infinite."
    ::= { vtssDhcpServerConfigPoolEntry 6 }

vtssDhcpServerConfigPoolLeaseHour OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Number of hours of lease time. DHCP option 51, 58 and 59. The value
         range is 0-23. Specify lease time that allows the client to request a
         lease time for the IP address. If all of LeaseDay, LeaseHour and
         LeaseMinute are 0's, then it means the lease time is infinite."
    ::= { vtssDhcpServerConfigPoolEntry 7 }

vtssDhcpServerConfigPoolLeaseMinute OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Number of minutes of lease time. DHCP option 51, 58 and 59. The value
         range is 0-59. Specify lease time that allows the client to request a
         lease time for the IP address. If all of LeaseDay, LeaseHour and
         LeaseMinute are 0's, then it means the lease time is infinite."
    ::= { vtssDhcpServerConfigPoolEntry 8 }

vtssDhcpServerConfigPoolDomainName OBJECT-TYPE
    SYNTAX      VTSSDisplayString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Domain name. DHCP option 15. Specify domain name that client should use
         when resolving hostname via DNS."
    ::= { vtssDhcpServerConfigPoolEntry 9 }

vtssDhcpServerConfigPoolDefaultRouter1 OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Default router 1."
