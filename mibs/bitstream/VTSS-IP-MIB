    OBJECTS     { vtssIpCapabilitiesHasIpv4HostCapabilities, 
                  vtssIpCapabilitiesHasIpv6HostCapabilities, 
                  vtssIpCapabilitiesHasIpv4UnicastRoutingCapabilities, 
                  vtssIpCapabilitiesHasIpv4UnicastHwRoutingCapabilities, 
                  vtssIpCapabilitiesHasIpv6UnicastRoutingCapabilities, 
                  vtssIpCapabilitiesHasIpv6UnicastHwRoutingCapabilities, 
                  vtssIpCapabilitiesMaxNumberOfIpInterfaces, 
                  vtssIpCapabilitiesMaxNumberOfStaticRoutes, 
                  vtssIpCapabilitiesNumberOfLpmHardwareEntries }
    STATUS      current
    DESCRIPTION
        "A collection of objects suitable for bulk operations."
    ::= { vtssIpMibGroups 1 }

