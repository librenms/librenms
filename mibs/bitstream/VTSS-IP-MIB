    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The device has IPv4 unicast routing capabilities."
    ::= { vtssIpCapabilities 3 }

vtssIpCapabilitiesHasIpv4UnicastHwRoutingCapabilities OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The device has IPv4 unicast hardware accelerated routing capabilities."
    ::= { vtssIpCapabilities 4 }

vtssIpCapabilitiesHasIpv6UnicastRoutingCapabilities OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The device has IPv6 unicast routing capabilities."
    ::= { vtssIpCapabilities 5 }

vtssIpCapabilitiesHasIpv6UnicastHwRoutingCapabilities OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The device has IPv6 unicast hardware accelerated routing capabilities."
    ::= { vtssIpCapabilities 6 }

vtssIpCapabilitiesMaxNumberOfIpInterfaces OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Maximum number of IP interfaces supported by the device."
    ::= { vtssIpCapabilities 7 }

vtssIpCapabilitiesMaxNumberOfStaticRoutes OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Maximum number of static configured IP routes (shared by IPv4 and
         IPv6)."
    ::= { vtssIpCapabilities 8 }

vtssIpCapabilitiesNumberOfLpmHardwareEntries OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Number of hardware LPM (longest prefix match) entries."
    ::= { vtssIpCapabilities 9 }

vtssIpConfig OBJECT IDENTIFIER
    ::= { vtssIpMibObjects 2 }

vtssIpConfigGlobals OBJECT IDENTIFIER
    ::= { vtssIpConfig 1 }

vtssIpConfigGlobalsMain OBJECT IDENTIFIER
    ::= { vtssIpConfigGlobals 1 }

vtssIpConfigGlobalsMainEnableRouting OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Enable routing."
    ::= { vtssIpConfigGlobalsMain 1 }

vtssIpConfigInterfaces OBJECT IDENTIFIER
    ::= { vtssIpConfig 2 }

vtssIpConfigInterfacesTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSIpConfigInterfacesEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is the IP interface table. When an IP interface is created it can
         be configured in the other tables found in this MIB."
    ::= { vtssIpConfigInterfaces 1 }

vtssIpConfigInterfacesEntry OBJECT-TYPE
    SYNTAX      VTSSIpConfigInterfacesEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Entries in this table represent IP interfaces created on the system"
    INDEX       { vtssIpConfigInterfacesIfIndex }
    ::= { vtssIpConfigInterfacesTable 1 }

VTSSIpConfigInterfacesEntry ::= SEQUENCE {
    vtssIpConfigInterfacesIfIndex   VTSSInterfaceIndex,
    vtssIpConfigInterfacesAction    VTSSRowEditorState
}

vtssIpConfigInterfacesIfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Interface index number."
    ::= { vtssIpConfigInterfacesEntry 1 }

vtssIpConfigInterfacesAction OBJECT-TYPE
    SYNTAX      VTSSRowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Action"
    ::= { vtssIpConfigInterfacesEntry 100 }

vtssIpConfigInterfacesTableRowEditor OBJECT IDENTIFIER
    ::= { vtssIpConfigInterfaces 2 }

vtssIpConfigInterfacesTableRowEditorIfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Interface index number."
    ::= { vtssIpConfigInterfacesTableRowEditor 1 }

vtssIpConfigInterfacesTableRowEditorAction OBJECT-TYPE
    SYNTAX      VTSSRowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Action"
    ::= { vtssIpConfigInterfacesTableRowEditor 100 }

vtssIpConfigInterfacesIpv4Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSIpConfigInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "IPv4 interface configuration table. This table enables IPv4 related
         configuration of the corresponding IP interface."
    ::= { vtssIpConfigInterfaces 3 }

vtssIpConfigInterfacesIpv4Entry OBJECT-TYPE
    SYNTAX      VTSSIpConfigInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents an IP interface."
    INDEX       { vtssIpConfigInterfacesIpv4IfIndex }
    ::= { vtssIpConfigInterfacesIpv4Table 1 }

VTSSIpConfigInterfacesIpv4Entry ::= SEQUENCE {
    vtssIpConfigInterfacesIpv4IfIndex   VTSSInterfaceIndex,
    vtssIpConfigInterfacesIpv4Active    TruthValue,
    vtssIpConfigInterfacesIpv4EnableDhcpClient      TruthValue,
    vtssIpConfigInterfacesIpv4Ipv4Address   IpAddress,
    vtssIpConfigInterfacesIpv4PrefixSize    Unsigned32,
    vtssIpConfigInterfacesIpv4DhcpClientFallbackTimeout     Unsigned32
}

vtssIpConfigInterfacesIpv4IfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Interface index number."
    ::= { vtssIpConfigInterfacesIpv4Entry 1 }

vtssIpConfigInterfacesIpv4Active OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Enable IPv4.
         
         IPv4 can only be enabled if either the DHCP client is enabled, or a
         valid address has been configured."
    ::= { vtssIpConfigInterfacesIpv4Entry 2 }

vtssIpConfigInterfacesIpv4EnableDhcpClient OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Enable IPv4 DHCP client.
         
         Note: the DHCP client can only be enabled if there is no conflict in
         the values of: ipv4Address, prefixSize, and dhcpClientFallbackTimeout."
    ::= { vtssIpConfigInterfacesIpv4Entry 3 }

vtssIpConfigInterfacesIpv4Ipv4Address OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "IPv4 address.
         
         Note: Multiple interfaces may not have overlapping networks."
    ::= { vtssIpConfigInterfacesIpv4Entry 4 }

vtssIpConfigInterfacesIpv4PrefixSize OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Prefix size of the network.
         
         Note: Multiple interfaces may not have overlapping networks."
    ::= { vtssIpConfigInterfacesIpv4Entry 5 }

vtssIpConfigInterfacesIpv4DhcpClientFallbackTimeout OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "DHCP client fallback timer.
         
         If DHCP is disabled then this object has no effect. If DHCP is enabled
         and the fallback timeout value is different from zero, then this timer
         will stop the DHCP process and assign the ipv4Address to the interface
         instead."
    ::= { vtssIpConfigInterfacesIpv4Entry 6 }

vtssIpConfigInterfacesIpv6Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSIpConfigInterfacesIpv6Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "IPv6 interface configuration table. This table enables IPv6 related
         configuration of the corresponding IP interface."
    ::= { vtssIpConfigInterfaces 4 }

vtssIpConfigInterfacesIpv6Entry OBJECT-TYPE
    SYNTAX      VTSSIpConfigInterfacesIpv6Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents an IP interface."
    INDEX       { vtssIpConfigInterfacesIpv6IfIndex }
    ::= { vtssIpConfigInterfacesIpv6Table 1 }

VTSSIpConfigInterfacesIpv6Entry ::= SEQUENCE {
    vtssIpConfigInterfacesIpv6IfIndex   VTSSInterfaceIndex,
    vtssIpConfigInterfacesIpv6Active    TruthValue,
    vtssIpConfigInterfacesIpv6Ipv6Address   InetAddressIPv6,
    vtssIpConfigInterfacesIpv6PrefixSize    Unsigned32
}

vtssIpConfigInterfacesIpv6IfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Interface index number."
    ::= { vtssIpConfigInterfacesIpv6Entry 1 }

vtssIpConfigInterfacesIpv6Active OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Enable the static configured IPv6 address.
         
         The static configured IPv6 address can only be configured if a valid
         address has been written into 'ipv6Address' and 'prefixSize'."
    ::= { vtssIpConfigInterfacesIpv6Entry 2 }

vtssIpConfigInterfacesIpv6Ipv6Address OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Static configured IPv6 address."
    ::= { vtssIpConfigInterfacesIpv6Entry 3 }

vtssIpConfigInterfacesIpv6PrefixSize OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Prefix size of the network."
    ::= { vtssIpConfigInterfacesIpv6Entry 4 }

vtssIpConfigRoutes OBJECT IDENTIFIER
    ::= { vtssIpConfig 3 }

vtssIpConfigRoutesIpv4Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSIpConfigRoutesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is the IPv4 route configuration table."
    ::= { vtssIpConfigRoutes 1 }

vtssIpConfigRoutesIpv4Entry OBJECT-TYPE
    SYNTAX      VTSSIpConfigRoutesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents a configured route.
         
         Note: a route may be configured without being active."
    INDEX       { vtssIpConfigRoutesIpv4NetworkAddress, vtssIpConfigRoutesIpv4NetworkPrefixSize, vtssIpConfigRoutesIpv4NextHop }
    ::= { vtssIpConfigRoutesIpv4Table 1 }

VTSSIpConfigRoutesIpv4Entry ::= SEQUENCE {
    vtssIpConfigRoutesIpv4NetworkAddress    IpAddress,
    vtssIpConfigRoutesIpv4NetworkPrefixSize     Integer32,
    vtssIpConfigRoutesIpv4NextHop   IpAddress,
    vtssIpConfigRoutesIpv4Action    VTSSRowEditorState
}

vtssIpConfigRoutesIpv4NetworkAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Network address."
    ::= { vtssIpConfigRoutesIpv4Entry 1 }

vtssIpConfigRoutesIpv4NetworkPrefixSize OBJECT-TYPE
    SYNTAX      Integer32 (0..32)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Network prefix size."
    ::= { vtssIpConfigRoutesIpv4Entry 2 }

