vtssIpStatusInterfaces OBJECT IDENTIFIER
    ::= { vtssIpStatus 2 }

vtssIpStatusInterfacesLinkTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSIpStatusInterfacesLinkEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This table provides link-layer status information for IP interfaces."
    ::= { vtssIpStatusInterfaces 1 }

vtssIpStatusInterfacesLinkEntry OBJECT-TYPE
    SYNTAX      VTSSIpStatusInterfacesLinkEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents an IP interface."
    INDEX       { vtssIpStatusInterfacesLinkIfIndex }
    ::= { vtssIpStatusInterfacesLinkTable 1 }

VTSSIpStatusInterfacesLinkEntry ::= SEQUENCE {
    vtssIpStatusInterfacesLinkIfIndex   VTSSInterfaceIndex,
    vtssIpStatusInterfacesLinkOsInterfaceIndex      Unsigned32,
    vtssIpStatusInterfacesLinkMtu   Unsigned32,
    vtssIpStatusInterfacesLinkMacAddress    MacAddress,
    vtssIpStatusInterfacesLinkUp    TruthValue,
    vtssIpStatusInterfacesLinkBroadcast     TruthValue,
    vtssIpStatusInterfacesLinkLoopback      TruthValue,
    vtssIpStatusInterfacesLinkRunning   TruthValue,
    vtssIpStatusInterfacesLinkNoarp     TruthValue,
    vtssIpStatusInterfacesLinkPromisc   TruthValue,
    vtssIpStatusInterfacesLinkMulticast     TruthValue
}

vtssIpStatusInterfacesLinkIfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Interface index number."
    ::= { vtssIpStatusInterfacesLinkEntry 1 }

vtssIpStatusInterfacesLinkOsInterfaceIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Interface index used by the operating system."
    ::= { vtssIpStatusInterfacesLinkEntry 2 }

vtssIpStatusInterfacesLinkMtu OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "MTU for the interface."
    ::= { vtssIpStatusInterfacesLinkEntry 3 }

vtssIpStatusInterfacesLinkMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "MAC-address of the interface."
    ::= { vtssIpStatusInterfacesLinkEntry 4 }

