vtssIpStatus OBJECT IDENTIFIER
    ::= { vtssIpMibObjects 3 }

vtssIpStatusGlobals OBJECT IDENTIFIER
    ::= { vtssIpStatus 1 }

vtssIpStatusGlobalsIpv4NeighbourTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSIpStatusGlobalsIpv4NeighbourEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is the IPv4 neighbour table (ARP)."
    ::= { vtssIpStatusGlobals 1 }

vtssIpStatusGlobalsIpv4NeighbourEntry OBJECT-TYPE
    SYNTAX      VTSSIpStatusGlobalsIpv4NeighbourEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents an entry in the underlying operting
         system's ARP table."
    INDEX       { vtssIpStatusGlobalsIpv4NeighbourIpv4 }
    ::= { vtssIpStatusGlobalsIpv4NeighbourTable 1 }

VTSSIpStatusGlobalsIpv4NeighbourEntry ::= SEQUENCE {
    vtssIpStatusGlobalsIpv4NeighbourIpv4    IpAddress,
    vtssIpStatusGlobalsIpv4NeighbourMacAddress      MacAddress,
    vtssIpStatusGlobalsIpv4NeighbourInterface   VTSSInterfaceIndex
}

vtssIpStatusGlobalsIpv4NeighbourIpv4 OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "IPv4 address."
    ::= { vtssIpStatusGlobalsIpv4NeighbourEntry 1 }

vtssIpStatusGlobalsIpv4NeighbourMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "MAC address associated with the IP address"
    ::= { vtssIpStatusGlobalsIpv4NeighbourEntry 2 }

vtssIpStatusGlobalsIpv4NeighbourInterface OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Interface the neighbour can be reached on."
    ::= { vtssIpStatusGlobalsIpv4NeighbourEntry 3 }

vtssIpStatusGlobalsIpv6NeighbourTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSIpStatusGlobalsIpv6NeighbourEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is the IPv6 neighbour table."
    ::= { vtssIpStatusGlobals 2 }

vtssIpStatusGlobalsIpv6NeighbourEntry OBJECT-TYPE
    SYNTAX      VTSSIpStatusGlobalsIpv6NeighbourEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents an entry in the underlying operting
         system's nieghbour table."
    INDEX       { vtssIpStatusGlobalsIpv6NeighbourIpAddress, vtssIpStatusGlobalsIpv6NeighbourInterfaceQuery }
    ::= { vtssIpStatusGlobalsIpv6NeighbourTable 1 }

VTSSIpStatusGlobalsIpv6NeighbourEntry ::= SEQUENCE {
    vtssIpStatusGlobalsIpv6NeighbourIpAddress   InetAddressIPv6,
    vtssIpStatusGlobalsIpv6NeighbourInterfaceQuery      Integer32,
    vtssIpStatusGlobalsIpv6NeighbourMacAddress      MacAddress,
    vtssIpStatusGlobalsIpv6NeighbourInterface   VTSSInterfaceIndex
}

vtssIpStatusGlobalsIpv6NeighbourIpAddress OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "IP address."
    ::= { vtssIpStatusGlobalsIpv6NeighbourEntry 1 }

vtssIpStatusGlobalsIpv6NeighbourInterfaceQuery OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "If 'ipAddress' is a link-local address, then the interface index where
         the host can be reached must be specified here, otherwise set this to
         zero."
    ::= { vtssIpStatusGlobalsIpv6NeighbourEntry 2 }

vtssIpStatusGlobalsIpv6NeighbourMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
