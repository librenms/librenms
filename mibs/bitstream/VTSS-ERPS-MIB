vtssErpsStatus OBJECT IDENTIFIER
    ::= { vtssErpsMibObjects 3 }

vtssErpsStatusTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSErpsStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This table contains status per EPRS group."
    ::= { vtssErpsStatus 1 }

vtssErpsStatusEntry OBJECT-TYPE
    SYNTAX      VTSSErpsStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Status."
    INDEX       { vtssErpsStatusGroupIndex }
    ::= { vtssErpsStatusTable 1 }

VTSSErpsStatusEntry ::= SEQUENCE {
    vtssErpsStatusGroupIndex    Integer32,
    vtssErpsStatusActive    TruthValue,
    vtssErpsStatusProtectionState   VTSSErpsProtectionState,
    vtssErpsStatusRplBlocked    TruthValue,
    vtssErpsStatusWtrRemaining      Unsigned32,
    vtssErpsStatusAdminCmd      VTSSErpsAdminCmd,
    vtssErpsStatusFopAlarm      TruthValue,
    vtssErpsStatusTxActive      TruthValue,
    vtssErpsStatusTxRequestOrState      VTSSErpsRequestState,
    vtssErpsStatusTxRplBlocked      TruthValue,
    vtssErpsStatusTxDoNotFlush      TruthValue,
    vtssErpsStatusBlockedPortReference      VTSSErpsPort,
    vtssErpsStatusPort0Blocked      TruthValue,
    vtssErpsStatusPort0State    VTSSErpsPortState,
    vtssErpsStatusPort0RxActive     TruthValue,
    vtssErpsStatusPort0RxRequestOrState     VTSSErpsRequestState,
    vtssErpsStatusPort0RxRplBlocked     TruthValue,
    vtssErpsStatusPort0RxDoNotFlush     TruthValue,
    vtssErpsStatusPort0RxBlockedPortReference   VTSSErpsPort,
    vtssErpsStatusPort0RxNodeId     OCTET STRING,
    vtssErpsStatusPort1Blocked      TruthValue,
    vtssErpsStatusPort1State    VTSSErpsPortState,
    vtssErpsStatusPort1RxActive     TruthValue,
    vtssErpsStatusPort1RxRequestOrState     VTSSErpsRequestState,
    vtssErpsStatusPort1RxRplBlocked     TruthValue,
    vtssErpsStatusPort1RxDoNotFlush     TruthValue,
    vtssErpsStatusPort1RxBlockedPortReference   VTSSErpsPort,
    vtssErpsStatusPort1RxNodeId     OCTET STRING
}

vtssErpsStatusGroupIndex OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "ERPS group index number. Valid range is (1..max groups). The maximum
         group number is platform-specific and can be retrieved from the ERPS
         capabilities."
    ::= { vtssErpsStatusEntry 1 }

vtssErpsStatusActive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies whether ERPS group is currently active or not."
    ::= { vtssErpsStatusEntry 2 }

vtssErpsStatusProtectionState OBJECT-TYPE
    SYNTAX      VTSSErpsProtectionState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies current ERPS group protection state."
    ::= { vtssErpsStatusEntry 3 }

vtssErpsStatusRplBlocked OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies whether the RPL is currently blocked or not."
    ::= { vtssErpsStatusEntry 4 }

vtssErpsStatusWtrRemaining OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies the remaining wait-time before restoring ring."
    ::= { vtssErpsStatusEntry 5 }

vtssErpsStatusAdminCmd OBJECT-TYPE
    SYNTAX      VTSSErpsAdminCmd
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies the currently active administrative command."
    ::= { vtssErpsStatusEntry 6 }

vtssErpsStatusFopAlarm OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies whether the FailureOfProtocol alarm is currently active or
         not."
    ::= { vtssErpsStatusEntry 7 }

vtssErpsStatusTxActive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies whether R-APS transmission is currently active."
    ::= { vtssErpsStatusEntry 8 }

vtssErpsStatusTxRequestOrState OBJECT-TYPE
    SYNTAX      VTSSErpsRequestState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies the currently transmitted request/state. Only relevant when
         Tx is active."
    ::= { vtssErpsStatusEntry 9 }

vtssErpsStatusTxRplBlocked OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies the currently transmitted RPL Blocked flag. Only relevant
         when Tx is active."
    ::= { vtssErpsStatusEntry 10 }

vtssErpsStatusTxDoNotFlush OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies the currently transmitted Do Not Flush flag. Only relevant
         when Tx is active."
    ::= { vtssErpsStatusEntry 11 }

vtssErpsStatusBlockedPortReference OBJECT-TYPE
    SYNTAX      VTSSErpsPort
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies the currently transmitted Blocked Port Reference. Only
         relevant when Tx is active."
    ::= { vtssErpsStatusEntry 12 }

vtssErpsStatusPort0Blocked OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies whether ring Port 0 is blocked or not."
    ::= { vtssErpsStatusEntry 13 }

vtssErpsStatusPort0State OBJECT-TYPE
    SYNTAX      VTSSErpsPortState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies the state of ring Port 0."
    ::= { vtssErpsStatusEntry 14 }

vtssErpsStatusPort0RxActive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies if ring Port0 receives R-APS."
    ::= { vtssErpsStatusEntry 15 }

