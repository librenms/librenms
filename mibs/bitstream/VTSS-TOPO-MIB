                  vtssTopoStatusUpdateTriggeredTxCntA, 
                  vtssTopoStatusUpdateTxPolicerDropA, 
                  vtssTopoStatusUpdateRxErrCntA, vtssTopoStatusUpdateTxErrCntA, 
                  vtssTopoStatusAlertRxCntA, vtssTopoStatusAlertTxCntA, 
                  vtssTopoStatusAlertTxPolicerDropCntA, 
                  vtssTopoStatusAlertRxErrCntA, vtssTopoStatusAlertTxErrCntA, 
                  vtssTopoStatusUpdateRxCntB, vtssTopoStatusUpdatePeriodicTxCntB, 
                  vtssTopoStatusUpdateTriggeredTxCntB, 
                  vtssTopoStatusUpdateTxPolicerDropB, 
                  vtssTopoStatusUpdateRxErrCntB, vtssTopoStatusUpdateTxErrCntB, 
                  vtssTopoStatusAlertRxCntB, vtssTopoStatusAlertTxCntB, 
                  vtssTopoStatusAlertTxPolicerDropCntB, 
                  vtssTopoStatusAlertRxErrCntB, vtssTopoStatusAlertTxErrCntB }
    STATUS      current
    DESCRIPTION
        "A collection of objects suitable for bulk operations."
    ::= { vtssTopoMibGroups 4 }

