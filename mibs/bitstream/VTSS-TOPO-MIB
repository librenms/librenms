vtssTopoStatusMaster OBJECT IDENTIFIER
    ::= { vtssTopoStatus 2 }

vtssTopoStatusMasterMasterChangeTime OBJECT-TYPE
    SYNTAX      VTSSDisplayString (SIZE(0..25))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Time of last master change since boot in seconds."
    ::= { vtssTopoStatusMaster 2 }

vtssTopoStatusMasterMacAddr OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(6))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "MAC address for the switch which is currently master."
    ::= { vtssTopoStatusMaster 3 }

vtssTopoControl OBJECT IDENTIFIER
    ::= { vtssTopoMibObjects 4 }

vtssTopoControlSwapSidTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSTopoControlSwapSidEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The topology configuration for swapping switch ids"
    ::= { vtssTopoControl 4 }

vtssTopoControlSwapSidEntry OBJECT-TYPE
    SYNTAX      VTSSTopoControlSwapSidEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Switch Id index"
    INDEX       { vtssTopoControlSwapSidSid }
    ::= { vtssTopoControlSwapSidTable 1 }

VTSSTopoControlSwapSidEntry ::= SEQUENCE {
    vtssTopoControlSwapSidSid   Integer32,
    vtssTopoControlSwapSidSwitchId      Integer32
}

vtssTopoControlSwapSidSid OBJECT-TYPE
    SYNTAX      Integer32 (1..16)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Switch Id."
    ::= { vtssTopoControlSwapSidEntry 1 }

vtssTopoControlSwapSidSwitchId OBJECT-TYPE
    SYNTAX      Integer32 (1..16)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Switch Id for the switch to swap id with. Each switch in the stack has
         a switch ID. Swapping switch id between two switches in the stack."
    ::= { vtssTopoControlSwapSidEntry 3 }

vtssTopoControlMstElectSid OBJECT IDENTIFIER
    ::= { vtssTopoControl 5 }

vtssTopoControlMstElectSidReElect OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Makes the stack re-elect a master."
    ::= { vtssTopoControlMstElectSid 1 }

vtssTopoControlDelSid OBJECT IDENTIFIER
    ::= { vtssTopoControl 6 }

vtssTopoControlDelSidDel OBJECT-TYPE
    SYNTAX      Integer32 (1..16)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Deletes a switch from the stack."
    ::= { vtssTopoControlDelSid 1 }

