    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Logical interface number index."
    ::= { vtssLldpConfigMedEntry 1 }

vtssLldpConfigMedOptionalTlvs OBJECT-TYPE
    SYNTAX      VTSSUnsigned8
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Enables/Disables the LLDP optional TLVs. Bit mask, where setting the
         bit to 1 means
         
         enable transmission of the corresponding optional TLV.
         
         Bit 0 represents the capabilities TLV.
         
         Bit 1 represents the network Policy TLV.
         
         Bit 2 represents the location TLV.
         
         Bit 3 represents the PoE TLV."
    ::= { vtssLldpConfigMedEntry 3 }

vtssLldpConfigMedDeviceType OBJECT-TYPE
    SYNTAX      VTSSlldpmedDeviceType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Setting device type to configure the mode the device shall operate as."
    ::= { vtssLldpConfigMedEntry 4 }

vtssLldpConfigMedPolicyTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSLldpConfigMedPolicyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is a table to configure LLDP MED Policies for the device."
    ::= { vtssLldpConfigMed 2 }

vtssLldpConfigMedPolicyEntry OBJECT-TYPE
    SYNTAX      VTSSLldpConfigMedPolicyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Network Policy Discovery enables the efficient discovery and diagnosis
         of mismatch issues with the VLAN configuration, along with the
         associated Layer 2 and Layer 3 attributes, which apply for a set of
         specific protocol applications on that port. Improper network policy
         configurations are a very significant issue in VoIP environments that
         frequently result in voice quality degradation or loss of service.
         Policies are only intended for use with applications that have specific
         'real-time' network policy requirements, such as interactive voice
         and/or video services."
    INDEX       { vtssLldpConfigMedPolicyLldpmedPolicy }
    ::= { vtssLldpConfigMedPolicyTable 1 }

VTSSLldpConfigMedPolicyEntry ::= SEQUENCE {
    vtssLldpConfigMedPolicyLldpmedPolicy    Integer32,
    vtssLldpConfigMedPolicyApplicationType      VTSSlldpmedRemoteNetworkPolicyApplicationType,
    vtssLldpConfigMedPolicyTagged   TruthValue,
    vtssLldpConfigMedPolicyVlanId   VTSSUnsigned16,
    vtssLldpConfigMedPolicyL2Priority   VTSSUnsigned8,
    vtssLldpConfigMedPolicyDscp     VTSSUnsigned8,
    vtssLldpConfigMedPolicyAction   VTSSRowEditorState
}

vtssLldpConfigMedPolicyLldpmedPolicy OBJECT-TYPE
    SYNTAX      Integer32 (0..31)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Policy index."
    ::= { vtssLldpConfigMedPolicyEntry 1 }

vtssLldpConfigMedPolicyApplicationType OBJECT-TYPE
    SYNTAX      VTSSlldpmedRemoteNetworkPolicyApplicationType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The LLDP policy application type."
    ::= { vtssLldpConfigMedPolicyEntry 3 }

vtssLldpConfigMedPolicyTagged OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The LLDP policy tagged flag. Defines if LLDP policy uses tagged VLAN."
    ::= { vtssLldpConfigMedPolicyEntry 4 }

