    INDEX       { vtssMstpStatusBridgeInstance }
    ::= { vtssMstpStatusBridgeTable 1 }

VTSSMstpStatusBridgeEntry ::= SEQUENCE {
    vtssMstpStatusBridgeInstance    Integer32,
    vtssMstpStatusBridgeBridgeId    OCTET STRING,
    vtssMstpStatusBridgeTimeSinceTopologyChange     Unsigned32,
    vtssMstpStatusBridgeTopologyChangeCount     Unsigned32,
    vtssMstpStatusBridgeTopologyChange      TruthValue,
    vtssMstpStatusBridgeDesignatedRoot      OCTET STRING,
    vtssMstpStatusBridgeRootPathCost    Unsigned32,
    vtssMstpStatusBridgeRootPort    Unsigned32,
    vtssMstpStatusBridgeMaxAge      Unsigned32,
    vtssMstpStatusBridgeForwardDelay    Unsigned32,
    vtssMstpStatusBridgeBridgeMaxAge    Unsigned32,
    vtssMstpStatusBridgeBridgeHelloTime     Unsigned32,
    vtssMstpStatusBridgeBridgeForwardDelay      Unsigned32,
    vtssMstpStatusBridgeTxHoldCount     Unsigned32,
    vtssMstpStatusBridgeForceVersion    VTSSMSTPForceVersion,
    vtssMstpStatusBridgeCistRegionalRoot    OCTET STRING,
    vtssMstpStatusBridgeCistInternalPathCost    Unsigned32,
    vtssMstpStatusBridgeMaxHops     VTSSUnsigned8
}

