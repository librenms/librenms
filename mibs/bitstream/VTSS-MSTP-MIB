    SYNTAX      SEQUENCE OF VTSSMstpStatusInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This table represent the status of the interface instances"
    ::= { vtssMstpStatus 2 }

vtssMstpStatusInterfaceEntry OBJECT-TYPE
    SYNTAX      VTSSMstpStatusInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "A MSTP interface instance set of status objects"
    INDEX       { vtssMstpStatusInterfaceInterfaceNo, vtssMstpStatusInterfaceInstance }
    ::= { vtssMstpStatusInterfaceTable 1 }

VTSSMstpStatusInterfaceEntry ::= SEQUENCE {
    vtssMstpStatusInterfaceInterfaceNo      VTSSInterfaceIndex,
    vtssMstpStatusInterfaceInstance     Integer32,
    vtssMstpStatusInterfaceEnabled      TruthValue,
    vtssMstpStatusInterfaceActive   TruthValue,
    vtssMstpStatusInterfaceParentPort   Unsigned32,
    vtssMstpStatusInterfaceUpTime   Unsigned32,
    vtssMstpStatusInterfacePortState    VTSSMstpPortState,
    vtssMstpStatusInterfacePortId   OCTET STRING,
    vtssMstpStatusInterfacePathCost     Unsigned32,
    vtssMstpStatusInterfaceDesignatedRoot   OCTET STRING,
    vtssMstpStatusInterfaceDesignatedCost   Unsigned32,
    vtssMstpStatusInterfaceDesignatedBridge     OCTET STRING,
    vtssMstpStatusInterfaceDesignatedPort   OCTET STRING,
    vtssMstpStatusInterfaceTcAck    TruthValue,
    vtssMstpStatusInterfaceHelloTime    Unsigned32,
    vtssMstpStatusInterfaceAdminEdgePort    TruthValue,
    vtssMstpStatusInterfaceOperEdgePort     TruthValue,
    vtssMstpStatusInterfaceAutoEdgePort     TruthValue,
    vtssMstpStatusInterfaceMacOperational   TruthValue,
    vtssMstpStatusInterfaceAdminPointToPointMAC     VTSSMstpPoint2Point,
    vtssMstpStatusInterfaceOperPointToPointMAC      TruthValue,
    vtssMstpStatusInterfaceRestrictedRole   TruthValue,
    vtssMstpStatusInterfaceRestrictedTcn    TruthValue,
    vtssMstpStatusInterfacePortRole     VTSSDisplayString,
    vtssMstpStatusInterfaceDisputed     TruthValue
}

