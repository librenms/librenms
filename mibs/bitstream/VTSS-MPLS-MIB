vtssMplsStatisticsPw OBJECT IDENTIFIER
    ::= { vtssMplsStatistics 5 }

vtssMplsStatisticsPwTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSMplsStatisticsPwEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This table contains MPLS Tunnel Counters."
    ::= { vtssMplsStatisticsPw 1 }

vtssMplsStatisticsPwEntry OBJECT-TYPE
    SYNTAX      VTSSMplsStatisticsPwEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Counters."
    INDEX       { vtssMplsStatisticsPwGroupIfIndex }
    ::= { vtssMplsStatisticsPwTable 1 }

VTSSMplsStatisticsPwEntry ::= SEQUENCE {
    vtssMplsStatisticsPwGroupIfIndex    VTSSInterfaceIndex,
    vtssMplsStatisticsPwRxGreenFrames   Counter64,
    vtssMplsStatisticsPwRxGreenBytes    Counter64,
    vtssMplsStatisticsPwRxYellowFrames      Counter64,
    vtssMplsStatisticsPwRxYellowBytes   Counter64,
    vtssMplsStatisticsPwRxRedFrames     Counter64,
    vtssMplsStatisticsPwRxRedBytes      Counter64,
    vtssMplsStatisticsPwRxDiscardFrames     Counter64,
    vtssMplsStatisticsPwRxDiscardBytes      Counter64,
    vtssMplsStatisticsPwTxGreenFrames   Counter64,
    vtssMplsStatisticsPwTxGreenBytes    Counter64,
    vtssMplsStatisticsPwTxYellowFrames      Counter64,
    vtssMplsStatisticsPwTxYellowBytes   Counter64,
    vtssMplsStatisticsPwTxDiscardFrames     Counter64,
    vtssMplsStatisticsPwTxDiscardBytes      Counter64
}

vtssMplsStatisticsPwGroupIfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "MPLS group interface index. Valid range is BASE+GROUP where the base is
         specified in the interface index textual convention and the group
         number range is 1..maximum group number. This maximum is
         platform-specific and can be retrieved from the MPLS capabilities."
    ::= { vtssMplsStatisticsPwEntry 1 }

