vtssMplsStatus OBJECT IDENTIFIER
    ::= { vtssMplsMibObjects 3 }

vtssMplsStatusGlobal OBJECT IDENTIFIER
    ::= { vtssMplsStatus 1 }

vtssMplsStatusLink OBJECT IDENTIFIER
    ::= { vtssMplsStatus 2 }

vtssMplsStatusLinkTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSMplsStatusLinkEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This table contains MPLS Link status per MPLS group."
    ::= { vtssMplsStatusLink 1 }

vtssMplsStatusLinkEntry OBJECT-TYPE
    SYNTAX      VTSSMplsStatusLinkEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Status."
    INDEX       { vtssMplsStatusLinkGroupIfIndex }
    ::= { vtssMplsStatusLinkTable 1 }

VTSSMplsStatusLinkEntry ::= SEQUENCE {
    vtssMplsStatusLinkGroupIfIndex      VTSSInterfaceIndex,
    vtssMplsStatusLinkOamActive     TruthValue
}

vtssMplsStatusLinkGroupIfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "MPLS group interface index. Valid range is BASE+GROUP where the base is
         specified in the interface index textual convention and the group
         number range is 1..maximum group number. This maximum is
         platform-specific and can be retrieved from the MPLS capabilities."
    ::= { vtssMplsStatusLinkEntry 1 }

vtssMplsStatusLinkOamActive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies whether Oam is currently active or not on this Link."
    ::= { vtssMplsStatusLinkEntry 2 }

vtssMplsStatusTunnel OBJECT IDENTIFIER
    ::= { vtssMplsStatus 3 }

vtssMplsStatusTunnelTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSMplsStatusTunnelEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This table contains MPLS Tunnel status per MPLS group."
    ::= { vtssMplsStatusTunnel 1 }

vtssMplsStatusTunnelEntry OBJECT-TYPE
    SYNTAX      VTSSMplsStatusTunnelEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Status."
    INDEX       { vtssMplsStatusTunnelGroupIfIndex }
    ::= { vtssMplsStatusTunnelTable 1 }

VTSSMplsStatusTunnelEntry ::= SEQUENCE {
    vtssMplsStatusTunnelGroupIfIndex    VTSSInterfaceIndex,
    vtssMplsStatusTunnelOamActive   TruthValue,
    vtssMplsStatusTunnelState   VTSSMplsStateType,
    vtssMplsStatusTunnelIngressLabel    Unsigned32,
    vtssMplsStatusTunnelEgressLabel     Unsigned32,
    vtssMplsStatusTunnelWorkingActive   TruthValue
}

vtssMplsStatusTunnelGroupIfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "MPLS group interface index. Valid range is BASE+GROUP where the base is
         specified in the interface index textual convention and the group
         number range is 1..maximum group number. This maximum is
         platform-specific and can be retrieved from the MPLS capabilities."
    ::= { vtssMplsStatusTunnelEntry 1 }

vtssMplsStatusTunnelOamActive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies whether Oam is currently active or not on this Tunnel."
    ::= { vtssMplsStatusTunnelEntry 2 }

