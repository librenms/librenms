vtssMplsConfigTunnel OBJECT IDENTIFIER
    ::= { vtssMplsConfig 3 }

vtssMplsConfigTunnelTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSMplsConfigTunnelEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is the MPLS Tunnel configuration table."
    ::= { vtssMplsConfigTunnel 1 }

vtssMplsConfigTunnelEntry OBJECT-TYPE
    SYNTAX      VTSSMplsConfigTunnelEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents an MPLS Tunnel."
    INDEX       { vtssMplsConfigTunnelGroupIfIndex }
    ::= { vtssMplsConfigTunnelTable 1 }

VTSSMplsConfigTunnelEntry ::= SEQUENCE {
    vtssMplsConfigTunnelGroupIfIndex    VTSSInterfaceIndex,
    vtssMplsConfigTunnelTunnelName      VTSSDisplayString,
    vtssMplsConfigTunnelTunnelMode      VTSSMplsTunnelMode,
    vtssMplsConfigTunnelSrcNodeId   IpAddress,
    vtssMplsConfigTunnelSrcGlobalId     Unsigned32,
    vtssMplsConfigTunnelDstNodeId   IpAddress,
    vtssMplsConfigTunnelDstGlobalId     Unsigned32,
    vtssMplsConfigTunnelDstTunnelTpNum      Unsigned32,
    vtssMplsConfigTunnelSrcTunnelTpNum      VTSSUnsigned16,
    vtssMplsConfigTunnelSrcLspNum   VTSSUnsigned16,
    vtssMplsConfigTunnelDstLspNum   VTSSUnsigned16,
    vtssMplsConfigTunnelIsSpme      TruthValue,
    vtssMplsConfigTunnelSrcNodeIsValid      TruthValue,
    vtssMplsConfigTunnelSrcGlobalIdValid    TruthValue,
    vtssMplsConfigTunnelDstGlobalIdValid    TruthValue,
    vtssMplsConfigTunnelIngressLabel    Unsigned32,
    vtssMplsConfigTunnelEgressLabel     Unsigned32,
    vtssMplsConfigTunnelAttachInterface     VTSSInterfaceIndex,
    vtssMplsConfigTunnelTrafficClass    VTSSUnsigned8,
    vtssMplsConfigTunnelTtl     VTSSUnsigned8,
    vtssMplsConfigTunnelInCosMapId      VTSSUnsigned8,
    vtssMplsConfigTunnelOutCosMapId     VTSSUnsigned8,
    vtssMplsConfigTunnelIsLLsp      TruthValue,
    vtssMplsConfigTunnelLLspCos     VTSSUnsigned8,
    vtssMplsConfigTunnelAction      VTSSRowEditorState
}

vtssMplsConfigTunnelGroupIfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "MPLS group interface index. Valid range is BASE+GROUP where the base is
         specified in the interface index textual convention and the group
         number range is 1..maximum group number. This maximum is
         platform-specific and can be retrieved from the MPLS capabilities."
    ::= { vtssMplsConfigTunnelEntry 1 }

