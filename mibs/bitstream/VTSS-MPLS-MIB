    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Destination global ID valid (for OAM)."
    ::= { vtssMplsConfigTunnelEntry 15 }

vtssMplsConfigTunnelIngressLabel OBJECT-TYPE
    SYNTAX      Unsigned32 (16..1048575)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Ingress MPLS label value or VTSS_APPL_MPLSTP_LABEL_NONE."
    ::= { vtssMplsConfigTunnelEntry 16 }

vtssMplsConfigTunnelEgressLabel OBJECT-TYPE
    SYNTAX      Unsigned32 (16..1048575)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Egress MPLS label value or VTSS_APPL_MPLSTP_LABEL_NONE."
    ::= { vtssMplsConfigTunnelEntry 17 }

vtssMplsConfigTunnelAttachInterface OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "MPLS link interface or MPLS tunnel endpoint interface that tunnel
         endpoint is attached to."
    ::= { vtssMplsConfigTunnelEntry 18 }

vtssMplsConfigTunnelTrafficClass OBJECT-TYPE
    SYNTAX      VTSSUnsigned8 (0..7)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Traffic class used on egress unless overruled by uniform tunnel mode."
    ::= { vtssMplsConfigTunnelEntry 19 }

vtssMplsConfigTunnelTtl OBJECT-TYPE
    SYNTAX      VTSSUnsigned8 (0..255)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Time-to-live used on egress unless overruled by uniform tunnel mode."
    ::= { vtssMplsConfigTunnelEntry 20 }

vtssMplsConfigTunnelInCosMapId OBJECT-TYPE
    SYNTAX      VTSSUnsigned8
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Ingress Class of Service Map ID or 0 for no mapping."
    ::= { vtssMplsConfigTunnelEntry 21 }

vtssMplsConfigTunnelOutCosMapId OBJECT-TYPE
    SYNTAX      VTSSUnsigned8
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Egress Class of Service Map ID or 0 for no mapping."
    ::= { vtssMplsConfigTunnelEntry 22 }

vtssMplsConfigTunnelIsLLsp OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "TRUE for L-LSP, FALSE for E-LSP. Note that for L-LSP, the Ingress Class
         of Service Map only maps the ingress Traffic Class to Drop Precedence.
         The Class of Service is given by the vtssMplsConfigTunnelLLspCos field."
    ::= { vtssMplsConfigTunnelEntry 23 }

vtssMplsConfigTunnelLLspCos OBJECT-TYPE
    SYNTAX      VTSSUnsigned8 (0..7)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Ingress Class of Service for L-LSP (vtssMplsConfigTunnelIsLLsp == TRUE)"
    ::= { vtssMplsConfigTunnelEntry 24 }

vtssMplsConfigTunnelAction OBJECT-TYPE
