VTSS-MPLS-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP
        FROM SNMPv2-CONF
    MODULE-IDENTITY, OBJECT-TYPE
        FROM SNMPv2-SMI
    TEXTUAL-CONVENTION
        FROM SNMPv2-TC
    vtssSwitchMgmt
        FROM VTSS-SMI
    Counter64 FROM SNMPv2-SMI
    Integer32 FROM SNMPv2-SMI
    IpAddress FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    MacAddress FROM SNMPv2-TC
    TruthValue FROM SNMPv2-TC
    VTSSDisplayString FROM VTSS-TC
    VTSSInterfaceIndex FROM VTSS-TC
    VTSSRowEditorState FROM VTSS-TC
    VTSSUnsigned16 FROM VTSS-TC
    VTSSUnsigned8 FROM VTSS-TC
    ;

vtssMplsMib MODULE-IDENTITY
    LAST-UPDATED "201506220000Z"
    ORGANIZATION
        "Vitesse Semiconductor Corporation"
    CONTACT-INFO
        "Application support <application_support@vitesse.com>"
    DESCRIPTION
        "Private MIB for Multi Protocol Label Switching, MPLS."
    REVISION    "201506220000Z"
    DESCRIPTION
        "Added vtssMplsStatisticsLinkTable"
    REVISION    "201411170000Z"
    DESCRIPTION
        "Changed range for ccCarrierCode"
    REVISION    "201411030000Z"
    DESCRIPTION
        "Added range to HQoSId, ForwardHQoSId, ReverseHQoSId and changed name of
         tunnelmode from undefined to useglobal"
    REVISION    "201410010000Z"
    DESCRIPTION
        "Added vtssMplsStatusTunnelIngressLabel, vtssMplsStatusTunnelEgressLabel
         and vtssMplsStatusTunnelWorkingActive to VTSSMplsStatusTunnelEntry"
    REVISION    "201408040000Z"
    DESCRIPTION
        "Initial version"
    ::= { vtssSwitchMgmt 127 }


VTSSMplsStateType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the MPLS state."
    SYNTAX      INTEGER { unconfigured(0), configured(1), up(2), down(3) }

VTSSMplsTagType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the MPLS tag type."
    SYNTAX      INTEGER { untagged(0), ctagged(1), stagged(2) }

VTSSMplsTunnelMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the MPLS tunnel mode."
    SYNTAX      INTEGER { pipe(0), shortpipe(1), uniform(2), useglobal(3) }

VTSSMplsVccvType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the MPLS Vccv type."
    SYNTAX      INTEGER { none(0), vccv1(1), vccv2(2), vccv3(3), vccv4(4) }

vtssMplsMibObjects OBJECT IDENTIFIER
    ::= { vtssMplsMib 1 }

vtssMplsCapabilities OBJECT IDENTIFIER
    ::= { vtssMplsMibObjects 1 }

