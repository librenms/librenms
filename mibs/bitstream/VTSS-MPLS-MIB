vtssMplsConfigLsp OBJECT IDENTIFIER
    ::= { vtssMplsConfig 4 }

vtssMplsConfigLspTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSMplsConfigLspEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is the MPLS Lsp configuration table."
    ::= { vtssMplsConfigLsp 1 }

vtssMplsConfigLspEntry OBJECT-TYPE
    SYNTAX      VTSSMplsConfigLspEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents an MPLS Lsp."
    INDEX       { vtssMplsConfigLspGroupIndex }
    ::= { vtssMplsConfigLspTable 1 }

VTSSMplsConfigLspEntry ::= SEQUENCE {
    vtssMplsConfigLspGroupIndex     Integer32,
    vtssMplsConfigLspXcName     VTSSDisplayString,
    vtssMplsConfigLspSrcNodeId      IpAddress,
    vtssMplsConfigLspSrcNodeIdIsDefined     TruthValue,
    vtssMplsConfigLspSrcGlobalId    Unsigned32,
    vtssMplsConfigLspSrcGlobalIdIsDefined   TruthValue,
    vtssMplsConfigLspSrcTunnelTpNum     VTSSUnsigned16,
    vtssMplsConfigLspDstNodeId      IpAddress,
    vtssMplsConfigLspDstGlobalId    Unsigned32,
    vtssMplsConfigLspDstGlobalIdIsDefined   TruthValue,
    vtssMplsConfigLspDstTunnelTpNum     VTSSUnsigned16,
    vtssMplsConfigLspSrcLspNumber   VTSSUnsigned16,
    vtssMplsConfigLspDstLspNumber   VTSSUnsigned16,
    vtssMplsConfigLspForwardIngressLabel    Unsigned32,
    vtssMplsConfigLspForwardEgressLabel     Unsigned32,
    vtssMplsConfigLspForwardAttachInterface     VTSSInterfaceIndex,
    vtssMplsConfigLspForwardInCosMapId      VTSSUnsigned8,
    vtssMplsConfigLspForwardOutCosMapId     VTSSUnsigned8,
    vtssMplsConfigLspForwardIsLLsp      TruthValue,
    vtssMplsConfigLspForwardHQoSId      VTSSUnsigned16,
    vtssMplsConfigLspForwardLLspCos     VTSSUnsigned8,
    vtssMplsConfigLspReverseIngressLabel    Unsigned32,
    vtssMplsConfigLspReverseEgressLabel     Unsigned32,
    vtssMplsConfigLspReverseAttachInterface     VTSSInterfaceIndex,
    vtssMplsConfigLspReverseInCosMapId      VTSSUnsigned8,
    vtssMplsConfigLspReverseOutCosMapId     VTSSUnsigned8,
    vtssMplsConfigLspReverseIsLLsp      TruthValue,
    vtssMplsConfigLspReverseLLspCos     VTSSUnsigned8,
    vtssMplsConfigLspReverseHQoSId      VTSSUnsigned16,
    vtssMplsConfigLspAction     VTSSRowEditorState
}

vtssMplsConfigLspGroupIndex OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "MPLS group index number. Valid range is (1..max groups). The maximum
         group number is platform-specific and can be retrieved from the MPLS
         capabilities."
    ::= { vtssMplsConfigLspEntry 1 }

