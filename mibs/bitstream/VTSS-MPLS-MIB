vtssMplsConfigPw OBJECT IDENTIFIER
    ::= { vtssMplsConfig 5 }

vtssMplsConfigPwTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSMplsConfigPwEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is the MPLS Pseudo Wire configuration table."
    ::= { vtssMplsConfigPw 1 }

vtssMplsConfigPwEntry OBJECT-TYPE
    SYNTAX      VTSSMplsConfigPwEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents an MPLS Pseudo Wire."
    INDEX       { vtssMplsConfigPwGroupIfIndex }
    ::= { vtssMplsConfigPwTable 1 }

VTSSMplsConfigPwEntry ::= SEQUENCE {
    vtssMplsConfigPwGroupIfIndex    VTSSInterfaceIndex,
    vtssMplsConfigPwInLabel     Unsigned32,
    vtssMplsConfigPwOutLabel    Unsigned32,
    vtssMplsConfigPwControlWord     Unsigned32,
    vtssMplsConfigPwUseControlWord      TruthValue,
    vtssMplsConfigPwTunnelMode      VTSSMplsTunnelMode,
    vtssMplsConfigPwTrafficClass    VTSSUnsigned8,
    vtssMplsConfigPwTtl     VTSSUnsigned8,
    vtssMplsConfigPwInCosMapId      VTSSUnsigned8,
    vtssMplsConfigPwOutCosMapId     VTSSUnsigned8,
    vtssMplsConfigPwIsLLsp      TruthValue,
    vtssMplsConfigPwLLspCos     VTSSUnsigned8,
    vtssMplsConfigPwAttachInterface     VTSSInterfaceIndex,
    vtssMplsConfigPwStitchPwInterface   VTSSInterfaceIndex,
    vtssMplsConfigPwVccvType    VTSSMplsVccvType,
    vtssMplsConfigPwHQoSId      VTSSUnsigned16,
    vtssMplsConfigPwSrcNodeId   IpAddress,
    vtssMplsConfigPwSrcNodeIdIsDefined      TruthValue,
    vtssMplsConfigPwSrcGlobalId     Unsigned32,
    vtssMplsConfigPwSrcGlobalIdIsDefined    TruthValue,
    vtssMplsConfigPwDstNodeId   IpAddress,
    vtssMplsConfigPwDstGlobalId     Unsigned32,
    vtssMplsConfigPwDstGlobalIdIsDefined    TruthValue,
    vtssMplsConfigPwSrcAcId     Unsigned32,
    vtssMplsConfigPwDstAcId     Unsigned32,
    vtssMplsConfigPwSrcAgiValue     OCTET STRING,
    vtssMplsConfigPwDstAgiValue     OCTET STRING,
    vtssMplsConfigPwSrcAgiType      VTSSUnsigned8,
    vtssMplsConfigPwSrcAgiLength    VTSSUnsigned8,
    vtssMplsConfigPwDstAgiType      VTSSUnsigned8,
    vtssMplsConfigPwDstAgiLength    VTSSUnsigned8,
    vtssMplsConfigPwAction      VTSSRowEditorState
}

vtssMplsConfigPwGroupIfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "MPLS group interface index. Valid range is BASE+GROUP where the base is
         specified in the interface index textual convention and the group
         number range is 1..maximum group number. This maximum is
         platform-specific and can be retrieved from the MPLS capabilities."
    ::= { vtssMplsConfigPwEntry 1 }

