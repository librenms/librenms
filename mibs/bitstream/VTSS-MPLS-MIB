    ::= { vtssMplsCapabilities 6 }

vtssMplsConfig OBJECT IDENTIFIER
    ::= { vtssMplsMibObjects 2 }

vtssMplsConfigGlobal OBJECT IDENTIFIER
    ::= { vtssMplsConfig 1 }

vtssMplsConfigGlobalMain OBJECT IDENTIFIER
    ::= { vtssMplsConfigGlobal 1 }

