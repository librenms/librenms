    ::= { vtssMplsConfigGlobalMain 4 }

vtssMplsConfigLink OBJECT IDENTIFIER
    ::= { vtssMplsConfig 2 }

vtssMplsConfigLinkTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSMplsConfigLinkEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is the MPLS Link configuration table."
    ::= { vtssMplsConfigLink 1 }

vtssMplsConfigLinkEntry OBJECT-TYPE
    SYNTAX      VTSSMplsConfigLinkEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents an MPLS Link."
    INDEX       { vtssMplsConfigLinkGroupIfIndex }
    ::= { vtssMplsConfigLinkTable 1 }

VTSSMplsConfigLinkEntry ::= SEQUENCE {
    vtssMplsConfigLinkGroupIfIndex      VTSSInterfaceIndex,
    vtssMplsConfigLinkPort      VTSSInterfaceIndex,
    vtssMplsConfigLinkMACAddressNextHop     MacAddress,
    vtssMplsConfigLinkMACAddress    MacAddress,
    vtssMplsConfigLinkVLANTagType   VTSSMplsTagType,
    vtssMplsConfigLinkVLANId    VTSSUnsigned16,
    vtssMplsConfigLinkVLANpcp   Unsigned32,
    vtssMplsConfigLinkVLANdei   VTSSUnsigned8,
    vtssMplsConfigLinkSrcNodeId     IpAddress,
    vtssMplsConfigLinkSrcGlobalId   Unsigned32,
    vtssMplsConfigLinkDstNodeId     IpAddress,
    vtssMplsConfigLinkDstGlobalId   Unsigned32,
    vtssMplsConfigLinkDstIfNum      Unsigned32,
    vtssMplsConfigLinkSrcNodeIdValid    TruthValue,
    vtssMplsConfigLinkSrcGlobalIdValid      TruthValue,
    vtssMplsConfigLinkDstGlobalIdValid      TruthValue,
    vtssMplsConfigLinkAction    VTSSRowEditorState
}

vtssMplsConfigLinkGroupIfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "MPLS group interface index. Valid range is BASE+GROUP where the base is
         specified in the interface index textual convention and the group
         number range is 1..maximum group number. This maximum is
         platform-specific and can be retrieved from the MPLS capabilities."
    ::= { vtssMplsConfigLinkEntry 1 }

vtssMplsConfigLinkPort OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Physical port interface index."
    ::= { vtssMplsConfigLinkEntry 2 }

vtssMplsConfigLinkMACAddressNextHop OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "MAC address of peer (next hop) MPLS node."
    ::= { vtssMplsConfigLinkEntry 3 }

