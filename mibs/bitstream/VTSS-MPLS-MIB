    SYNTAX      VTSSMplsStateType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies current MPLS-TP Tunnel state."
    ::= { vtssMplsStatusTunnelEntry 3 }

vtssMplsStatusTunnelIngressLabel OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Current MPLS-TP Tunnel ingress MPLS label value or
         VTSS_APPL_MPLSTP_LABEL_NONE."
