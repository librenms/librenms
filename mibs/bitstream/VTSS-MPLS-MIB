vtssMplsConfigCosMap OBJECT IDENTIFIER
    ::= { vtssMplsConfig 6 }

vtssMplsConfigCosMapTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSMplsConfigCosMapEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is the MPLS Class of Service map configuration table."
    ::= { vtssMplsConfigCosMap 1 }

vtssMplsConfigCosMapEntry OBJECT-TYPE
    SYNTAX      VTSSMplsConfigCosMapEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents an MPLS Class of Service mapping."
    INDEX       { vtssMplsConfigCosMapGroupIndex }
    ::= { vtssMplsConfigCosMapTable 1 }

VTSSMplsConfigCosMapEntry ::= SEQUENCE {
    vtssMplsConfigCosMapGroupIndex      Integer32,
    vtssMplsConfigCosMapInTcToCos0      VTSSUnsigned8,
    vtssMplsConfigCosMapInTcToCos1      VTSSUnsigned8,
    vtssMplsConfigCosMapInTcToCos2      VTSSUnsigned8,
    vtssMplsConfigCosMapInTcToCos3      VTSSUnsigned8,
    vtssMplsConfigCosMapInTcToCos4      VTSSUnsigned8,
    vtssMplsConfigCosMapInTcToCos5      VTSSUnsigned8,
