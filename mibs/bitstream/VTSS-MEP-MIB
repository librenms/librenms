vtssMepConfigInstanceTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSMepConfigInstanceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is a table of created instance configuration parameters"
    ::= { vtssMepConfigInstance 1 }

vtssMepConfigInstanceEntry OBJECT-TYPE
    SYNTAX      VTSSMepConfigInstanceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is a created instance configuration parameters. Not all parameters
         can be changed"
    INDEX       { vtssMepConfigInstanceId }
    ::= { vtssMepConfigInstanceTable 1 }

VTSSMepConfigInstanceEntry ::= SEQUENCE {
    vtssMepConfigInstanceId     Integer32,
    vtssMepConfigInstanceMode   VTSSMepInstanceMode,
    vtssMepConfigInstanceDirection      VTSSMepInstanceDirection,
    vtssMepConfigInstanceFlow   VTSSInterfaceIndex,
    vtssMepConfigInstancePort   VTSSInterfaceIndex,
    vtssMepConfigInstanceLevel      Unsigned32,
    vtssMepConfigInstanceVid    VTSSUnsigned16,
    vtssMepConfigInstanceVoe    TruthValue,
    vtssMepConfigInstanceMac    MacAddress,
    vtssMepConfigInstanceFormat     VTSSMepMegIdFormat,
    vtssMepConfigInstanceName   VTSSDisplayString,
    vtssMepConfigInstanceMeg    VTSSDisplayString,
    vtssMepConfigInstanceMep    Unsigned32,
    vtssMepConfigInstanceEvcPag     Unsigned32,
    vtssMepConfigInstanceEvcQos     Unsigned32,
    vtssMepConfigInstanceAction     VTSSRowEditorState
}

