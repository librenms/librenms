VTSS-MEP-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP
        FROM SNMPv2-CONF
    MODULE-IDENTITY, OBJECT-TYPE
        FROM SNMPv2-SMI
    TEXTUAL-CONVENTION
        FROM SNMPv2-TC
    vtssSwitchMgmt
        FROM VTSS-SMI
    Counter64 FROM SNMPv2-SMI
    Integer32 FROM SNMPv2-SMI
    IpAddress FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    MacAddress FROM SNMPv2-TC
    TruthValue FROM SNMPv2-TC
    VTSSDisplayString FROM VTSS-TC
    VTSSInterfaceIndex FROM VTSS-TC
    VTSSRowEditorState FROM VTSS-TC
    VTSSUnsigned16 FROM VTSS-TC
    VTSSUnsigned64 FROM VTSS-TC
    VTSSUnsigned8 FROM VTSS-TC
    ;

vtssMepMib MODULE-IDENTITY
    LAST-UPDATED "201501190000Z"
    ORGANIZATION
        "Vitesse Semiconductor Corporation"
    CONTACT-INFO
        "Application support <application_support@vitesse.com>"
    DESCRIPTION
        "This is a private service OAM (Y.1731/G802.1ag/G8113.1/G8113.2) MIB"
    REVISION    "201501190000Z"
    DESCRIPTION
        "Added Loss Measurement Availability, High Loss Interval and Signal
         Degrade"
    REVISION    "201501020000Z"
    DESCRIPTION
        "Removed DEI"
    REVISION    "201412030000Z"
    DESCRIPTION
        "Adding Link State Tracking"
    REVISION    "201411260000Z"
    DESCRIPTION
        "Adding Loss Measurement OAM frame counting object"
    REVISION    "201411140000Z"
    DESCRIPTION
        "Refactoring regarding Ifindex. Domain element is removed and flow
         element is changed to be Ifindex containing domain"
    REVISION    "201409300000Z"
    DESCRIPTION
        "Added vtssMepConfigRt and vtssMepStatusRt for Route Trace configuration
         and status"
    REVISION    "201409030000Z"
    DESCRIPTION
        "Added vtssMepConfigClientFlowDomain and vtssMepConfigClientFlowFlowId
         to vtssMepConfigClientFlowTable"
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { vtssSwitchMgmt 46 }


VTSSMepApsType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Type of APS."
    SYNTAX      INTEGER { invalidAps(0), linearAps(1), ringAps(2) }

VTSSMepBfdAuthenticationType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "BFD authentication type."
    SYNTAX      INTEGER { disabled(0), simplePwd(1), keyedMD5(2), meticulousKeyedMD5(3), keyedSHA1(4), meticulousKeyedSHA1(5) }

VTSSMepBfdDiagnosticCode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "BFD diagnostic code."
    SYNTAX      INTEGER { none(0), detectExpired(1), echoFailed(2), remoteDown(3), fwdPlaneReset(4), pathDown(5), concatPathDown(6), adminDown(7), reverseConcatDown(8), misconnectivity(9) }

VTSSMepBfdSessionState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "State of BFD session."
    SYNTAX      INTEGER { adminDown(0), down(1), init(2), up(3) }

VTSSMepCast ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Unicast or Multicast selection."
    SYNTAX      INTEGER { uniCast(0), multiCast(1) }

VTSSMepDmCalcWay ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Round trip or flow calculation way selection."
    SYNTAX      INTEGER { roundTrip(0), flow(1) }

VTSSMepDmOverflowAction ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Action when internal total delay counter overflow."
    SYNTAX      INTEGER { disableDm(0), continueDm(1) }

VTSSMepDmTimeUnit ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Calculation time unit."
    SYNTAX      INTEGER { microSeconds(0), nanoSeconds(1) }

VTSSMepEnded ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Dual/single ended selection."
    SYNTAX      INTEGER { singleEnded(0), dualEnded(1) }

VTSSMepInstanceDirection ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "The instance direction."
    SYNTAX      INTEGER { down(0), up(1) }

VTSSMepInstanceMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "The instance mode."
    SYNTAX      INTEGER { mep(0), mip(1) }

VTSSMepLmAvailState ::= TEXTUAL-CONVENTION
