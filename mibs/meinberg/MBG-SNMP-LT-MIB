    --- " 1: sync"
    --- " 2: not sync now"
    --- " 3: never synced"
    DEFVAL { 0 }
    ::= { mbgLtRefclock 12 }

mbgLtRefPzfKorrelation OBJECT-TYPE
    SYNTAX      Integer32 (0..255)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "current Korrelation of DCF77/PZF refclock connected to NTP time server "
    --- "   0: not available"
    --- " 254: Antenna Faulty"
    --- " 255: Gsync"
    --- " minimum of 60 to get synchronization"
    --- " optimal value of 75"
    DEFVAL { 0 }
    ::= { mbgLtRefclock 13 }

