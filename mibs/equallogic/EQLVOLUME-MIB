       DISPLAY-HINT       "d"
       STATUS              current
       DESCRIPTION        "This textual convention defines the possible virtual volume created as values"
       SYNTAX              INTEGER  {                      
                                   stand-alone(1),
                                   snapshot (2),
                                   fast-clone (3)                        
                           }

