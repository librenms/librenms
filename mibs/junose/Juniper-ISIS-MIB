juniIsisCircBFDGroup  OBJECT-GROUP
    OBJECTS {
         juniIsisCircBfdEnable,
         juniIsisCircBfdMinRxInterval,
    	 juniIsisCircBfdMinTxInterval,
    	 juniIsisCircBfdMultiplier
 }
    STATUS      current
    DESCRIPTION
        "The circuit level ISIS BFD configuration parameters."
    ::= { juniIsisMIBGroups 5 }
    
