    ::= { juniIsisCircEntry 36 }

juniIsisCircMinLSPTransInt OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "milliseconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
