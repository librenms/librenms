    SYNTAX      DisplayString (SIZE(0..3))
    MAX-ACCESS  read-only
    STATUS      deprecated
    DESCRIPTION
        "The revision number of the card assembly in this slot."
    DEFVAL    { "" }
    ::= { juniERXSysSlotEntry 25 }

