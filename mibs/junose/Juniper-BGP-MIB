    SYNTAX      JuniBgpStorageInteger
    MAX-ACCESS  read-write
    STATUS      obsolete
    DESCRIPTION
        "Obsolete maximum size of the peer-group pool in entries."
    DEFVAL    { 500000000 }
    ::= { juniBgpStorageGroup 13 }

