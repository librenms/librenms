juniAtmConformance OBJECT IDENTIFIER ::= { juniAtmMIB 4 }
juniAtmCompliances OBJECT IDENTIFIER ::= { juniAtmConformance 1 }
juniAtmGroups      OBJECT IDENTIFIER ::= { juniAtmConformance 2 }

