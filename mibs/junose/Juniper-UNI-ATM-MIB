    juniAtmIfSvcSignallingVpi            AtmVpIdentifier,
    juniAtmIfSvcSignallingVci            AtmVcIdentifier,
    juniAtmIfSvcSignallingVcd            Integer32,
    juniAtmIfSvcSignallingAdminStatus    AtmVorXAdminStatus }

