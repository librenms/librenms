        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "GRE destination IP address"
        ::= { peakflowTmsMgr 15 }

tmsTrapNexthop          OBJECT-TYPE
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "Nexthop IP address"
        ::= { peakflowTmsMgr 16 }

