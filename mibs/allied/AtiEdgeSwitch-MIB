        SYNTAX          INTEGER {
                                disabled(1),
                                enabled(2)
                                }
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
                "The enable status."
        DEFVAL          { 1 }
        ::= { atiSflowEnableEntry 2 }

