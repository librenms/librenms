AtiSflowEnableEntry ::=
        SEQUENCE {
                atiSflowEnablePort                      Integer32,
                atiSflowEnableStatus                    INTEGER
        }

atiSflowEnablePort      OBJECT-TYPE
        SYNTAX          Integer32 (0..255)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "Index into sFlowEnableTable."
        ::= { atiSflowEnableEntry 1 }

