        SYNTAX          INTEGER {
                                any (1),
                                host (2),
                                ip (3)
                                }
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
                "The Object specifies the destination type of the ACL."
        ::= { atiStkSwACLConfigEntry 9 }

