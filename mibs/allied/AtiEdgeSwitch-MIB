        ::= { atiStkSwEthPortErrTable 1 }

AtiStkSwEthPortErrEntry ::=
        SEQUENCE {
                atiStkSwEthPortErrId                    Integer32,
                atiStkSwEthPortErrRxBadFrames           Counter64,
                atiStkSwEthPortErrCollisions            Counter64
                }

