        SYNTAX          INTEGER {
                                monday (1),
                                tuesday (2),
                                wednesday (3),
                                thursday (4),
                                friday (5),
                                saturday (6),
                                sunday (7)
                                }
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
                "This object specifies the start day for periodic  time range ACL."
        ::= { atiStkSwAACLTimeRangeConfigEntry 7 }

