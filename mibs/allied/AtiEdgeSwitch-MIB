                                select (6)
                                }
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
                "This object specifies the Frequency of days for periodic  time range ACL."
        ::= { atiStkSwAACLTimeRangeConfigEntry 6 }

