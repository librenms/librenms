            ::= { hwL2VlanTrapObjects 8 }			
			
     hwVlantransVlanBgn OBJECT-TYPE
            SYNTAX OCTET STRING
            MAX-ACCESS accessible-for-notify
            STATUS current
            DESCRIPTION
                "The begin mapping or stacking vlan configurations on the interface."
            ::= { hwL2VlanTrapObjects 9 }			

     hwVlantransVlanEnd OBJECT-TYPE
            SYNTAX OCTET STRING
            MAX-ACCESS accessible-for-notify
            STATUS current
            DESCRIPTION
                "The end mapping or stacking vlan configurations on the interface."
            ::= { hwL2VlanTrapObjects 10 }

     hwVlantransVlanPriBgn OBJECT-TYPE
            SYNTAX OCTET STRING
            MAX-ACCESS accessible-for-notify
            STATUS current
            DESCRIPTION
                "The begin mapping or stacking vlan-priority configurations on the interface."
            ::= { hwL2VlanTrapObjects 11 }

     hwVlantransVlanPriEnd OBJECT-TYPE
            SYNTAX OCTET STRING
            MAX-ACCESS accessible-for-notify
            STATUS current
            DESCRIPTION
                "The end mapping or stacking vlan-priority configurations on the interface."
            ::= { hwL2VlanTrapObjects 12 }

     hwVlantransInnerVlanBgn OBJECT-TYPE
            SYNTAX OCTET STRING
            MAX-ACCESS accessible-for-notify
            STATUS current
            DESCRIPTION
                "The begin mapping or stacking inner vlan configurations on the interface."
            ::= { hwL2VlanTrapObjects 13 }			

     hwVlantransInnerVlanEnd OBJECT-TYPE
            SYNTAX OCTET STRING
            MAX-ACCESS accessible-for-notify
            STATUS current
            DESCRIPTION
                "The end mapping or stacking inner vlan configurations on the interface."
            ::= { hwL2VlanTrapObjects 14 }

     hwVlantransChipSpec OBJECT-TYPE
            SYNTAX Integer32 (0..2147483647)
            MAX-ACCESS accessible-for-notify
            STATUS current
            DESCRIPTION
                "Specification for the number of vlan mapping or vlan stacking configurations on the chip."
            ::= { hwL2VlanTrapObjects 15 }
   
