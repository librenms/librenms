    -- 1.3.6.1.4.1.2011.5.1.1.26
	hwAclIPPoolHostNameTable OBJECT-TYPE
	    SYNTAX SEQUENCE OF HwAclIPPoolHostNameEntry
	    MAX-ACCESS not-accessible
	    STATUS current
	    DESCRIPTION
	    "Configure the host name of IP pool."
	    ::= { hwAclMibObjects 26 }

	-- 1.3.6.1.4.1.2011.5.1.1.26.1
	hwAclIPPoolHostNameEntry OBJECT-TYPE
	    SYNTAX HwAclIPPoolHostNameEntry
	    MAX-ACCESS not-accessible
	    STATUS current
	    DESCRIPTION
	    "Each entry is a host name of IP pool."
	    INDEX { hwAclIPPoolHostIPPoolIndex, hwAclIPPoolHostNameIndex }
	    ::= { hwAclIPPoolHostNameTable 1 }

	HwAclIPPoolHostNameEntry ::=
        SEQUENCE {
        hwAclIPPoolHostIPPoolIndex
        Integer32,
        hwAclIPPoolHostNameIndex
        Integer32,
        hwAclIPPoolHostNameName
        OCTET STRING,
        hwAclIPPoolHostNameRowStatus
        RowStatus
        }

	-- 1.3.6.1.4.1.2011.5.1.1.26.1.1
	hwAclIPPoolHostIPPoolIndex OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "This object indicates the level 1 index, that is, the index of an IP address pool used by an ACL."
        ::= { hwAclIPPoolHostNameEntry 1 }

	-- 1.3.6.1.4.1.2011.5.1.1.26.1.2
	hwAclIPPoolHostNameIndex OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "This object indicates the level 2 index, that is, the index of an host name in an IP address pool used by an ACL."
        ::= { hwAclIPPoolHostNameEntry 2 }

	-- 1.3.6.1.4.1.2011.5.1.1.26.1.3
	hwAclIPPoolHostNameName OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE (1..255))
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The object indicates the host name."
        ::= { hwAclIPPoolHostNameEntry 3 }

	-- 1.3.6.1.4.1.2011.5.1.1.26.1.4
        hwAclIPPoolHostNameRowStatus OBJECT-TYPE
        SYNTAX RowStatus
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "Row status."
        ::= { hwAclIPPoolHostNameEntry 4 }
