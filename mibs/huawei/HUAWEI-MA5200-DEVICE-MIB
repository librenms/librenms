                "
            ::= { hw52DevUserAttack 1 }

        hw52TrapSwitch OBJECT IDENTIFIER ::= { hwMA5200Device 9 }
        
        hw52HwdeviceOrBasetrap OBJECT-TYPE
            SYNTAX INTEGER
            {
                disable(1),
                hwdevice(2),
                basetrap(3)
            }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "
                Trap switches between basetrap and hwdevice.
                "
            ::= { hw52TrapSwitch 1 }            
            
