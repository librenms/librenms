            INDEX { hwMinMSIIndex, hwMinMSIStaticMacFwdCDMac }
            ::= { hwMinMSIStaticMacFwdTable 1 }
        
        HwMinMSIStaticMacFwdEntry ::=
            SEQUENCE { 
                hwMinMSIStaticMacFwdCDMac
                    MacAddress,
