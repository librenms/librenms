			SEQUENCE { 
				hwGrpProfileNameIndex
					DisplayString,
				hwGrpListNameIndex
					DisplayString,
				hwGrpNameIndex 
					DisplayString,
				hwGrpIpAddress
					IpAddress, 
				hwGrpRowStatus
				    RowStatus
			 }

		hwGrpProfileNameIndex OBJECT-TYPE
			SYNTAX DisplayString 
			MAX-ACCESS not-accessible
