        hwAuthorSchemeTable OBJECT-TYPE
            SYNTAX SEQUENCE OF HwAuthorSchemeEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "authorization Scheme Table

                授权方案表 "
            ::= { hwAAAMibObjects 8 }
        hwAuthorSchemeEntry OBJECT-TYPE
            SYNTAX HwAuthorSchemeEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "authorization Scheme Entry
                授权方案表实体入口。授权方案表，由方案名称唯一标识。方案名称作为关键字，建立时写入，以后不能更改；系统中永远存在一个“Default”的授权方案。
                索引项为节点hwAuthorSchemeName。本表用于实现对授权方案的配置。本表中配置的授权方案将被域信息表引用。
                操作约束描述：
                1、创建操作输入的方案名称不可以重复。
                2、修改操作时，方案名称创建时设置，不可修改。
                3、缺省授权方案不能删除。
                4、当前授权方案正在被某个域使用时不能删除。"
            INDEX { hwAuthorSchemeName }
            ::= { hwAuthorSchemeTable 1 }
        HwAuthorSchemeEntry ::=
            SEQUENCE {
                hwAuthorSchemeName
                    DisplayString,
                hwAuthorMethod
                    INTEGER,
                hwAuthorRowStatus
                    RowStatus
             }
        hwAuthorSchemeName OBJECT-TYPE
            SYNTAX DisplayString (SIZE (1..32))
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "authorization Scheme name
                方案名称作为关键字，建立时写入，以后不能更改；系统中永远存在一个'Default'的授权方案"
            ::= { hwAuthorSchemeEntry 1 }
        hwAuthorMethod OBJECT-TYPE
            SYNTAX INTEGER
                {
                none(1),
                local(2),
                hwtacacs(3),
                ifauthenticated(4),
                hwtacacsnone(5),
                hwtacacslocal(6),
                hwtacacsifauthenticated(7)                
                }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "authorization Scheme
                授权方法

                none(1)：不授权   
                local(2)：本地授权 
                hwtacacs(3)：TACACS授权
                ifauthenticated(4)：通过认证就授权成功
                hwtacacsnone(5):先Tac授权再不授权
                hwtacacslocal(6):先Tac授权再本地授权
                hwtacacsifauthenticated(7):先Tac授权再If授权
                "
            ::= { hwAuthorSchemeEntry 2 }
        hwAuthorRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "row admin status,only Add or Del
                支持添加、删除"
            ::= { hwAuthorSchemeEntry 3 }      

        hwRecordSchemeTable OBJECT-TYPE
            SYNTAX SEQUENCE OF HwRecordSchemeEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "record Scheme Table

                记录策略表 "
            ::= { hwAAAMibObjects 30 }

        hwRecordSchemeEntry OBJECT-TYPE
