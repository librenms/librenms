           hwSTPRelayTable OBJECT-TYPE
                        SYNTAX SEQUENCE OF HwSTPRelayEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "
                                STP透传专线用户表
                                "
                        ::= { hwSrvcfgInterfaceMibObjects 4 }
                
                hwSTPRelayEntry OBJECT-TYPE
                        SYNTAX HwSTPRelayEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                               "
                               Description.
                               "
                        INDEX { hwSTPRelayIndex }
                        ::= { hwSTPRelayTable 1 }
                
                HwSTPRelayEntry ::=
                        SEQUENCE { 
                                hwSTPRelayIndex
                                        INTEGER,
