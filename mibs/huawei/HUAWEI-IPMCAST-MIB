    SYNTAX      INTEGER {
                         starG(1),
                         sG(2)
    }						 
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION                                                                
             "The limit type:
             1:star-group;
             2:source-group;"
    ::= { hwIpMcast 33 }
		
