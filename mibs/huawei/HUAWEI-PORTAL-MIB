            DESCRIPTION
                "The row status.(V2)"
            ::= { hwPortalConfigSecretKeyEntryV2 6 }          
            
      hwPortalVrfNameV2 OBJECT-TYPE
            SYNTAX DisplayString (SIZE (1..31))
