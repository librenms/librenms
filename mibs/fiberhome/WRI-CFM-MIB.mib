wriDot1agCfmMepDbTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF WriDot1agCfmMepDbEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "The MEP Database. A database, maintained by every MEP, that
        maintains received information about other MEPs in the
        Maintenance Domain.

        The SMI does not allow to state in a MIB that an object in
        a table is an array. The solution is to take the index (or
        indices) of the first table and add one or more indices.
       "
    REFERENCE
       "19.2.15"
    ::= { wriDot1agCfmMep 3 }

wriDot1agCfmMepDbEntry OBJECT-TYPE
    SYNTAX      WriDot1agCfmMepDbEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "The MEP Database table entry."
    INDEX { wriDot1agCfmMdIndex,
            wriDot1agCfmMaIndex,
            wriDot1agCfmMepIdentifier,
            wriDot1agCfmMepDbRMepIdentifier
          }
    ::= { wriDot1agCfmMepDbTable 1 }

WriDot1agCfmMepDbEntry ::= SEQUENCE {
      wriDot1agCfmMepDbRMepIdentifier         WriDot1agCfmMepId,
      wriDot1agCfmMepDbRMepState              WriDot1agCfmRemoteMepState,
      wriDot1agCfmMepDbRMepFailedOkTime       TimeStamp,
      wriDot1agCfmMepDbMacAddress             MacAddress,
      wriDot1agCfmMepDbRdi                    TruthValue,
      wriDot1agCfmMepDbPortStatusTlv          WriDot1agCfmPortStatus,
      wriDot1agCfmMepDbInterfaceStatusTlv     WriDot1agCfmInterfaceStatus,
      wriDot1agCfmMepDbChassisIdSubtype       LldpChassisIdSubtype,
      wriDot1agCfmMepDbChassisId              LldpChassisId,
      wriDot1agCfmMepDbManAddressDomain       TDomain,
      wriDot1agCfmMepDbManAddress             TAddress,
      wriDot1agCfmMepDbOrgSpeValue            OCTET STRING, 
      wriDot1agCfmMepDbOrgSpeOui              OCTET STRING,
      wriDot1agCfmMepDbOrgSpeSubtype          INTEGER,
      wriDot1agCfmMepDbCcHwRxEnable           INTEGER,  
      wriDot1agCfmMepDbUncastCcHwRxEnable     INTEGER,
      wriDot1agCfmMepDbStorageType            INTEGER  
    }

wriDot1agCfmMepDbRMepIdentifier OBJECT-TYPE
    SYNTAX      WriDot1agCfmMepId
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "Maintenance association End Point Identifier of a remote MEP
        whose information from the MEP Database is to be returned.
       "
    REFERENCE
       "12.14.7.6.2:b"
    ::= { wriDot1agCfmMepDbEntry 1 }

wriDot1agCfmMepDbRMepState OBJECT-TYPE
    SYNTAX      WriDot1agCfmRemoteMepState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The operational state of the remote MEP IFF State machines."
    REFERENCE
       "12.14.7.6.3:b and 20.22"
    ::= { wriDot1agCfmMepDbEntry 2}

wriDot1agCfmMepDbRMepFailedOkTime OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The time (SysUpTime) at which the IFF Remote MEP state machine
        last entered either the RMEP_FAILED or RMEP_OK state.
       "
    REFERENCE
       "12.14.7.6.3:c"
    ::= { wriDot1agCfmMepDbEntry 3 }

wriDot1agCfmMepDbMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The MAC address of the remote MEP."
    REFERENCE
       "12.14.7.6.3:d and 20.19.7"
    ::= { wriDot1agCfmMepDbEntry 4 }

wriDot1agCfmMepDbRdi OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "State of the RDI bit in the last received CCM (true for
