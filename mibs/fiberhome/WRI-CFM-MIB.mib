       "12.14.7.6.3:f and 20.19.3"
    DEFVAL { psNoPortStateTLV }
    ::= { wriDot1agCfmMepDbEntry 6}
   
wriDot1agCfmMepDbInterfaceStatusTlv OBJECT-TYPE
