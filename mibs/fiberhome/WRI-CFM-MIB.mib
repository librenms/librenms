-- ******************************************************************
-- The Maintenance Association (MA) Component Table
-- ******************************************************************

wriDot1agCfmMaCompTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF WriDot1agCfmMaCompEntry
    MAX-ACCESS  not-accessible
    STATUS      deprecated
    DESCRIPTION
       "The Maintenance Association table.  Each row in the table
        represents an MA.  An MA is a set of MEPs, each configured
        with a single service instance.

        This is the part of the complete MA table that is variable
        across the Bridges in a Maintenance Domain, or across the
        components of a single Bridge.  That part of the MA table that
        is constant across the Bridges and their components in a
        Maintenance Domain is contained in the wriDot1agCfmMaNetTable.

        This table uses three indices, first index is the
        WriDot1agCfmPbbComponentIdentifier that identifies the component
        within the Bridge for which the information in the
        wriDot1agCfmMaCompEntry applies.  The second is the index of the
        Maintenance Domain table.  The third index is the same as the
        index of the wriDot1agCfmMaNetEntry for the same MA.

        The writable objects in this table need to be persistent
        upon reboot or restart of a device.

        **NOTE: this object is deprecated due to re-indexing of the 
	table.
       "
    REFERENCE
       "18.2"
    ::= { wriDot1agCfmMa 2 }

wriDot1agCfmMaCompEntry OBJECT-TYPE
    SYNTAX      WriDot1agCfmMaCompEntry
    MAX-ACCESS  not-accessible
    STATUS      deprecated
    DESCRIPTION
       "The MA table entry.
        **NOTE: this object is deprecated due to re-indexing of the 
	table.
       "
    INDEX {wriDot1agCfmMaComponentId,
           wriDot1agCfmMdIndex, wriDot1agCfmMaIndex }
    ::= { wriDot1agCfmMaCompTable 1 }

WriDot1agCfmMaCompEntry ::= SEQUENCE {
      wriDot1agCfmMaComponentId         WriDot1agCfmPbbComponentIdentifier,
      wriDot1agCfmMaCompPrimaryVlanId   VlanIdOrNone,
      wriDot1agCfmMaCompMhfCreation     WriDot1agCfmMhfCreation,
      wriDot1agCfmMaCompIdPermission    WriDot1agCfmIdPermission,
      wriDot1agCfmMaCompNumberOfVids    Unsigned32,
      wriDot1agCfmMaCompRowStatus       RowStatus}

wriDot1agCfmMaComponentId OBJECT-TYPE
    SYNTAX      WriDot1agCfmPbbComponentIdentifier
    MAX-ACCESS  not-accessible
    STATUS      deprecated
    DESCRIPTION
       "The bridge component within the system to which the information
        in this wriDot1agCfmMaCompEntry applies.  If the system is not a
        Bridge, or if only one component is present in the Bridge, then
        this variable (index) MUST be equal to 1.
	**NOTE: this object is deprecated due to re-indexing of the 
	table.
       "
    REFERENCE
       "12.3 l)"
    ::= { wriDot1agCfmMaCompEntry 1 }

wriDot1agCfmMaCompPrimaryVlanId OBJECT-TYPE
    SYNTAX      VlanIdOrNone
    MAX-ACCESS  read-create
    STATUS      deprecated
    DESCRIPTION
       "The Primary VLAN ID with which the Maintenance Association is
        associated, or 0 if the MA is not attached to any VID.  If
        the MA is associated with more than one VID, the
        wriDot1agCfmVlanTable lists them.
        **NOTE: this object is deprecated due to re-indexing of the 
	table.
       "
    REFERENCE
       "12.14.6.1.3:b"
    ::= { wriDot1agCfmMaCompEntry 2 }

wriDot1agCfmMaCompMhfCreation OBJECT-TYPE
    SYNTAX      WriDot1agCfmMhfCreation
    MAX-ACCESS  read-create
    STATUS      deprecated
    DESCRIPTION
       "Indicates if the Management entity can create MHFs (MIP Half
        Function) for this MA.
        **NOTE: this object is deprecated due to re-indexing of the 
	table.
       "
    REFERENCE
       "12.14.6.1.3:c"
    DEFVAL { defMHFdefer }
    ::= { wriDot1agCfmMaCompEntry 3 }

wriDot1agCfmMaCompIdPermission OBJECT-TYPE
    SYNTAX      WriDot1agCfmIdPermission
    MAX-ACCESS  read-create
    STATUS      deprecated
    DESCRIPTION
       "Enumerated value indicating what, if anything, is to be
        included in the Sender ID TLV (21.5.3) transmitted by MPs
