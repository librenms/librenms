       "
    REFERENCE
       "12.14.3.1.3:b"
    ::= { wriDot1agCfmDefaultMdEntry 3 }

wriDot1agCfmDefaultMdLevel OBJECT-TYPE
    SYNTAX      WriDot1agCfmMDLevelOrNone
