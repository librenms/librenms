        jnxIfOpticsPMDayMaxRxInputPower             
                                           Integer32,
        jnxIfOpticsPMDayAvgRxInputPower             
                                           Integer32,
        jnxIfOpticsPMDayTimeStamp
                                           DateAndTime,
        jnxIfOpticsPMDaySuspectedFlag
                                           TruthValue,
        jnxIfOpticsPMDaySuspectReason
                                           Integer32,
