        jnxIfOpticsPMDayMinPolarState        
                                           Integer32,
        jnxIfOpticsPMDayMaxPolarState        
                                           Integer32,
        jnxIfOpticsPMDayAvgPolarState        
                                           Integer32,
        jnxIfOpticsPMDayMinPolarDependentLoss
                                           Integer32,
