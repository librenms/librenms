    prvtIgmpSnoopMvrGrpSsmIndex         Unsigned32,
    prvtIgmpSnoopMvrGrpSsmRowStatus     RowStatus,
    prvtIgmpSnoopMvrGrpSsmAddr          IpAddress,
    prvtIgmpSnoopMvrGrpSsmSrcList       OCTET STRING,
    prvtIgmpSnoopMvrGrpSsmMode          INTEGER
}

