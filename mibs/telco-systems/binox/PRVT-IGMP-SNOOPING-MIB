    prvtIgmpSnoopMemoryToAllocate       Unsigned32,
    prvtIgmpSnoopMemoryInitCount        Unsigned32,
    prvtIgmpSnoopMemorySize             Unsigned32,
    prvtIgmpSnoopMemoryAllAllocated     Unsigned32
}

