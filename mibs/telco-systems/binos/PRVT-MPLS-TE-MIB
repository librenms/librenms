  ::= { mplsTunnelEntry 57 }

mplsTunnelDestTNAAddressType OBJECT-TYPE
  SYNTAX         INTEGER {
       none (0),
       ipv4 (1),
       ipv6 (2),
       nsap (3)
  }
  MAX-ACCESS     read-create
  STATUS         current
  DESCRIPTION
      "Type of the destination TNA address of this tunnel.  This
       field is used for UNI.  For standard MPLS and GMPLS, this
       field must be set to none."
   DEFVAL        { none }
  ::= { mplsTunnelEntry 58 }

