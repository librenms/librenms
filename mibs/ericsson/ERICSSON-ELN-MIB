      elnMezzanineBoardIndex          INTEGER,
      elnMezzanineBoardType           DisplayString,
      elnMezzanineBoardProductNo      DisplayString,
      elnMezzanineBoardRevision       DisplayString,
      elnMezzanineBoardSerialNo       DisplayString
    }
    
