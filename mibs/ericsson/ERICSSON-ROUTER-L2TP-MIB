eriRouterL2tpTunnelCntTable   OBJECT-TYPE
        SYNTAX          SEQUENCE OF EriRouterL2tpTunnelCntEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
              "The L2TP Tunnel Statistics Table."
        ::= { eriRouterL2tpMibObjects 5 }

eriRouterL2tpTunnelCntEntry   OBJECT-TYPE
        SYNTAX          EriRouterL2tpTunnelCntEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
              "An L2TP Tunnel Statistics entry. An entry in this
               table corresponds to a single tunnel."
        INDEX { eriRouterL2tpMibTunnelID }
        ::= { eriRouterL2tpTunnelCntTable 1 }

EriRouterL2tpTunnelCntEntry ::=
        SEQUENCE {
	    eriRouterL2tpTCDataPktSent     Counter32,
	    eriRouterL2tpTCDataPktRcvd     Counter32,
	    eriRouterL2tpTCDataByteSent    Counter32,
	    eriRouterL2tpTCDataByteRcvd    Counter32,
	    eriRouterL2tpTCCntrlPktSent    Counter32,
	    eriRouterL2tpTCCntrlPktRcvd    Counter32,
	    eriRouterL2tpTCCntrlByteSent   Counter32,
	    eriRouterL2tpTCCntrlByteRcvd   Counter32,
	    eriRouterL2tpTCPolicePktDrop   Counter32,
	    eriRouterL2tpTCRatePktDrop     Counter32,
	    eriRouterL2tpTCCntrlErr	     Unsigned32,
	    eriRouterL2tpTCLastCtlErr	     CtlErrType,
	    eriRouterL2tpTCLastCtlErrTime  DateAndTime,
	    eriRouterL2tpTCMaxResendQ	     Gauge32,
	    eriRouterL2tpTCMaxUnsentQ      Gauge32,
	    eriRouterL2tpTCCurResendQ      Gauge32,
	    eriRouterL2tpTCCurUnsentQ      Gauge32,
	    eriRouterL2tpTCCurWindow       Gauge32

	}

