eriRouterQosHierarchicalPClassStatsTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF EriRouterQosHierarchicalPClassStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
           "A table of drop counters for child circuits of hierarchical 
            qos policy classes applied to the child."
    ::= { eriRouterQosMibObjects 7 }

eriRouterQosHierarchicalPClassStatsEntry    OBJECT-TYPE
    SYNTAX      EriRouterQosHierarchicalPClassStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
          "An entry containing the drop counters for one child circuit
           of hierarchical qos policy classes applied to the child."
    INDEX { ifIndex, eriRouterQosHierarchicalPolicyType, eriRouterQosHierarchicalPClassId }
    ::=  { eriRouterQosHierarchicalPClassStatsTable 1 }
    
EriRouterQosHierarchicalPClassStatsEntry ::=  SEQUENCE {
    eriRouterQosHierarchicalPClassId              EriRouterQosClassId,
    eriRouterQosHierarchicalPClassName            SnmpAdminString,
    eriRouterQosHierarchicalPClassDroppedOctets   Counter64,
    eriRouterQosHierarchicalPClassDroppedPkts     Counter64
}

eriRouterQosHierarchicalPClassId OBJECT-TYPE
    SYNTAX      EriRouterQosClassId
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
          "The class id of the hierarchical parent policy class which 
           is applied to the child circuit."
    ::= { eriRouterQosHierarchicalPClassStatsEntry 1 }

eriRouterQosHierarchicalPClassName OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(1..39))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
          "Name of the hierarchical parent policy class which
           is applied to the child circuit."
    ::= { eriRouterQosHierarchicalPClassStatsEntry 2 }

eriRouterQosHierarchicalPClassDroppedOctets OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
          "The number of octets that were dropped due to the hierarchical
           parent's policy class applied to the child circuit."
    ::= { eriRouterQosHierarchicalPClassStatsEntry 3 }

eriRouterQosHierarchicalPClassDroppedPkts OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
          "The number of packets that dropped due to the hierarchical
           parent's policy class applied to the child circuit."
    ::= { eriRouterQosHierarchicalPClassStatsEntry 4 }



