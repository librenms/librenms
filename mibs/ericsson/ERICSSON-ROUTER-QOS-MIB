--
-- QoS subscriber rate-limit class objects
--

eriRouterQosSubscriberRLClassStatsTable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF EriRouterQosSubscriberRLClassStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
           "This table specifies per class statistical information on
            the subscriber circuits.  It contains the class packet/octet
            counts for each active subscriber session to which the
            class-based policing/metering policy is applied when the
            counters are enabled for the class.

            All MIB objects defined in the table are viewed within the
            context identified in the SNMP protocol (i.e. the community
            string in v1/v2c or the contextName in v3)."
    ::= { eriRouterQosMibObjects 5 }

eriRouterQosSubscriberRLClassStatsEntry OBJECT-TYPE
    SYNTAX      EriRouterQosSubscriberRLClassStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
           "An entry containing statistical information before and after
            the class-based rate-limit action is taken on the subscriber
            session."
    INDEX { eriRouterSubsActiveName,
            eriRouterSubsActiveSessionId,
            eriRouterQosSubsRLPolicyType,
            eriRouterQosSubsRLClassId }
    ::= { eriRouterQosSubscriberRLClassStatsTable 1 }

EriRouterQosSubscriberRLClassStatsEntry ::= SEQUENCE {
    eriRouterQosSubsRLPolicyType                EriRouterQosPolicyType,
    eriRouterQosSubsRLClassId                   EriRouterQosClassId,
    eriRouterQosSubsRLPolicyName                SnmpAdminString,
    eriRouterQosSubsRLClassName                 SnmpAdminString,
    eriRouterQosSubsRLClassConformOctets        Counter64,
    eriRouterQosSubsRLClassConformPkts          Counter64,
    eriRouterQosSubsRLClassConformDroppedOctets Counter64,
    eriRouterQosSubsRLClassConformDroppedPkts   Counter64,
    eriRouterQosSubsRLClassExceedOctets         Counter64,
    eriRouterQosSubsRLClassExceedPkts           Counter64,
    eriRouterQosSubsRLClassExceedDroppedOctets  Counter64,
    eriRouterQosSubsRLClassExceedDroppedPkts    Counter64,
    eriRouterQosSubsRLClassViolateOctets        Counter64,
    eriRouterQosSubsRLClassViolatePkts          Counter64,
    eriRouterQosSubsRLClassViolateDroppedOctets Counter64,
    eriRouterQosSubsRLClassViolateDroppedPkts   Counter64,
    eriRouterQosSubsRLClassIPv4ConformOctets        Counter64,
    eriRouterQosSubsRLClassIPv4ConformPkts          Counter64,
    eriRouterQosSubsRLClassIPv4ConformDroppedOctets Counter64,
    eriRouterQosSubsRLClassIPv4ConformDroppedPkts   Counter64,
    eriRouterQosSubsRLClassIPv4ExceedOctets         Counter64,
    eriRouterQosSubsRLClassIPv4ExceedPkts           Counter64,
    eriRouterQosSubsRLClassIPv4ExceedDroppedOctets  Counter64,
    eriRouterQosSubsRLClassIPv4ExceedDroppedPkts    Counter64,
    eriRouterQosSubsRLClassIPv4ViolateOctets        Counter64,
    eriRouterQosSubsRLClassIPv4ViolatePkts          Counter64,
    eriRouterQosSubsRLClassIPv4ViolateDroppedOctets Counter64,
    eriRouterQosSubsRLClassIPv4ViolateDroppedPkts   Counter64,
    eriRouterQosSubsRLClassIPv6ConformOctets        Counter64,
    eriRouterQosSubsRLClassIPv6ConformPkts          Counter64,
    eriRouterQosSubsRLClassIPv6ConformDroppedOctets Counter64,
    eriRouterQosSubsRLClassIPv6ConformDroppedPkts   Counter64,
    eriRouterQosSubsRLClassIPv6ExceedOctets         Counter64,
    eriRouterQosSubsRLClassIPv6ExceedPkts           Counter64,
    eriRouterQosSubsRLClassIPv6ExceedDroppedOctets  Counter64,
    eriRouterQosSubsRLClassIPv6ExceedDroppedPkts    Counter64,
    eriRouterQosSubsRLClassIPv6ViolateOctets        Counter64,
    eriRouterQosSubsRLClassIPv6ViolatePkts          Counter64,
    eriRouterQosSubsRLClassIPv6ViolateDroppedOctets Counter64,
    eriRouterQosSubsRLClassIPv6ViolateDroppedPkts   Counter64
}

