eriRouterQosIntfRLClassStatsTable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF EriRouterQosIntfRLClassStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
           "This table specifies per class statistical information.
            It contains the class packet/octet counts for each packet
            oriented IF-MIB layer to which the class-based policing/
            metering policy is applied."
    ::= { eriRouterQosMibObjects 4 }

eriRouterQosIntfRLClassStatsEntry OBJECT-TYPE
    SYNTAX      EriRouterQosIntfRLClassStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
           "An entry containing statistical information before and after
            the class-based rate-limit action is taken on the interface."
    INDEX { ifIndex,
            eriRouterQosIfRLPolicyType,
            eriRouterQosIfRLClassId }
    ::= { eriRouterQosIntfRLClassStatsTable 1 }
    
EriRouterQosIntfRLClassStatsEntry ::= SEQUENCE {
    eriRouterQosIfRLPolicyType                EriRouterQosPolicyType,
    eriRouterQosIfRLClassId                   EriRouterQosClassId,
    eriRouterQosIfRLPolicyName                SnmpAdminString,
    eriRouterQosIfRLClassName                 SnmpAdminString,
    eriRouterQosIfRLClassConformOctets        Counter64,
    eriRouterQosIfRLClassConformPkts          Counter64,
    eriRouterQosIfRLClassConformDroppedOctets Counter64,
    eriRouterQosIfRLClassConformDroppedPkts   Counter64,
    eriRouterQosIfRLClassExceedOctets         Counter64,
    eriRouterQosIfRLClassExceedPkts           Counter64,
    eriRouterQosIfRLClassExceedDroppedOctets  Counter64,
    eriRouterQosIfRLClassExceedDroppedPkts    Counter64,
    eriRouterQosIfRLClassViolateOctets        Counter64,
    eriRouterQosIfRLClassViolatePkts          Counter64,
    eriRouterQosIfRLClassViolateDroppedOctets Counter64,
    eriRouterQosIfRLClassViolateDroppedPkts   Counter64,
    eriRouterQosIfRLClassIPv4ConformOctets        Counter64,
    eriRouterQosIfRLClassIPv4ConformPkts          Counter64,
    eriRouterQosIfRLClassIPv4ConformDroppedOctets Counter64,
    eriRouterQosIfRLClassIPv4ConformDroppedPkts   Counter64,
    eriRouterQosIfRLClassIPv4ExceedOctets         Counter64,
    eriRouterQosIfRLClassIPv4ExceedPkts           Counter64,
    eriRouterQosIfRLClassIPv4ExceedDroppedOctets  Counter64,
    eriRouterQosIfRLClassIPv4ExceedDroppedPkts    Counter64,
    eriRouterQosIfRLClassIPv4ViolateOctets        Counter64,
    eriRouterQosIfRLClassIPv4ViolatePkts          Counter64,
    eriRouterQosIfRLClassIPv4ViolateDroppedOctets Counter64,
    eriRouterQosIfRLClassIPv4ViolateDroppedPkts   Counter64,
    eriRouterQosIfRLClassIPv6ConformOctets        Counter64,
    eriRouterQosIfRLClassIPv6ConformPkts          Counter64,
    eriRouterQosIfRLClassIPv6ConformDroppedOctets Counter64,
    eriRouterQosIfRLClassIPv6ConformDroppedPkts   Counter64,
    eriRouterQosIfRLClassIPv6ExceedOctets         Counter64,
    eriRouterQosIfRLClassIPv6ExceedPkts           Counter64,
    eriRouterQosIfRLClassIPv6ExceedDroppedOctets  Counter64,
    eriRouterQosIfRLClassIPv6ExceedDroppedPkts    Counter64,
    eriRouterQosIfRLClassIPv6ViolateOctets        Counter64,
    eriRouterQosIfRLClassIPv6ViolatePkts          Counter64,
    eriRouterQosIfRLClassIPv6ViolateDroppedOctets Counter64,
    eriRouterQosIfRLClassIPv6ViolateDroppedPkts   Counter64
}

eriRouterQosIfRLPolicyType OBJECT-TYPE
    SYNTAX      EriRouterQosPolicyType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
           "The type of the rate-limit policy which is applied to the
            interface."
    ::= { eriRouterQosIntfRLClassStatsEntry 1 }

eriRouterQosIfRLClassId OBJECT-TYPE
    SYNTAX      EriRouterQosClassId
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
           "A value that uniquely identifies a class defined in the 
            rate-limit policy access list."
    ::= { eriRouterQosIntfRLClassStatsEntry 2 }

eriRouterQosIfRLPolicyName OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(1..39))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
           "Name of the rate-limit policy."
    ::= { eriRouterQosIntfRLClassStatsEntry 3 }

eriRouterQosIfRLClassName OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(1..39))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
           "Name of the class in the rate-limit policy access list."
    ::= { eriRouterQosIntfRLClassStatsEntry 4 }

