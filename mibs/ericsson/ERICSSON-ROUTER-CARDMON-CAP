ERICSSON-ROUTER-CARDMON-CAP DEFINITIONS ::= BEGIN

IMPORTS
        MODULE-IDENTITY
                FROM SNMPv2-SMI
        AGENT-CAPABILITIES
                FROM SNMPv2-CONF
        eriRouterCapabilities
                FROM ERICSSON-ROUTER-SMI;

eriRouterCardMonCap MODULE-IDENTITY
        LAST-UPDATED    "201501141800Z" -- Jan 14, 2015
        ORGANIZATION    "Ericsson Inc."
        CONTACT-INFO
                "       Ericsson, Inc.

                Postal: 100 Headquarters Dr.
                        San Jose, CA  95134
                        USA

                 Phone: +1 408 750 5000
                   Fax: +1 408 750 5599
                "
        DESCRIPTION
                "The Agent Capabilities of the 
                Card Monitor MIB (ERICSSON-ROUTER-CARDMON-MIB).

                If this MIB implementation is platform-dependent,
                the PRODUCT-RELEASE clause contains Ericsson SEOS version and
                at least one of the following Ericsson product families:
                    [SE]  SmartEdge Multi-service Edge Router (MSER) family
                    [SM]  SM family of Metro Ethernet Service Transport platforms
                    [SSR] Smart Service Router (SSR) family"

        REVISION    "201501141800Z" -- Jan 14, 2015
        DESCRIPTION
                "Updated RBN/rbn/Rbn as ERICSSON-ROUTER/eriRouter/EriRouter
                 to migrate RBN MIB to ERICSSON MIB."

        REVISION    "201106150000Z" -- Jun 15, 2011
        DESCRIPTION
                "Update DESCRIPTION clause of MODULE-IDENTITY
                 and PRODUCT-RELEASE clause of AGENT-CAPABILITIES."

        REVISION    "201011010000Z" -- Nov 1, 2010
        DESCRIPTION
                "Modified PRODUCT-RELEASE clause for consistent naming.
                 Updated CONTACT-INFO."

        REVISION    "200903270000Z" -- March 27, 2009
        DESCRIPTION
	        "Added new OID: 
                    eriRouterCardMonCap6 to include new 
               syntax for eriRouterCardAlarmId."

        REVISION    "200903160000Z" -- March 16, 2009
        DESCRIPTION
	        "Added new OID: 
                    eriRouterCardMonCap5 to include new 
               syntax for eriRouterCardAlarmId."

        REVISION    "200705080000Z" -- May 8, 2007
        DESCRIPTION
	        "Updated eriRouterCardMonCap4 to include new 
               syntax for eriRouterCardAlarmId."

        REVISION    "200610020000Z" -- October 2, 2006
        DESCRIPTION
	        "Added new OID:
	            eriRouterCardMonCap4."

        REVISION    "200505180000Z" -- May 18, 2005
        DESCRIPTION
	        "Added new OID:
	            eriRouterCardMonCap3."

        REVISION    "200410190000Z" -- October 19, 2004
        DESCRIPTION
	        "Added new OID:
	            eriRouterCardMonCap2."

        REVISION        "200406290000Z" -- Jun 29, 2004
        DESCRIPTION
                "Initial version."
        ::= { eriRouterCapabilities 58 }

eriRouterCardMonCap1 AGENT-CAPABILITIES
        PRODUCT-RELEASE "SEOS 2.6"
        STATUS          current
        DESCRIPTION     "ERICSSON-ROUTER-CARDMON-MIB capabilities"
        SUPPORTS        ERICSSON-ROUTER-CARDMON-MIB
        INCLUDES {
                eriRouterCardMonMIBObjectGroup,
                eriRouterCardMonMIBNotificationGroup
                }

        VARIATION eriRouterCardAlarmId
        SYNTAX INTEGER {
                unknown(1),
                cpFail(2),                    -- circuit pack failure
                cpBackplaneFail(3),           -- circuit pack backplane failure
                voltageFail(5),               -- voltage failure
                controllerSwitchFail(6),      -- controller switch failure
                controllerTempCritical(7),    -- controller temp critical
                controllerFail(8),            -- controller failure
                cpPowerOnDiagFail(10),        -- circuit pack power-on 
                                              -- diagnostic failure
                controllerPowerOnDiagFail(11) -- controller power-on diagnostic
                                              -- failure 
                }
        DESCRIPTION
                "The value diagFail(4) or controllerDiagFail(9) is not 
                supported."

        ::= { eriRouterCardMonCap 1 }

eriRouterCardMonCap2 AGENT-CAPABILITIES
        PRODUCT-RELEASE "SEOS 4.0.3"
        STATUS          current
        DESCRIPTION     "ERICSSON-ROUTER-CARDMON-MIB capabilities"
        SUPPORTS        ERICSSON-ROUTER-CARDMON-MIB
        INCLUDES {
                eriRouterCardMonMIBObjectGroup,
                eriRouterCardMonMIBNotificationGroup,
                eriRouterCardStatsMIBObjectGroup
                }

        VARIATION eriRouterCardAlarmId
        SYNTAX INTEGER {
                unknown(1),
                cpFail(2),                    -- circuit pack failure
                cpBackplaneFail(3),           -- circuit pack backplane failure
                voltageFail(5),               -- voltage failure
                controllerSwitchFail(6),      -- controller switch failure
                controllerTempCritical(7),    -- controller temp critical
                controllerFail(8),            -- controller failure
                cpPowerOnDiagFail(10),        -- circuit pack power-on 
                                              -- diagnostic failure
                controllerPowerOnDiagFail(11) -- controller power-on diagnostic
                                              -- failure 
                }
        DESCRIPTION
                "The value diagFail(4) or controllerDiagFail(9) is not 
                supported."

        ::= { eriRouterCardMonCap 2 }

