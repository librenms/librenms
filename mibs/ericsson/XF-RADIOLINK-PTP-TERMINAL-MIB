        XfRLPMAMConfigEntry ::=
            SEQUENCE {
                xfPMAMSetThreshold15m
                    Integer32,
                xfPMAMSetThreshold24h
                    Integer32,
                xfPMAMResetThreshold15m
                    Integer32,
                xfPMAMStatus
                    BITS,
                xfPMAMValidData
                    BITS
             }

