    SYNTAX      SEQUENCE OF EriRouterSubsEncapsCountEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains counts of active subscribers by
        encapsulation type."
    ::= { eriRouterSubsStats 2 }

eriRouterSubsEncapsCountEntry OBJECT-TYPE
    SYNTAX      EriRouterSubsEncapsCountEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A conceptual row in the eriRouterSubsEncapsCountTable. "
    INDEX { eriRouterSubsEncapsulationType }
    ::= { eriRouterSubsEncapsCountTable 1 }

EriRouterSubsEncapsCountEntry ::= SEQUENCE {
    eriRouterSubsEncapsulationType  INTEGER,
    eriRouterSubsEncapsCount        Gauge32
}

eriRouterSubsEncapsulationType OBJECT-TYPE
    SYNTAX      INTEGER {
                    ppp(1),
                    pppoe(2),
                    bridged1483(3),
                    routed1483(4),
                    multi1483(5),
                    dot1q1483(6),
                    dot1qEnet(7),
                    bridged1490(8),
                    routed1490(9),
                    multi1490(10),
                    dot1q1490(11),
                    clips(12),
                    other(255)
                }
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object identifies the type of encapsulation the
        subscriber is using.  Note that 'ppp' includes all types of
        supported ppp, excluding pppoe."
    ::= { eriRouterSubsEncapsCountEntry 1 }

eriRouterSubsEncapsCount OBJECT-TYPE
    SYNTAX      Gauge32
    UNITS       "subscribers"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The count of active subscribers for the encapsulation type."
    ::= { eriRouterSubsEncapsCountEntry 2 }

