ERICSSON-ROUTER-SUBSCRIBER-ACTIVE-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY,
    OBJECT-TYPE,
    Gauge32,
    Integer32,
    Counter32,
    Counter64,
    Unsigned32,
	IpAddress,
    zeroDotZero,
    NOTIFICATION-TYPE
            FROM SNMPv2-SMI

    DateAndTime,
    TruthValue,
    TimeStamp,
    RowPointer
            FROM SNMPv2-TC

    MODULE-COMPLIANCE,
    OBJECT-GROUP,
    NOTIFICATION-GROUP
            FROM SNMPv2-CONF

    SnmpAdminString
            FROM SNMP-FRAMEWORK-MIB

    vacmContextName
            FROM SNMP-VIEW-BASED-ACM-MIB

    InetAddressType,
    InetAddress,
    InetAddressPrefixLength
            FROM INET-ADDRESS-MIB

    EriRouterCircuitHandle,
    EriRouterPortMediumType,
    EriRouterPercentage,
    EriRouterSubscriberState
            FROM ERICSSON-ROUTER-TC

    eriRouterMgmt
            FROM ERICSSON-ROUTER-SMI;

eriRouterSubscriberActiveMib MODULE-IDENTITY
    LAST-UPDATED "201712060000Z" -- Dec 06, 2017
    ORGANIZATION "Ericsson AB."
    CONTACT-INFO
        "   
                Ericsson AB.
                2755 Augustine Drive
                Santa Clara, CA 95054
                USA

         Phone: +1 408 750 5000
           Fax: +1 408 750 5599
        "
    DESCRIPTION
        "Defines the objects necessary to support the management of
        active subscribers.

        All mib objects, except eriRouterSubsCntxtCountTable, defined in
        the module are viewed within the context identified in the
        SNMP protocol (i.e. the community string in v1/v2c or the
        contextName in v3).
        "

    REVISION      "201712060000Z" -- Dec 06, 2017
    DESCRIPTION
        "Added eriRouterSubsActiveIpv4GlobalTable"

    REVISION      "201705120000Z" -- May 12, 2017
    DESCRIPTION
        "Added eriRouterSubsActiveSessionNx entry to eriRouterSubsActiveTable
         Added eriRouterSubsActiveAddrInt entry to eriRouterSubsActiveIpTable"

    REVISION      "201601040000Z" -- Nov 01, 2015
    DESCRIPTION
        " Modified below attribute names of table eriRouterSubsServiceVolumeTable
          for better readability.
          eriRouterSubsServiceIPv4VolumeOctetsIn  --> eriRouterSubsIPv4ServiceVolumeOctetsIn
          eriRouterSubsServiceIPv4VolumeOctetsOut --> eriRouterSubsIPv4ServiceVolumeOctetsOut
          eriRouterSubsServiceIPv4VolumePktsIn    --> eriRouterSubsIPv4ServiceVolumePktsIn
          eriRouterSubsServiceIPv4VolumePktsOut   --> eriRouterSubsIPv4ServiceVolumePktsOut
          eriRouterSubsServiceIPv6VolumeOctetsIn  --> eriRouterSubsIPv6ServiceVolumeOctetsIn
          eriRouterSubsServiceIPv6VolumeOctetsOut --> eriRouterSubsIPv6ServiceVolumeOctetsOut
          eriRouterSubsServiceIPv6VolumePktsIn    --> eriRouterSubsIPv6ServiceVolumePktsIn
          eriRouterSubsServiceIPv6VolumePktsOut   --> eriRouterSubsIPv6ServiceVolumePktsOut
        "

