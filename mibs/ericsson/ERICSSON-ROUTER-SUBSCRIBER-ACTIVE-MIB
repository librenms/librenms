EriRouterSubsStatsEntry ::= SEQUENCE {
    eriRouterSubsOctetsSent            Counter64,
    eriRouterSubsOctetsReceived        Counter64,
    eriRouterSubsPktsSent              Counter64,
    eriRouterSubsPktsReceived          Counter64,
    eriRouterSubsXmitOctetsDropped     Counter32,
    eriRouterSubsXmitPktsDropped       Counter32,
    eriRouterSubsMcastOctetsSent       Counter64,
    eriRouterSubsMcastOctetsReceived   Counter64,
    eriRouterSubsMcastPktsSent         Counter64,
    eriRouterSubsMcastPktsReceived     Counter64,
    eriRouterSubsIPv4OctetsSent            Counter64,
    eriRouterSubsIPv4OctetsReceived        Counter64,
    eriRouterSubsIPv6OctetsSent            Counter64,
    eriRouterSubsIPv6OctetsReceived        Counter64,
    eriRouterSubsIPv4PktsSent              Counter64,
    eriRouterSubsIPv4PktsReceived          Counter64,
    eriRouterSubsIPv6PktsSent              Counter64,
    eriRouterSubsIPv6PktsReceived          Counter64,
    eriRouterSubsIPv4McastOctetsSent       Counter64,
    eriRouterSubsIPv4McastOctetsReceived   Counter64,
    eriRouterSubsIPv6McastOctetsSent       Counter64,
    eriRouterSubsIPv6McastOctetsReceived   Counter64,
    eriRouterSubsIPv4McastPktsSent         Counter64,
    eriRouterSubsIPv4McastPktsReceived     Counter64,
    eriRouterSubsIPv6McastPktsSent         Counter64,
    eriRouterSubsIPv6McastPktsReceived     Counter64

}

