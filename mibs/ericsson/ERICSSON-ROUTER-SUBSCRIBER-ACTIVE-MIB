--
-- Subscriber software license
--
eriRouterSubsLicense OBJECT IDENTIFIER ::= { eriRouterSubsStats 5 }

eriRouterSubsMaxSupportedSessions OBJECT-TYPE
   SYNTAX      Gauge32
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
        "The maximum number of active subscriber sessions that
        are supported on the SmartEdge router."
   ::= { eriRouterSubsLicense 1 }

eriRouterSubsLicensedSessions OBJECT-TYPE
   SYNTAX      Gauge32
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
        "The number of licensed subscriber sessions on the 
        SmartEdge router."
   ::= { eriRouterSubsLicense 2 }

eriRouterSubsActiveSessions OBJECT-TYPE
   SYNTAX      Gauge32
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
        "The number of active subscriber sessions on the SmartEdge
        router."
   ::= { eriRouterSubsLicense 3 }

eriRouterSubsCapacityPercentageUsed OBJECT-TYPE
   SYNTAX      EriRouterPercentage 
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
        "The percentage of licensed subscriber sessions in use 
        on the SmartEdge router. This value is computed as: 

        if eriRouterSubsLicensedSessions equals 0 then 
            if eriRouterSubsActiveSessions equals 0 then 0 else 100    
        else 
            MIN((eriRouterSubsActiveSessions * 100)/eriRouterSubsLicensedSessions), 100)."
   ::= { eriRouterSubsLicense 4 }

--
-- Services Volume Limits
--
eriRouterSubsServiceVolumeTable    OBJECT-TYPE
    SYNTAX      SEQUENCE OF EriRouterSubsServiceVolumeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table specifies service volume information on
         the subscriber circuits.  It contains the volume octet
         counts for each active subscriber session for which the
         service is active.

         All MIB objects defined in the table are viewed within the
         context identified in the SNMP protocol (i.e. the community
         string in v1/v2c or the contextName in v3)."
    ::= { eriRouterSubsActive 4 }

eriRouterSubsServiceVolumeEntry OBJECT-TYPE
    SYNTAX      EriRouterSubsServiceVolumeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A conceptual row in the EriRouterSubsServiceVolumeTable. Each
         row contains the service volume limit and volume octet count 
         for each active service session."
    INDEX { eriRouterSubsActiveName,
            eriRouterSubsActiveSessionId,
            eriRouterSubsServiceIndex }
    ::= { eriRouterSubsServiceVolumeTable 1 }

EriRouterSubsServiceVolumeEntry ::= SEQUENCE {
    eriRouterSubsServiceVolumeLimitIn         Unsigned32,
    eriRouterSubsServiceVolumeLimitOut        Unsigned32,
    eriRouterSubsServiceActiveTime            TimeStamp,
    eriRouterSubsServiceVolumeOctetsIn        Counter64,
    eriRouterSubsServiceVolumeOctetsOut       Counter64,
    eriRouterSubsServiceVolumePktsIn          Counter64,
    eriRouterSubsServiceVolumePktsOut         Counter64,
    eriRouterSubsIPv4ServiceVolumeOctetsIn    Counter64,
    eriRouterSubsIPv4ServiceVolumeOctetsOut   Counter64,
    eriRouterSubsIPv4ServiceVolumePktsIn      Counter64,
    eriRouterSubsIPv4ServiceVolumePktsOut     Counter64,
    eriRouterSubsIPv6ServiceVolumeOctetsIn    Counter64,
    eriRouterSubsIPv6ServiceVolumeOctetsOut   Counter64,
    eriRouterSubsIPv6ServiceVolumePktsIn      Counter64,
    eriRouterSubsIPv6ServiceVolumePktsOut     Counter64

}
    
eriRouterSubsServiceVolumeLimitIn   OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "KiloBytes"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The volume limit in kilobytes (KB) allowed for the subscriber
        service to input (or receive) for this subscriber. If
        the value is 0, then there is no limit."
    ::= { eriRouterSubsServiceVolumeEntry 1 }
    
eriRouterSubsServiceVolumeLimitOut OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "KiloBytes"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The volume limit in kilobytes (KB) allowed for the subscriber
        service to output (or transmit) for this subscriber. If
        the value is 0, then there is no limit."
    ::= { eriRouterSubsServiceVolumeEntry 2 }
    
eriRouterSubsServiceActiveTime  OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of sysUpTime when the service became active for this
        subscriber. 
        
        If sysUpTime is reset to zero as a result of a reinitialization
        of the network management (sub)system, then the values of all
        TimeStamp objects are also reset."
    ::= { eriRouterSubsServiceVolumeEntry 3 }

eriRouterSubsServiceVolumeOctetsIn  OBJECT-TYPE
