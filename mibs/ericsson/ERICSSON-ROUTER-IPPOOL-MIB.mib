--
-- IP Pool context-level summary
--
eriRouterInetIpPoolSummary          OBJECT IDENTIFIER ::= { eriRouterIpPoolMIBObjects 4 }

eriRouterInetIpPoolCtxName OBJECT-TYPE
    SYNTAX       SnmpAdminString
    MAX-ACCESS   accessible-for-notify
    STATUS       current
    DESCRIPTION
        "The name of the context containing the IP Pool."
    ::= { eriRouterInetIpPoolSummary 1 }
    
eriRouterInetIpPoolCtxTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF EriRouterInetIpPoolCtxEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "The purpose of this table is to allow customers to monitor
         the status of IP Pool context summary and thresholds through
         SNMP.

         This table contains the objects which identify information
         about per-context IP Pool thresholds expressed in absolute value 
         or percentage. Each row in the table corresponds to one IP Pool 
         threshold configured within a particular context.

         Objects defined in this table are only accessible within the
         context identified in the SNMP protocol (i.e. the community
         string in v1/v2c or the contextName in v3)."
    ::= { eriRouterInetIpPoolSummary 2 }

eriRouterInetIpPoolCtxEntry OBJECT-TYPE
    SYNTAX       EriRouterInetIpPoolCtxEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "A conceptual row in the eriRouterInetIpPoolCtxTable."
    INDEX        {eriRouterInetIpPoolCtxPoolType}
    ::= { eriRouterInetIpPoolCtxTable 1 }

EriRouterInetIpPoolCtxEntry ::=
    SEQUENCE {
        eriRouterInetIpPoolCtxPoolType         EriRouterInetPoolType,
        -- IP Pool statistics
        eriRouterInetIpPoolCtxAvailable        Unsigned32,
        eriRouterInetIpPoolCtxPoolSize         Unsigned32,
        -- IP Pool falling thresholds
        eriRouterInetIpPoolCtxThrshType        EriRouterInetIpPoolThresholdType,
        eriRouterInetIpPoolCtxHiFallingThrsh   EriRouterInetIpPoolThreshold,
        eriRouterInetIpPoolCtxHiFallingNotify  EriRouterInetIpPoolThrshNotify,
        eriRouterInetIpPoolCtxLoFallingThrsh   EriRouterInetIpPoolThreshold,
        eriRouterInetIpPoolCtxLoFallingNotify  EriRouterInetIpPoolThrshNotify
    }

eriRouterInetIpPoolCtxPoolType  OBJECT-TYPE
    SYNTAX       EriRouterInetPoolType
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "The type of ip pool. "
    ::= { eriRouterInetIpPoolCtxEntry 1 }
    
eriRouterInetIpPoolCtxAvailable OBJECT-TYPE
    SYNTAX       Unsigned32
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The number of IP address prefixes of an IP Pool that are available."
    ::= { eriRouterInetIpPoolCtxEntry 2 }
    
eriRouterInetIpPoolCtxPoolSize OBJECT-TYPE
    SYNTAX       Unsigned32
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The total number of IP prefixes configured in all pools of
         all interfaces in the context valid in the current view.
         It is the sum of the available plus the unusable prefixes."
    ::= { eriRouterInetIpPoolCtxEntry 3 }
    
eriRouterInetIpPoolCtxThrshType OBJECT-TYPE
    SYNTAX       EriRouterInetIpPoolThresholdType
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "This object indicates the value type of all threshold values in
         this logical row, including eriRouterInetIpPoolCtxHiFallingThrsh,
         eriRouterInetIpPoolCtxLoFallingThrsh. "
    ::= { eriRouterInetIpPoolCtxEntry 4 }
    
eriRouterInetIpPoolCtxHiFallingThrsh OBJECT-TYPE
    SYNTAX       EriRouterInetIpPoolThreshold
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The falling threshold of an IP Pool. When the value of eriRouterInetIpPoolCtxAvailable 
         crosses from (eriRouterInetIpPoolCtxHiFallingThrsh + 1) to eriRouterInetIpPoolCtxHiFallingThrsh
         in either absolute value or percentage specified by eriRouterInetIpPoolCtxThrshType,
         a eriRouterInetIpPoolCtxHiFallingThrshMet notification and/or log is generated.

         The value of this object should never be smaller than the value
         of eriRouterInetIpPoolCtxLoFallingThrsh.
        
         A value of zero means that this falling threshold is not configured. "
    ::= { eriRouterInetIpPoolCtxEntry 5 }

eriRouterInetIpPoolCtxHiFallingNotify OBJECT-TYPE
    SYNTAX       EriRouterInetIpPoolThrshNotify
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "This object specifies if eriRouterInetIpPoolCtxHiFallingThrshMet and/or log
         is generated.
        
         See the description of eriRouterInetIpPoolCtxHiFallingThrsh for conditions
         which generate the notification. "
    ::= { eriRouterInetIpPoolCtxEntry 6 }

eriRouterInetIpPoolCtxLoFallingThrsh OBJECT-TYPE
    SYNTAX       EriRouterInetIpPoolThreshold
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The falling threshold of an IP Pool. When the value of eriRouterInetIpPoolCtxAvailable 
         crosses from (eriRouterInetIpPoolCtxLoFallingThrsh + 1) to eriRouterInetIpPoolCtxLoFallingThrsh
         (either absolute value or percentage) specified by eriRouterInetIpPoolCtxThrshType,
         a eriRouterInetIpPoolCtxLoFallingThrshMet notification and/or log is generated.

         The value of this object should never be greater than the value
