            sdcDiv(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "SDC actual path (current value)."
        ::= { xfSdcEntry 10 }

    xfSdcDadeDelay OBJECT-TYPE
        SYNTAX Integer32 (-2147483647..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
            DADE delay in nsec.
            "
        ::= { xfSdcEntry 11 }

