                udp(3),
                icmp(4),
                ipsecESP(5),
                ipsecAH(6)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specific protocol to filter."
            ::= { vnsFilterRuleEntry 7 }

        
