            SYNTAX INTEGER
                {
                disable(0),
                enable(1)
                }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Enable/disable security engine to scan for threats."
            ::= { dedicatedScanGroupEntry 2 }

        
