            SYNTAX Unsigned32 (5..60 | 121)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Maximum number of client on this radio.  This field is only applicable to a group with 
                radioBalancing(1) type."
            DEFVAL { 121 }
            ::= { loadGroupEntry 7 }

        
        -- 1.3.6.1.4.1.4329.15.3.5.5.1.1.8
        loadGroupLoadControlEnableR1 OBJECT-TYPE
            SYNTAX INTEGER
                {
                disabled(0),
                enabled(1)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "If it is enabled then load control is applicable to radio #1 of all access points that are 
                assigned to this load group.
                This field has meaning only for the load group that is of radioBalancing(1) type. "
            ::= { loadGroupEntry 8 }

        
        -- 1.3.6.1.4.1.4329.15.3.5.5.1.1.9
