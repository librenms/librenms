                    INTEGER,
                vnsAPFilterAction
                    Integer32,
                vnsAPFilterIPAddress
                    IpAddress,
                vnsAPFilterMask
                    IpAddress,
                vnsAPFilterPortLow
                    Integer32,
                vnsAPFilterPortHigh
                    Integer32,
                vnsAPFilterProtocol
                    Integer32,
                vnsAPFilterEtherType
                    INTEGER,
                vnsAPFilterRowStatus
                    Integer32
             }

        -- 1.3.6.1.4.1.4329.15.3.3.1.14.1.1
