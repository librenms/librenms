                option1:  mac address format as XXXXXXXXXXXX
                option2:  mac address format as XX:XX:XX:XX:XX:XX
                option3:  mac address format as XX-XX-XX-XX-XX-XX
                option4:  mac address format as XXXX.XXXX.XXXX
                option5:  mac address format as XXXXXX-XXXXXX
                option6:  mac address format as XX XX XX XX XX XX
                option7:  mac address format as xxxxxxxxxxxx
                option8:  mac address format as xx:xx:xx:xx:xx:xx
                option9:  mac address format as xx-xx-xx-xx-xx-xx
                option10:  mac address format as xxxx.xxxx.xxxx
                option11:  mac address format as xxxxxx-xxxxxx
                option12:  mac address format as xx xx xx xx xx xx"
            ::= { vnsGlobalSetting 12 }

            


        
