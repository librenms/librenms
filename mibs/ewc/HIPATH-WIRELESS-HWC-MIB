                    BITS,
                dedicatedScanGrpScan2400MHzFreq
                    BITS,
                dedicatedScanGrpScan5GHzFreq
                    BITS,
                dedicatedScanGrpBlockAdHocPeriod
                    Integer32,
                dedicatedScanGrpClassifySourceIF
                    INTEGER,
                dedicatedScanGrpRowStatus
                    RowStatus,
                dedicatedScanGrpDetectRogueAP
                    INTEGER,
                dedicatedScanGrpListeningPort  
                    Integer32
             }


        
