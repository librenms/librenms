        -- 1.3.6.1.4.1.4329.15.3.11.20
        dedicatedScanGroupTable OBJECT-TYPE
            SYNTAX SEQUENCE OF DedicatedScanGroupEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "dedicated scan group enables the subsystem full time scan for threats based 
                on 37xx-based APs. The threats are discovered and identified in the deployment 
                environment and then classified for further actions. "
            ::= { widsWips 20 }

        
        -- 1.3.6.1.4.1.4329.15.3.11.20.1
