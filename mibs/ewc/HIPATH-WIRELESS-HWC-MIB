        -- 1.3.6.1.4.1.4329.15.3.19.1
        apEventId OBJECT-TYPE
            SYNTAX INTEGER { 
                        apPollTimeout(1),
                        apRegister(2)
                        }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Identifies event associated with AP or AP tunnel:
                apPollTimeout - an event triggered when the AP disconnects from the controller.
                apRegister - an event triggered when the AP connects to the controller."
            ::= { apNotifications 1 }


        -- 1.3.6.1.4.1.4329.15.3.19.2
