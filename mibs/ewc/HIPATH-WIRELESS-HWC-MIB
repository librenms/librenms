            ::= { outOfServiceScanGroupEntry 2 }

        
        -- 1.3.6.1.4.1.4329.15.3.11.13.1.3
        outOfSrvScanGrpChannelList OBJECT-TYPE
            SYNTAX INTEGER
                {
                allChannel(0),
                currentChannel(999)
                }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
