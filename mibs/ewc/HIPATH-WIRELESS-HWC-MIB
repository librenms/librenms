            DESCRIPTION
                "Rate Control Burst Size (CBS) in bytes."
            ::= { vnsRateControlProfEntry 4 }

        
        -- 1.3.6.1.4.1.4329.15.3.3.1.14
        vnsAPFilterTable OBJECT-TYPE
            SYNTAX SEQUENCE OF VnsAPFilterEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Filters applied to Access Points via VNS settings and assignments."
            ::= { vnsConfigObjects 14 }

        
        -- 1.3.6.1.4.1.4329.15.3.3.1.14.1
        vnsAPFilterEntry OBJECT-TYPE
            SYNTAX VnsAPFilterEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry containing filters definition for Access Points assigned
                to VNS that is identified by VNS index."
            INDEX { ifIndex, vnsFilterID, vnsAPFilterRuleOrder }
            ::= { vnsAPFilterTable 1 }

        
