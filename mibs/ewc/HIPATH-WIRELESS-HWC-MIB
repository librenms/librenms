            SYNTAX Integer32 (1..4)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Table index for the WEP key."
            ::= { vnsWEPKeyEntry 1 }

        
        -- 1.3.6.1.4.1.4329.15.3.3.1.9.1.2
