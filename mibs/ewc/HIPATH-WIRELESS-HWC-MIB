            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "VLAN tag for the packets trasmitted to/from Access Point.
                Value ranges: (-1) it is not tagged, zero(0) means reserved, 1-4094 tag value."
            DEFVAL { -1 }
            ::= { apEntry 11 }

        
        -- 1.3.6.1.4.1.4329.15.3.5.1.2.1.12
        apIpAssignmentType OBJECT-TYPE
            SYNTAX INTEGER
                {
                dhcp(1),
                static(2)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "IP address assignment type, dhcp(1) = uses DHCP to obtain IP address, 
                static(2) = static IP address is assigned to the access point."
            ::= { apEntry 12 }

        
        -- 1.3.6.1.4.1.4329.15.3.5.1.2.1.13
