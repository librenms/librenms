            SYNTAX OutOfServiceScanGroupEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry in this table that defines attributes of a out-of-service scan group."
            INDEX { scanGroupProfileID }
            ::= { outOfServiceScanGroupTable 1 }

        
        OutOfServiceScanGroupEntry ::=
            SEQUENCE { 
                outOfSrvScanGrpName
                    DisplayString,
                outOfSrvScanGrpRadio
                    INTEGER,
                outOfSrvScanGrpChannelList
                    INTEGER,
                outOfSrvScanGrpScanType
                    INTEGER,
                outOfSrvScanGrpChannelDwellTime
                    INTEGER,
                outOfSrvScanGrpScanTimeInterval
                    INTEGER,
                outOfSrvScanGrpSecurityScan
                    INTEGER,
                outOfSrvScanGrpScanActivity
                    INTEGER,
                outOfSrvScanGrpScanRowStatus
                    Integer32
             }

        -- 1.3.6.1.4.1.4329.15.3.11.13.1.1
