            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Human readable textual description identifying scan group."
            ::= { outOfServiceScanGroupEntry 1 }

        
        -- 1.3.6.1.4.1.4329.15.3.11.13.1.2
