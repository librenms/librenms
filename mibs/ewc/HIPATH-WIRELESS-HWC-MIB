            SYNTAX INTEGER
                {
                none(0),
                tls(1),
                peap(2),
                all(3)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Supported certificate type used by AP for commnuication.
                none(0) = not supported,
                TLS(1) = Trasport Layer Security (TLS),
                PEAP(2) = Protected Extensible Authentication Protocol,
                all(2) = supports all supported EAP."
            ::= { apStatsEntry 12 }

        
