        -- 1.3.6.1.4.1.4329.15.3.2.5.1
        vnRole OBJECT-TYPE
            SYNTAX INTEGER
                {
                none(1),
                vnMgr(2),
                vnAgent(3)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies the role of this EWC in inter-EWC mobility.  
                None indicates that mobile units cannot roam to or 
                from this EWC.  In any EWC cluster, only one EWC should
                be specified as vnMgr, all others should be have 
                vnRole = vnAgent."
            ::= { vnManagerObjects 1 }


        -- 1.3.6.1.4.1.4329.15.3.2.5.2
        vnIfIndex OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
