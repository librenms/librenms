                {
                bits24(24),
                bits36(36),
                bits48(48)
                }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Length of bitmask associated to the MAC address in the entry."
            ::= { muAccessListEntry 2 }

        
        -- 1.3.6.1.4.1.4329.15.3.6.6.1.3
        muAccessListRowStatus OBJECT-TYPE
