                    TruthValue,
                muAPSerialNo
                    OCTET STRING,
                muVnsSSID
                    OCTET STRING,
                muTxPackets
                    Counter64,
                muRxPackets
                    Counter64,
                muTxOctets
                    Counter64,
                muRxOctets
                    Counter64,
                muDuration
                    Counter64,
                muAPName
                    DisplayString,
                muTopologyName
                    DisplayString,
                muPolicyName
                    DisplayString,
                muDefaultCoS
                    DisplayString,
                muConnectionProtocol
                    INTEGER,
                muConnectionCapability
                    INTEGER,
                muWLANID
                    Unsigned32,
                muBSSIDMac
                    MacAddress,
                muDot11ConnectionCapability
                    BITS
             }

