                reports for stations when a fast failover incident occurs "
            ::= { radiusFastFailoverEvents 1 }

        
        -- 1.3.6.1.4.1.4329.15.3.3.3.6.1.1
        radiusFastFailoverEventsEntry OBJECT-TYPE
            SYNTAX RadiusFastFailoverEventsEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry for each radius server."
            INDEX { radiusFFOEid }
            ::= { radiusFastFailoverEventsTable 1 }

        
        RadiusFastFailoverEventsEntry ::=
            SEQUENCE { 
                radiusFFOEid
                    DisplayString,
                fastFailoverEvents
                    INTEGER
             }
                    
        -- 1.3.6.1.4.1.4329.15.3.3.3.6.1.1
