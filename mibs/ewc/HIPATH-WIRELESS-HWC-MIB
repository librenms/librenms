            SYNTAX IpAddress
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "IP address to apply the filter."
            ::= { vnsFilterRuleEntry 4 }

        
        -- 1.3.6.1.4.1.4329.15.3.3.1.7.1.5
