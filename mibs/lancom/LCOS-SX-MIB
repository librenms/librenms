    lcsSNMPv3UserConfUserName                  DisplayString,
    lcsSNMPv3UserConfSecurityLevel             SNMPv3SecurityLevel,
    lcsSNMPv3UserConfAuthenticationProtocol    SNMPv3AuthenticationProtocol,
    lcsSNMPv3UserConfAuthenticationPassword    DisplayString,
