   SYNTAX      Integer32 (1..4094)
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       " "
   ::= { lcsMirrorEntry 1 }

lcsMirrorMode OBJECT-TYPE
   SYNTAX      Integer32(0..3)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "default: 0
       0: disable
       1: rx only
       2: tx only
       3: enable 
       Setting Tx mirroring for analyzer port has no effect. Tx mirroring ignored"
   ::= { lcsMirrorEntry 2 }

