    SEQUENCE {
        lcsEPSIndex                                Integer32,
        lcsEpsID                                   Integer32,
        lcsEpsDomain                               Integer32,
        lcsEpsArchitecture                         Integer32,
        lcsEpsWFlow                                Integer32,
        lcsEpsPFlow                                Integer32,   
        lcsEpsWSFMEP                               Integer32,       
        lcsEpsPSFMEP                               Integer32,       
        lcsEpsAPSMEP                               Integer32,       
        lcsEpsAlarm                                Integer32,       
        lcsEpsRowStatus                            RowStatus
    }

