   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
       " The current link state. "
   ::= { lcsPortConfigurationEntry 3 }

lcsPortConfSpeedStatus OBJECT-TYPE
   SYNTAX      DisplayString(SIZE (6..12))
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
       " The current link speed of the port. "
   ::= { lcsPortConfigurationEntry 4 }

lcsPortConfSpeedMode OBJECT-TYPE
   SYNTAX      Integer32 (0..19)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "0: disable state,
        1: auto,
        2: 10 Half,
        3: 10 Full,
        4: 100 Half,
        5: 100 Full,
        6: 1G full,
        7: SFP_Auto_AMS,
        8: 100FX_AMS,
        9: 1000X_AMS,
        10: 100-FX,
        11: 1000-X,
        12: 10G Full,
        13: Auto (2.5G/1G),
        14: Auto (2.5G/100M),
        15: Auto (2.5G),
        16: Auto (1G/100M),
        17: Auto (1G),
        18: Auto (100M),
        19: 2.5G Full."
   ::= { lcsPortConfigurationEntry 5 }

