                lcsQosPortSchedulersQ3Weight Integer32,
                lcsQosPortSchedulersQ4Weight Integer32,
                lcsQosPortSchedulersQ5Weight Integer32,
                lcsQosPortSchedulersQ6Weight Integer32,
                lcsQosPortSchedulersQ7Weight Integer32               
                }

