        lcsIPInterfacesIPv4Mask                    Integer32,
        lcsIPInterfacesIPv6DHCP                    Integer32,
        lcsIPInterfacesIPv6RapidCommit             Integer32,
        lcsIPInterfacesIPv6CurrentLease            DisplayString,
        lcsIPInterfacesIPv6Address                 DisplayString,
        lcsIPInterfacesIPv6Mask                    Integer32,
        lcsIPInterfacesRowStatus                   RowStatus
    }

