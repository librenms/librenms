   SYNTAX      Integer32(1..5)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "Monitor session."
   ::= { lcsMirroring 1 }

lcsPortToMirrorOn OBJECT-TYPE
   SYNTAX      Integer32(0..4094)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "default: 0
        0: disable,
        range : 1 - port number. "
   ::= { lcsMirroring 2 }

lcsMirrorTable OBJECT-TYPE
   SYNTAX      SEQUENCE OF LCSMirrorEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       " "
   ::= { lcsMirroring 3 }

lcsMirrorEntry OBJECT-TYPE
   SYNTAX      LCSMirrorEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       " "
   INDEX    { lcsMirrorPort  }
   ::= { lcsMirrorTable 1 }

