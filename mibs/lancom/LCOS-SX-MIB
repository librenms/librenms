lcsACLPortConfGroup OBJECT-GROUP
    OBJECTS {
                -- lcsACLPortConfTable
                 lcsACLPortConfPolicyID,
                 lcsACLPortConfAction,
                 lcsACLPortConfRateLimiterID,
                 lcsACLPortConfPortRedirect,
                 lcsACLPortConfMirror,
                 lcsACLPortConfLogging,
                 lcsACLPortConfShutdown,
                 lcsACLPortConfState,
                 lcsACLPortConfCounter
            }
     STATUS  current
     DESCRIPTION
            "The collection of objects which are used to indicate
            lcs ACL information."
    ::= { lcsAclGroups 1 }

lcsACLRateLimiterGroup OBJECT-GROUP
    OBJECTS {
                -- lcsACLRateLimiterTable
                 lcsACLRateLimiterRate,
                 lcsACLRateLimiterUnit
            }
     STATUS  current
     DESCRIPTION
            "The collection of objects which are used to indicate
            lcs ACL information."
    ::= { lcsAclGroups 2 }

