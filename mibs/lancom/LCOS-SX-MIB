   lcsPTPClockConfEntryPortConfDelayAsymmetry   Integer32,
   lcsPTPClockConfEntryPortConfIngressLatency   Integer32,
   lcsPTPClockConfEntryPortConfEgressLatency    Integer32,
   lcsPTPClockConfEntryPortConfVersion          Integer32
}

