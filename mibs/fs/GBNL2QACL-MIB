               INTEGER,
           qosTrafficInsertVlanBlock9Priority
               INTEGER,    
           qosTrafficInsertVlanBlock10Priority
               INTEGER,
           qosTrafficInsertVlanBlock11Priority
               INTEGER    
       }
 
