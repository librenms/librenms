			TimeTicks,
		winlink1000OduAirSpectrumChannelAvailable
			INTEGER,
		winlink1000OduAirSpectrumChannelMaxBeaconRss
			INTEGER,
		winlink1000OduAirSpectrumChannelCompressed
			OCTET STRING
		}

