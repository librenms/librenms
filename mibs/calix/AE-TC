                    static(19),
                    dhcpconfig(20),
                    dhcpacquire(21),
                    configpend(22),
                    tftpfail(23),
                    tftpfilenotfound(24),
                    proxyfail(25),
		    running(26),
		    enabled(27),
		    assnconnected(28),
		    disabled(29),
		    assndisconnected(30)
                }

