-- -- rcEponTransceiverInfoTable
rcEponTransceiverInfoTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF RcEponTransceiverInfoEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
				"
				This table defines objects to manage optical transceiver's product
				information. Transceiver's information includes types of transceiver,
				connector type, laser wavelength, vendor information, and so on. 
				"
	REFERENCE   
				"SFF-8472 Specification for Diagnotics Monitoring Interface for
				Optical Transceiver, Rev 10.3, December 1rt, 2007.
				"
		::= {rcEponTransceiverGroup 1}

rcEponTransceiverInfoEntry OBJECT-TYPE
	SYNTAX		RcEponTransceiverInfoEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION 
				"An entry in the rcEponTransceiverInfoTable provides objects to get
				transceiver's product information.
				
				Rows in the table are denoted by the TransceiverIfIndex. For the 
				OLT TransceiverIfIndex referrring to the unique transcever interface
				at the OLT, rows are created at system initialization or at service
				port creation. For the ONU TransceiverIfIndex referring to the unique
				transcever interface at the ONU, rows are created when ONU registers.
				"
	INDEX		{rcEponTransceiverIndex} 
		::= {rcEponTransceiverInfoTable 1}

RcEponTransceiverInfoEntry ::= SEQUENCE {
				rcEponTransceiverIndex  			INTEGER,
				rcEponTransceiverType 	        	OCTET STRING,
				rcEponTransceiverConnectorType		OCTET STRING,
				rcEponTransceiverWavelength 		INTEGER,
				rcEponTransceiverVenderName			OCTET STRING,
				rcEponTransceiverVenderPN			OCTET STRING,
				rcEponTransceiverVenderSN			OCTET STRING,
				rcEponTransceiverDiameterType		INTEGER,
				rcEponTransceiverDiagImplemented 	TruthValue,
				rcEponTransceiverTransferDistance   INTEGER
			}	

rcEponTransceiverIndex OBJECT-TYPE
	SYNTAX 		INTEGER
	MAX-ACCESS	read-only
	STATUS 		current
	DESCRIPTION 
				"This object indicates the optical interface in EPON network
				elements.
				"
		::= {rcEponTransceiverInfoEntry 1}
		
rcEponTransceiverType OBJECT-TYPE
	SYNTAX		OCTET STRING (SIZE (0..64))
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
				"This object indicates the type of transceiver module, such as
				1000_BASE_SX_SFP.
				"
		::= {rcEponTransceiverInfoEntry 2}
		
rcEponTransceiverConnectorType OBJECT-TYPE
	SYNTAX		OCTET STRING (SIZE (0..16))
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
				"This object indicates the external optical or electrical cable
				connector provided as the media interface, such as LC/SC.
				"
		::= {rcEponTransceiverInfoEntry 3}

rcEponTransceiverWavelength OBJECT-TYPE
	SYNTAX		INTEGER
	MAX-ACCESS	read-only
	STATUS 		current
	DESCRIPTION
				"This object indicates the nominal transmitter output wavelength
				at room temperature.
				"
		::= {rcEponTransceiverInfoEntry 4}
		
rcEponTransceiverVenderName OBJECT-TYPE
	SYNTAX		OCTET STRING (SIZE (0..16))
	MAX-ACCESS	read-only
	STATUS 		current
	DESCRIPTION
				"This object indicates the vendor name which contains 16 
				characters, left-aligned and padded on the right with ASCII
				spaces.
				"
		::= {rcEponTransceiverInfoEntry 5}
		
rcEponTransceiverVenderPN OBJECT-TYPE
	SYNTAX		OCTET STRING (SIZE (0..16))
	MAX-ACCESS	read-only
	STATUS 		current
	DESCRIPTION
				"This object indicates the vendor part number (vendor PN) which
				contains 16 characters, left-aligned and padded on the right
				with ASCII spaces.
				"
		::= {rcEponTransceiverInfoEntry 6}
		
rcEponTransceiverVenderSN OBJECT-TYPE
	SYNTAX		OCTET STRING (SIZE (0..16))
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
				"This object indicates the vendor serial number (vendor SN) which
				contains 16 characters, left-aligned and padded on the right with
				ASCII spaces.
				"
		::= {rcEponTransceiverInfoEntry 7}
                                
rcEponTransceiverDiameterType OBJECT-TYPE
	SYNTAX		INTEGER {
				fiber9(1),
				fiber50(2),
				fiber625(3)
				}
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION 
				"This object indicates the diameter of the fiber, measured in micron.
				fiber9  : 9 micron singel-mode fiber
				fiber50 : 50 micron multi-mode fiber
				fiber625: 62.5 micron multi-mode fiber
				"
		::= {rcEponTransceiverInfoEntry 8}
		
rcEponTransceiverDiagImplemented OBJECT-TYPE
	SYNTAX		TruthValue
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION 
				"This object indicates whether digital diagontic monitoring has 
				been implemented in the specified transceiver.
				"
		::= {rcEponTransceiverInfoEntry 9}
		                    
rcEponTransceiverTransferDistance OBJECT-TYPE
	SYNTAX		INTEGER
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
				"
				"
		::= {rcEponTransceiverInfoEntry 10}

-- -- rcEponTransceiverMonitorTable		                    
                                
rcEponTransceiverMonitorTable OBJECT-TYPE
	SYNTAX 		SEQUENCE OF RcEponTransceiverMonitorEntry
	MAX-ACCESS	not-accessible
	STATUS 		current
	DESCRIPTION 
				"This table defines objects to monitor optical parameters of 
				optical transceivers in EPON network elements. It stores the
				last alarm history record of optical parameter to provide a 
				simple history performance. It also defines objects to setting
				high alarm thresholds, high warning thresholds, low warning 
				thresholds and low alarm thresholds on monitoring parameters. 
				"
	REFERENCE	"SFF-8472 Specification for Diagonitcs Monitoring Interface for
				Optical Transceiver, Rev 10.3, December 1rt, 2007. 
				"				 	
		::= {rcEponTransceiverGroup 2}
    
rcEponTransceiverMonitorEntry OBJECT-TYPE
	SYNTAX 		RcEponTransceiverMonitorEntry
	MAX-ACCESS  not-accessible
	STATUS 		current
	DESCRIPTION 
				"An entry in the rcEponTransceiverMonitorTable provides objects
				to monitor an optical parameter and set threshold value on that 
				parameter, at an optical interface.
				
				Rows in the table are denoted by the TransceiverIfIndex. For the 
				OLT TransceiverIfIndex referrring to the unique transcever interface
				at the OLT, rows are created at system initialization or at service
				port creation. For the ONU TransceiverIfIndex referring to the unique
				transcever interface at the ONU, rows are created when ONU registers.
				"
	INDEX		{rcEponTransceiverMonitorType, rcEponTransceiverIndex} 
		::= {rcEponTransceiverMonitorTable 1}
	
RcEponTransceiverMonitorEntry ::= SEQUENCE {				
				rcEponTransceiverMonitorType				 INTEGER,
				rcEponTransceiverMonitorValue				 OpticalParameterValue,
				rcEponTransceiverMonitorAlarmLastValue		 OpticalParameterValue,
				rcEponTransceiverMonitorAlarmLastChange		 DateAndTime,
				rcEponTransceiverMonitorHighAlarmThreshold	 OpticalParameterValue,
				rcEponTransceiverMonitorHighWarningThreshold OpticalParameterValue,
				rcEponTransceiverMonitorLowWarningThreshold	 OpticalParameterValue,  
				rcEponTransceiverMonitorLowAlarmThreshold	 OpticalParameterValue,
				rcEponTransceiverMonitorHighAlarmClearThreshold	  OpticalParameterValue,
				rcEponTransceiverMonitorHighWarningClearThreshold OpticalParameterValue,
				rcEponTransceiverMonitorLowWarningClearThreshold  OpticalParameterValue,  
				rcEponTransceiverMonitorLowAlarmClearThreshold	  OpticalParameterValue
			}

rcEponTransceiverMonitorType OBJECT-TYPE
	SYNTAX		INTEGER {
				temp(1), 
				voltage(2),
				bias(3),
				txPower(4),
				recvPower(5)
				}
	MAX-ACCESS	read-only
	STATUS 		current
	DESCRIPTION	
				"This objects indicates the optical parameters being monitored.
				temp(1)     : Temperature parameter
				voltage(2)  : Supplied voltage
				bias(3)     : Laser bias current
				txPower(4)  : Transmitted optical power
				recvPower(5): Received optical power
				"
		::= {rcEponTransceiverMonitorEntry 1}

rcEponTransceiverMonitorValue OBJECT-TYPE
	SYNTAX		OpticalParameterValue
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	
				"This object indicates the value of optical parameter that is
				being monitored.
				
				The range of values varies depending on the type of optical 
				parameter being monitored, as identified by a corresponding
				object with syntax rcEponTransceiverMonitorType.
				
				When the optical parameter being monitored is 'temp', the 
				supported range is from -32768 to 32768, in units of 1/256 
				degrees centigrade. Example: A value of 6016 represents a 
				temperature reading of 23.5 degrees C.
				
				When the optical parameter being monitored is 'voltage', the 
				supported range is from 0 to 65535, in units of 1/10 millivolts.
				Example: A value of 55000 represents a supplyVoltage reading of 
				5.5 V.
				
				When the optical parameter being monitored is 'bias', the 
				supported range is from 0 to 65535, in units of 1/500 microamperes.
				Example: A value of 500 represents a bias current reading of 
				1 microamperes.

        		When the optical parameter being monitored is 'txPower', the 
        		supported range is from 0 to 65535, in units of 1/10000 mW 
        		and express in units of dBm¡£Example: A value of 60000 represents
        		a txPower reading of 7.78 dBm.    
        
        		When the optical parameter being monitored is 'recvPower', 
        		the supported range is from 0 to 65535, in units of 1/10000 mW ..
        		and express in units of dBm¡£Example: A value of 60000 represents 
        		a recvPower reading of 7.78 dBm.        
        		"
        ::= {rcEponTransceiverMonitorEntry 2}
  
rcEponTransceiverMonitorAlarmLastValue OBJECT-TYPE
	SYNTAX		OpticalParameterValue  
	MAX-ACCESS 	read-only
	STATUS		current
	DESCRIPTION
				"This object indicates the optical parameter value recorded when 
				Threshold Crossing Alert (TCA) occurred. If no TCA occurred, the
				value '-1000000' is returned.
				"
		::= {rcEponTransceiverMonitorEntry 3}
		
rcEponTransceiverMonitorAlarmLastChange OBJECT-TYPE
	SYNTAX		DateAndTime
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION 
				"This object indicates the timestamp(sysUpTime) when last Threshold 
				Crossing Alert (TCA) occurred. It is valid when the recorded value 
				doesn't equal '-1000000'. Refer to the description of 
				rcEponTransceiverMonitorAlarmLastValue object.
				"
		::= {rcEponTransceiverMonitorEntry 4}				

rcEponTransceiverMonitorHighAlarmThreshold OBJECT-TYPE
	SYNTAX		OpticalParameterValue
	MAX-ACCESS  read-write
	STATUS 		current
	DESCRIPTION
				"This object indicates the high alarm threshold of optical parameter.
				If rcEponTransceiverMonitorValue reaches or exceeds this value, a
				Threshold Crossing Alert (TCA) will be sent. Writing will set the 
				value of the threshold and can be done at any time. Changing this 
				value can lead to a Threshold Crossing Alert (TCA) being sent for
				the respective interface.
				"
		::= {rcEponTransceiverMonitorEntry 5}

rcEponTransceiverMonitorHighWarningThreshold OBJECT-TYPE
	SYNTAX		OpticalParameterValue
	MAX-ACCESS  read-write
	STATUS 		current
	DESCRIPTION
				"This object indicates the high warning threshold of optical 
				parameter. If rcEponTransceiverMonitorValue reaches or exceeds
				this value, a Threshold Crossing Alert (TCA) will be sent. 
				Writing will set the value of the threshold and can be done 
				at any time. Changing this value can lead to a Threshold Crossing
				Alert (TCA) being sent for the respective interface.
				"
		::= {rcEponTransceiverMonitorEntry 6}
                     

rcEponTransceiverMonitorLowWarningThreshold OBJECT-TYPE
	SYNTAX		OpticalParameterValue
	MAX-ACCESS  read-write
	STATUS 		current
	DESCRIPTION
				"This object indicates the low warning threshold of optical 
				parameter. If rcEponTransceiverMonitorValue drops to this value
				or belows, a Threshold Crossing Alert (TCA) will be sent. 
				Writing will set the value of the threshold and can be done at 
				any time. Changing this value can lead to a Threshold Crossing 
				Alert (TCA) being sent for the respective interface.
				"
		::= {rcEponTransceiverMonitorEntry 7}
  
rcEponTransceiverMonitorLowAlarmThreshold OBJECT-TYPE
	SYNTAX		OpticalParameterValue
	MAX-ACCESS  read-write
	STATUS 		current
	DESCRIPTION
				"This object indicates the low alarm threshold of optical 
				parameter. If rcEponTransceiverMonitorValue drops to this value
				or belows, a Threshold Crossing Alert (TCA) will be sent. 
				Writing will set the value of the threshold and can be done at 
				any time. Changing this value can lead to a Threshold Crossing 
				Alert (TCA) being sent for the respective interface.
				"
		::= {rcEponTransceiverMonitorEntry 8}  

rcEponTransceiverMonitorHighAlarmClearThreshold OBJECT-TYPE
	SYNTAX		OpticalParameterValue
	MAX-ACCESS  read-write
	STATUS 		current
	DESCRIPTION
				"."
		::= {rcEponTransceiverMonitorEntry 9}

rcEponTransceiverMonitorHighWarningClearThreshold OBJECT-TYPE
	SYNTAX		OpticalParameterValue
	MAX-ACCESS  read-write
	STATUS 		current
	DESCRIPTION
				"."
		::= {rcEponTransceiverMonitorEntry 10}
                     

rcEponTransceiverMonitorLowWarningClearThreshold OBJECT-TYPE
