EPON-OPTICAL-MONITOR-MIB DEFINITIONS ::= BEGIN

	IMPORTS
		MODULE-IDENTITY, OBJECT-TYPE, Integer32,  NOTIFICATION-TYPE
        	FROM SNMPv2-SMI			
        MODULE-COMPLIANCE, OBJECT-GROUP, NOTIFICATION-GROUP
            FROM SNMPv2-CONF 
        TruthValue
            FROM SNMPv2-TC
		ifIndex
			FROM IF-MIB
		dot3ExtPkgOptIfInputPower, dot3ExtPkgOptIfUpperInputPowerThreshold
			FROM DOT3-EPON-MIB                            
		DateAndTime, EnableVar
			FROM SWITCH-TC
       	raisecomEponMgt            			
        	FROM RAISECOM-PONSERIES-BASE-MIB;

rcEponOpticalMonitor MODULE-IDENTITY
	LAST-UPDATED   	"200904210000Z"		--21rt Apr, 2009, midnight
	ORGANIZATION   	"Raisecom Technology Co., Inc."
	CONTACT-INFO   
	               	"Liu Jinguang
	               	Raisecom Technology CO., LTD.
	               	Shangdi Xinxi Road, Haidian Distribution, Beijing
	               	Tel: 86-010-82884499
	               	E-mail: liujinguang@raisecom.com"
	DESCRIPTION
					"The MIB module to describe generic objects to monitor 
					optical characteristics and configure corresponding threshold
					on optical interfaces on EPON network elements.
					"
	REVISION   		"200904210000Z"
	DESCRIPTION	    "The initial revision of this MIB."
		::= { raisecomEponMgt 8 }  

-- Textual Conventons
TransceiverIfIndex ::= TEXTUAL-CONVENTION
	STATUS 		current
	DESCRIPTION 
				"TransceiverIfIndex defines to locate transceiver modules in EPON
				network system which include OLT elements and ONU elements. It
				equals to 'SlotId*10000000+PONPortId*100000+OnuId'. If the onuId is
				zero, TransceiverIfIndex indicates an interface on an OLT network
				element. 
				"
	SYNTAX 		INTEGER 

OpticalParameterValue ::= TEXTUAL-CONVENTION
	STATUS 		current
	DESCRIPTION 
				"
				This type indicates the optical parameter value on the optical interface.
				
				The range of the value varies depending on the optical parameter being
				monitored.
				"
	SYNTAX		Integer32 (-1000000..65535)				


-- groups in this MIB           

rcEponTransceiverGroup OBJECT IDENTIFIER
	::= {rcEponOpticalMonitor 1}
	
rcEponDot3OptIfExtGroup OBJECT IDENTIFIER
	::= {rcEponOpticalMonitor 2}

