-- -- rcEponOltTransceiverConfigGroup
rcEponOltTransceiverConfigGroup OBJECT IDENTIFIER
		::= {rcEponTransceiverGroup 4}
		
rcEponOltTransceiverMonitorPollInterval OBJECT-TYPE
	SYNTAX		INTEGER (5..60)
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION 
				"This object defines the interval in minutes after which optical
				parameters will be polled by system repeatedly and updated in 
				rcEponTransceiverMonitorTable.
				"
	DEFVAL		{15}
		::= {rcEponOltTransceiverConfigGroup 1}

rcEponOltTransceiverConfigTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF RcEponOltTransceiverConfigEntry
	MAX-ACCESS	not-accessible
	STATUS 		current
	DESCRIPTION
				"This table provides switch which can be set on specified 
				interface and it includes only interfaces on OLT network elements.
				"
		::= {rcEponOltTransceiverConfigGroup 2}
		
rcEponOltTransceiverConfigEntry OBJECT-TYPE
	SYNTAX		RcEponOltTransceiverConfigEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	
				"An entry in rcEponOltTransceiverConfigTable to provides object
				to set whether digital diagnotics monitoring is switched on 
				or off on specified transceiver interface. 
				"
	INDEX		{rcEponTransceiverIndex}
		::= {rcEponOltTransceiverConfigTable 1}
		
RcEponOltTransceiverConfigEntry ::= SEQUENCE {
				rcEponOltTransceiverMonitorEnable EnableVar
				}						

rcEponOltTransceiverMonitorEnable OBJECT-TYPE
	SYNTAX		EnableVar
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
				"This object indicates whether digital diagnotics monitoring is
				switched on or off on the interface.
				"
	DEFVAL		{disable}
		::= {rcEponOltTransceiverConfigEntry 1}
			   
-- -- rcEponOnuTransceiverConfigGroup
rcEponOnuTransceiverConfigGroup OBJECT IDENTIFIER
		::= {rcEponTransceiverGroup 5} 

rcEponOnuTransceiverConfigTable OBJECT-TYPE
	SYNTAX 		SEQUENCE OF RcEponOnuTransceiverConfigEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
				"This table provides objects to set whether a Threshold Crossing
				Alert (TCA) will be sent or not. Threshold Crossing Alerts (TCA)
				include high alarm notification, high warning notification,
				low warning notifcation and low alarm notification. If the specified
				objects is set to 'enable', the corresponding recovery traps should
				be sent too.
				"
		::= {rcEponOnuTransceiverConfigGroup 1}
		
rcEponOnuTransceiverConfigEntry	OBJECT-TYPE
	SYNTAX		RcEponOnuTransceiverConfigEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
				"An entry in rcEponOnuTransceiverConfigTable.
				"
	INDEX		{ rcEponTransceiverIndex, rcEponTransceiverType }
		::= {rcEponOnuTransceiverConfigTable 1}

RcEponOnuTransceiverConfigEntry ::= SEQUENCE {
				rcEponOnuTransceiverHighAlarmNotifyEnable		EnableVar,
				rcEponOnuTransceiverHighWarningNotifyEnable		EnableVar,
				rcEponOnuTransceiverLowWarningNotifyEnable		EnableVar,
				rcEponOnuTransceiverLowAlarmNotifyEnable		EnableVar				
				}

rcEponOnuTransceiverHighAlarmNotifyEnable OBJECT-TYPE
	SYNTAX		EnableVar
	MAX-ACCESS  read-write
	STATUS		current
	DESCRIPTION
				"This object provides to set whether a Threshold Crossing Alert (TCA) 
				will be sent or not when a high alarm optical parameter occurs. If this
				object is set to 'enable', the corresponding recovery traps should
				be sent too.
				"
	DEFVAL		{disable}
		::= {rcEponOnuTransceiverConfigEntry 1}
		
rcEponOnuTransceiverHighWarningNotifyEnable OBJECT-TYPE
	SYNTAX		EnableVar
	MAX-ACCESS  read-write
	STATUS		current
	DESCRIPTION
				"This object provides to set whether a Threshold Crossing Alert (TCA) 
				will be sent or not when a high warning optical parameter occurs. If this
				object is set to 'enable', the corresponding recovery traps should
				be sent too.
				"
	DEFVAL		{disable}
		::= {rcEponOnuTransceiverConfigEntry 2}
		
rcEponOnuTransceiverLowWarningNotifyEnable OBJECT-TYPE
	SYNTAX		EnableVar
	MAX-ACCESS  read-write
	STATUS		current
	DESCRIPTION
				"This object provides to set whether a Threshold Crossing Alert (TCA) 
				will be sent or not when a low warning optical parameter occurs. If this
				object is set to 'enable', the corresponding recovery traps should
				be sent too.
				"
	DEFVAL		{disable}
		::= {rcEponOnuTransceiverConfigEntry 3}
		
rcEponOnuTransceiverLowAlarmNotifyEnable OBJECT-TYPE
	SYNTAX		EnableVar
	MAX-ACCESS  read-write
	STATUS		current
	DESCRIPTION
				"This object provides to set whether a Threshold Crossing Alert (TCA) 
				will be sent or not when a low alarm optical parameter occurs. If this
				object is set to 'enable', the corresponding recovery traps should
				be sent too.
				"
	DEFVAL		{disable}
		::= {rcEponOnuTransceiverConfigEntry 4}						

-- rcEponDot3OptIfExtGroup
rcEponDot3OptIfNotifyGroup OBJECT IDENTIFIER
		::= {rcEponDot3OptIfExtGroup 1}
		
rcEponDot3OptIfHighInputPowerTrap NOTIFICATION-TYPE
	OBJECTS		{
				ifIndex,
				dot3ExtPkgOptIfInputPower,
				dot3ExtPkgOptIfUpperInputPowerThreshold
				}
	STATUS		current
	DESCRIPTION
				"This notification will be sent when input power parameter of the
				specified reaches or exceeds the high alarm threshold on an interface. 
				
				It is an extansion to dot3ExtPkgOptIfTable of DOT3-EPON-MIB.
				"
	REFERENCE	"dot3ExtPkgOptIfTable in DOT3-EPON-MIB, in rfc4837."
		::= {rcEponDot3OptIfNotifyGroup 1}				
		
rcEponDot3OptIfHighInputPowerRecoveryTrap NOTIFICATION-TYPE
	OBJECTS		{
				ifIndex,
				dot3ExtPkgOptIfInputPower,
				dot3ExtPkgOptIfUpperInputPowerThreshold
				}
	STATUS		current
	DESCRIPTION
				"This notification will be sent when input power parameter of the
				specified reaches or exceeds the high alarm threshold on an interface
				from a high alarm value.
				"
	REFERENCE	"dot3ExtPkgOptIfTable in DOT3-EPON-MIB, in rfc4837."
		::= {rcEponDot3OptIfNotifyGroup 2}		
				                          
rcEponDot3OptIfLowInputPowerTrap NOTIFICATION-TYPE
	OBJECTS		{
				ifIndex,
				dot3ExtPkgOptIfInputPower,
				dot3ExtPkgOptIfLowerInputPowerThreshold
				}
	STATUS		current
	DESCRIPTION
				"This notification will be sent when input power parameter of the
				specified drops to the high alarm threshold on an interface from 
				normal value.
				"
	REFERENCE	"dot3ExtPkgOptIfTable in DOT3-EPON-MIB, in rfc4837."
		::= {rcEponDot3OptIfNotifyGroup 3}				
		
rcEponDot3OptIfLowInputPowerRecoveryTrap NOTIFICATION-TYPE
	OBJECTS		{
				ifIndex,
				dot3ExtPkgOptIfInputPower,
				dot3ExtPkgOptIfLowerInputPowerThreshold
				}
	STATUS		current
	DESCRIPTION
				"This notification will be sent when input power parameter of the
				specified reacheds or exceeds the low alarm threshold on an interface
				from a low alarm value.
				"
	REFERENCE	"dot3ExtPkgOptIfTable in DOT3-EPON-MIB, in rfc4837."
		::= {rcEponDot3OptIfNotifyGroup 4}
						                          
rcEponDot3OptIfRogueOnuTrap NOTIFICATION-TYPE
	OBJECTS		{
				ifIndex
				}
	STATUS		current
	DESCRIPTION
				"This notification will be sent when there is an ONU whose laser
				is always tured on.    
				"
		::= {rcEponDot3OptIfNotifyGroup 5}

rcEponDot3OptIfCtrlTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF RcEponDot3OptIfCtrlEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
				"This table provides switch which can be set on specified virtual
				interface on the PON port. 

				This table is an extension to the dot3ExtPkgOptIfTable in 
				DOT3-EPON-MIB, in rfc4837.				
				"
		::= {rcEponDot3OptIfExtGroup 2}
				
rcEponDot3OptIfCtrlEntry OBJECT-TYPE
	SYNTAX		RcEponDot3OptIfCtrlEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
				"An entry in rcEponDot3OptIfCtrlTable.
				"                       
	INDEX		{ifIndex}
		::= {rcEponDot3OptIfCtrlTable 1}
		
RcEponDot3OptIfCtrlEntry ::= SEQUENCE {
				rcEponDot3OptIfMonitorEnable EnableVar
				}

rcEponDot3OptIfMonitorEnable OBJECT-TYPE
	SYNTAX		EnableVar
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
				"This object indicates whether digital diagnotics monitoring is
				switched on or off on the virtual interface. Writing will set
				the value of the object and turn the switch on or off.				
				"
	DEFVAL		{disable}
			::= {rcEponDot3OptIfCtrlEntry 1}
					                          
END                                                                                                                                     
		
 