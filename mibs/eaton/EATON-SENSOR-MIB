    humidityAlarm
         AlarmType,
    humidityAlarmChangeSince
         UnixTimeStamp,
    humidityValue
         Integer32,
    humidityCommunicationStatus
         CommunicationStatus,
    humidityCommunicationStatusSince
         UnixTimeStamp
}

