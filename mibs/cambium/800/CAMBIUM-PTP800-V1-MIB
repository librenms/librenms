      STATUS mandatory
      DESCRIPTION
             "Radio license identifier"  
      ::= { radioLicense 1 } 
 
   radioLicenseBand OBJECT-TYPE
      SYNTAX INTEGER {
                       band-lower-6-ghz(0),
                       band-7-ghz(1),
                       band-8-ghz(2),
                       band-11-ghz(3),
                       band-13-ghz(4),
                       band-15-ghz(5),
                       band-18-ghz(6),
                       band-23-ghz(7),
                       band-26-ghz(8),
                       band-32-ghz(9),
                       band-38-ghz(10),
