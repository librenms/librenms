-- SNMP Trap & Notifications

snmpMIBRegisteredTrap NOTIFICATION-TYPE
        OBJECTS {
            fsSnmpListenAgentPort 
                }
        STATUS             current
        DESCRIPTION
         "Generated when a MIB is registered with SNMP Agent"
   ::= { futuresnmptraps 1 }

snmpMIBDeRegisteredTrap NOTIFICATION-TYPE
        OBJECTS {
            fsSnmpListenAgentPort 
                }
        STATUS             current
        DESCRIPTION
         "Generated when a MIB is de-registered with SNMP Agent"
   ::= { futuresnmptraps 2 }
   
   fsSnmpTrapFilterTable  OBJECT-TYPE
        SYNTAX             SEQUENCE OF FsSnmpTrapFilterEntry
        MAX-ACCESS         not-accessible
        STATUS             current
        DESCRIPTION
             "This table contains the Trap-OID entries that is set
        during disabling of traps and is used to filter the
        traps that are disabled."
        ::= { futuresnmptraps 3 }
   
    fsSnmpTrapFilterEntry OBJECT-TYPE
        SYNTAX             FsSnmpTrapFilterEntry
        MAX-ACCESS         not-accessible
        STATUS             current
        DESCRIPTION
             "The individual entry in the above table."
        INDEX   { IMPLIED fsSnmpTrapFilterOID }
        ::= { fsSnmpTrapFilterTable 1 }

     FsSnmpTrapFilterEntry  ::=
        SEQUENCE   {
                        fsSnmpTrapFilterOID 
                          OBJECT IDENTIFIER,
                        fsSnmpTrapFilterRowStatus 
                          RowStatus
                   }

     fsSnmpTrapFilterOID    OBJECT-TYPE
        SYNTAX             OBJECT IDENTIFIER
        MAX-ACCESS         not-accessible
        STATUS             current
        DESCRIPTION
            "Index of this table. The OID entry for which 
            the trap has to be filtered."
        ::= { fsSnmpTrapFilterEntry 1 }

     fsSnmpTrapFilterRowStatus   OBJECT-TYPE
        SYNTAX            RowStatus
        MAX-ACCESS        read-create
        STATUS            current
        DESCRIPTION
             "Denotes the RowStatus of the Trap Filter Entry."
        ::= { fsSnmpTrapFilterEntry 2 } 

-- *******************************************************************
--  SNMP Target Address Table extension - Starts
-- *******************************************************************
     fsSnmpTargetAddrTable OBJECT-TYPE
        SYNTAX       SEQUENCE OF FsSnmpTargetAddrEntry
        MAX-ACCESS   not-accessible
        STATUS       current
        DESCRIPTION
             "This Table is a proprietary extension to the
              standard snmpTargetAddrEntry."
        ::= { fsSnmpTarget 1 }

     fsSnmpTargetAddrEntry OBJECT-TYPE
        SYNTAX       FsSnmpTargetAddrEntry
        MAX-ACCESS   not-accessible
        STATUS       current
        DESCRIPTION
          "This object specifies an entry in the 
          fsSnmpTargetAddrTable. This object contains the additional 
          operational characteristics of SNMPv3 Target MIB apart 
          from snmpTargetAddrEntry."
      AUGMENTS    { snmpTargetAddrEntry }
      ::= { fsSnmpTargetAddrTable 1 }


FsSnmpTargetAddrEntry ::=
      SEQUENCE {
         fsSnmpTargetHostName
         OCTET STRING
       }
fsSnmpTargetHostName OBJECT-TYPE
     SYNTAX      OCTET STRING (SIZE (0..255))
     MAX-ACCESS  read-write
     STATUS      current
     DESCRIPTION
      "This object configures the DNS host name."
     ::= { fsSnmpTargetAddrEntry 1 }

-- *******************************************************************
--  Cambium SNMP Agent Management Configuration Extensions
-- *******************************************************************

fsSnmpCnMgmtReadOnlyCommunity OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "A human readable string representing the default management
         read-only community string."

    ::= { cnSnmpAgentMgmtCfg 1 }

fsSnmpCnMgmtReadWriteCommunity OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "A human readable string representing the default management
         read-write community string."

    ::= { cnSnmpAgentMgmtCfg 2 }

fsSnmpCnMgmtTrapRcvrIpAddress OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(0..48))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The default management SNMP Trap Receiver IP address."

    ::= { cnSnmpAgentMgmtCfg 3 }

fsSnmpCnMgmtUsmUserAuthProtocol OBJECT-TYPE
    SYNTAX      INTEGER {
                    none(1),
                    md5(2),
                    sha(3),
                    sha224(4),
                    sha256(5),
                    sha384(6),
                    sha512(7)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Authentication protocol options for use with the default
         management user."

    ::= { cnSnmpAgentMgmtCfg 4 }

fsSnmpCnMgmtUsmUserPrivProtocol OBJECT-TYPE
    SYNTAX      INTEGER {
                    none(1),
                    des(2),
                    tdes(3),
                    aes(4),
                    aes192(5),
                    aes256(6)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Privacy protocol options for use with the default
         management user."

    ::= { cnSnmpAgentMgmtCfg 5 }

fsSnmpCnMgmtUsmUserAccess OBJECT-TYPE
    SYNTAX      INTEGER {
                    readOnly(1),
                    readWrite(2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Management object access options for use with 
         the default management user."

    ::= { cnSnmpAgentMgmtCfg 6 }

fsSnmpCnMgmtUsmUserAuthKey OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(8..32))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Octets representing the default management 
         user authentication key."

    ::= { cnSnmpAgentMgmtCfg 7 }

fsSnmpCnMgmtUsmUserPrivKey OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(8..32))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Octets representing the default management 
         user privacy key."

    ::= { cnSnmpAgentMgmtCfg 8 }

fsSnmpCnMgmtUsmUserName OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "A human readable string representing the default 
         management user name."

    ::= { cnSnmpAgentMgmtCfg 9 }

