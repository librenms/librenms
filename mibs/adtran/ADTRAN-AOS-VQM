         ::= { adVQMCfg 13 }

     adVqmCfgRoundTripPingType OBJECT-TYPE
        SYNTAX  INTEGER {
                ping (1),
                timestamp (2)
                }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "ICMP Ping or ICMP Timestamp used for delay measurements."
         ::= { adVQMCfg 14 }

     adVqmCfgCallHistorySize OBJECT-TYPE
