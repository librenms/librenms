                        speeX16k8       (84),
                        speeX20k6       (85),
                        speeX23k8       (86),
                        speeX27k8       (87),
                        speeX34k2       (88),
                        speeX42k2       (89)
         }
         MAX-ACCESS     read-only
         STATUS         current
         DESCRIPTION
            "Last voice CODEC detected on this stream."
         ::= { adVQMCallHistoryEntry 17 }

