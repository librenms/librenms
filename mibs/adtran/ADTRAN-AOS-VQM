                        amrnb12k2       (60),
                        amrnb10k2       (61),
                        amrnb7k95       (62),
                        amrnb7k4        (63),
                        amrnb6k7        (64),
                        amrnb5k9        (65),
                        amrnb5k15       (66),
                        amrnb4k75       (67),
                        ilbc13k3        (68),
                        ilbc15k2        (69),
                        g711u56k        (70),
                        g711uPLC56k     (71),
                        g711A56k        (72),
                        g711APLC56k     (73),
                        g7231C          (74),
                        speex2k15       (75),
                        speex5k95       (76),
                        speeX8k         (77),
                        speeX11k        (78),
                        speeX15k        (79),
                        speeX18k2       (80),
                        speeX24k6       (81),
                        speeX3k95       (82),
