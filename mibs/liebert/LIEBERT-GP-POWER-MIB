lgpPwrStateStaticSwitchType OBJECT-TYPE
    SYNTAX      INTEGER 
    { 
        notApplicable(1), 
        continuousDuty(2), 
        momentaryDuty(3) 
    }
    MAX-ACCESS  read-only
    STATUS      current
