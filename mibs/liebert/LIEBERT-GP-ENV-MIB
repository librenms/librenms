lgpEnvStateOperatingReason OBJECT-TYPE
    SYNTAX      INTEGER 
    { 
        none(1), 
        localUser(2), 
        alarm(3), 
        schedule(4), 
        remoteUser(5), 
        externalDevice(6), 
        localDisplay(7)
    }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The reason for the current operating state of the system."
    ::= { lgpEnvState 14 }

