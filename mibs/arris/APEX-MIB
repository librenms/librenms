    SYNTAX INTEGER
        {
         gbe      (1),
         fastEnet (2)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Input Type of input from which to obtain data.
        "
::= { apexPidMapEntry 3 }

apexPidMapInputInterface OBJECT-TYPE
    SYNTAX Integer32(0..4)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Number of the input, of type configured by Input Type,
         from which to obtain data.

         Range:
            '0'      = Not Applicable
            GBE      = 1-4
            FastEnet = 1-2
        "
