::= { apexPsiStatusEntry 8 }

apexPsiStatusGpsTime OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The GPS time when the PSI was added to the table."
::= { apexPsiStatusEntry 9 }


