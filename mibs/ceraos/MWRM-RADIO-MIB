			INTEGER,
	    genEquipStm1AbcAttrPri2IfIndex
			INTEGER,
		genEquipStm1AbcAttrPri3IfIndex
			INTEGER,
	    genEquipStm1AbcAttrPri4IfIndex
			INTEGER
	}

