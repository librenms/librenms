        DESCRIPTION
		"Status Device Error"
		::= { genEquipRfuStatusEntry 22 }


genEquipRfuStatusBand 	OBJECT-TYPE
        SYNTAX		INTEGER {
        				unknown(0),
        				band-18(2),
        				band-23(3),
        				band-26(4),
        				band-28(5),
        				band-38(6),
        				band-29(7),
