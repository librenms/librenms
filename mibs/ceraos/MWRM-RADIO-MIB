        SYNTAX GenEquipRadioCfgEntry
        ACCESS not-accessible
        STATUS mandatory
          DESCRIPTION
          "Radio Configuration table."
        INDEX { genEquipRadioCfgRadioId }
        ::= { genEquipRadioCfgTable 1 }

