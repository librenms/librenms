    zxr10ShelfTemperatureTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Zxr10ShelfTemperatureEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
              "the description of ShelfTemperature information."
        ::= { zxr10SystemHardwareMIB    4 }
               
    zxr10ShelfTemperatureEntry  OBJECT-TYPE
        SYNTAX      Zxr10ShelfTemperatureEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
              "the description of ShelfTemperature information"
        INDEX{ 
                zxr10ShelfTemperatureRackNo,
                zxr10ShelfTemperatureShelfNo
             }
        ::= { zxr10ShelfTemperatureTable 1 }              

    Zxr10ShelfTemperatureEntry ::=
        SEQUENCE {
        zxr10ShelfTemperatureRackNo
            Integer32,
        zxr10ShelfTemperatureShelfNo
            Integer32,              
        zxr10EnvironTemperature
            Integer32,
        zxr10EnvironTemperatureLowThreshold
            Integer32,
        zxr10EnvironTemperatureHighThreshold1
            Integer32,
        zxr10EnvironTemperatureHighThreshold2
            Integer32,
        zxr10EnvironTemperaturecollectioncycle
            Integer32, 
        zxr10EnvTempOverheatProtectEn
            INTEGER
        }

    zxr10ShelfTemperatureRackNo  OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
          "The rack No."
        ::= { zxr10ShelfTemperatureEntry 1 }
               
    zxr10ShelfTemperatureShelfNo  OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
          "The shelf No."
        ::= { zxr10ShelfTemperatureEntry 2 }
              
    zxr10EnvironTemperature  OBJECT-TYPE
        SYNTAX      Integer32
        UNITS       "Centigrade"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
          "The temperature of the environment."
        ::= { zxr10ShelfTemperatureEntry 3 }
              
    zxr10EnvironTemperatureLowThreshold  OBJECT-TYPE
        SYNTAX      Integer32
        UNITS       "Centigrade"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
          "The low temperature threshold of the environment."
        ::= { zxr10ShelfTemperatureEntry 4 }
              
    zxr10EnvironTemperatureHighThreshold1  OBJECT-TYPE
        SYNTAX      Integer32(45..85)
        UNITS       "Centigrade"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
          "The warnning temperature threshold of the environment."
        DEFVAL { 55 }
        ::= { zxr10ShelfTemperatureEntry 5 }
              
    zxr10EnvironTemperatureHighThreshold2  OBJECT-TYPE
        SYNTAX      Integer32(45..85)
        UNITS       "Centigrade"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
          "The high temperature threshold of the environment."
        DEFVAL { 70 }
        ::= { zxr10ShelfTemperatureEntry 6 }
              
    zxr10EnvironTemperaturecollectioncycle  OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
              "Environ Temperature collection cycle."
        ::= { zxr10ShelfTemperatureEntry 7 }

    zxr10EnvTempOverheatProtectEn OBJECT-TYPE
        SYNTAX      INTEGER
        {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION   
            "Enables or disables the overheat protection function."
        DEFVAL  { enabled }
        ::= { zxr10ShelfTemperatureEntry  8 }
                                                                                                                                                                                                                                   
