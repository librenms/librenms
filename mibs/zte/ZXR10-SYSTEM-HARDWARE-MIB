ZXR10-SYSTEM-HARDWARE-MIB DEFINITIONS ::= BEGIN

   IMPORTS
       MODULE-IDENTITY, 
       OBJECT-TYPE, OBJECT-IDENTITY, 
       TimeTicks, Counter32, Integer32,
       enterprises        
       FROM SNMPv2-SMI
       zxr10RackNo, zxr10ShelfNo,zxr10SlotNo, RunStatus
       FROM ZXR10-RACK-MIB 
       DisplayString,RowStatus         FROM SNMPv2-TC 
       zxUPN,BoardType                 FROM ZXR10-PROJECT-MIB;

  zxr10SystemHardwareMIB MODULE-IDENTITY
            LAST-UPDATED "201802281817Z"  
            ORGANIZATION "ZTE Corporation"
            CONTACT-INFO
            "ZTE Corporation 
            Nanjing Institute of ZTE Corporation
         
            No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China
         
            Tel: +86-25-52870000"
    
            DESCRIPTION
                "ZXR10 T8000 ZXR10-SYSTEM-HARDWARE-MIB"

            REVISION "201802281817Z"
            DESCRIPTION
            "change the MAX-ACCESS of zxr10EnvironTemperatureHighThreshold1 
            from read-only to read-write
             change the SYNTAX of zxr10EnvironTemperatureHighThreshold1
            from Integer32 to Integer32(45..85)
             add the DEFVAL of zxr10EnvironTemperatureHighThreshold1"
            
            REVISION "201709160000Z"
            DESCRIPTION
            "add zxr10CardMgmtObjects."
            
            REVISION "201706161017Z"
            DESCRIPTION
            "change the MAX-ACCESS of zxr10BoardTempMajorThreshold from
            read-only to read-write"

            REVISION "201707140000Z"
            DESCRIPTION
            "add zxr10SystemMgmtObjects."

            REVISION "201707051352Z"
            DESCRIPTION
            "add zxr10FanMgmtObjects."

            REVISION "201706301559Z"  
            DESCRIPTION
            "add node zxr10EnvTempOverheatProtectEn in 
            zxr10ShelfTemperatureTable."
            
            REVISION "201706161017Z"
            DESCRIPTION
            "change the SYNTAX of zxr10FanRatio from DisplayString to Integer32
             change the MAX-ACCESS of zxr10FanRatio from read-only to
            read-write"
                
            REVISION "201609020000Z"
            DESCRIPTION
            "import zxUPN from ZXR10-PROJECT-MIB and delete zxr10M6000-S
            OBJECT IDENTIFIER."

    ::= { zxUPN 2 }
                  
              
-- zxr10CpusystemTable
    zxr10CpusystemTable OBJECT-TYPE
                  SYNTAX        SEQUENCE OF Zxr10CpusystemEntry
                  MAX-ACCESS    not-accessible
                  STATUS        current
                  DESCRIPTION
                          "the description of Cpusystem information
                           in the zxr10 card .
                           it is a list of Cpusystem entries."
                   ::= { zxr10SystemHardwareMIB 1 }
                   
    zxr10CpusystemEntry  OBJECT-TYPE
                  SYNTAX        Zxr10CpusystemEntry
                  MAX-ACCESS    not-accessible
                  STATUS        current
                  DESCRIPTION
                          "A Cpusystem entry containing objects that
                           descript the Cpusystem information,such as:
                           zxr10CpuSlotBoardType,zxr10SystemUnitIndex,
                           zxr10SystemUnitDescription,etc."
                  INDEX { zxr10RackNo, zxr10ShelfNo,zxr10SlotNo,
                          zxr10SystemUnitIndex}
                  ::= { zxr10CpusystemTable 1 }            
                  
    Zxr10CpusystemEntry ::=
        SEQUENCE {            
            zxr10CpuSlotBoardType
                BoardType,
            zxr10SystemUnitIndex
                INTEGER,
            zxr10SystemUnitDescription
                DisplayString,
            zxr10SystemUnitRunStatus
                RunStatus,
            zxr10SystemMemSize
                Counter32,   
            zxr10SystemMemUsed
                INTEGER,    
            zxr10SystemCpuUtility5s
                INTEGER,
            zxr10SystemCpuUtility1m
                INTEGER,
            zxr10SystemCpuUtility5m
                INTEGER,
            zxr10SystemPeakCpuUtility
                INTEGER,
            zxr10SystemUnitUpTime
                TimeTicks,   
            zxr10SystemUnitPidNum
                INTEGER,
            zxr10SystemOamCpuName
                DisplayString,
            zxr10SystemUnitBootVer
                DisplayString,
            zxr10SystemCardCpuLoadThreshold
                Integer32,
            zxr10SystemCardMemUsageThreshold
                Integer32
        }             
    
    zxr10CpuSlotBoardType OBJECT-TYPE
        SYNTAX      BoardType
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "the slot board type of system Unit."
        ::= { zxr10CpusystemEntry 1 }

    zxr10SystemUnitIndex OBJECT-TYPE
        SYNTAX      INTEGER  
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "the index of system Unit."
        ::= { zxr10CpusystemEntry 2}

    zxr10SystemUnitDescription OBJECT-TYPE
        SYNTAX      DisplayString  
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "the description of system Unit. "
        ::= { zxr10CpusystemEntry 3 }

    zxr10SystemUnitRunStatus OBJECT-TYPE
        SYNTAX      RunStatus
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "the run status of system Unit.
             1 is running,
             0 is shutdown."
        ::= { zxr10CpusystemEntry 4}

    zxr10SystemMemSize OBJECT-TYPE
        SYNTAX      Counter32 
        UNITS       "MB"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The memory size of the unit."
        ::= { zxr10CpusystemEntry 5}    

    zxr10SystemMemUsed OBJECT-TYPE
        SYNTAX      INTEGER
        UNITS       "percent"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The memory utilization."
        ::= { zxr10CpusystemEntry 6 }    
        
    zxr10SystemCpuUtility5s OBJECT-TYPE
        SYNTAX      INTEGER
        UNITS       "percent"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The CPU utilization (5 seconds)."
        ::= { zxr10CpusystemEntry 7}
        
    zxr10SystemCpuUtility1m OBJECT-TYPE
        SYNTAX      INTEGER
        UNITS       "percent"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The CPU utilization (1 minute)"
        ::= { zxr10CpusystemEntry 8}

    zxr10SystemCpuUtility5m OBJECT-TYPE
        SYNTAX      INTEGER
        UNITS       "percent"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The CPU utilization (5 minutes)"
        ::= { zxr10CpusystemEntry 9}    
        

    zxr10SystemPeakCpuUtility OBJECT-TYPE
        SYNTAX      INTEGER  
        UNITS       "percent"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The peak CPU utilization, which is measured in 1 minute."
        ::= { zxr10CpusystemEntry 10}


    zxr10SystemUnitUpTime OBJECT-TYPE
        SYNTAX      TimeTicks 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "the total time of system running."
        ::= { zxr10CpusystemEntry 11}   

    zxr10SystemUnitPidNum OBJECT-TYPE
        SYNTAX      INTEGER
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "the total process running on the unit. Now we don't use it."
        ::= { zxr10CpusystemEntry 12} 

    zxr10SystemOamCpuName OBJECT-TYPE
        SYNTAX      DisplayString  
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "the description of logic cpu name. "
        ::= { zxr10CpusystemEntry 13 }
        
    zxr10SystemUnitBootVer OBJECT-TYPE
        SYNTAX      DisplayString  
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "the BootVer of the cpu. "
        ::= { zxr10CpusystemEntry 21 }

    zxr10SystemCardCpuLoadThreshold OBJECT-TYPE
        SYNTAX      Integer32(0|50..100)
        UNITS       "percent"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The CPU load threshold of the card. 
             0 means unconfigured."
        DEFVAL { 0 }
        ::= { zxr10CpusystemEntry 31}

    zxr10SystemCardMemUsageThreshold OBJECT-TYPE
        SYNTAX      Integer32(0..100)
        UNITS       "percent"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The memory usage threshold of the card. 
             0 means unconfigured."
        DEFVAL { 0 }
        ::= { zxr10CpusystemEntry 32}
        
---------------------------------------------------fan table
    zxr10FanTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Zxr10FanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
              "The description of fan module information."
        ::= { zxr10SystemHardwareMIB    3 } 
              
    zxr10FanEntry  OBJECT-TYPE
        SYNTAX      Zxr10FanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
              "The description of fan module information."
        INDEX  { 
                zxr10FanRackNo,
                zxr10FanShelfNo,
                zxr10FanGroupNo,
                zxr10FanIndex
             }
        ::= { zxr10FanTable 1 }

    Zxr10FanEntry ::=
        SEQUENCE {
        zxr10FanRackNo
            Integer32,
        zxr10FanShelfNo
            Integer32,
        zxr10FanGroupNo
            Integer32,
        zxr10FanIndex
            Integer32,
        zxr10FanPosition
            INTEGER,
        zxr10FanType
            DisplayString,
        zxr10FanStatus            
            INTEGER,
        zxr10FanSpeed
            Integer32,
        zxr10FanPartNumber
            DisplayString,
        zxr10FanManufactureDate
            DisplayString,
        zxr10FanSerialNumber
            DisplayString,
        zxr10FanAssemblySerialNo
            DisplayString,
        zxr10FanMaximumSpeed
            INTEGER,
        zxr10FanRatio
            Integer32
        }

