-- The Fan MIB

    zxr10FanMgmtObjects OBJECT IDENTIFIER  ::= { zxr10SystemHardwareMIB  8}

    zxr10FanSystemTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Zxr10FanSystemEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
              "The fan system table."
        ::= { zxr10FanMgmtObjects  2 } 
        
    zxr10FanSystemEntry OBJECT-TYPE   
        SYNTAX      Zxr10FanSystemEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An entry in the fan system table."
        INDEX {zxr10FanRackNo,
               zxr10FanShelfNo}
        ::= { zxr10FanSystemTable 1 } 
        
    Zxr10FanSystemEntry ::= SEQUENCE {
        zxr10FanSysSpeedAdjustStrategy  INTEGER
        }

    zxr10FanSysSpeedAdjustStrategy OBJECT-TYPE   
        SYNTAX      INTEGER
        {
            pidMode(1),
            envTempMode(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The fan speed adjustment strategy of the NE shelf."
        DEFVAL      { pidMode }
        ::= { zxr10FanSystemEntry 1 }  

-- The System Management MIB

    zxr10SystemMgmtObjects OBJECT IDENTIFIER  ::= { zxr10SystemHardwareMIB  20}

    zxr10SystemCpuLoadThreshTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Zxr10SystemCpuLoadThreshEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
              "The system cpu load threshold table."
        ::= { zxr10SystemMgmtObjects  2 } 
        
    zxr10SystemCpuLoadThreshEntry OBJECT-TYPE   
        SYNTAX      Zxr10SystemCpuLoadThreshEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An entry in the system cpu load threshold table."
        INDEX {zxr10SystemCpuLoadThreshLevel}
        ::= { zxr10SystemCpuLoadThreshTable 1 } 
        
    Zxr10SystemCpuLoadThreshEntry ::= SEQUENCE {
        zxr10SystemCpuLoadThreshLevel    INTEGER,
        zxr10SystemCpuLoadThreshold      Integer32,       
        zxr10SystemCpuLoadThreshRowStaus RowStatus             
        }

    zxr10SystemCpuLoadThreshLevel OBJECT-TYPE   
        SYNTAX      INTEGER
        {
            low(1),
            middle(2),
            high(3)
        }
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The level of cpu load threshold."
        ::= { zxr10SystemCpuLoadThreshEntry 1 }
        
    zxr10SystemCpuLoadThreshold OBJECT-TYPE   
        SYNTAX      Integer32(50..100)
        UNITS       "percent"
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The CPU load threshold."
        ::= { zxr10SystemCpuLoadThreshEntry 2 }   

    zxr10SystemCpuLoadThreshRowStaus  OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The status of this conceptual row.
            
             The supported actions of this conceptual row only include:
             'createAndGo' and 'destroy'.

             To create a row in this table, a manager must set this object to 
             createAndGo(4).
    
             To delete a row in this table, a manager must set this object to 
             destroy(6). 
             
             To get this object, the agent always returns active(1)."  
    ::= { zxr10SystemCpuLoadThreshEntry 50 }
    
    zxr10SystemMemUsageThreshTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Zxr10SystemMemUsageThreshEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
              "The system memory usage threshold table."
        ::= { zxr10SystemMgmtObjects  3 } 
        
    zxr10SystemMemUsageThreshEntry OBJECT-TYPE   
        SYNTAX      Zxr10SystemMemUsageThreshEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An entry in the system memory usage threshold table."
        INDEX {zxr10SysMemUsageThreshLevel}
        ::= { zxr10SystemMemUsageThreshTable 1 } 
        
    Zxr10SystemMemUsageThreshEntry ::= SEQUENCE {
        zxr10SysMemUsageThreshLevel    INTEGER,
        zxr10SysMemUsageThreshold      Integer32,       
        zxr10SysMemUsageThreshRowStaus RowStatus             
        }                               
                              
    zxr10SysMemUsageThreshLevel OBJECT-TYPE   
        SYNTAX      INTEGER {
            low(1), 
            middle(2), 
            high(3)
        }
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The level of memory usage threshold."
        ::= { zxr10SystemMemUsageThreshEntry 1 }
        
    zxr10SysMemUsageThreshold OBJECT-TYPE   
        SYNTAX      Integer32(1..100)
        UNITS       "percent"
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The memory usage threshold."
        ::= { zxr10SystemMemUsageThreshEntry 2 }   

    zxr10SysMemUsageThreshRowStaus  OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The status of this conceptual row.
            
             The supported actions of this conceptual row only include:
             'createAndGo' and 'destroy'.

             To create a row in this table, a manager must set this object to 
             createAndGo(4).
    
             To delete a row in this table, a manager must set this object to 
             destroy(6). 
             
             To get this object, the agent always returns active(1)."  
    ::= { zxr10SystemMemUsageThreshEntry 50 }


    zxr10CardMgmtObjects  OBJECT IDENTIFIER  ::= { zxr10SystemHardwareMIB  9}

    zxr10CardPowerSupplyTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Zxr10CardPowerSupplyEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
              "The card power supply table."
        ::= { zxr10CardMgmtObjects  1 } 
        
    zxr10CardPowerSupplyEntry OBJECT-TYPE   
        SYNTAX      Zxr10CardPowerSupplyEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An entry in the zxr10CardPowerSupplyTable."
        INDEX { zxr10CardPowerSupplyRack, 
                zxr10CardPowerSupplyShelf, 
                zxr10CardPowerSupplySlot,
                zxr10CardPowerSupplySubcard}
        ::= { zxr10CardPowerSupplyTable 1 } 
        
    Zxr10CardPowerSupplyEntry ::= SEQUENCE {
        zxr10CardPowerSupplyRack       Integer32,
        zxr10CardPowerSupplyShelf      Integer32,
        zxr10CardPowerSupplySlot       Integer32,       
        zxr10CardPowerSupplySubcard    Integer32,
        zxr10CardPowerSupplyOperation  INTEGER,
        zxr10CardPowerSupplyStatus     INTEGER
        }

    zxr10CardPowerSupplyRack OBJECT-TYPE   
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The rack number."
        ::= { zxr10CardPowerSupplyEntry 1 }

    zxr10CardPowerSupplyShelf OBJECT-TYPE   
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The shelf number."
        ::= { zxr10CardPowerSupplyEntry 2 }

    zxr10CardPowerSupplySlot OBJECT-TYPE   
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The slot number."
        ::= { zxr10CardPowerSupplyEntry 3 }

    zxr10CardPowerSupplySubcard OBJECT-TYPE   
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The subcard number.
             The range of the subcard is 0-3;
             A special value 255 indicates that there has no subcard in the NE."
        ::= { zxr10CardPowerSupplyEntry 4 }
  
    zxr10CardPowerSupplyOperation OBJECT-TYPE   
        SYNTAX      INTEGER {
            on(1), 
            off(2)
            }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Open/close the power supply of the card or subcard."
        ::= { zxr10CardPowerSupplyEntry 5 }

    zxr10CardPowerSupplyStatus OBJECT-TYPE   
        SYNTAX      INTEGER {
            powerSupplyOn(1),      
            powerSupplyOff(2),
            duringPowerOn(3),
            abnormal(4)
            }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The power supply status of the card or subcard."
        ::= { zxr10CardPowerSupplyEntry 6 }
END              