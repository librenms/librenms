--zxr10SFLinkTable
zxr10SFLinkTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Zxr10SFLinkEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "the description of zxr10 sf link information,
                       it is a list of SFLink entries.
                       Only the equipment supported cluster use this table."
               ::= { zxr10RackMIB 13 }
               
zxr10SFLinkEntry  OBJECT-TYPE
              SYNTAX  Zxr10SFLinkEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "A SFLink entry containing objects that
                       descript the link information,such as:
                       zxr10PortPhyStatus,zxr10PortBandwidth,zxr10FEDownSerdes,zxr10PeerPortNo,etc."
              INDEX { zxr10RackNo,zxr10SFLinkShelfNo,zxr10SFLinkSlotNo,zxr10SFLinkPortNo}
              ::= { zxr10SFLinkTable 1 }               
              
Zxr10SFLinkEntry ::=
        SEQUENCE {
            zxr10SFLinkShelfNo
                INTEGER,
            zxr10SFLinkSlotNo
                INTEGER,
            zxr10SFLinkPortNo
                INTEGER,
            zxr10SFLinkPortType
                INTEGER,
            zxr10SFLinkPortPhyStatus
                INTEGER,
            zxr10SFLinkPortBandwidth
                INTEGER,   
            zxr10SFLinkDownSerdes
                INTEGER,
            zxr10SFLinkErrSerdes
                INTEGER,
            zxr10SFLinkErrSerdestotal
                INTEGER,
            zxr10SFLinkPeerRackNo
                INTEGER,
            zxr10SFLinkPeerShelfNo
                INTEGER,
            zxr10SFLinkPeerSlotNo
                INTEGER,
            zxr10SFLinkPeerPortNo
                INTEGER,
            zxr10SFLinkPeerPortType
                INTEGER,
            zxr10SFLinkCascadeTimeStamp
                TimeTicks                                    
        }             
    
