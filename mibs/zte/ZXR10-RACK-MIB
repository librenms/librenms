--zxr10PortTable--------------------------------------------
zxr10PortTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Zxr10PortEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "the description of port information
                       in the zxr10 card .
                       it is a list of port entries."
               ::= { zxr10RackMIB 5 }
               
zxr10PortEntry  OBJECT-TYPE
              SYNTAX  Zxr10PortEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "A port entry containing objects that
                       descript the port information,such as:
                       zxr10PortNo
                       zxr10PortIfIndex
                       zxr10PortType."
              INDEX { zxr10RackNo,zxr10PortShelfNo,zxr10PortSlotNo,zxr10PortSubCardNo,zxr10PortNo}                  
              ::= { zxr10PortTable 1 }             
              
Zxr10PortEntry ::=
        SEQUENCE {
            zxr10PortNo
                INTEGER,
            zxr10PortIfIndex
                INTEGER,          
            zxr10PortType
                PortType,
            zxr10PortPhyStatus
                PhyStatus,
            zxr10PortPhyEOProperty
                PortProperty,
            zxr10PortPhyPhyIfSpeed
                INTEGER,
            zxr10PortPhyIfMTU
                INTEGER,
            zxr10PortName
                DisplayString,
            zxr10PortTimeStamp
                TimeTicks,
            zxr10PortShelfNo
                INTEGER,
            zxr10PortSlotNo
                INTEGER,
            zxr10PortSubCardNo
                INTEGER
                
        }             

zxr10PortNo OBJECT-TYPE
    SYNTAX  INTEGER  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the No. of the port on the subcard,
                 which contains several ports . "
    ::= { zxr10PortEntry 1 }
    
zxr10PortIfIndex OBJECT-TYPE
    SYNTAX  INTEGER  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the index of the port on the subcard. "
    ::= { zxr10PortEntry 2 }    

zxr10PortType OBJECT-TYPE
    SYNTAX  PortType  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the type of the port. "
    ::= { zxr10PortEntry 3 }


zxr10PortPhyStatus OBJECT-TYPE
    SYNTAX  PhyStatus
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the physical status. of the special port. "
    ::= { zxr10PortEntry 4 }

zxr10PortPhyEOProperty OBJECT-TYPE
    SYNTAX  PortProperty
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the physical photoelectricity property of the special port. "
    ::= { zxr10PortEntry 5 }

