                      port-mng(1292),   
                      port-timer(1294), 
                      port-gps(1295),
                      port-esi(1296),
                      port-sfi(1297),
                      port-gpon-olt(1116)
                    }
        LightType ::=INTEGER
                      {
                         alarm(0),
                         indication(1)
                      }
       LightStatus::=INTEGER
                      {
                         down(0),
                         up(1)
                      } 

--zxr10RackNo
  zxr10RackNo OBJECT-TYPE   
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the No. of the special rack."
    ::= { zxr10RackMIB 1 }
    
--zxr10ShelfTable
zxr10ShelfTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Zxr10ShelfEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "the description of zxr10 shelf information,
                       it is a list of shelf entries."
               ::= { zxr10RackMIB 2 }
               
zxr10ShelfEntry  OBJECT-TYPE
              SYNTAX  Zxr10ShelfEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "A shelf entry containing objects that
                       descript the shelf information,such as:
                       zxr10ShelfNo ,zxr10ShelfAttrib,zxr10ShelfAvailStatus."
              INDEX { zxr10RackNo, zxr10ShelfNo}
              ::= { zxr10ShelfTable 1 }            
              
Zxr10ShelfEntry ::=
        SEQUENCE {
            zxr10ShelfNo
                INTEGER,    
           zxr10ShelfAttrib
                ShelfAttrib,
            zxr10ShelfType
                ShelfType,
            zxr10ShelfSlotNumber
                INTEGER,
            zxr10ShelfAvailStatus
                AvailStatus,
            zxr10ShelfMasterStatus
                MasterStatus,
            zxr10ShelfTimeStamp
                TimeTicks,
            zxr10ShelfName 
                DisplayString,
            zxr10ShelfService
                Integer32,
            zxr10ShelfLocation
                DisplayString,
            zxr10ShelfMPUFNum
                INTEGER,
            zxr10ShelfPFUNum
                INTEGER,
            zxr10ShelfPIUNum
                INTEGER,
            zxr10ShelfSFUNum  
                INTEGER,
            zxr10ShelfCESUNum
                INTEGER,
            zxr10ShelfPartNumber    
                DisplayString,
            zxr10ShelfManufactureDate
                DisplayString,
            zxr10ShelfSerialNumber
                DisplayString,
            zxr10ShelfFiberLength
                INTEGER
        }             

zxr10ShelfNo OBJECT-TYPE
    SYNTAX  INTEGER 
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the No. of the special shelf."
    ::= { zxr10ShelfEntry 1 }
    
zxr10ShelfAttrib OBJECT-TYPE
    SYNTAX  ShelfAttrib
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the attribute. of the special shelf.
                eg, if the current shelf is the Central Fabric Chassis in a cluster,
                we use 0. Else if the shelf is the Line Card Chassis in a cluster or in a no-cluster,
                we use 1."
    ::= { zxr10ShelfEntry 2 }
    
zxr10ShelfType OBJECT-TYPE
    SYNTAX  ShelfType
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the type. of the special shelf.
         eg,the shelf is M6000-16,we use 0,
         the shelf is M6000-8,we use 1,
         the shelf is T8000,we use 8,
         the shelf is T8000-8,we use 9,
         the shelf is M6000-16E, we use 16777216,
         the shelf is M6000-8E, we use 16777217.  "
    ::= { zxr10ShelfEntry 3 }

zxr10ShelfSlotNumber OBJECT-TYPE
    SYNTAX  INTEGER  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the slot number of the special shelf.
         eg,the slot number of M6000-16 and T8000-16 and M6000-16E is 23,
         the slot number of M6000-8 and T8000-08 and M6000-8E is 14."
    ::= { zxr10ShelfEntry 4 }

zxr10ShelfAvailStatus OBJECT-TYPE
    SYNTAX  AvailStatus  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the status. of the special shelf.
                eg ,the current shelf is in use,
                we use 1,else if the shelf is unuse,
                we use 0 . "
    ::= { zxr10ShelfEntry 5 }

zxr10ShelfMasterStatus OBJECT-TYPE
    SYNTAX  MasterStatus
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the master status. of the special shelf.
         eg,the current shelf is master,we use 0, 
         else if the shelf is slave, we use 1,
         else, we use 2."
    ::= { zxr10ShelfEntry 6}

zxr10ShelfTimeStamp OBJECT-TYPE
    SYNTAX  TimeTicks  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the time of the special shelf changed lastly. "
    ::= { zxr10ShelfEntry 7 }     
    
zxr10ShelfName OBJECT-TYPE
    SYNTAX  DisplayString  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "we don't use ShelfName to descript Shelf in any equipment, so the Shelf Name is empty. "
    ::= { zxr10ShelfEntry 8 }     
    
zxr10ShelfService OBJECT-TYPE
    SYNTAX  Integer32  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the server of the special shelf.
         eg, we use 78.
         Only the equipment supported cluster use this node."
    ::= { zxr10ShelfEntry 9 }   
    
zxr10ShelfLocation OBJECT-TYPE
    SYNTAX  DisplayString  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the location of the special shelf.
         eg, we use SysLocation.
         Only the equipment supported cluster use this node."
    ::= { zxr10ShelfEntry 10 }     
    
zxr10ShelfMPUFNum OBJECT-TYPE
    SYNTAX  INTEGER  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the Main Processing Unit of the special shelf.
         Only the equipment supported cluster use this node."
    ::= { zxr10ShelfEntry 11 }   
    
zxr10ShelfPFUNum OBJECT-TYPE
    SYNTAX  INTEGER  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the Packet Forwarding Unit of the special shelf. 
        Only the equipment supported cluster use this node."
    ::= { zxr10ShelfEntry 12 }     
    
zxr10ShelfPIUNum OBJECT-TYPE
    SYNTAX  INTEGER  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the Physical line Interface Unit of the special shelf.
        Only the equipment supported cluster use this node."
    ::= { zxr10ShelfEntry 13 }   
    
zxr10ShelfSFUNum OBJECT-TYPE
    SYNTAX  INTEGER  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the Switch Fabric Unit of the special shelf.
        Only the equipment supported cluster use this node. "
    ::= { zxr10ShelfEntry 14 }      
    
zxr10ShelfCESUNum OBJECT-TYPE
    SYNTAX  INTEGER  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the Central Ethernet Switch Unit of the special shelf.
        Only the equipment supported cluster use this node."
    ::= { zxr10ShelfEntry 15 } 
    
zxr10ShelfPartNumber OBJECT-TYPE
    SYNTAX  DisplayString  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the part number of the special shelf. "
    ::= { zxr10ShelfEntry 16 }  
    
zxr10ShelfManufactureDate OBJECT-TYPE
    SYNTAX  DisplayString  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the manufacture date of the special shelf. "
    ::= { zxr10ShelfEntry 17 }  
    
zxr10ShelfSerialNumber OBJECT-TYPE
    SYNTAX  DisplayString  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the serial number of the special shelf. "
    ::= { zxr10ShelfEntry 18 }  
    
zxr10ShelfFiberLength OBJECT-TYPE
    SYNTAX  INTEGER 
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the fiber length of the shelf.
        Only the equipment supported cluster use this node."
    ::= { zxr10ShelfEntry 19 }
    
-- zxr10SlotBoardTable
 zxr10SlotBoardTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Zxr10SlotBoardEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "the description of zxr10 card information,
                       it is a list of card entries."
               ::= { zxr10RackMIB 3 }
               
zxr10SlotBoardEntry  OBJECT-TYPE
              SYNTAX  Zxr10SlotBoardEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "A card entry containing objects that
                       descript the card information,such as:
                       zxr10SlotNo,zxr10SlotBoardType,
                       zxr10SlotSubboardNumber,zxr10SlotPortNumber,
                       zxr10SlotAvailStatus,etc."
              INDEX { zxr10RackNo,zxr10SlotBoardShelfNo,zxr10SlotNo}
              ::= { zxr10SlotBoardTable 1 }            
              
Zxr10SlotBoardEntry ::=
        SEQUENCE {  
            zxr10SlotNo
                INTEGER,
            zxr10SlotBoardType
                BoardType,
            zxr10SlotSubboardNumber
                INTEGER,
            zxr10SlotONSubboardNumber
                INTEGER,
            zxr10SlotPortNumber
                INTEGER,
            zxr10SlotMasterStatus
                MasterStatus,   
            zxr10SlotOperStatus
                OnlineStatus,
            zxr10SlotAvailStatus
                AvailStatus,
            zxr10SlotCpuUpTime
                TimeTicks,
            zxr10SlotReceiveTick
                TimeTicks,
            zxr10SlotELElable
                DisplayString,
            zxr10SlotPowerVoltageStatus
                PowerStatus,
            zxr10SlotBootVer
                DisplayString,
            zxr10SlotSystemVer
                DisplayString,
            zxr10SlotPCBVer
                DisplayString,
            zxr10SlotBoardName
                DisplayString,
            zxr10SlotReload
                INTEGER,
            zxr10SlotReloadDesc
                DisplayString,
            zxr10SlotCardPowerRatedPower
                INTEGER, 
            zxr10SlotLightTotalNum
                 INTEGER,    
            zxr10SlotTimeStamp
                TimeTicks,
            zxr10SlotBoardShelfNo
                INTEGER,
            zxr10SlotEPLD1Ver
                DisplayString,
            zxr10SlotEPLD2Ver
                DisplayString,
            zxr10SlotEPLD3Ver
                DisplayString,
            zxr10SlotEPLD4Ver
                DisplayString,
            zxr10SlotPartNumber
                DisplayString,
            zxr10SlotManufactureDate
                DisplayString,
            zxr10SlotAssemblySerialNo
                DisplayString           
        }             
    
zxr10SlotNo OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the No. of the slot where the special card is inserted."
    ::= { zxr10SlotBoardEntry 1 }
    
zxr10SlotBoardType OBJECT-TYPE
    SYNTAX  BoardType
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the type of the card."
    ::= { zxr10SlotBoardEntry 2 }

zxr10SlotSubboardNumber OBJECT-TYPE
    SYNTAX  INTEGER  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "It describes how many sub slots which can load the sub card in  the  board. "
    ::= { zxr10SlotBoardEntry 3 }

zxr10SlotONSubboardNumber OBJECT-TYPE
    SYNTAX  INTEGER  
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the number of online subboard in the special card. "
    ::= { zxr10SlotBoardEntry 4}

zxr10SlotPortNumber OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "It describes how many ports are in the card."
    ::= { zxr10SlotBoardEntry 5 }

zxr10SlotMasterStatus OBJECT-TYPE
    SYNTAX  MasterStatus 
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "as for board MPU,
                master indicates MSC,
                slave indicates SSC,
        none indicates no SC.
                for PFU, SFU and other boards, only one status : master status "
    ::= { zxr10SlotBoardEntry 6 }   

zxr10SlotOperStatus OBJECT-TYPE
    SYNTAX  OnlineStatus
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the operstatus. of the special card.
                eg ,the current card can work, we use 1,
                else if the card can't,we use 0."
    ::= { zxr10SlotBoardEntry 7 }

zxr10SlotAvailStatus OBJECT-TYPE
    SYNTAX  AvailStatus
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the status. of the special card.
                 eg ,the current card is running,
                 we use 1,else if the card is down,
                 we use 0 ."
    ::= { zxr10SlotBoardEntry 8 }

zxr10SlotCpuUpTime OBJECT-TYPE
    SYNTAX  TimeTicks
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the cpu running time of the special card. "
    ::= { zxr10SlotBoardEntry 9 }

