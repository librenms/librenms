ZXR10-RACK-MIB DEFINITIONS ::= BEGIN

   IMPORTS
       MODULE-IDENTITY, OBJECT-TYPE, OBJECT-IDENTITY, Counter32, --Gauge32,
       TimeTicks, Integer32,enterprises                      
       FROM SNMPv2-SMI
        DisplayString   --MacAddress  
       FROM SNMPv2-TC 
       zxUPN, ShelfType, BoardType, SubBoardType, SPboardType
       FROM ZXR10-PROJECT-MIB;     
    
    zxr10RackMIB MODULE-IDENTITY
        LAST-UPDATED "201609020000Z"  
        ORGANIZATION "ZTE Corporation"
        CONTACT-INFO
        "ZTE Corporation 
         Nanjing Institute of ZTE Corporation
         
         No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China
         
         Tel: +86-25-52870000"  
        DESCRIPTION
        "ZXR10 T8000 ZXR10-RACK-MIB"
        
        REVISION "201609220000Z"
            DESCRIPTION
            "modify the MAX-ACCESS of zxr10EthPhyPortNegotiation and zxr10EthPhyPortDuplex.
             add leaf node zxr10EthPhyPortSpeed."


        REVISION "201609020000Z"
            DESCRIPTION
            "import zxUPN from ZXR10-PROJECT-MIB and delete zxr10M6000-S OBJECT IDENTIFIER."

        REVISION "201406110000Z"  
            DESCRIPTION
            "add leaf node zxr10ShelfFiberLength." 
       
        REVISION "201311280000Z"  
            DESCRIPTION
            "add leaf node zxr10PeripheralFreeVolume." 
       
        REVISION "201310240000Z"  
            DESCRIPTION
            "modify the spelling mistakes of word information." 
       
        REVISION "201310230000Z"  
            DESCRIPTION
            "modify PeripheralType enumeration label peripheral to harddisk." 
       
        REVISION "201308200000Z"  
            DESCRIPTION
            "add PeripheralType enumeration label to zxr10PeripheralType SYNTAX." 
       
        REVISION "201308160000Z"  
            DESCRIPTION
            "modify the name of node zxr10T8000-16 to zxr10T8000."    
       
        REVISION "201308010000Z"  
            DESCRIPTION
            "add notsupport type to zxr10EthPhyPortMdixOrMdi SYNTAX."    
        
        REVISION "201304150000Z"  
            DESCRIPTION
            "add leaf node zxr10EthPhyPortDuplex."
       
        REVISION "200908120000Z"  
            DESCRIPTION
            "Initial revision"
                
    ::= { zxUPN 1 } 
   
       -- Nanjing Institute of Telecom,ZTE Corporation
       -- Postal: 9F. No.54 Building, 301# East ZhongShan Rd.
       -- Nan Jing, China
       -- Tel:    86-25-4826606-3821
       -- E-Mail: li.xinshuang@mail.zte.com.cn
