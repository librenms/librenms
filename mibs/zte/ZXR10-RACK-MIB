    ::= { zxr10SlotBoardEntry 15 }

zxr10SlotBoardName OBJECT-TYPE
    SYNTAX  DisplayString
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "It describes the name the  board. "
    ::= { zxr10SlotBoardEntry 16}

zxr10SlotReload OBJECT-TYPE
    SYNTAX  INTEGER
        {
            forced(1),
            graceful(2)
        }    
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "Reload the board.
         forced(1): Reload the board even if the control boards are 
            asynchronous.
         graceful(2): Reload the board after synchronization between the 
            control boards.
         Only the control boards support forced reloading and graceful 
         reloading. There is no difference for the other boards."
    ::= { zxr10SlotBoardEntry 17 }

zxr10SlotReloadDesc OBJECT-TYPE
    SYNTAX  DisplayString
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "the reload description of the board. Now we don't use it."
    ::= { zxr10SlotBoardEntry 18 }

zxr10SlotCardPowerRatedPower OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "It describes the RatedPower of the board. "
    ::= { zxr10SlotBoardEntry 19 }

zxr10SlotLightTotalNum OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the total number of lights in the board. Now we don't use it."
    ::= { zxr10SlotBoardEntry 20 } 

