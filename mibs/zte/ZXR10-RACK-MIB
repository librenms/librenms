--zxr10EthPhyPortTable
zxr10EthPhyPortTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Zxr10EthPhyPortEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "the description of zxr10 ethernet physical port information,
                       it is a list of EthPhyPort entries."
               ::= { zxr10RackMIB 16 }
               
zxr10EthPhyPortEntry  OBJECT-TYPE
              SYNTAX  Zxr10EthPhyPortEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "A EthPhyPort entry containing objects that
                       descript the Ethernet physical port information,such as:
                       zxr10EthPhyPortNegotiation,zxr10EthPhyPortMdixOrMdi,zxr10EthPhyPortInflowCtrl,etc."
              INDEX { zxr10EthPhyPortName}
              ::= { zxr10EthPhyPortTable 1 }               
              
Zxr10EthPhyPortEntry ::=
        SEQUENCE {
            zxr10EthPhyPortName
                DisplayString,
            zxr10EthPhyPortIndex
                INTEGER,
            zxr10EthPhyPortFrameType
                INTEGER,
            zxr10EthPhyPortNegotiation
                INTEGER,
            zxr10EthPhyPortJumbo
                INTEGER,
            zxr10EthPhyPortMdixOrMdi
                INTEGER,   
            zxr10EthPhyPortIpg
                Integer32,
            zxr10EthPhyPortLoopback
                INTEGER,
            zxr10EthPhyPortEOProperty
                INTEGER,
            zxr10EthPhyPortInflowCtrl
                INTEGER,
            zxr10EthPhyPortOutflowCtrl
                INTEGER,
            zxr10EthPhyPortDuplex
                INTEGER,
            zxr10EthPhyPortSpeed
                INTEGER                                  
        }             
    
zxr10EthPhyPortName OBJECT-TYPE
    SYNTAX  DisplayString
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        " the name of the ether physical port."
    ::= { zxr10EthPhyPortEntry 1 }

zxr10EthPhyPortIndex OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        " the ifindex of the ether physical port"
    ::= { zxr10EthPhyPortEntry 2}

zxr10EthPhyPortFrameType OBJECT-TYPE
    SYNTAX  INTEGER{ethernetII(1)}
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        " the frame type of the ether physical port"
    ::= { zxr10EthPhyPortEntry 3 }

zxr10EthPhyPortNegotiation OBJECT-TYPE
    SYNTAX  INTEGER{auto(1),noAuto(0),unknown(2)} 
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "the negotiation property of the ether physical port."
    ::= { zxr10EthPhyPortEntry 4 }  
    
zxr10EthPhyPortJumbo OBJECT-TYPE
    SYNTAX  INTEGER{enable(0), disable(1)}
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the jumbo property of the ether physical port."
    ::= { zxr10EthPhyPortEntry 5}

zxr10EthPhyPortMdixOrMdi OBJECT-TYPE
    SYNTAX  INTEGER{auto(0),mdix(1),mdi(2),reserved(3),notsupport(4)}
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the mid mode of the ether physical port."
    ::= { zxr10EthPhyPortEntry 6}  
    
zxr10EthPhyPortIpg OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the ipg property of the ether physical port."
    ::= { zxr10EthPhyPortEntry 7}

zxr10EthPhyPortLoopback OBJECT-TYPE
    SYNTAX  INTEGER{noLoopback(0), loopbackInternal(1), loopbackLine(2), loopbackExternal(3)}
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the loopback property of the ether physical port."
    ::= { zxr10EthPhyPortEntry 8}

zxr10EthPhyPortEOProperty OBJECT-TYPE
    SYNTAX  INTEGER{optical(0), electrical(1), multiplex(2)}
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the electric or opitical property of the ether physical port."
    ::= { zxr10EthPhyPortEntry 9} 
    
zxr10EthPhyPortInflowCtrl OBJECT-TYPE
    SYNTAX  INTEGER{nosupport(0), on(1), off(2)}
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the inflow control of the ether physical port."
    ::= { zxr10EthPhyPortEntry 10}

zxr10EthPhyPortOutflowCtrl OBJECT-TYPE
    SYNTAX  INTEGER{nosupport(0), on(1), off(2)}
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the outflow control of the ether physical port."
    ::= { zxr10EthPhyPortEntry 11} 

zxr10EthPhyPortDuplex OBJECT-TYPE
    SYNTAX  INTEGER{full(1),half(2),unknown(3)}
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "the duplex type of the ether physical port."
    ::= { zxr10EthPhyPortEntry 12}  

zxr10EthPhyPortSpeed OBJECT-TYPE
    SYNTAX  INTEGER{speed-10M(1),speed-100M(2),speed-1G(3),speed-10G(4)}
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "the speed mode of the ether physical port."
    ::= { zxr10EthPhyPortEntry 13}  

    
