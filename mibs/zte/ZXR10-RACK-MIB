       -- The MIB module which descript the zxr10rack mib
       -- object which defined in enterprise's ZTE(3902)
 --  textual conventions


      AvailStatus ::= INTEGER
                      {
                        unavailable(0),
                        available(1)
                      }

    
      OnlineStatus ::= INTEGER
                      {
                        down(0),
                        up(1),
                        offline(2)

                      }                  
       PhyStatus ::= INTEGER
                      {
                        down(0),
                        up(1),
                        offline(2)
                      }  
       RunStatus ::= INTEGER
                      {
                        down(0),
                        up(1),
                        offline(2)
                      }     
      PowerStatus ::= INTEGER
                      {
                        off(0),
                        on(1)
                        
                      }               
       PeripheralType::= INTEGER
                      {
                        flash(128),
                        harddisk(131),
                        cfcard(156),
                        sdcard(157),
                        usb1(158),
                        usb2(159)
                      } 
        PeripheralRemoved::= INTEGER
                      {
                        cannot(0),
                        can(1)
                        
                      } 
        PeripheralStaus::= INTEGER
                      {
                        down(0),
                        up(1)
                      }                  
                
      PortProperty ::= INTEGER
                      {
                        photo(0),
                        electricity(1)                       
                      }                     

      MasterStatus ::= INTEGER
                       {
                         master(0),
                         slave(1),
                         none(2)                         
                       }
                       
      ShelfAttrib ::= INTEGER
                       {
                         cfc(0),
                         lcc(1)
                       }     

       PortType ::= INTEGER
                    {
                      port-fei(2),
                      port-gei(131),
                      port-xgei(132),
                      port-xgeiw(133),                      
                      port-cgei(139),
                      port-xlgei(140),                    
                      port-cgei-sl(141),                      
                      port-pos3(260),
                      port-pos12(261),
                      port-pos48(262),                    
