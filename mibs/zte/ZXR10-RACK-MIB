    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the transmit speed of this port, unit is Mbps. "
    ::= { zxr10PortEntry 6 }

zxr10PortPhyIfMTU OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the mtu of the 2 layer transmitting ability, unit is BYTES. "
    ::= { zxr10PortEntry 7 }

