--zxr10SlotSubCardTable
zxr10SlotSubCardTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Zxr10SlotSubCardEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "the description of zxr10 subcard information,
                       it is a list of subcard entries."
               ::= { zxr10RackMIB 4 }
               
zxr10SlotSubCardEntry  OBJECT-TYPE
              SYNTAX  Zxr10SlotSubCardEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "A subcard entry containing objects that
                       descript the subcard information,such as:
                       zxr10SlotSubCardNo,zxr10SlotSubCardType,
                       zxr10SlotSubCardPortNumber,zxr10SlotSubCardELElable.etc."
              INDEX { zxr10RackNo,zxr10SlotSubCardShelfNo,zxr10SlotSubCardSlotNo,zxr10SlotSubCardNo}
              ::= { zxr10SlotSubCardTable 1 }              
              
Zxr10SlotSubCardEntry ::=
        SEQUENCE {
            zxr10SlotSubCardNo
                INTEGER,
            zxr10SlotSubCardType
                SubBoardType,
            zxr10SlotSubCardPortNumber
                INTEGER,
            zxr10SlotSubCardCpuUpTime
                TimeTicks,
            zxr10SlotSubCardELElable
                DisplayString,   
            zxr10SlotSubCardAvailStatus
                AvailStatus,
            zxr10SlotSubCardPCBVer
                DisplayString,
            zxr10SlotSubCardDescription
                DisplayString,
            zxr10SlotSubCardPowerVoltageStatus
                PowerStatus,
            zxr10SlotSubCardOperStatus
                OnlineStatus,
            zxr10SlotSubCardTimeStamp
                TimeTicks,
            zxr10SlotSubCardShelfNo
                INTEGER,
            zxr10SlotSubCardSlotNo
                INTEGER,
            zxr10SlotSubCardEPLD1Ver
                DisplayString,
            zxr10SlotSubCardEPLD2Ver
                DisplayString,
            zxr10SlotSubCardEPLD3Ver
                DisplayString,
            zxr10SlotSubCardEPLD4Ver
                DisplayString,
            zxr10SlotSubCardPartNumber
                DisplayString,
            zxr10SlotSubCardManufactureDate
                DisplayString               
        }             
    
zxr10SlotSubCardNo OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "the No. of the subcard where the special subcard is inserted. "
    ::= { zxr10SlotSubCardEntry 1 }

