              SYNTAX  Zxr10PeripheralEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "A peripheral entry containing objects that
                       descript the peripheral information,such as:
                       zxr10MPUBoardType,zxr10PeripheralNo,etc."
              INDEX { zxr10RackNo,zxr10PeripheralShelfNo,zxr10PeripheralSlotNo,zxr10PeripheralNo,zxr10PeripheralType}
              ::= { zxr10PeripheralTable 1 }               
              
Zxr10PeripheralEntry ::=
        SEQUENCE {
            zxr10MPUBoardType
                BoardType,
            zxr10PeripheralNo
                INTEGER,
            zxr10PeripheralType
                PeripheralType,
            zxr10PeripheralDescription
                DisplayString,
            zxr10PeripheralTotalVolume
                Counter32,   
            zxr10PeripheralUsedVolume
                Counter32,
            zxr10PeripheralPartitionNum
                INTEGER,
            zxr10PeripheralCanbeRemoved
                PeripheralRemoved,
            zxr10PeripheralStartupTime
                TimeTicks,
            zxr10PeripheralStatus
                PeripheralStaus,
            zxr10PeripheralShelfNo
                INTEGER,
            zxr10PeripheralSlotNo
                INTEGER,
            zxr10PeripheralFreeVolume
                Counter32               
        }             
    
