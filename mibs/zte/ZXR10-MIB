    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "the cpu utility described by percent( 5 seconds)."
    ::= { zxr10SystemUnitEntry 6}
 
 zxr10SystemCpuUtility30s   OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "the cpu utility described by percent( 30 seconds)."
    ::= { zxr10SystemUnitEntry 7}
    
 zxr10SystemPeakCpuUtility   OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "the cpu peak utility described by percent.(measured in 30 seconds)"
    ::= { zxr10SystemUnitEntry 8}
    
