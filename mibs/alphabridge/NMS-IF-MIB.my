              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Interface Storm Control Broadcast AutoResume, range is 60s-36000s"
              ::= { ifStormControlEntry 6 }

          ifStormControlMulticastAction OBJECT-TYPE
              SYNTAX  INTEGER{
