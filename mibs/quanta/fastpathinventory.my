                    enable(1),
                    disable(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Initiates STK copy from management unit to all other management capable units in
            the chassis."
        ::= { agentInventoryChassisGroup 1 }
    
    agentInventoryChassisReload OBJECT-TYPE
        SYNTAX      INTEGER {
