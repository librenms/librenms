        ::= { agentInventoryChassisGroup 2 }
        
    agentInventoryChassisMaxUnitNumber OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicates the maximum allowed Unit Number configurable on the chassis."
