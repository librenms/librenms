streams OBJECT-IDENTITY
	STATUS current
	DESCRIPTION
		"No description"
	::= { configuration 4 }

streamTable OBJECT-TYPE
	SYNTAX SEQUENCE OF StreamEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Stream (de)activation table"
	::= { streams 1 }

streamEntry OBJECT-TYPE
	SYNTAX StreamEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Stream (de)activation row"
	INDEX { streamId }
	::= { streamTable 1 }

StreamEntry ::=
	SEQUENCE { 
		streamId
			Integer32,
		streamStatus
			INTEGER,
		streamDestinationIp
			DisplayString,
		streamIpPort
			Integer32
	}

streamId OBJECT-TYPE
	SYNTAX Integer32 (0..24)
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Stream Index"
	::= { streamEntry 1 }

streamStatus OBJECT-TYPE
	SYNTAX INTEGER { notconfigured(-1), inactive(0), active(1) }
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"-1: Not present
		0: Inactive
		1: Active"
	::= { streamEntry 2 }
	
streamDestinationIp OBJECT-TYPE
	SYNTAX DisplayString(SIZE(0..255))
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"Stream Destination IP"
	::= { streamEntry 3 }

streamIpPort OBJECT-TYPE
	SYNTAX Integer32 (0..65535)
