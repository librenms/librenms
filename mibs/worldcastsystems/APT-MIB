-- IP
ipTable OBJECT-TYPE
	SYNTAX SEQUENCE OF IpEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"IP table (0..7) 0 -> ETH0 etc."
	::= { stMisc 5 }

ipEntry OBJECT-TYPE
	SYNTAX IpEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"IP row"
	INDEX { ipEthId }
	::= { ipTable 1 }

IpEntry ::=
	SEQUENCE { 
		ipEthId
			Integer32,
		ipEthEnabled
			INTEGER,
		ipEthAddress
			DisplayString,
		ipEthNetmask
			DisplayString,
		ipEthGateway
			DisplayString,
        ipMacAddress
            DisplayString
	}

ipEthId OBJECT-TYPE
	SYNTAX Integer32 (0..7)
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"IP Index"
	::= { ipEntry 1 }

ipEthEnabled OBJECT-TYPE
	SYNTAX INTEGER { false(0), true(1) }
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"0: Inactive
		1: Active"
	::= { ipEntry 2 }

ipEthAddress OBJECT-TYPE
	SYNTAX DisplayString(SIZE(0..255))
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"IP Address"
	::= { ipEntry 3 }

ipEthNetmask OBJECT-TYPE
	SYNTAX DisplayString(SIZE(0..255))
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"IP Netmask"
	::= { ipEntry 4 }

ipEthGateway OBJECT-TYPE
	SYNTAX DisplayString(SIZE(0..255))
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"IP Gateway"
	::= { ipEntry 5 }
	
ipMacAddress OBJECT-TYPE
    SYNTAX DisplayString(SIZE(0..255))
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "IP Mac Address"
    ::= { ipEntry 6 }

-- Distant redirections
distantRedirectionTable OBJECT-TYPE
	SYNTAX SEQUENCE OF DistantRedirectionEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Distant redirections table"
	::= { stMisc 6 }

distantRedirectionEntry OBJECT-TYPE
	SYNTAX DistantRedirectionEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Distant redirection row"
	INDEX { distantRedirectionId }
	::= { distantRedirectionTable 1 }

DistantRedirectionEntry ::=
	SEQUENCE { 
		distantRedirectionId
			Integer32,
		distantRedirectionDescription
			DisplayString,
		distantRedirectionInterface
			DisplayString,
		distantRedirectionPortNumber
			Integer32,
		distantRedirectionToAddress
			DisplayString,
		distantRedirectionToPortNumber
			Integer32,
		distantRedirectionProtocol
			INTEGER,
		distantRedirectionEnabled
			INTEGER
	}

distantRedirectionId OBJECT-TYPE
	SYNTAX Integer32(0..32)
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Distant Redirection Index"
	::= { distantRedirectionEntry 1 }

distantRedirectionDescription OBJECT-TYPE
	SYNTAX DisplayString(SIZE(0..255))
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Description / service of the redirection (HTTP, HTTPS...)"
	::= { distantRedirectionEntry 2 }

distantRedirectionInterface OBJECT-TYPE
	SYNTAX DisplayString(SIZE(0..31))
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Network interface of the incoming packet to redirect"
	::= { distantRedirectionEntry 3 }

distantRedirectionPortNumber OBJECT-TYPE
	SYNTAX Integer32(0..65535)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Port of the incoming packet to redirect"
	::= { distantRedirectionEntry 4 }

distantRedirectionToAddress OBJECT-TYPE
	SYNTAX DisplayString(SIZE(0..255))
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"IP address where filtered incoming packet have to be redirected"
	::= { distantRedirectionEntry 5 }

distantRedirectionToPortNumber OBJECT-TYPE
	SYNTAX Integer32(0..65535)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Port where filtered incoming packet have to be redirected"
	::= { distantRedirectionEntry 6 }

distantRedirectionProtocol OBJECT-TYPE
	SYNTAX INTEGER { tcp(0), udp(1) }
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"0: TCP
		1: UDP"
	::= { distantRedirectionEntry 7 }

distantRedirectionEnabled OBJECT-TYPE
	SYNTAX INTEGER { disabled(0), enabled(1) }
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"0: Disabled
		1: Enabled"
	::= { distantRedirectionEntry 8 }
-- Firewall
firewallTable OBJECT-TYPE
	SYNTAX SEQUENCE OF FirewallEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Firewall rules table"
	::= { stMisc 7 }

firewallEntry OBJECT-TYPE
	SYNTAX FirewallEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Firewall rule row"
	INDEX { firewallRuleId }
	::= { firewallTable 1 }

FirewallEntry ::=
	SEQUENCE { 
		firewallRuleId
			Integer32,
		firewallDescription
			DisplayString,
		firewallNetworkInterface
			DisplayString,
		firewallPortNumber
			Integer32,
		firewallProtocol
			INTEGER,
		firewallEnabled
			INTEGER
	}

firewallRuleId OBJECT-TYPE
	SYNTAX Integer32(0..48)
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Rule Index"
	::= { firewallEntry 1 }

firewallDescription OBJECT-TYPE
	SYNTAX DisplayString(SIZE(0..255))
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Description / service of the rule (HTTP, HTTPS...)"
	::= { firewallEntry 2 }


firewallNetworkInterface OBJECT-TYPE
	SYNTAX DisplayString(SIZE(0..16))
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Network interface concerned (eth0, eth1...)"
	::= { firewallEntry 3 }

firewallPortNumber OBJECT-TYPE
	SYNTAX Integer32(0..65535)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Port of filtered service"
	::= { firewallEntry 4 }

firewallProtocol OBJECT-TYPE
	SYNTAX INTEGER { tcp(0), udp(1) }
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"0: TCP
		1: UDP"
	::= { firewallEntry 5 }

firewallEnabled OBJECT-TYPE
	SYNTAX INTEGER { disabled(0), enabled(1) }
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"0: Disable filtering
		1: Enable filtering"
	::= { firewallEntry 6 }
	
ioCardType OBJECT-TYPE
	SYNTAX INTEGER { noneOrUnknown(-1), standard(0), analogMpx(1) }
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
