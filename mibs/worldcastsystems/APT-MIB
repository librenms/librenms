-- License
licenseTable OBJECT-TYPE
    SYNTAX SEQUENCE OF LicenseEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "license table"
    ::= { misc 9 }

licenseEntry OBJECT-TYPE
    SYNTAX LicenseEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "License row"
    INDEX { licenseIndex }
    ::= { licenseTable 1 }

LicenseEntry ::=
    SEQUENCE { 
        licenseIndex
            Integer32,
        licenseName
            DisplayString,
        licenseActive
            INTEGER,
        licenseExpireDate
            Integer32
    }

licenseIndex OBJECT-TYPE
    SYNTAX Integer32 (0..255)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "IP Index"
    ::= { licenseEntry 1 }

licenseName OBJECT-TYPE
    SYNTAX DisplayString(SIZE(0..255))
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "License name"
    ::= { licenseEntry 2 }
    
licenseActive OBJECT-TYPE
    SYNTAX INTEGER { false(0), true(1) }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "0: Inactive
        1: Active"
    ::= { licenseEntry 3 }
    
licenseExpireDate OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The expiration date"
    ::= { licenseEntry 4 }

-- End license
	
