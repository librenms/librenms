APT-MIB DEFINITIONS ::= BEGIN

IMPORTS
	OBJECT-GROUP, NOTIFICATION-GROUP
		FROM SNMPv2-CONF
	sysDescr, sysName, sysLocation
		FROM SNMPv2-MIB
	DisplayString, DateAndTime
		FROM SNMPv2-TC
	enterprises, Integer32, Gauge32, OBJECT-TYPE, MODULE-IDENTITY, OBJECT-IDENTITY, NOTIFICATION-TYPE
		FROM SNMPv2-SMI;

nextGen MODULE-IDENTITY
	LAST-UPDATED "202012011025Z"
	ORGANIZATION
		"WorldCastSystems"
	CONTACT-INFO
		"support@worldcastsystems.com"
	DESCRIPTION
		"MIB for WorldCast Systems NextGen units based on the Phoenix platform"

    REVISION "202012011025Z"
    DESCRIPTION
        "Added reset streams performance"

    REVISION "202008241025Z"
    DESCRIPTION
        "Added Rx playout fine nudge"
        
    REVISION "202001201206Z"
    DESCRIPTION
        "Added ntp status"
        
	REVISION "202001141245Z"
    DESCRIPTION
        "Added SFN alarms and traps"
        
    REVISION "202001061610Z"
    DESCRIPTION
        "Added monoSourceSelection and monoOutputModeSelection"
        
    REVISION "201911291108Z"
    DESCRIPTION
        "Added AGC Get"
        
	REVISION "201907041126Z"
	DESCRIPTION
		"Added unitClockMode Set and Get"

	REVISION "201903290857Z"
	DESCRIPTION
		"Added additional system information {MAC address, license and Hardware alarms}"

	REVISION "201810151144Z"
	DESCRIPTION
		"MIB File for System Release 3.0.2 Added SNMP Set for stream Address and Port"

	REVISION "201708011442Z"
	DESCRIPTION
		"MIB File for System Release 3.0."
	
	REVISION "201612151645Z"
	DESCRIPTION
		"MIB File for System Release 2.2. Added ScriptEasy state variable"
	
	REVISION "201604280845Z"
	DESCRIPTION
		"Update MIB name. Add loss of IP connection"

	REVISION "201603251238Z"
	DESCRIPTION
		"Add relay invert controls"
		
	REVISION "201602100746Z"
	DESCRIPTION
		"Add NTP Sync support"
	
	REVISION "201507030931Z"
	DESCRIPTION
		"Rename from APT_OSLO_AOIP_MIB.mib to APT_MIB.mib"
	
	REVISION "201504031427Z"
	DESCRIPTION
		"Add distant redirection"

	REVISION "201402191518Z"
	DESCRIPTION
		"MIB File for System Release 2.0"
	::= { aptltd 10 }

aptltd OBJECT-IDENTITY
	STATUS current
	DESCRIPTION
		"No description"
	::= { enterprises 22425 }

commonEventBindings OBJECT-IDENTITY
	STATUS current
	DESCRIPTION
		"No description"
	::= { nextGen 3 }

eventTimestamp OBJECT-TYPE
	SYNTAX DateAndTime
	MAX-ACCESS accessible-for-notify
	STATUS current
	DESCRIPTION
		"YYYY/MM/DD HH:MM:SS"
	::= { commonEventBindings 1 }

eventsParams OBJECT-IDENTITY
	STATUS current
	DESCRIPTION 
		"Params for each trap: enable and priority"
	::= { nextGen 7 }

