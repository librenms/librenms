    SYNTAX SEQUENCE OF AudioAgcConfigurationEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Audio AGC Configuration table"
    ::= { audio 26 }

audioAgcConfigurationEntry OBJECT-TYPE
    SYNTAX AudioAgcConfigurationEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Audio AGC Configuration row"
    INDEX { channelIdAgc }
    ::= { audioAgcConfigurationTable 1 }

AudioAgcConfigurationEntry ::=
    SEQUENCE { 
        channelIdAgc
            Integer32,
        agcEnabled
            INTEGER,
        agcDrive
            INTEGER,
        agcSpeed
            Integer32,
        agcReference
            Integer32,
        limiterEnabled
            INTEGER,
        limiterDrive
            Integer32
    }

channelIdAgc OBJECT-TYPE
    SYNTAX Integer32 (0..3)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Audio AGC Configuration Index. 0=A, 1=B, 2=C and 3=D"
    ::= { audioAgcConfigurationEntry 1 }

agcEnabled OBJECT-TYPE
    SYNTAX INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "0: Disabled
        1: Enabled"
    ::= { audioAgcConfigurationEntry 2 }

agcDrive OBJECT-TYPE
    SYNTAX INTEGER
        {
            dB0(0),
            dB1p5(1),
            dB3(2),
            dB4p5(3),
            dB6(4),
            dB7p5(5),
            dB9(6),
            dB10p5(7),
            dB12(8)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "AGC drive
        0: 0dB
        1: 1_5dB
        2: 3dB
        3: 4_5dB
        4: 6dB,
        5: 7_5dB,
        6: 9dB,
        7: 10_5dB,
        8: 12dB"
    ::= { audioAgcConfigurationEntry 3 }

agcSpeed OBJECT-TYPE
    SYNTAX Integer32 (1..8)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "AGC Speed (Attack / Release characteristic)"
    ::= { audioAgcConfigurationEntry 4 }

agcReference OBJECT-TYPE
    SYNTAX Integer32 (-18..0)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "AGC Reference Level (dBFS)"
    ::= { audioAgcConfigurationEntry 5 }

limiterEnabled OBJECT-TYPE
    SYNTAX INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "AGC Limiter Enabled Status
        0: Disabled
        1: Enabled"
    ::= { audioAgcConfigurationEntry 6 }
    
limiterDrive OBJECT-TYPE
    SYNTAX Integer32 (1..8)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "AGC Limiter Drive Level"
    ::= { audioAgcConfigurationEntry 7}

