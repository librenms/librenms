			ipAddressEthernet0, ipAddressEthernet1, lossOfPhysicalEth0, 
			lossOfPhysicalEth1, ddnsEth0Failure, ddnsEth1Failure,
			eventDdnsEth0FailurePriority, eventDdnsEth0FailureEnable, 
			eventDdnsEth1FailurePriority, eventDdnsEth1FailureEnable,
			enableSilenceDetectChannelC, silenceDetectThresholdChannelC, 
			enableSilenceDetectChannelD, silenceDetectThresholdChannelD, enableSilenceDetectChannelA, silenceDetectThresholdChannelA, 		
			enableSilenceDetectChannelB, silenceDetectThresholdChannelB, audioAlarmTimeout
		}
	STATUS obsolete
	DESCRIPTION
		"All obsolete objects"
	::= { groups 2 }

	eventGroup NOTIFICATION-GROUP
		NOTIFICATIONS {
			eventIpTXError, eventIpRXError, eventAudioMismatch, eventPsu1Failure, eventPsu2Failure, 
			eventSyncFailed, eventSilDetChanA, eventSilDetChanB, 
			eventSilDetChanC, eventSilDetChanD, eventLossDigitalInputAB, eventLossDigitalReference, eventCustomAlarm, 
			eventLossDigitalInputCD, eventIoBoardRemoved, eventNtpSyncFault, eventLossOfIpConnection, eventIoBoardTypeChanged, 
			eventMinimumJitterBufferAlarm, eventVpnConnectionAlarm,eventExt10MhzClockAlarm, eventExtPPSClockAlarm, eventExtClockModuleMissingAlarm,
			eventEquipmentOn, eventHeartBeat, eventConfigChanged, eventEquipmentFault, eventLossOfPhysicalEth, eventDdnsEthFailure
		}
	STATUS current
	DESCRIPTION
		"All current events"
	::= { groups 3 }

	eventObsoleteGroup NOTIFICATION-GROUP
		NOTIFICATIONS {
			eventLossOfPhysicalEth1, eventLossOfPhysicalEth0, 
			eventDdnsEth0Failure, eventDdnsEth1Failure
		}
	STATUS obsolete
	DESCRIPTION
		"All obsolete events"
	::= { groups 4 }


END
