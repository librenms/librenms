streamsrouteType OBJECT-TYPE
	SYNTAX INTEGER
		{
			audioRtp(0),
			auxDataUdp(1),
			gpioUdp(2),
			ipForwardUdp(3),
			mediaForwardRtp(4)
		}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"
		                    Route type:
		                    0: Audio (RTP)
		                    1: Aux Data (UDP)
		                    2: GPIO (UDP)
		                    3: IP Forward (UDP)
		                    4: Media Forward (RTP)
		                  "
	::= { streamsEntry 4 }

streamsencodeSlot OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"
		                    Encode audio codec id (see also : phoenixbridge.audiocodec.id)
		                    -1 : None
		                    0  : Tx Audio codec (Left for Dual mono)
		                    1  : Tx Audio codec right (Dual mono only)
		                  "
	::= { streamsEntry 5 }

