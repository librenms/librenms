    SYNTAX INTEGER
        {
            duplex(0),
            encodeOnly(1),
            decodeOnly(2)
        }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Get system duplex mode
        0: duplex
        1: encode only
        2: decode only"
    ::= { audio 22 }
    

lowLatencyMode OBJECT-TYPE
    SYNTAX INTEGER
        {
            off(0),
            on(1)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Get/Set Low Latency Mode
        0: off
        1: on"
    ::= { audio 23 }
    
syncAlarmFailTime OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Get/Set decoder sync. alarm fail time"
    ::= { audio 24 }

unitClockMode OBJECT-TYPE
    SYNTAX INTEGER
        {
            asymmetric(0),
            master(1),
            slave(2),
            systemTimeSync(3)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Get/Set Unit Clock Mode
        0: Asymmetric
        1: Master
        2: Slave
        4: System Time Sync"
    ::= { audio 25 }

