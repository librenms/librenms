-- silence configuration --
audioAlarmConfigurationTable OBJECT-TYPE
	SYNTAX SEQUENCE OF AudioAlarmConfigurationEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Audio Silent Alarm Configuration table"
	::= { audio 21 }

audioAlarmConfigurationEntry OBJECT-TYPE
	SYNTAX AudioAlarmConfigurationEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Audio Silent Alarm Configuration row"
	INDEX { channelId }
	::= { audioAlarmConfigurationTable 1 }

AudioAlarmConfigurationEntry ::=
	SEQUENCE { 
		channelId
			Integer32,
		alarmEnabled
			INTEGER,
		alarmThreshold
			INTEGER,
		alarmFailTime
			Integer32,
		alarmRecoverTime
			Integer32
	}

channelId OBJECT-TYPE
	SYNTAX Integer32 (0..3)
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Audio Silent Alarm Configuration Index. 0=A, 1=B, 2=C and 3=D"
	::= { audioAlarmConfigurationEntry 1 }

alarmEnabled OBJECT-TYPE
	SYNTAX INTEGER { disabled(0), enabled(1) }
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"0: Disabled
		1: Enabled"
	::= { audioAlarmConfigurationEntry 2 }

alarmThreshold OBJECT-TYPE
	SYNTAX INTEGER
		{
			minus3(0),
			minus6(1),
			minus9(2),
			minus12(3),
			minus15(4),
			minus18(5),
			minus21(6),
			minus24(7),
			minus27(8),
			minus30(9),
			minus33(10),
			minus36(11),
			minus39(12),
			minus42(13)
		}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"Silence detect alarm - Channel B threshold
		0: -3
		1: -6
		2: -9
		3: -12
		4: -15,
		5: -18,
		6: -21,
		7: -24,
		8: -27,
		9: -30,
		10: -33,
		11: -36,
		12: -39,
		13: -42"
	::= { audioAlarmConfigurationEntry 3 }

alarmFailTime OBJECT-TYPE
	SYNTAX Integer32 (1..65535)
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"Number of seconds before rising the alarm"
	::= { audioAlarmConfigurationEntry 4 }


alarmRecoverTime OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"Number of seconds before turning off the alarm"
	::= { audioAlarmConfigurationEntry 5 }
	
