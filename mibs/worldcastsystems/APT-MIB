streamsTable OBJECT-TYPE
	SYNTAX SEQUENCE OF StreamsEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"No description"
	::= { performance 5 }

streamsEntry OBJECT-TYPE
	SYNTAX StreamsEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"An entry of the streamsTable table"
	INDEX { streamsid }
	::= { streamsTable 1 }

StreamsEntry ::=
	SEQUENCE {
		streamsid
			Integer32,
		streamsstreamEnabled
			Integer32,
		streamsstreamName
			DisplayString,
		streamsrouteType
			INTEGER,
		streamsencodeSlot
			Integer32,
		streamsdecodeSlot
			Integer32,
		streamscastingMode
			INTEGER,
		streamsethernetInterface
			Integer32,
		streamssourceIPAddress
			DisplayString,
		streamssourceIPort
			Integer32,
		streamsdestinationIPAddress
			DisplayString,
		streamsipPort
			Integer32,
		streamssureStreamLevel
			INTEGER,
		streamstxPacketCount
			Integer32,
		streamsrxPacketCount
			Integer32,
		streamstxkbCount
			Integer32,
		streamsrxkbCount
			Integer32,
		streamstxBitrate
			Integer32,
		streamsrxBitrate
			Integer32,
		streamsresequencedPacketCount
			Integer32,
		streamsdroppedPacketCount
			Integer32,
		streamslossOfConnectionErrors
			Integer32,
		streamsduplicatePacketCount
			Integer32,
		streamsreceiveBufferLevel
			Integer32,
		streamsreceiveBufferThreshold
			Integer32,
		streamsrxLatency
			Integer32,
		streamstxPacketSize
			Integer32,
		streamsrxPacketSize
			Integer32,
		streamsfloodedBufferCount
			Integer32,
		streamsauxRouteType
			INTEGER,
		streamsrxForwardingChannel
			INTEGER,
		streamstxForwardingChannel
			INTEGER,
		streamsextPort
			Integer32,
		streamsupnpEnable
			Integer32,
		streamsrxPacketInterval
			Integer32,
		streamstxPacketInterval
			Integer32,
		streamslocAlarmStatus
			INTEGER,
		streamstxInactiveAlarmStatus
			INTEGER,
		streamsrxInactiveAlarmStatus
			INTEGER,
		streamstxContentSyncEnabled
            INTEGER,
        streamstxPlayoutOffset
            Integer32,
        streamsrxBufferMode
            INTEGER,
        streamsrxPlayoutOffset
            Integer32,
        streamsrxPlayoutNudge
            Integer32,
        streamsrxLinkLatency
            Integer32,
        streamsrxBufferHeadroom
            Integer32,
        streamsrxLinkJitter
            Integer32,
        streamsMinimumJitterBufferAlarm
            INTEGER,    
        streamsMinJitterBufferMicroS    
            Integer32,
        streamsPacketsOrderingEnable
            INTEGER,
        streamsWanProtection
            INTEGER,
        streamsSsmMode
            INTEGER,
        streamsSsmGroup
            INTEGER,
        streamsrxPlayoutFineNudgeNanoS
            Integer32
	}

