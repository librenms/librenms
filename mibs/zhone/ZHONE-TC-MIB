		ZhoneAlarmSeverity ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"This type defines the severity of the alarm raised/cleared."
			SYNTAX INTEGER
				{
				critical(1),
				major(2),
				minor(3),
				warning(4)
				}

		ZhoneEnabledFlag ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"A field which is either enabled or disabled."
			SYNTAX INTEGER
				{
				enabled(1),
				disabled(2)
				}

		ZhoneAdminState ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"This represents the admin state with the values - UP, DOWN and TESTING."
			SYNTAX INTEGER
				{
				up(1),
				down(2),
				testing(3)
				}

	
