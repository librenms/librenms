    -- the sehNat groups
    -- sehNat is PS19, sehNatDrop is Heinz extension
    -- This group specifies NAT gateway parameters of SEH.
        
    natLocal OBJECT-TYPE
        SYNTAX  IpAddress
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This parameter specifies the local IP Address of the NAT device"
        ::= { sehNat 1 }
    
