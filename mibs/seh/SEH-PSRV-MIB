        SYNTAX  DisplayString (SIZE (0..64))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Current myUTN-800 dispay error value(s)"
        ::= { sehUtn 12 }

    utnBeepPwr OBJECT-TYPE
        SYNTAX  INTEGER {off(0),on(1)}
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "myTUN-800 buzzer mode, beep if only one supply works"
        ::= { sehUtn 13 }
    
    utnBeepSDc OBJECT-TYPE
        SYNTAX  INTEGER {off(0),on(1)}
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "myTUN-800 buzzer mode, beep if SDCard doesn't work"
        ::= { sehUtn 14 }

    utnHid OBJECT-TYPE
        SYNTAX  INTEGER {off(0),on(1)}
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "myTUN accept (on) or discard (off) HID devices"
        ::= { sehUtn 15 }


