    MailConnState ::= INTEGER {
    -- p3NONE(0), just for completeness, never reached
    p3INIT(1),
    p3DNSDONE(2),
    p3CONNECT(3),
    p3CONTACT(4),
    p3LOGIN(5),
    p3WORK(6),
    p3CLOSED(7),
    -- failures are following:
    p3APOPLOGINFAIL(8),
    p3APOPMD5FAIL(9),
    p3CONNECTFAIL(10),
    p3DATAFAIL(11),
    p3DELFAIL(12),
    p3DNSFAIL(13),
    p3LISTFAIL(14),
    p3LOGINFAIL(15),
    p3MEMFAIL(16),
    p3MFROMFAIL(17),
    p3AUTHFAIL(18),
    p3POPFAIL(19),
    p3PWDFAIL(20),
    p3RCPTFAIL(21),
    p3RETRFAIL(22),
    p3SCANFAIL(23),
    p3SENTFAIL(24),
    p3STATFAIL(25),
    p3STATSCANFAIL(26),
    p3USERFAIL(27)
    }
    
