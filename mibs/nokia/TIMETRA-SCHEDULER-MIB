    INDEX       {
        tTimeRangeName,
        tAbsoluteTimeRangeStartYear,
        tAbsoluteTimeRangeStartMonth,
        tAbsoluteTimeRangeStartDay,
        tAbsoluteTimeRangeStartHour,
        tAbsoluteTimeRangeStartMinute
    }
    ::= { tmnxAbsoluteTimeRangeParmsTable 1 }

