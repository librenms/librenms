      asamifxTblifHCInUcastPkts            INTEGER,
      asamifxTblifHCInMulticastPkts        INTEGER,
      asamifxTblifHCInBroadcastPkts        INTEGER,
      asamifxTblifHCOutOctets              INTEGER,
      asamifxTblifHCOutUcastPkts           INTEGER,
