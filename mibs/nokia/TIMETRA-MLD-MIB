    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of vRtrMldGrpSrcGrpIfSapUpTime indicates the time since this
         source group entry got created."
    ::= { vRtrMldGrpSrcGrpIfSapEntry 1 }

vRtrMldGrpSrcSlaProfInstTable    OBJECT-TYPE
