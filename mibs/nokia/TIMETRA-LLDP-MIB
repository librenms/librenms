    STATUS      obsolete
    DESCRIPTION
        "The compliance statement for SNMP entities which implement the
         LLDP MIB."
    MODULE -- this module
        MANDATORY-GROUPS {
            tmnxLldpConfigGroup,
            tmnxLldpStatsRxGroup,
            tmnxLldpStatsTxGroup,
            tmnxLldpLocSysGroup,
            tmnxLldpRemSysGroup,
            tmnxLldpRemManAddrGroup
        }
    ::= { tmnxLldpCompliances   1 }

