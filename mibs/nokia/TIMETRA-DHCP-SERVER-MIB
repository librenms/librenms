    tmnxDhcpsPoolStats6FoFreeBlkPT   TimeStamp,
    tmnxDhcpsPoolStats6UsedPct       Integer32,
    tmnxDhcpsPoolStats6UsedPctP      Integer32,
    tmnxDhcpsPoolStats6UsedPctPT     TimeStamp,
    tmnxDhcpsPoolStats6FoUsedPct     Integer32,
