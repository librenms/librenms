--
-- CAPABILITY MANAGER RTU FEATURE INFORMATION
--
alaCapManRtuFeatureInfoTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF AlaCapManRtuInfoEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     "Table of RTU feature information."
    ::= { alaCapabilityManager 17 }

alaCapManRtuInfoEntry  OBJECT-TYPE
    SYNTAX          AlaCapManRtuInfoEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     "Information of valid license information."
    INDEX { alaCapabilityRtuSerialId, alaCapabilityRtuMacAddress, alaCapabilityRtuFeatureId }
    ::= { alaCapManRtuFeatureInfoTable 1 }

AlaCapManRtuInfoEntry ::=
    SEQUENCE
    {
        alaCapabilityRtuSerialId            OCTET STRING,
        alaCapabilityRtuMacAddress          MacAddress,
        alaCapabilityRtuFeatureId           INTEGER,
        alaCapabilityRtuRunningMode         INTEGER,
        alaCapabilityRtuFeatureStatus       INTEGER,
        alaCapabilityRtuFeaturePort         SnmpAdminString,
        alaCapabilityRtuPortCount           Unsigned32,
        alaCapabilityRtuTrialPeriod         SnmpAdminString
    }

alaCapabilityRtuSerialId  OBJECT-TYPE
    SYNTAX          OCTET STRING (SIZE(25))
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     "String displaying the serial number for which this feature is valid."
    ::= { alaCapManRtuInfoEntry 1 }

alaCapabilityRtuMacAddress  OBJECT-TYPE
    SYNTAX          MacAddress
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     "String displaying the MAC address for which this feature is valid."
    ::= { alaCapManRtuInfoEntry 2 }

alaCapabilityRtuFeatureId  OBJECT-TYPE
    SYNTAX          INTEGER {
                        noFeature (1),
                        u10gRtuType (6)
                    }
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     "Value for different features.
                     (1) is corresponded to RTU no feature special case."
    ::= { alaCapManRtuInfoEntry 3 }

alaCapabilityRtuRunningMode  OBJECT-TYPE
    SYNTAX          INTEGER {
                        onTrial (1),
                        endOfTrial (2),
                        onPermanent (3)
                    }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Current RTU running modes on the switch."
    ::= { alaCapManRtuInfoEntry 4 }

alaCapabilityRtuFeatureStatus  OBJECT-TYPE
    SYNTAX          INTEGER {
                        enable (1),
                        disable (2)
                    }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "Current status for a RTU feature."
    DEFVAL  { enable }
    ::= { alaCapManRtuInfoEntry 5 }

alaCapabilityRtuFeaturePort  OBJECT-TYPE
    SYNTAX          SnmpAdminString (SIZE (0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "String displaying enabled ports used by a RTU feature."
    DEFVAL  { "" }
    ::= { alaCapManRtuInfoEntry 6 }

alaCapabilityRtuPortCount  OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Integer displaying the count of enabled ports used by a RTU feature."
    DEFVAL  { 0 }
    ::= { alaCapManRtuInfoEntry 7 }

alaCapabilityRtuTrialPeriod  OBJECT-TYPE
    SYNTAX          SnmpAdminString (SIZE(25))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "String displaying total days in trial period of a RTU feature."
    ::= { alaCapManRtuInfoEntry 8 }

