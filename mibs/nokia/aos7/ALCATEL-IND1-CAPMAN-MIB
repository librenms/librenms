--
-- CAPABILITY MANAGER TRAP-THRESHOLD
--

alaCapManTrapThreshold OBJECT IDENTIFIER ::= { alaCapabilityManager 13 }

alaCapManTrapThresholdMacLow OBJECT-TYPE
        SYNTAX      Unsigned32 (5..90)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This object presents the minimum utilization percentage of L2 MAC table for trigging SNMP trap
             and clearing the trap."
        DEFVAL { 90 }
        ::= { alaCapManTrapThreshold 1 }


alaCapManTrapThresholdMacHigh OBJECT-TYPE
        SYNTAX      Unsigned32 (10..95)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This object presents the maximum utilization percentage of L2 MAC table for trigging SNMP trap
             and clearing the trap."
        DEFVAL { 95 }
        ::= { alaCapManTrapThreshold 2 }

alaCapManTrapThresholdArpLow OBJECT-TYPE
        SYNTAX      Unsigned32 (5..90)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This object presents the minimum utilization percentage of L3 ARP table for trigging SNMP trap
             and clearing the trap."
        DEFVAL { 90 }
        ::= { alaCapManTrapThreshold 3 }

alaCapManTrapThresholdArpHigh OBJECT-TYPE
        SYNTAX      Unsigned32 (10..95)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This object presents the maximum utilization percentage of L3 ARP table for trigging SNMP trap
             and clearing the trap."
        DEFVAL { 95 }
        ::= { alaCapManTrapThreshold 4 }

