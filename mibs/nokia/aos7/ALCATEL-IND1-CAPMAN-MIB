-- -------------------------------------------------------------
-- License Manager TRAPS
-- -------------------------------------------------------------
alaLicenseManagerDemoDayAlert NOTIFICATION-TYPE
    OBJECTS 
    {
        alaLicenseTimeRemaining
    }
    STATUS  current
    DESCRIPTION
        "Generate trap for license manager." 
    ::= {alaCapabilityMibNotifications 1}

-- -------------------------------------------------------------
-- Capability Manager Threshold TRAPS
-- -------------------------------------------------------------
alaHWMacCapacityExceeded NOTIFICATION-TYPE
    OBJECTS
    {
        alaHWMacMaxThresholdState
    }
    STATUS  current
    DESCRIPTION
        "alaHWMacCapacityExceeded trap indicates that there has been a change in
         the maximum HW MAC utilization on this system. When HW MAC utilization reaches 95%,
         this trap is generated with the alaHWMacMaxThresholdState set to reached.
         When HW MAC utilization transitions back to below 90% utilization,
         this trap is generated again with the alaHWMacMaxThresholdState set to cleared."
    ::= {alaCapabilityMibNotifications 2}

--
-- CAPABILITY MANAGER RTU FEATURE TRAPS
--
alaCapManRtuExpiryDayAlert NOTIFICATION-TYPE
    OBJECTS 
    {
        alaCapabilityRtuSerialId,
        alaCapabilityRtuFeatureId,
        alaCapManRtuExpiredDateStatus
    }
    STATUS  current
    DESCRIPTION
        "This trap is sent when a RTU feature expires.
         7 days before the expiry, 1 day before expiry, at expiry of trial period and switch enters ended trial period." 
    ::= { alaCapabilityMibNotifications 3 }

alaCapManRtuNoConnectivityAlert NOTIFICATION-TYPE
    OBJECTS 
    {
        alaCapabilityRtuSerialId,
        alaCapManRtuServerStatus
    }
    STATUS  current
    DESCRIPTION
        "This trap is sent when the switch operates in RTU mode but it cannot call-home due to connectivity issue." 
    ::= { alaCapabilityMibNotifications 4 }

-- -------------------------------------------------------------
-- Capability Manager Trap Notification Objects
-- -------------------------------------------------------------

alaHWMacMaxThresholdState OBJECT-TYPE
    SYNTAX      INTEGER {
                    reached (1),
                    cleared (2)
                }
    MAX-ACCESS   accessible-for-notify
    STATUS       current
    DESCRIPTION
        "Reached: This trap value indicates if the MAC table had exceeded the configured MAC high threshold limit.
        Cleared: This trap value indicates if the MAC table had fallen below the configured MAC low threshold limit."
    ::= { alaCapManNotificationObjects 1 }

--
-- CAPABILITY MANAGER RTU FEATURE TRAP NOTIFICATION OBJECTS
--
alaCapManRtuExpiredDateStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                    weekBeforeExpired (1),
                    dayBeforeExpired (2),
                    expired (3)
                }
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "weekBeforeExpired: 7 days before a RTU feature expired.
         dayBeforeExpired: 1 day before a RTU feature expired.
         expired: A RTU feature comes to an expired time."
    ::= { alaCapManNotificationObjects 2 }

alaCapManRtuServerStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                    noConnectivity (1),
                    withConnectivity (2)
                }
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "noConnectivity: Switch is identified as RTU but RTU Server is not reachable for the current call-home.
         withConnectivity: Switch is identified as RTU and RTU Server is reachable."
    ::= { alaCapManNotificationObjects 3 }

