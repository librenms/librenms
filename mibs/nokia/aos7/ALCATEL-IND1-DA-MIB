        ::= { alaDaHICHostEntry 1}

    alaDaHICHostStatus OBJECT-TYPE
        SYNTAX        INTEGER {inprogress(1),success(2),fail(3),timeout(4)}
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
        "HIC status of each HIC Host."
        DEFVAL { fail }
        ::= { alaDaHICHostEntry 2}


