    SYNTAX      SnmpAdminString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS      deprecated
    DESCRIPTION
        "The UNP to be used for classification if the authentication server
        is not reachable."
    ::= { alaDaUNPGlobalConfiguration 2 }

