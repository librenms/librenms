    ::= { alcatelIND1VRRPMIBGroups 13 }


alaVrrpv3AssoTrackEntryGroup OBJECT-GROUP
    OBJECTS     {
                        alaVrrpv3AssoTrackRowStatus
                }
    STATUS     current
    DESCRIPTION
