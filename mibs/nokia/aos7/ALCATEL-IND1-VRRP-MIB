    ::= { alcatelIND1VRRPMIBGroups 12 }


alaVrrpv3AssoGroupEntryGroup OBJECT-GROUP
    OBJECTS     {
                        alaVrrpv3AssoGroupRowStatus
                }
    STATUS     current
    DESCRIPTION
