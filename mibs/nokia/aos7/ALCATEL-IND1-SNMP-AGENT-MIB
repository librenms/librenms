
ALCATEL-IND1-SNMP-AGENT-MIB DEFINITIONS ::= BEGIN

IMPORTS
        OBJECT-TYPE,
        OBJECT-IDENTITY,
        MODULE-IDENTITY,
        IpAddress               FROM SNMPv2-SMI
        TEXTUAL-CONVENTION      FROM SNMPv2-TC
        OBJECT-GROUP,
        MODULE-COMPLIANCE       FROM SNMPv2-CONF
        softentIND1SnmpAgt      FROM ALCATEL-IND1-BASE;


