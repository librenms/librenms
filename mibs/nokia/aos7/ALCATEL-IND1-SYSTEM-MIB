                                systemStorageGroup,
                                systemStorageLockSftpServerGroup,
                                systemStorageLockOutputFileGroup,
                                systemStorageLockingGroup,
                                systemSslCipherGroup,
                                systemMgmtAgentGroup
            }

        ::= { alcatelIND1SystemMIBCompliances 1 }

