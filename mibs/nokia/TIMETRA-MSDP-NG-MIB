    tmnxMsdpNgSourcePrefixType       InetAddressType,
    tmnxMsdpNgSourcePrefix           InetAddress,
    tmnxMsdpNgSourceMask             InetAddressPrefixLength,
    tmnxMsdpNgSourceRowStatus        RowStatus,
    tmnxMsdpNgSourceMaxActiveSources Integer32,
    tmnxMsdpNgSourceDiscMethod       INTEGER,
    tmnxMsdpNgSourceSrcActMsgsExMax  Counter32,
    tmnxMsdpNgSourceLastExEventTime  TimeStamp
}

