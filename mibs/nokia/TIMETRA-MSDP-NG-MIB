    tmnxMsdpNgSAPeerLearntFrom       InetAddress,
    tmnxMsdpNgSARPFPeerType          InetAddressType,
    tmnxMsdpNgSARPFPeer              InetAddress,
    tmnxMsdpNgSAUpTime               TimeInterval,
    tmnxMsdpNgSAExpiryTime           TimeInterval
}

