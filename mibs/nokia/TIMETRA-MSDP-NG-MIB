    ::= { tmnxMsdpNgObjects 6 }

tmnxMsdpNgSourceEntry            OBJECT-TYPE
    SYNTAX      TmnxMsdpNgSourceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An tmnxMsdpNgSourceEntry represents a MSDP Source configuration.

         Entries can be created and deleted via SNMP SET operations."
    INDEX       {
        vRtrID,
        tmnxMsdpNgSourcePrefixType,
        tmnxMsdpNgSourcePrefix,
        tmnxMsdpNgSourceMask
    }
