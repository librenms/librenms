    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxMsdpNgPeerGrpPeerStateTimer indicates the number of
         seconds to wait before another message is sent to a peer."
    ::= { tmnxMsdpNgPeerGrpPeerEntry 27 }

tmnxMsdpNgPeerGrpPeerTimeout     OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
