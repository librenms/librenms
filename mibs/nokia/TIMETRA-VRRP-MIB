    vrrpRouterStatisticsEntry
                                                         FROM TIMETRA-VRRP-V3-MIB
    vrrpOperEntry, vrrpOperMasterIpAddr,
    vrrpOperVrId
                                                         FROM VRRP-MIB
    ;

