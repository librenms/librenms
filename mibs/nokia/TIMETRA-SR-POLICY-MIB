    vRtrStPSrv6BSidLocOperFuncVal    TSrv6FunctionValue,
    vRtrStPSrv6BSidLocFuncOpErrCode  TSrv6FunctionErrorCode,
    vRtrStPSrv6BSidOperAddrType      InetAddressType,
    vRtrStPSrv6BSidOperAddr          InetAddress
}

