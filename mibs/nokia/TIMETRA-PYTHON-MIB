    DEFVAL      { 0 }
    ::= { tmnxPythonPolicyCacheEntry 11 }

tmnxPyPlcyCacheMinLifetimeHa     OBJECT-TYPE
    SYNTAX      Unsigned32 (0..600)
    UNITS       "seconds"
