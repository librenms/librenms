                 SYNTAX            DisplayString (SIZE (0..255))	 
                 MAX-ACCESS        read-only	 
                 STATUS            current	 
                 DESCRIPTION	 
                         "This is to get the NI slot numbers which have gone	 
                          down after reboot/takeover to display in warning 	 
                          while issuing write memory from webview."	 
                 DEFVAL  { "" }	 
                 ::= { configManager 82 }


         virtualChassisTopologyMissingChassisIDs   OBJECT-TYPE	 
                 SYNTAX            VirtualChassisTopologyChassisIDs	 
                 MAX-ACCESS        read-only	 
                 STATUS            current	 
        	   DESCRIPTION
		          "Stack topology change missing Chassis ID bit map. 
		           Only the missing slot numbers will be set in this bit map.
		           Default value is empty."
                 DEFVAL { {} }	 
                 ::= { configManager 83 }

--
-- Trap description
--
 
   	alcatelIND1ConfigMgrTraps  OBJECT-IDENTITY
         	STATUS  current
         	DESCRIPTION
             	    "Branch For Configuration Manager Subsystem Trap Information."
         ::= { alcatelIND1ConfigMgrMIB 3 }

        alcatelIND1ConfigMgrTrapsRoot OBJECT IDENTIFIER   ::= { alcatelIND1ConfigMgrTraps 0 }
        alcatelIND1ConfigMgrTrapsObj  OBJECT IDENTIFIER   ::= { alcatelIND1ConfigMgrTraps 1 }
        configSaveSucceededTrap  NOTIFICATION-TYPE
                 OBJECTS {
                  configSaveSucceededTrapReason
                         }
                 STATUS  current
                 DESCRIPTION
                         "The save config succeeded event is generated when the saving of 
                          configuration finishes without errors."
                 ::= {alcatelIND1ConfigMgrTrapsRoot 1} 


-- Object used in the trap	
	
    configSaveSucceededTrapReason  OBJECT-TYPE
	    SYNTAX  SnmpAdminString 
	    MAX-ACCESS  accessible-for-notify
	    STATUS  current
	    DESCRIPTION
	        "The reason of trap for successful execution of write memory command"
	    ::= { alcatelIND1ConfigMgrTrapsObj 1 }	
--
-- Compliance Statements
--

	alcatelIND1ConfigMgrMIBCompliance  MODULE-COMPLIANCE
		STATUS  current
		DESCRIPTION
		    "Compliance statement for Configuration Manager Subsystem."
		MODULE  -- this module

		    MANDATORY-GROUPS
		    {
			configFileGroup,
			configTimerFileGroup,
			configSnapshotGroup,
			configTechSupportLogGroup,
			configWriteMemoryGroup,
                        configMgrTrapsGroup,
			configMgrTrapReasonGroup
		    }

		::= { alcatelIND1ConfigMgrMIBCompliances  1 }


--
-- Units of Conformance
--

	configFileGroup  OBJECT-GROUP
		OBJECTS
		{
		  configFileName,
		  configFileAction,
		  configErrorFileName,
		  configFileStatus,
		  configFileMode,
	          configErrorFileMaximum,
		  configChangeStatus
		}
		STATUS  current
		DESCRIPTION
		    "Collection of objects for file configuration."
		::= { alcatelIND1ConfigMgrMIBGroups  1}


	configTimerFileGroup  OBJECT-GROUP
		OBJECTS
		{
	 	  configTimerFileName,
		  configTimerFileTime,
		  configTimerFileStatus,
		  configTimerClear
		}
		STATUS  current
		DESCRIPTION
		    "Collection of objects for timer file configuration."
		::= { alcatelIND1ConfigMgrMIBGroups  2}


