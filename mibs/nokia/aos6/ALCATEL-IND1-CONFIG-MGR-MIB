ALCATEL-IND1-CONFIG-MGR-MIB DEFINITIONS ::= BEGIN


	IMPORTS
		OBJECT-TYPE,
		MODULE-IDENTITY,
		NOTIFICATION-TYPE,
		OBJECT-IDENTITY		FROM SNMPv2-SMI
		DisplayString		FROM SNMPv2-TC
		SnmpAdminString		FROM SNMP-FRAMEWORK-MIB
		NOTIFICATION-GROUP,
		MODULE-COMPLIANCE,
		OBJECT-GROUP		FROM SNMPv2-CONF
		softentIND1Confmgr	FROM ALCATEL-IND1-BASE
                TEXTUAL-CONVENTION     FROM SNMPv2-TC
		;

VirtualChassisTopologyChassisIDs ::= TEXTUAL-CONVENTION
    STATUS current
    DESCRIPTION
    "This bit map object specifies the Chassis IDs of the Virtual Chassis related to
    a specific group. It could be used to list Chassis IDs that make up the VC, or
    Chassis IDs missing from the VC. The actual context will determine the contents.
     "
    SYNTAX BITS {
        chassis1(0),
        chassis2(1),
        chassis3(2),
        chassis4(3),
        chassis5(4),
        chassis6(5),
        chassis7(6),
        chassis8(7)
    }

