	::={alaChasEntPhysFanEntry 2}

 -- CHASSIS SUPERVISION HASH FDB CONTROL OBJECTS
  
alaChasFdbHashMode OBJECT-TYPE
        SYNTAX   INTEGER
  		 {
			xor(1),
  			crc(2)
              }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
  		"when set to xor, the hash configuration mode is set to default xor-mode.
  		 when set to crc, the hash configuration mode is set to crc-mode."
  	DEFVAL {xor}
  	::={alcatelIND1ChassisMIBObjects 19}
	
alaChasFdbHashChainLength OBJECT-TYPE
        SYNTAX   INTEGER {
		default(4),
                extend(8) }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "when set to 4, the number of entries to be looked up in the MAC table lookup (the hash chain length) is 4.
                when set to 8, the number of entries to be looked up in the MAC table lookup (the hash chain length) is 8."
        DEFVAL {4}
        ::= { alcatelIND1ChassisMIBObjects 34 }

