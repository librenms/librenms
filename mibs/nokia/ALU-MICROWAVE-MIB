    OBJECTS {
        aluMwNotifyRadioPortID
    }
    STATUS  current
    DESCRIPTION
        "Generated when a microwave radio device is detected."
    ::= { aluMwNotification 3 }

aluMwRadioNotPresent    NOTIFICATION-TYPE
    OBJECTS {
        aluMwNotifyRadioPortID
    }
    STATUS  current
    DESCRIPTION
