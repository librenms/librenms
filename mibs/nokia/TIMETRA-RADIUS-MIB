    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxRadIsaPlcySrvIp6SrcPrefixLen specifies the IPv6
         prefix length of source addresses."
    DEFVAL      { 0 }
    ::= { tmnxRadIsaPlcyEntry 116 }

tmnxRadIsaPlcySrvIp6Mtu          OBJECT-TYPE
    SYNTAX      Unsigned32 (1280..9000)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxRadIsaPlcySrvIp6Mtu specifies the MTU (Maximum
         Transmission Unit) for outgoing IPv6 RADIUS traffic.

         The system fragments IP datagrams exceeding the MTU."
    DEFVAL      { 9000 }
    ::= { tmnxRadIsaPlcyEntry 117 }

