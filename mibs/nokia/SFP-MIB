                   ltHostConfigExtSfpFaceplateNumber INTEGER,
                   ltHostSfpCableToRack              INTEGER,
                   ltHostSfpCableToShelf             INTEGER,
                   ltHostSfpCableToSlot              INTEGER,
                   ltHostConfigSfpLogSlotId          EqptSlotIndex
          }

