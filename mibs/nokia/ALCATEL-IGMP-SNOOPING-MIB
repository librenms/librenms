eVxlanIgmpSnpgGroupTable         OBJECT-TYPE
    SYNTAX      SEQUENCE OF EVxlanIgmpSnpgGroupEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "eVxlanIgmpSnpgGroupTable together with the table
         eVxlanIgmpSnpgGrpSrcTable contains multicast group address information
         collected by IGMP snooping for each VXLAN."
    ::= { alxIgmpSnoopingVxlanObjs 5 }

eVxlanIgmpSnpgGroupEntry         OBJECT-TYPE
    SYNTAX      EVxlanIgmpSnpgGroupEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table indicates a multicast group address learned
         by IGMP snooping on this VXLAN. If a Source list is present it is
         described in the corresponding entries of the
         eVxlanIgmpSnpgGrpSrcTable table."
    INDEX       {
        svcId,
        eVxlanVTEPAddrType,
        eVxlanVTEPAddr,
        eVxlanVNI,
        eVxlanIgmpSnpgGrpAddress
    }
    ::= { eVxlanIgmpSnpgGroupTable 1 }

EVxlanIgmpSnpgGroupEntry         ::= SEQUENCE
{
    eVxlanVTEPAddrType               InetAddressType,
    eVxlanVTEPAddr                   InetAddress,
    eVxlanVNI                        Unsigned32,
    eVxlanIgmpSnpgGrpAddress         IpAddress,
    eVxlanIgmpSnpgGrpType            TmnxIgmpSnpgGroupType,
    eVxlanIgmpSnpgGrpFilterMode      TmnxIgmpGroupFilterMode,
    eVxlanIgmpSnpgGrpUpTime          TimeTicks,
    eVxlanIgmpSnpgGrpExpiryTime      Unsigned32,
    eVxlanIgmpSnpgGrpCompatMode      Unsigned32,
    eVxlanIgmpSnpgGrpV1HostExpTime   Unsigned32,
    eVxlanIgmpSnpgGrpV2HostExpTime   Unsigned32
}

eVxlanVTEPAddrType               OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanVTEPAddrType indicates the type of the
         address eVxlanVTEPAddr."
    ::= { eVxlanIgmpSnpgGroupEntry 1 }

eVxlanVTEPAddr                   OBJECT-TYPE
    SYNTAX      InetAddress (SIZE (4|16))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanVTEPAddr indicates the VXLAN (virtual
         extensible LAN) Tunnel Endpoint address of ethernet VPN (EVPN)
         destination on which the active Querier is detected."
    ::= { eVxlanIgmpSnpgGroupEntry 2 }

eVxlanVNI                        OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanVNI indicates the VXLAN (virtual
         extensible LAN) Network Identifier (VNI) of the end-point of ethernet
         VPN (EVPN) destination interface on which the active Querier is
         detected."
    ::= { eVxlanIgmpSnpgGroupEntry 3 }

eVxlanIgmpSnpgGrpAddress         OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanIgmpSnpgGrpAddress indicates the IPv4
         multicast group address for which this entry contains information."
    ::= { eVxlanIgmpSnpgGroupEntry 4 }

eVxlanIgmpSnpgGrpType            OBJECT-TYPE
    SYNTAX      TmnxIgmpSnpgGroupType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanIgmpSnpgGrpType indicates whether this
         entry was learned dynamically or was statically configured."
    ::= { eVxlanIgmpSnpgGroupEntry 5 }

eVxlanIgmpSnpgGrpFilterMode      OBJECT-TYPE
    SYNTAX      TmnxIgmpGroupFilterMode
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanIgmpSnpgGrpFilterMode indicates the
         filter-mode for this group."
    ::= { eVxlanIgmpSnpgGroupEntry 6 }

eVxlanIgmpSnpgGrpUpTime          OBJECT-TYPE
    SYNTAX      TimeTicks
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanIgmpSnpgGrpUpTime indicates how long (in
         centiseconds) that the group address already exists in the database
         for this VXLAN."
    ::= { eVxlanIgmpSnpgGroupEntry 7 }

eVxlanIgmpSnpgGrpExpiryTime      OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanIgmpSnpgGrpExpiryTime indicates the
         amount of time remaining before this entry will be aged out."
    ::= { eVxlanIgmpSnpgGroupEntry 8 }

eVxlanIgmpSnpgGrpCompatMode      OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanIgmpSnpgGrpCompatMode indicates the IGMP
         version applicable on this VXLAN for this multicast group address.
         This corresponds to the lowest version seen for this group address on
         this VXLAN."
    ::= { eVxlanIgmpSnpgGroupEntry 9 }

eVxlanIgmpSnpgGrpV1HostExpTime   OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanIgmpSnpgGrpV1HostExpTime indicates the
         time remaining until the system will assume that there are no longer
         any IGMPv1 members on this VXLAN.

         Upon hearing any IGMPv1 Membership Report, this value is reset to the
         group membership interval. While this time remaining is non-zero, the
         system ignores any IGMPv2 and IGMPv3 'leave' messages for this group
         that it receives on this VXLAN."
    ::= { eVxlanIgmpSnpgGroupEntry 10 }

eVxlanIgmpSnpgGrpV2HostExpTime   OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanIgmpSnpgGrpV2HostExpTime indicates the
         time remaining until the system will assume that there are no longer
         any IGMPv2 members on the VXLAN.

         Upon hearing any IGMPv2 Membership Report, this value is reset to the
         group membership interval. While this time remaining is non-zero, the
         system ignores any IGMPv3 'leave' messages for this group that it
         receives on this VXLAN."
    ::= { eVxlanIgmpSnpgGroupEntry 11 }

eVxlanIgmpSnpgGrpSrcTable        OBJECT-TYPE
    SYNTAX      SEQUENCE OF EVxlanIgmpSnpgGrpSrcEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "eVxlanIgmpSnpgGrpSrcTable lists the IPv4 multicast Sources
         corresponding to the IPv4 multicast groups listed in the
         eVxlanIgmpSnpgGroupTable."
    ::= { alxIgmpSnoopingVxlanObjs 6 }

eVxlanIgmpSnpgGrpSrcEntry        OBJECT-TYPE
    SYNTAX      EVxlanIgmpSnpgGrpSrcEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry in the eVxlanIgmpSnpgGrpSrcTable. Each entry represents a
         Source address from which receivers are interested/not interested in
         receiving multicast traffic from."
    INDEX       {
        svcId,
        eVxlanVTEPAddrType,
        eVxlanVTEPAddr,
        eVxlanVNI,
        eVxlanIgmpSnpgGrpAddress,
        eVxlanIgmpSnpgGrpSrcAddr
    }
    ::= { eVxlanIgmpSnpgGrpSrcTable 1 }

EVxlanIgmpSnpgGrpSrcEntry        ::= SEQUENCE
{
    eVxlanIgmpSnpgGrpSrcAddr         IpAddress,
    eVxlanIgmpSnpgGrpSrcType         TmnxIgmpSnpgGroupType,
    eVxlanIgmpSnpgGrpSrcUpTime       TimeTicks,
    eVxlanIgmpSnpgGrpSrcExpiryTime   Unsigned32,
    eVxlanIgmpSnpgGrpSrcFwdOrBlk     INTEGER
}

eVxlanIgmpSnpgGrpSrcAddr         OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanIgmpSnpgGrpSrcAddr indicates the address
         of the Source sending multicast traffic to the group identified by
         eVxlanIgmpSnpgGrpAddress.

         If the value of eVxlanIgmpSnpgGrpFilterMode corresponding to this
         entry is 'include(1)', the receivers are interested in receiving
         traffic from this Source.

         If the value of eVxlanIgmpSnpgGrpFilterMode corresponding to this
         entry is 'exclude(2)', the receivers are not interested in receiving
         traffic from this Source."
    ::= { eVxlanIgmpSnpgGrpSrcEntry 1 }

eVxlanIgmpSnpgGrpSrcType         OBJECT-TYPE
    SYNTAX      TmnxIgmpSnpgGroupType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanIgmpSnpgGrpSrcType indicates how this
         source entry was learned.

         If this source entry was learned, it is set to 'dynamic(2)'. For
         statically configured sources, the value is set to 'static(1)'."
    ::= { eVxlanIgmpSnpgGrpSrcEntry 2 }

eVxlanIgmpSnpgGrpSrcUpTime       OBJECT-TYPE
    SYNTAX      TimeTicks
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanIgmpSnpgGrpSrcUpTime indicates how long
         (in centiseconds) that the Source address already exists in the
         database for this SAP."
    ::= { eVxlanIgmpSnpgGrpSrcEntry 3 }

eVxlanIgmpSnpgGrpSrcExpiryTime   OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of the object eVxlanIgmpSnpgGrpSrcExpiryTime indicates the
         minimum amount of time remaining before this entry will be aged out.

         If the value of eVxlanIgmpSnpgGrpSrcExpiryTime is non-zero and the
         value of eVxlanIgmpSnpgGrpFilterMode corresponding to this entry is
         'include(1)', it means that there are currently one or more systems
         which desire to receive multicast traffic for this Source.

         If this timer expires and the value of eVxlanIgmpSnpgGrpFilterMode is
         'include(1)', the system concludes that traffic from this particular
         Source is no longer desired on the attached network, and deletes this
         entry.

         If the value of eVxlanIgmpSnpgGrpSrcExpiryTime is non-zero and the
         filter-mode for the group is 'exclude(2)', it means that at least one
         system desires to receive multicast traffic from this source.

         If the value of eVxlanIgmpSnpgGrpSrcExpiryTime is zero and the
         filter-mode for the group is 'exclude(2)', it means that no system
         desires to receive multicast traffic from this Source."
    ::= { eVxlanIgmpSnpgGrpSrcEntry 4 }

eVxlanIgmpSnpgGrpSrcFwdOrBlk     OBJECT-TYPE
    SYNTAX      INTEGER {
        forward (1),
        block   (2)
    }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "eVxlanIgmpSnpgGrpSrcFwdOrBlk indicates whether this entry is on the
         forward list or block list."
    ::= { eVxlanIgmpSnpgGrpSrcEntry 5 }

eVxlanIgmpSnpgStatsTable         OBJECT-TYPE
    SYNTAX      SEQUENCE OF EVxlanIgmpSnpgStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "eVxlanIgmpSnpgStatsTable contains statistics on IGMP snooping per
         VXLAN."
    ::= { alxIgmpSnoopingVxlanObjs 7 }

eVxlanIgmpSnpgStatsEntry         OBJECT-TYPE
    SYNTAX      EVxlanIgmpSnpgStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "eVxlanIgmpSnpgStatsEntry is an entry in the eVxlanIgmpSnpgStatsTable.
         Each entry contains IGMP snooping statics for a VXLAN in a Tls."
    INDEX       {
        svcId,
        eVxlanVTEPAddrType,
        eVxlanVTEPAddr,
        eVxlanVNI
    }
    ::= { eVxlanIgmpSnpgStatsTable 1 }

EVxlanIgmpSnpgStatsEntry         ::= SEQUENCE
{
    eVxlanIgmpSnpgTxGenQueries       Counter32,
    eVxlanIgmpSnpgTxGrpSpecQueries   Counter32,
    eVxlanIgmpSnpgTxSrcSpecQueries   Counter32,
    eVxlanIgmpSnpgTxV1Reports        Counter32,
    eVxlanIgmpSnpgTxV2Reports        Counter32,
    eVxlanIgmpSnpgTxV3Reports        Counter32,
    eVxlanIgmpSnpgTxV2Leaves         Counter32,
    eVxlanIgmpSnpgRxGenQueries       Counter32,
    eVxlanIgmpSnpgRxGrpSpecQueries   Counter32,
    eVxlanIgmpSnpgRxSrcSpecQueries   Counter32,
    eVxlanIgmpSnpgRxV1Reports        Counter32,
    eVxlanIgmpSnpgRxV2Reports        Counter32,
    eVxlanIgmpSnpgRxV3Reports        Counter32,
    eVxlanIgmpSnpgRxV2Leaves         Counter32,
    eVxlanIgmpSnpgRxUnknownType      Counter32,
    eVxlanIgmpSnpgFwdGenQueries      Counter32,
    eVxlanIgmpSnpgFwdGrpSpecQueries  Counter32,
    eVxlanIgmpSnpgFwdSrcSpecQueries  Counter32,
    eVxlanIgmpSnpgFwdV1Reports       Counter32,
    eVxlanIgmpSnpgFwdV2Reports       Counter32,
    eVxlanIgmpSnpgFwdV3Reports       Counter32,
    eVxlanIgmpSnpgFwdV2Leaves        Counter32,
    eVxlanIgmpSnpgFwdUnknownType     Counter32,
    eVxlanIgmpSnpgRxBadLenPkts       Counter32,
    eVxlanIgmpSnpgRxBadIpChksmPkts   Counter32,
    eVxlanIgmpSnpgRxBadIgmpChksmPkts Counter32,
    eVxlanIgmpSnpgRxBadEncodedPkts   Counter32,
    eVxlanIgmpSnpgRxNoRtrAlertPkts   Counter32,
    eVxlanIgmpSnpgRxZeroSrcAdrPkts   Counter32,
    eVxlanIgmpSnpgSendQueryCfgDrops  Counter32,
    eVxlanIgmpSnpgImportPolicyDrops  Counter32,
    eVxlanIgmpSnpgMaxNumGroupsDrops  Counter32,
    eVxlanIgmpSnpgRxWrongVersionPkts Counter32,
    eVxlanIgmpSnpgMcacPolicyDrops    Counter32,
    eVxlanIgmpSnpgMcsFailures        Counter32,
    eVxlanIgmpSnpgRxLocalScopePkts   Counter32,
    eVxlanIgmpSnpgRxRsvdScopePkts    Counter32,
    eVxlanIgmpSnpgMaxNumSourcesDrops Counter32,
    eVxlanIgmpSnpgMaxNumGrpSrcsDrops Counter32
}

