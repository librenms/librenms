        ""
    INDEX       {
        svcId,
        vxlanVTEPAddrType,
        vxlanVTEPAddress,
        vxlanVNI
    }
    ::= { vxlanIgmpSnpgStateTable 1 }

VxlanIgmpSnpgStateEntry          ::= SEQUENCE
{
    vxlanVTEPAddrType                InetAddressType,
    vxlanVTEPAddress                 InetAddress,
