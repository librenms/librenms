    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "vRtrIgmpGrpSrcGrpIfSapUpTime indicates the time since this source
         group entry got created."
    ::= { vRtrIgmpGrpSrcGrpIfSapEntry 1 }

