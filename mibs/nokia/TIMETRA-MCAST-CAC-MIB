    tmnxMcacBdlChlNgEndAddrType      InetAddressType,
    tmnxMcacBdlChlNgEndAddr          InetAddress,
    tmnxMcacBdlChlNgSrcPfxType       TmnxAddressAndPrefixType,
    tmnxMcacBdlChlNgSrcPfx           TmnxAddressAndPrefixAddress,
    tmnxMcacBdlChlNgSrcPfxLen        TmnxAddressAndPrefixPrefix,
    tmnxMcacBdlChlNgRowStatus        RowStatus,
    tmnxMcacBdlChlNgBW               Unsigned32,
    tmnxMcacBdlChlNgClass            TmnxMcacChlClassTc,
    tmnxMcacBdlChlNgType             TmnxMcacChlTypeTc
}

