        SYNTAX         Unsigned32
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "."
        ::= { tnPowerMgmtPsdEntry 1 }

    tnPowerMgmtPsdPowerPerThirtySevenDotFiveGHz OBJECT-TYPE
        SYNTAX         Integer32
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "Set the power spectral density target as a power in
                        37.5 GHz width.  Indexed by the lower frequency edge
                        of each bin, which must be non-overlapping."
        ::= { tnPowerMgmtPsdEntry 2 }

    tnPowerMgmtPsdEquivalentPowerSpectralDensityPerGHz OBJECT-TYPE
        SYNTAX         Integer32
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "The power spectral density target power equivalent in
                        dBm per GHz."
        ::= { tnPowerMgmtPsdEntry 3 }

