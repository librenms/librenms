--
-- Capabilities for TROPIC-EXPRSCALARS
-- Based on MIB version:  R15.0-LatestMIBs-221105
--

    SUPPORTS            TROPIC-EXPRSCALARS
        INCLUDES        { tnExprSysRadiusServersGroup,
                          tnExprSysRadiusGroup }

        VARIATION       tnSysRadiusServerIpAddress
            ACCESS      not-implemented
            DESCRIPTION "This object is not supported."

        VARIATION       tnSysRadiusServerIpPort
            DESCRIPTION "This object must always be included in an SNMP SET
                         PDU together with object tnSysRadiusServerRowStatus,
                         otherwise error 'inconsistent value' is returned."

        VARIATION       tnSysRadiusServerSharedSecret
            DESCRIPTION "This object must always be included in an SNMP SET
                         PDU together with object tnSysRadiusServerRowStatus,
                         otherwise error 'inconsistent value' is returned."

        VARIATION       tnSysRadiusServerInetAddressType
            DESCRIPTION "This object must always be included in an SNMP SET
                         PDU together with objects tnSysRadiusServerRowStatus,
                         and tnSysRadiusServerInetAddress, otherwise error
                         'inconsistent value' is returned."

        VARIATION       tnSysRadiusServerInetAddress
            DESCRIPTION "This object must always be included in an SNMP SET
                         PDU together with object tnSysRadiusServerRowStatus,
                         and tnSysRadiusServerInetAddressType, otherwise
                         error 'inconsistent value' is returned."

        VARIATION       tnSysRadiusServerRowStatus
            SYNTAX      INTEGER { active(1),
                                  createAndGo(4),
                                  destroy(6) }
            DESCRIPTION "Limited set of values.
                         The row status can only be set to createAnGo(4) for
                         row creation, or to destroy(6) for row deletion.
                         When reading, always value active(1) is read.

                         This object must always be included in an SNMP SET
                         PDU together with the following objects :
                          - tnSysRadiusServerIpPort
                          - tnSysRadiusServerSharedSecret
                          - tnSysRadiusServerInetAddress
                          - tnSysRadiusServerInetAddressType
                         otherwise error 'inconsistent value' is returned."

        VARIATION       tnSysRadiusServerType
            ACCESS      not-implemented
            DESCRIPTION "This object is not supported."

        VARIATION       tnSysRadiusServerDomain
            ACCESS      not-implemented
            DESCRIPTION "This object is not supported."

        VARIATION       tnSysRadiusTimeout
            SYNTAX      Unsigned32 (1..1000)
            DEFVAL      { 5 }
            DESCRIPTION "Limited value range."

        VARIATION       tnSysRadiusRetries
            SYNTAX      Unsigned32 (1..100)
            DEFVAL      { 3 }
            DESCRIPTION "Limited value range."

        VARIATION       tnSysAuthenOrder
            SYNTAX      INTEGER { local(1),
                                  radius(2),
                                  radiusThenLocal(3) }
            DEFVAL      { local }
            DESCRIPTION "Limited set of values."

        VARIATION       tnSysRadiusSecState
            ACCESS      not-implemented
            DESCRIPTION "This object is not supported."

