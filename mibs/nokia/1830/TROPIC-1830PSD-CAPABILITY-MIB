    ::= { tnPsdAgentCapability 1 }

tn1830Capability AGENT-CAPABILITIES
    PRODUCT-RELEASE "Release 5.0.0."
    STATUS          current
    DESCRIPTION     "Agent Capabilities for the 1830 PSD product."

--
-- Capabilities for SNMPv2-MIB
--

    SUPPORTS            SNMPv2-MIB
        INCLUDES        { systemGroup,
                          snmpBasicNotificationsGroup,
                          snmpWarmStartNotificationGroup }

        VARIATION       sysContact
            DESCRIPTION "The following ASCII characters from ITU Recommendation
                         T.50 are supported:
                         The G0 character set - values 0x20 - 0x7E, with the
                         following exceptions:
                         - value 0x22: double quotes
                         - value 0x27: single quote
                         - value 0x3f: question mark
                         - value 0x5c: backslash
                         The reason of these exceptions is that those
                         characters conflict with the Command Line Interface
                         (CLI)."

