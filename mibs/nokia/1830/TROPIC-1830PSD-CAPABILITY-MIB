--
-- Capabilities for TN-PMON-MIB
-- Based on MIB version:  R15.0-LatestMIBs-221105
--
-- There are no conformance groups defined in TN-PMON-MIB.
-- Due to this omission this section is listed in comment style and
-- the supported tables are included instead of the groups.
--
--  SUPPORTS            TN-PMON-MIB
--      INCLUDES        { tnPmonPolicyTable,
--                        tnPmonTcaTable,
--                        tnEthPortStatsTable,
--                        tnEthCfmTWSlmStatsTable,
--                        tnEthCfmTWDmStatsTable }
--
--      VARIATION       tnPmonPolicyType
--          SYNTAX      INTEGER { port(1),
--                                eth-cfm-two-way-slm(3),
--                                eth-cfm-two-way-dm(4) }
--          DESCRIPTION "Limit set of values."
--
--      VARIATION       tnPmonPolicyId
--          SYNTAX      Integer32 ( 1 | 2 | 8 )
--          DESCRIPTION "Limit set of values, the following meaning applies:
--                       1=Disabled: all TCA values are zero, read only.
--                       2=User: user can provision TCA values, read write.
--                       8=Default: all TCA values are set to default values,
--                         read-only."
--
--      VARIATION       tnPmonPolicyRowStatus
--          SYNTAX      INTEGER { active(1) }
--          DESCRIPTION "Limited set of values.
--                       Three policies are created by the system with the
--                       following policy IDs and properties:
--                       1=Disabled: all TCA values are zero, read only.
--                       2=User: user can provision TCA values, read write.
--                       8=Default: all TCA values are set to default values,
--                         read-only."
--
--      VARIATION       tnPmonPolicyDescription
--          DESCRIPTION "The following ASCII characters from ITU Recommendation
--                       T.50 are supported:
--                       The G0 character set - values 0x20 - 0x7E, with the
--                       following exceptions:
--                       - value 0x22: double quotes
--                       - value 0x27: single quote
--                       - value 0x3f: question mark
--                       - value 0x5c: backslash
--                       The reason of these exceptions is that those
--                       characters conflict with the Command Line Interface
--                       (CLI)."
--
--      VARIATION       tnPmonPolicyNumOfBins15Min
--          SYNTAX      Integer32 ( 33 )
--          DESCRIPTION "Limited set of values.
--                       For port policies:
--                       - tnPmonPolicyType equals port(1)
--                       - value is fixed to 33 history bins.
--                       For SLM policies:
--                       - tnPmonPolicyType equals eth-cfm-two-way-slm(3).
--                       - value is fixed to 33 history bins.
--                       For DM policies:
--                       - tnPmonPolicyType equals eth-cfm-two-way-dm(4).
--                       - value is fixed to 33 history bins."
--
--      VARIATION       tnPmonPolicyNumOfBins1Day
--          SYNTAX      Integer32 ( 8 )
--          DESCRIPTION "Limited set of values.
--                       For port policies:
--                       - tnPmonPolicyType equals port(1)
--                       - value is fixed to 8 history bins.
--                       For SLM policies:
--                       - tnPmonPolicyType equals eth-cfm-two-way-slm(3).
--                       - value is fixed to 8 history bins.
--                       For DM policies:
--                       - tnPmonPolicyType equals eth-cfm-two-way-dm(4).
--                       - value is fixed to 8 history bins."
--
--      VARIATION       tnPmonPolicyFlrInterval15Min
--          ACCESS      not-implemented
--          DESCRIPTION "This object is not supported."
--
--      VARIATION       tnPmonPolicyFlrInterval1Day
--          ACCESS      not-implemented
--          DESCRIPTION "This object is not supported."
--
--      VARIATION       tnPmonPolicyFlrAvailabilityInterval15Min
--          ACCESS      not-implemented
--          DESCRIPTION "This object is not supported."
--
--      VARIATION       tnPmonPolicyFlrAvailabilityInterval1Day
--          ACCESS      not-implemented
--          DESCRIPTION "This object is not supported."
--
--      VARIATION       tnPmonPolicyNumOfBinsContinuous
--          DESCRIPTION "For port policies:
--                       - tnPmonPolicyType equals port(1)
--                       - value is fixed to 0, no continuous bin supported.
--                       For SLM policies:
--                       - tnPmonPolicyType equals eth-cfm-two-way-slm(3).
--                       - value is fixed to 1, continuous bin is supported.
--                       For DM policies:
--                       - tnPmonPolicyType equals eth-cfm-two-way-dm(4).
--                       - value is fixed to 0, no continuous bin supported."
--
--      VARIATION       tnPmonPolicyAvailFlrThreshold
--          ACCESS      not-implemented
--          DESCRIPTION "This object is not supported. It is superseded by
--                      tnPsdOamEthCfmPingCtlAvailFlrThreshold
--                      (TROPIC-PSD-MIB)."
--
--      VARIATION       tnPmonPolicyAvailFlrNumOfIntervals
--          ACCESS      not-implemented
--          DESCRIPTION "This object is not supported. It is superseded by
--                      tnPsdOamEthCfmPingCtlAvailFlrNumOfIntervals
--                      (TROPIC-PSD-MIB)."
--
--      VARIATION       tnPmonPolicyFramesPerDeltaT
--          ACCESS      not-implemented
--          DESCRIPTION "This object is not supported."
--
--      VARIATION       tnPmonTcaId
--          DESCRIPTION "Following TCA IDs are assigned:
--                       For Port:
--                       - IFINOCTETS  :  1 (15min, 1Day)
--                       - IFINDISCARDS:  3 (15min, 1Day)
--                       - IFINERRORS  :  4 (15min, 1Day)
--                       - IFOUTOCTETS :  8 (15min, 1Day)
--                       - IFINPACKETS : 14 (15min, 1Day)
--                       - IFOUTPACKETS: 15 (15min, 1Day)
--                       For SLM:
--                       - aN_FLR      :  2 (15min, 1Day, Cont)
--                       - xN_FLR      :  3 (             Cont)
--                       - aF_FLR      :  5 (15min, 1Day, Cont)
--                       - xF_FLR      :  6 (             Cont)
--                       - N_HLI       :  7 (15min, 1Day)
--                       - F_HLI       :  8 (15min, 1Day)
--                       For DM:
--                       - aB_FD       :  2 (15min, 1Day)
--                       - xB_FD       :  3 (15min, 1Day)
--                       - aN_FDV      :  5 (15min, 1Day)
--                       - xN_FDV      :  6 (15min, 1Day)
--                       - AF_FDV      :  8 (15min, 1Day)
--                       - XF_FDV      :  9 (15min, 1Day)"
--
--      VARIATION       tnPmonTcaSubgroup
--          SYNTAX      Integer32 ( 1 )
--          DESCRIPTION "Value is fixed to 1.
--                       Value 1 is used for Port, SLM and DM TCAs."
--
--      VARIATION       tnPmonTcaVariable
--          DESCRIPTION "Following OIDs are supported.
--                       For Port (tnEthPortStatsTable):
--                       - tnEthPortStatsIfInPkts
--                       - tnEthPortStatsIfInOctets
--                       - tnEthPortStatsIfInDiscards
--                       - tnEthPortStatsIfInErrors
--                       - tnEthPortStatsIfOutPkts
--                       - tnEthPortStatsIfOutOctets
--                       For SLM (tnEthCfmTWSlmStatsTable):
--                       - tnEthCfmTWSlmStatsNearEndFrameLossRatioAverage
--                       - tnEthCfmTWSlmStatsNearEndFrameLossRatioMax
--                       - tnEthCfmTWSlmStatsFarEndFrameLossRatioAverage
--                       - tnEthCfmTWSlmStatsFarEndFrameLossRatioMax
--                       - tnEthCfmTWSlmStatsNearEndHighLoss
--                       - tnEthCfmTWSlmStatsFarEndHighLoss
--                       For DM (tnEthCfmTWDmStatsTable):
--                       - tnEthCfmTWDmStatsRoundTripFrameDelayAverage
--                       - tnEthCfmTWDmStatsRoundTripFrameDelayMax
--                       - tnEthCfmTWDmStatsNearEndFrameDelayVariationAverage
--                       - tnEthCfmTWDmStatsNearEndFrameDelayVariationMax
--                       - tnEthCfmTWDmStatsFarEndFrameDelayVariationAverage
--                       - tnEthCfmTWDmStatsFarEndFrameDelayVariationMax"
--
--      VARIATION       tnPmonTcaValue
--          DESCRIPTION "Only the entries in this table with policyId equals to
--                       value 2 (User policy) are allowed to be modified.
--                       Table entries with policy IDs other then value 2 are
--                       not allowed to be modified, in case a user attempts to
--                       modify such entries an error will be returned.
--                       The following TCA ranges are defined:
--                       In case a value is not in the range an error is
--                       returned.
--                       For Port:
--                       - IFINOCTETS   15min range: 0 - 1099511627775
--                       - IFINOCTETS   1Day  range: 0 - 1099511627775
--                       - IFINDISCARDS 15min range: 0 - 1099511627775
--                       - IFINDISCARDS 1Day  range: 0 - 1099511627775
--                       - IFINERRORS   15min range: 0 - 1099511627775
--                       - IFINERRORS   1Day  range: 0 - 1099511627775
--                       - IFOUTOCTETS  15min range: 0 - 1099511627775
--                       - IFOUTOCTETS  1Day  range: 0 - 1099511627775
--                       - IFINPACKETS  15min range: 0 - 1099511627775
--                       - IFINPACKETS  1Day  range: 0 - 1099511627775
--                       - IFOUTPACKETS 15min range: 0 - 1099511627775
--                       - IFOUTPACKETS 1Day  range: 0 - 1099511627775
--                       For SLM:
--                       The FLR stats values 100%=1000000000
--                       The HLI stats are based on a fixed
--                       interval of 60 seconds.
--                       - aN_FLR       15min range: 0 - 1000000000
--                       - aN_FLR       1Day  range: 0 - 1000000000
--                       - aN_FLR       Cont  range: 0 - 1000000000
--                       - xN_FLR       Cont  range: 0 - 1000000000
--                       - aF_FLR       15min range: 0 - 1000000000
--                       - aF_FLR       1Day  range: 0 - 1000000000
--                       - aF_FLR       Cont  range: 0 - 1000000000
--                       - xF_FLR       Cont  range: 0 - 1000000000
--                       - N_HLI        15min range: 0 - 15
--                       - N_HLI        1Day  range: 0 - 1440
--                       - F_HLI        15min range: 0 - 15
--                       - F_HLI        1Day  range: 0 - 1440
--                       For DM:
--                       The value represents micro seconds.
--                       - aB_FD        15min range: 0 - 2147583
--                       - aB_FD        1Day  range: 0 - 2147583
--                       - xB_FD        15min range: 0 - 2147583
--                       - xB_FD        1Day  range: 0 - 2147583
--                       - aN_FDV       15min range: 0 - 2147583
--                       - aN_FDV       1Day  range: 0 - 2147583
--                       - xN_FDV       15min range: 0 - 2147583
--                       - xN_FDV       1Day  range: 0 - 2147583
--                       - AF_FDV       15min range: 0 - 2147583
--                       - AF_FDV       1Day  range: 0 - 2147583
--                       - XF_FDV       15min range: 0 - 2147583
--                       - XF_FDV       1Day  range: 0 - 2147583"
--
--      VARIATION       tnEthPortStatsBin
--          DESCRIPTION "Only the current bin (value 0) can be retrieved
--                       with SNMP GET command query. History bins
--                       (bin number 1 and greater) can only be retrieved using
--                       PM file transfer."
--
