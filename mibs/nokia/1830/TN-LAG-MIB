tnLagObjects                 OBJECT IDENTIFIER ::= { tnSRObjs 15 }
tnLagNotifyPrefix      OBJECT IDENTIFIER ::= { tnSRNotifyPrefix 15 }
    tnLagNotifications    OBJECT IDENTIFIER ::= { tnLagNotifyPrefix 0 }
