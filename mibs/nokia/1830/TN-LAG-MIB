tnLagAdminState OBJECT-TYPE
    SYNTAX  INTEGER {
              up(1),       -- ready to pass packets
              down(2),
              testing(3)   -- in some test mode
            }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "."
    DEFVAL { down }
    ::= { tnLagConfigXEntry 7 }

tnLagLPTConsequenceAction    OBJECT-TYPE
