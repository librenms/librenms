--------------------------------------------------------------------------------
-- Switch Card Cable Table
--------------------------------------------------------------------------------
    tnSwitchCardCableTable OBJECT-TYPE
        SYNTAX         SEQUENCE OF TnSwitchCardCableEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "."
        ::= { tnOpticalCardObjs 51 }

    tnSwitchCardCableEntry OBJECT-TYPE
        SYNTAX         TnSwitchCardCableEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "."
        INDEX { tnShelfIndex,
                tnSlotIndex,
                tnSwitchCardCableStatusNum }
        ::= { tnSwitchCardCableTable 1 }

    TnSwitchCardCableEntry ::= SEQUENCE {
        tnSwitchCardCableStatusNum               Integer32,
        tnSwitchCardCableConnector               INTEGER,
        tnSwitchCardCableRepPartnerConnector     Unsigned32,
        tnSwitchCardCableExpPartnerConnector     Unsigned32,
        tnSwitchCardCableLinkStatus              INTEGER,
        tnSwitchCardCableReportedPartnerCard     SnmpAdminString,
        tnSwitchCardCableExpectedPartnerCard     SnmpAdminString
    }

    tnSwitchCardCableStatusNum OBJECT-TYPE
        SYNTAX         Integer32 (1..16)
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "."
        ::= { tnSwitchCardCableEntry 1 }

    tnSwitchCardCableConnector OBJECT-TYPE
        SYNTAX         INTEGER {
                         connector(1),
                         missing(2),
                         unAvailable(3)
                       }
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "Connector."
        ::= { tnSwitchCardCableEntry 2 }

    tnSwitchCardCableRepPartnerConnector OBJECT-TYPE
        SYNTAX         Unsigned32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "."
        ::= { tnSwitchCardCableEntry 3 }

    tnSwitchCardCableExpPartnerConnector OBJECT-TYPE
        SYNTAX         Unsigned32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "."
        ::= { tnSwitchCardCableEntry 4 }

    tnSwitchCardCableLinkStatus OBJECT-TYPE
        SYNTAX         INTEGER {
                         unknown(1),
                         calibrating(2),
                         linksUp(3),
                         linksFailed(4),
                         linksDegraded(5),
                         peerFailed(6)
                       }
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "."
        ::= { tnSwitchCardCableEntry 5 }

    tnSwitchCardCableReportedPartnerCard OBJECT-TYPE
        SYNTAX         SnmpAdminString (SIZE(0..255))
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "."
        ::= { tnSwitchCardCableEntry 6 }

    tnSwitchCardCableExpectedPartnerCard OBJECT-TYPE
        SYNTAX         SnmpAdminString (SIZE(0..255))
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "."
        ::= { tnSwitchCardCableEntry 7 }

