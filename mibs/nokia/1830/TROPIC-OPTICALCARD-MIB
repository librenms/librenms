  tnOpticalCardConf        OBJECT IDENTIFIER ::= { tnOpticalCardMIB 1 }
  tnOpticalCardGroups      OBJECT IDENTIFIER ::= { tnOpticalCardConf 1 }
  tnOpticalCardCompliances OBJECT IDENTIFIER ::= { tnOpticalCardConf 2 }
