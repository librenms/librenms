                         uFec(4),
                         enhancedFec2(5),
                         aFec(6),
                         eSDFec(10),
                         hpFec(11),
                         usdFec(12),
                         bjFec(13),
                         scFec(14),
                         sdFecAcc(15),
                         eSDFecG2(16),
                         eSDFecExt(17),
                         ePuncturedSDFecG2(18),
                         sdFecCE(19),
                         sdFecV(20),
                         eOFec(21)
                       }
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "Fec Mode."
        DEFVAL         { eSDFecG2 }
        ::= { tnOtsigCustomProfileEntry 7 }

    tnOtsigCustomProfilePhaseEnc OBJECT-TYPE
