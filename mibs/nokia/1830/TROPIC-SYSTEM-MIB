--------------------------------------------------------------------------------
-- Transfer PM Discovery Table
--------------------------------------------------------------------------------
    tnTransferPMDiscoveryTable OBJECT-TYPE
        SYNTAX         SEQUENCE OF TnTransferPMDiscoveryEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "."
        ::= { tnTransferPMDiscovery 1 }

    tnTransferPMDiscoveryEntry OBJECT-TYPE
        SYNTAX         TnTransferPMDiscoveryEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "."
        INDEX { tnTransferPMDiscoveryIndex }
        ::= { tnTransferPMDiscoveryTable 1 }

    TnTransferPMDiscoveryEntry ::=
        SEQUENCE {
            tnTransferPMDiscoveryIndex                              SnmpAdminString,
            tnTransferPMDiscoveryRowStatus                          RowStatus,
            tnTransferPMDiscoveryFilename                           SnmpAdminString,
            tnTransferPMDiscoveryControl                            INTEGER,
            tnTransferPMDiscoveryStatus                             INTEGER,
            tnTransferPMDiscoveryErrorCode                          INTEGER,
            tnTransferPMDiscoveryServerProtocol                     AluWdmNewTransferProtocol,
            tnTransferPMDiscoveryServerIp                           IpAddress,
            tnTransferPMDiscoveryServerUserId                       SnmpAdminString,
            tnTransferPMDiscoveryServerPassword                     SnmpAdminString,
            tnTransferPMDiscoveryServerInetAddressType              InetAddressType,
            tnTransferPMDiscoveryServerInetAddress                  InetAddress,
            tnTransferPMDiscoveryPort                               Unsigned32,
            tnTransferPMDiscoveryPrevious15minBinFilesTransfer      Unsigned32,
            tnTransferPMDiscoveryPrevious1dayBinFilesTransfer       Unsigned32,
            tnTransferPMDiscoveryTransferTimeOffset                 Unsigned32,
            tnTransferPMDiscovery15minBinFileTransferCount          Unsigned32,
            tnTransferPMDiscovery1dayBinFileTransferCount           Unsigned32,
            tnTransferPMDiscovery15minBinFileTransferFailureCount   Unsigned32,
            tnTransferPMDiscovery1dayBinFileTransferFailureCount    Unsigned32
        }

    tnTransferPMDiscoveryIndex OBJECT-TYPE
        SYNTAX         SnmpAdminString
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "Unique identifier associated with this tnTransferPMDiscoveryEntry."
        ::= { tnTransferPMDiscoveryEntry 1 }

    tnTransferPMDiscoveryRowStatus OBJECT-TYPE
        SYNTAX         RowStatus
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "Currently all entries have a row status of active.
                        Sets are permitted with values of createAndGo and
                        destroy, exclusively."
        ::= { tnTransferPMDiscoveryEntry 2 }

    tnTransferPMDiscoveryFilename OBJECT-TYPE
        SYNTAX         SnmpAdminString (SIZE(0..255))
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "This attribute indicates remote destination file path."
        DEFVAL         { "" }
        ::= { tnTransferPMDiscoveryEntry 3 }

    tnTransferPMDiscoveryControl OBJECT-TYPE
        SYNTAX         INTEGER {
                         noCmd(1),
                         start(2),
                         abort(3)
                       }
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "Controls the Discovery, i.e., starts after the next
                        bin rolled over trap is generated or aborts."
        DEFVAL         { noCmd }
        ::= { tnTransferPMDiscoveryEntry 4 }

    tnTransferPMDiscoveryStatus OBJECT-TYPE
        SYNTAX         INTEGER {
                         idle(1),
                         inProgress(2),
                         completedSuccessfully(3),
                         failed(4),
                         aborted(5)
                       }
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "This attribute indicates the status of the most recent
                        file transfer."
        DEFVAL         { idle }
        ::= { tnTransferPMDiscoveryEntry 5 }

    tnTransferPMDiscoveryErrorCode OBJECT-TYPE
        SYNTAX         INTEGER {
                         noError(1),
                         unknown(2),
                         timeout(3),
                         serverError(4),
                         networkError(5),
                         fileSystemError(6),
                         statsBinsRolled(7)
                       }
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "This attribute indicates the error code of the most
                        recent file transfer."
        DEFVAL         { noError }
        ::= { tnTransferPMDiscoveryEntry 6 }

    tnTransferPMDiscoveryServerProtocol OBJECT-TYPE
        SYNTAX         AluWdmNewTransferProtocol
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "This attributes indicates the server protocol."
        DEFVAL         { ftp }
        ::= { tnTransferPMDiscoveryEntry 7 }

    tnTransferPMDiscoveryServerIp OBJECT-TYPE
        SYNTAX         IpAddress
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "This attributes indicates the server IPv4 address."
        DEFVAL         { '00000000'H }
        ::= { tnTransferPMDiscoveryEntry 8 }

    tnTransferPMDiscoveryServerUserId OBJECT-TYPE
        SYNTAX         SnmpAdminString (SIZE(0..128))
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "This attribute indicates the userId used to access
                        the server."
        DEFVAL         { "" }
        ::= { tnTransferPMDiscoveryEntry 9 }

    tnTransferPMDiscoveryServerPassword OBJECT-TYPE
        SYNTAX         SnmpAdminString (SIZE(0..128))
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "This attribute indicates the password used to access
                        the server.

                        When read, this attribute will return an empty string
                        if the password has not been set and '********' if it
                        has been set."
        DEFVAL         { "" }
        ::= { tnTransferPMDiscoveryEntry 10 }

    tnTransferPMDiscoveryServerInetAddressType OBJECT-TYPE
        SYNTAX         InetAddressType
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "The value specifies the type of Internet address stored
                        in tnTransferPMDiscoveryServerInetAddress."
        DEFVAL         { unknown }
        ::= { tnTransferPMDiscoveryEntry 11 }

    tnTransferPMDiscoveryServerInetAddress OBJECT-TYPE
        SYNTAX         InetAddress
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "This attribute indicates the IPv6 address of the server."
        DEFVAL         { ''H }
        ::= { tnTransferPMDiscoveryEntry 12 }

    tnTransferPMDiscoveryPort OBJECT-TYPE
        SYNTAX         Unsigned32 (1..65535)
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "This attributes indicates the server port."
        DEFVAL         { 21 }
        ::= { tnTransferPMDiscoveryEntry 13 }

    tnTransferPMDiscoveryPrevious15minBinFilesTransfer OBJECT-TYPE
        SYNTAX         Unsigned32
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "This attribute indicates the number of previous 15
                        min bin files that needs to be transferred in the
                        range of 0 to 8."
        DEFVAL         { 0 }
        ::= { tnTransferPMDiscoveryEntry 14 }

    tnTransferPMDiscoveryPrevious1dayBinFilesTransfer OBJECT-TYPE
        SYNTAX         Unsigned32
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "This attribute indicates the number of previous 1 day
                        bin files that needs to be transferred in the range
                        of 0 to 7."
        DEFVAL         { 0 }
        ::= { tnTransferPMDiscoveryEntry 15 }

    tnTransferPMDiscoveryTransferTimeOffset OBJECT-TYPE
        SYNTAX         Unsigned32
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "This attribute indicates the file transfer time offset
                        in the range 0 to 600 seconds."
        DEFVAL         { 0 }
        ::= { tnTransferPMDiscoveryEntry 16 }

    tnTransferPMDiscovery15minBinFileTransferCount OBJECT-TYPE
        SYNTAX         Unsigned32 (0..8)
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "This attribute indicates the recent number of 15 min
                        bin files transferred."
        DEFVAL         { 0 }
        ::= { tnTransferPMDiscoveryEntry 17 }

    tnTransferPMDiscovery1dayBinFileTransferCount OBJECT-TYPE
        SYNTAX         Unsigned32 (0..8)
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "This attribute indicates the recent number of 1 day
                        bin files transferred."
        DEFVAL         { 0 }
        ::= { tnTransferPMDiscoveryEntry 18 }

    tnTransferPMDiscovery15minBinFileTransferFailureCount OBJECT-TYPE
        SYNTAX         Unsigned32 (0..8)
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "This attribute indicates the number of 15 min bin
                        files transferred failure count."
        DEFVAL         { 0 }
        ::= { tnTransferPMDiscoveryEntry 19 }

    tnTransferPMDiscovery1dayBinFileTransferFailureCount OBJECT-TYPE
        SYNTAX         Unsigned32 (0..8)
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "This attribute indicates the number of 1 day bin files
                        transferred failure count."
        DEFVAL         { 0 }
        ::= { tnTransferPMDiscoveryEntry 20 }

---------------------------------------------------------------
-- SSH Profile Table
---------------------------------------------------------------
    tnSshProfileTable OBJECT-TYPE
        SYNTAX         SEQUENCE OF TnSshProfileEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "."
        ::= { tnKeyObjs 15 }

    tnSshProfileEntry OBJECT-TYPE
        SYNTAX         TnSshProfileEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "."
        INDEX { tnSshProfileName }
        ::= { tnSshProfileTable 1 }

    TnSshProfileEntry ::= SEQUENCE {
        tnSshProfileName            OCTET STRING,
        tnSshProfileDescription     SnmpAdminString
    }

    tnSshProfileName OBJECT-TYPE
        SYNTAX         OCTET STRING
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "."
        ::= { tnSshProfileEntry 1 }

    tnSshProfileDescription OBJECT-TYPE
        SYNTAX         SnmpAdminString (SIZE(0..64))
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "."
        ::= { tnSshProfileEntry 2 }

