-- Ocsp Profile Table
---------------------------------------------------------------
    tnOcspProfileTable OBJECT-TYPE
        SYNTAX         SEQUENCE OF TnOcspProfileEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "."
        ::= { tnKeyObjs 14 }

    tnOcspProfileEntry OBJECT-TYPE
        SYNTAX         TnOcspProfileEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "."
        INDEX { tnOcspProfileName }
        ::= { tnOcspProfileTable 1 }

    TnOcspProfileEntry ::= SEQUENCE {
        tnOcspProfileName            OCTET STRING,
        tnOcspProfileRowStatus       RowStatus,
        tnOcspProfileRetry           Unsigned32,
        tnOcspProfileServerUrl       SnmpAdminString,
        tnOcspProfileStatus          AluWdmEnabledDisabled,
        tnOcspProfileTimeout         Unsigned32
    }

    tnOcspProfileName OBJECT-TYPE
        SYNTAX         OCTET STRING
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "."
        ::= { tnOcspProfileEntry 1 }

    tnOcspProfileRowStatus OBJECT-TYPE
        SYNTAX         RowStatus
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "The row status."
        ::= { tnOcspProfileEntry 2 }

    tnOcspProfileRetry OBJECT-TYPE
        SYNTAX         Unsigned32 (1..10)
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "Number of entries if an OCSP responder does not respond."
        DEFVAL         { 1 }
        ::= { tnOcspProfileEntry 3 }

    tnOcspProfileServerUrl OBJECT-TYPE
        SYNTAX         SnmpAdminString (SIZE(0..255))
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "URL of the OCSP responder."
        DEFVAL         { "" }
        ::= { tnOcspProfileEntry 4 }

    tnOcspProfileStatus OBJECT-TYPE
        SYNTAX         AluWdmEnabledDisabled
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "Enables or disables the X.509v3 certificate revocation
                        using the specified OCSP responder.

                        If the service is disabled, then the revocation check
                        will be not done."
        ::= { tnOcspProfileEntry 5 }

    tnOcspProfileTimeout OBJECT-TYPE
        SYNTAX           Unsigned32 (1..3600)
        MAX-ACCESS       read-create
        STATUS           current
        DESCRIPTION      "Timeout for OCSP responses."
        DEFVAL           { 10 }
        ::= { tnOcspProfileEntry 6 }

