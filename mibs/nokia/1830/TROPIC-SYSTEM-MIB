        tnSysIpv6AclNetIfFilterEnable         INTEGER,
        tnSysIpv6AclNetIfFilterSystemDefault  TruthValue
    }

    tnSysIpv6AclNetIfFilterDir OBJECT-TYPE
        SYNTAX         INTEGER {
                         rx(1),
                         tx(2)
                       }
        MAX-ACCESS     not-accessible
        STATUS         current
