        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "GNSS module PTPClock Time traceability status."
        ::= { tnGnssModuleEntry 39 }

    tnGnssModulePTPPortmAnnounceMsgRate OBJECT-TYPE
        SYNTAX         INTEGER {
                         notApplicable(0),
                         pkt1per16secs(1),
                         pkt1per8secs(2),
                         pkt1per4secs(3),
                         pkt1per2secs(4),
                         pkt1per1sec(5),
                         pkts2per1sec(6),
                         pkts4per1sec(7),
