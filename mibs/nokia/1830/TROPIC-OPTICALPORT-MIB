        SYNTAX         InterfaceIndexOrZero
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "The line port that is connected to the PTPIO SIG
                        port in the transmit direction."
        DEFVAL         { 0 }
        ::= { tnPtpioPortConfigEntry 3 }

