    TnOpOutStatsEntry ::=
        SEQUENCE {
            tnOpOutStatsBin         TnStatsBinType,
            tnOpOutStatsBinStatus   TnStatsBinStatus,
            tnOpOutStatsStartTime   DateAndTime,
            tnOpOutStatMinPower     Integer32,
            tnOpOutStatMaxPower     Integer32,
            tnOpOutStatAveragePower Integer32
