                        alarm."
        ::= { tnActiveOrMaskedAlarmEntry 7 }

    tnActiveOrMaskedAlarmData OBJECT-TYPE
        SYNTAX         SnmpAdminString (SIZE(0..255))
        MAX-ACCESS     read-only
