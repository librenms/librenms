	SYNTAX     INTEGER {
		null (0),
		b (1),
		a (2),
		ab (3)
	}
	MAX-ACCESS read-write
	STATUS     current
	DESCRIPTION
		"In-band signalling"
	::= { dvbt2PlpEntry 13 }

