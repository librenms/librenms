rackCoolingEntry  OBJECT-TYPE
	SYNTAX 	RackCoolingEntry
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION
		"Entry for an rack cooling board"
	INDEX { rcIndex }
	::= { rackCoolingTable 1 }

RackCoolingEntry ::= SEQUENCE {
	rcIndex
		Unsigned32,
	rcTag
		DisplayString,
	rcControlStatus
		INTEGER,
	rcHwStatus
		DisplayString,
	rcCoolingMode
		INTEGER,
	rcFan1
		DisabledEnabled,
	rcFan2
		DisabledEnabled,
	rcTemperature
		FloatingPoint,
	rcTemperatureTarget  
		Unsigned32,
---	rcPressureDifference
---		FloatingPoint,
	rcFan1SpeedManual
		FloatingPoint,
	rcFan2SpeedManual
		FloatingPoint,
	rcFan1Speed
		FloatingPoint,
	rcFan1Tachometer
		Unsigned32,
	rcFan1OperatingTime
		Unsigned32,
	rcFan1Reset
		NullApply,
	rcFan2Speed
		FloatingPoint,
	rcFan2Tachometer
		Unsigned32,
	rcFan2OperatingTime
		Unsigned32,
	rcFan2Reset
		NullApply,
	rcIdentifyBoard
		NullApply,
	rcExternalSensor1
		OpenClose,
	rcExternalSensor2
		OpenClose
}

