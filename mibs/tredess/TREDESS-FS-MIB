		DisplayString,
	amStCurrent2
		DisplayString,
	amStCurrent3
		DisplayString,
	amStCurrent4
		DisplayString,
	amStCurrent5
		DisplayString,
	amStCurrent6
		DisplayString,
	amStReversePowerFlt
		FloatingPoint
}

