		Integer32,
	muxEchoCancelerOn
		INTEGER,
	muxConfigurationType
		INTEGER,
	muxUmbralTempFPGA
		Integer32,
	muxTcfgPPSedge
		INTEGER,
	muxTtxId
		INTEGER
}

