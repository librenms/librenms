dvbtBroadcastBasicCompilance  MODULE-COMPLIANCE
	STATUS     current
	DESCRIPTION 
		"The compliance statement for dvbtBroadcastTredessMIB entities which
		            implement the dvbtBroadcast-TRedess MIB."
	MODULE
		MANDATORY-GROUPS { groupAdmin,
				groupAlarmsTraps,
				groupAlarmTrapTable,
				groupContacts,
				groupGPS,
				groupMUX,
				groupSystem,
				groupUCA,
				groupUcan,
					groupUca1 } 
	::= { groupCompilance 1 }

dvbtDeprecatedBasicCompilance  MODULE-COMPLIANCE
	STATUS     deprecated
	DESCRIPTION 
		"The compliance statement for dvbtBroadcastTredessMIB entities which
		implement the dvbtBroadcast-TRedess MIB.
		Deprecated!"
	MODULE
		MANDATORY-GROUPS { groupDeprecated,
						groupDeprecatedTraps } 
	::= { groupCompilance 2 }
