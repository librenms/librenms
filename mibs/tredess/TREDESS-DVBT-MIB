	SYNTAX     Integer32 (71..85)
	MAX-ACCESS read-only
	STATUS     current
	DESCRIPTION 
		"Control address."
	::= { uca1Entry 2 }

uca1Mode  OBJECT-TYPE
	SYNTAX     Integer32 (0|1|2|3)
	MAX-ACCESS read-write
	STATUS     current
	DESCRIPTION 
		"UCA 1+1 operation mode
		
		0: Manual
		1: Auto-return off
		2: Auto
		3: Auto-return on"
	::= { uca1Entry 3 }

uca1Preferential  OBJECT-TYPE
	SYNTAX     INTEGER {
		uca1PreferentialA (1),
		uca1PreferentialB (2)
	}
	MAX-ACCESS read-write
	STATUS     current
	DESCRIPTION 
		"UCA 1+1 preferential MUX
		
		1: MUX A
		2: MUX B
		
		Note: it is only possible to set this parameter if UCA 1+1 is in automatic mode."
	::= { uca1Entry 4 }

