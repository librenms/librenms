::= { tredessProducts 1 }


BitsStates ::= TEXTUAL-CONVENTION
	STATUS     deprecated
	DESCRIPTION 
		"Bit String providing a status summary for all
		alarms defined in this MIB module. A bit is set
		to one if the state of that alarm is 'fail'."
	SYNTAX  BITS {
		transmittedPowerMinor (0),
		transmittedPowerMajor (1),
		channelBERMinor (2),
		channelBERMajor (3),
		trtCurrentsOutRange (4),
		temperature (5),
		reflectedPower (6),
		unlockTS (7),
		noInputSignal (8),
		asiError (9),
		mipError (10),
		ppsError (11),
		iFDVBTSignal (12),
		gps10MHz (13),
		powerAmplifierCommunication (14),
		moduleCommunication (15),
		powerSupply1 (16),
		powerSupply2 (17),
		externalSupply (18),
		gpsHoldover (19),
		gpsAntenna (20)
	}


Mutes ::= TEXTUAL-CONVENTION
	STATUS     current
	DESCRIPTION 
		"Mutes status.
		
		Bit - Mutes
		1 - MPEG Error (lsb)
		2 - No MIP
		3 - GPS
		4 - ASI
		5 - MPEG overflow
		"
	SYNTAX  BITS {
		null0 (0),
		null1 (1),
		null2 (2),
		mpegOverflow (3),
		asi (4),
		gps (5),
		noMIP (6),
		mpegError (7)
	}


RateStatus ::= TEXTUAL-CONVENTION
	STATUS     current
	DESCRIPTION 
		"RateStatus is used in trap control 
		                            
		                            1: disabled 2: enabled 3: enabledPeriod"
	SYNTAX  INTEGER {
		disabled (1),
		enabled (2),
		enabledPeriod (3)
	}

