sysRedundanceDBTM  OBJECT-TYPE
	SYNTAX     INTEGER {
		noRedundance (1),
		onePlusOneRedundance (2),
		nPlusOneRedundance (3),
		multichannel (4),
		compact (5)
	}
	MAX-ACCESS read-only
	STATUS     current
	DESCRIPTION 
		"Information redundancy.
		
		1: system is not redundant
		2: 1+1 system
		3: n+1 system
		4: multichannel Serie
		5: Compact Serie"
	::= { systemDBTM 8 }

date  OBJECT-TYPE
	SYNTAX     DisplayString
	MAX-ACCESS read-write
	STATUS     current
	DESCRIPTION 
		"System date DDMMYYYY (for example: 22012008 22/01/2007)"
	::= { adminDBTM 1 }

hour  OBJECT-TYPE
	SYNTAX     DisplayString
	MAX-ACCESS read-write
	STATUS     current
	DESCRIPTION 
		"System time HHMMSS (for example 180300 18:03:00)"
	::= { adminDBTM 2 }

status  OBJECT-TYPE
	SYNTAX     INTEGER {
		systemOK (1),
		systemReboot (2),
		systemSoftReset (3),
		systemHardReset (4),
		systemFindModules (5),
		systemReadConfigurations (6),
		hardwareResetMuxs (7)
	}
	MAX-ACCESS read-write
	STATUS     current
	DESCRIPTION 
		"If the manager writes in this variable:
		
		1 = it does not efect (telecontrol is ON and polling the modules)
		2 = telecontrol system will be reboot (telecontrol application)
		3 = telecontrol system will be soft reset (reboot with actual values)
		4 = telecontrol system will be hard reset (reboot with default values)
		5 = telecontrol system will find new modules connected to the control bus
		6 = telecontrol system will read module configurations - Obsolete in Compact Range
		7 = telecontrol system will resets MUXs
		"
	::= { adminDBTM 3 }

