		INTEGER,
	muxAutoMIP
		INTEGER,
	muxFFTSize
		INTEGER,
	muxGuardInterval
		INTEGER,
	muxBW
		INTEGER,
	muxConstellation
		INTEGER,
	muxFEC
		INTEGER,
	muxUserLocalDelay
		Integer32,
	muxMutes
		Mutes,
	muxVcorr1
		Integer32,
	muxVcorr2
		Integer32,
	muxHysteresis
		Integer32,
	muxCBERwarningLevel
		OCTET STRING,
	muxCBERcriticalLevel
		OCTET STRING,
	muxTrapsConfiguration
		BitsStates,
	muxVBERwarningLevel
		OCTET STRING,
	muxVBERcriticalLevel
		OCTET STRING,
	muxEchoAdaptation
		INTEGER,
	muxAsiInput
		INTEGER,
	muxUmbralTemp
		Integer32,
