networkRoutingTable  OBJECT-TYPE
	SYNTAX SEQUENCE OF NetworkRoutingEntry
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION 
		"Static Routing Table"
	::= { networkAdmin 4 }

networkRoutingEntry  OBJECT-TYPE
	SYNTAX 	NetworkRoutingEntry
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION 
		"Each row is IP route"
	INDEX { networkRoutingIndex }
	::= { networkRoutingTable 1 }

NetworkRoutingEntry ::= SEQUENCE {
	networkRoutingIndex
		Integer32,
	networkRoutingDestination
		IpAddress,
	networkRoutingMask
		IpAddress,
	networkRoutingGateway
		IpAddress,
	networkRoutingInterfaz
		INTEGER,
	networkRoutingEnabled
		TruthValue
}

networkRoutingIndex  OBJECT-TYPE
	SYNTAX     Integer32 (1..4)
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION 
		"Routing table index"
	::= { networkRoutingEntry 1 }

networkRoutingDestination  OBJECT-TYPE
