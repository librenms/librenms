muxT2SystemType  OBJECT-TYPE
	SYNTAX     INTEGER {
		dvbt (1),
		dvbt2 (2),
		unknown (3),
		notApply (4)
	}
	MAX-ACCESS read-only
	STATUS     current
	DESCRIPTION 
		"DVB-T / DVB-T2 selector
		
		1: DVB-T
		2: DVB-T2
		3: Unknown (Gap-Filler)
		4: Not Apply (dummy)"
	::= { muxT2SystemEntry 6 }

muxT2cfgSource  OBJECT-TYPE
	SYNTAX     INTEGER {
		t2mi (1),
		ts (2),
		unknown (3),
		notApply (4)
	}
	UNITS 	"Hertzios"
	MAX-ACCESS read-write
	STATUS     current
	DESCRIPTION 
		"Source Mode:
		
		1: T2MI
		2: TS
		3: unknown (Gap-Filler)
		4: notApply (Dummy)
		"
	::= { muxT2SystemEntry 7 }

muxT2cfgNetworkMode  OBJECT-TYPE
	SYNTAX     INTEGER {
		sfn (1),
		mfn (2),
		unknown (3),
		notApply (4)
	}
	UNITS 	"Hertzios"
	MAX-ACCESS read-write
	STATUS     current
	DESCRIPTION 
		"Network Mode:
		
		1: SFN
		2: MFN
		3: unknown (Gap-Filler)
		4: notApply (Dummy)
		"
	::= { muxT2SystemEntry 8 }

