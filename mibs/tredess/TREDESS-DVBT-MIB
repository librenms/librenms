	SYNTAX     INTEGER {
		ntpServerEnabled (0),
		ntpServerDisabled (1),
		ntpServerUpdated (2),
		ntpServerUpdating (3),
		ntpServerError (4)
	}
	MAX-ACCESS read-write
	STATUS     current
	DESCRIPTION 
		"Indicates the NTP server status
		
		0: enabled
		1: disabled
		2: udpated
		3: udpating
		4: error during updating process
		
		It is possible only write values 0 (enabled) and 1(disabled)"
	::= { ntpServer 1 }

