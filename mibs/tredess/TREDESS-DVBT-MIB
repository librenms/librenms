groupSystem  OBJECT-GROUP
	OBJECTS { sysCycleDBTM, 
		sysDescrDBTM, 
		sysFileDateHourDBTM, 
		sysNameDBTM, 
		sysManagementVersionDBTM, 
		sysKernelVersionDBTM, 
		sysRedundanceDBTM, 
		sysSnmpVersionDBTM }
	STATUS     current
	DESCRIPTION 
		"Contains all the objects from system branch of the MIB"
	::= { objectGroups 1 }

groupAdmin  OBJECT-GROUP
	OBJECTS { date, 
		hour, 
		status, 
		networkIpAddress, 
		networkMaskAddress, 
		networkGatewayAddress, 
		networkRoutingDestination, 
		networkRoutingEnabled, 
		networkRoutingGateway, 
		networkRoutingInterfaz, 
		networkRoutingMask, 
		ntpServerIp, 
		ntpServerOffset, 
		ntpServerStatus, 
		powerSupplyStatus, 
		snmpFastTrapsSync, 
		snmpInformAck, 
		snmpInformRetries, 
		snmpInformTimeout, 
		snmpReadCommunity, 
		snmpTrapsPeriodic, 
		snmpWriteCommunity, 
		snmpTrapControlRateStatus, 
		snmpTrapControlPeriod, 
		snmpTrapsDestionationEnable, 
		snmpTrapsSync, 
		snmpTrapsAuthTrapEnable, 
		snmpTrapsCommunity, 
		snmpTrapsDestinationIP, 
		webserverAutoRefresh, 
		webserverUser1, 
		webserverUser2, 
		webserverNewPass }
	STATUS     current
	DESCRIPTION 
		"Contains all the objects from general branch of the MIB"
	::= { objectGroups 2 }

