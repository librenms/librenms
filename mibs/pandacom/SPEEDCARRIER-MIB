		::= { convSxLPortEventEntry 6 }

	convSxLSFPDMITxLowAlarmEvent OBJECT-TYPE
		SYNTAX INTEGER
		{
			normal (0),
			active (1),
			notimplemented (255)
		}
		MAX-ACCESS  read-only
		STATUS  current
		DESCRIPTION
			"Output power - low alarm"
		::= { convSxLPortEventEntry 7 }

