IMPORTS
	DisplayString	FROM SNMPv2-TC
	Integer32, IpAddress, TimeTicks, MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
	converter	FROM SPEEDCARRIER-MIB
; -- END of IMPORTS

--*******************************************************************

    speedDuallineFC MODULE-IDENTITY
        LAST-UPDATED "201911070000Z"
