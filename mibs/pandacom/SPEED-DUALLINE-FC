	speedDuallineFCMIpAddress OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"device IP-address"
		DEFVAL { 'c0a80066'H }	-- 192.168.0.102
		::= { speedDuallineFCMConfigEntry 3 }

	speedDuallineFCMIpNetmask OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-write
