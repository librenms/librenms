        ethernetInterfaceIPv6  OBJECT IDENTIFIER ::= { networkInterfaces 4 }

--          ethernetInterfaceIPv6Type OBJECT-TYPE
--                     SYNTAX  OCTET STRING (SIZE(0..16))
