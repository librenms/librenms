                ::=  { bwPSTTHWMTable 1 }

        BwPSTTHWMEntry  ::=  SEQUENCE {
    bwPSTTHWMIndex Unsigned32,
    bwPSTTHWMName DisplayString,
    bwPSTTHWMValue Gauge32,
    bwPSTTHWMTimestamp DisplayString,
    bwPSTTHWMStackTrace DisplayString
  }

