	fext201E			OBJECT IDENTIFIER ::=	{fextModel 201}
	fext202E			OBJECT IDENTIFIER ::=	{fextModel 202}
	fext211E			OBJECT IDENTIFIER ::=	{fextModel 211}
	fext212E			OBJECT IDENTIFIER ::=	{fextModel 212}
	fext311F			OBJECT IDENTIFIER ::=	{fextModel 311}
	fext312F			OBJECT IDENTIFIER ::=	{fextModel 312}
	fext511F			OBJECT IDENTIFIER ::=	{fextModel 511}
	fextF11A			OBJECT IDENTIFIER ::=	{fextModel 1011}
	fextF11E			OBJECT IDENTIFIER ::=	{fextModel 1012}
	fext212F			OBJECT IDENTIFIER ::=	{fextModel 2121}
	fextF21A			OBJECT IDENTIFIER ::=	{fextModel 2011}
	fextF21E			OBJECT IDENTIFIER ::=	{fextModel 2012}
	fev211FA			OBJECT IDENTIFIER ::=	{fextModel 2111}
	fev211F				OBJECT IDENTIFIER ::=	{fextModel 2112}
	fev212FA			OBJECT IDENTIFIER ::=	{fextModel 2121}
	fev212F				OBJECT IDENTIFIER ::=	{fextModel 2122}
	fextF22A			OBJECT IDENTIFIER ::=	{fextModel 2021}
	fextF22E			OBJECT IDENTIFIER ::=	{fextModel 2022}
	fer511G				OBJECT IDENTIFIER ::=	{fextModel 1511}
