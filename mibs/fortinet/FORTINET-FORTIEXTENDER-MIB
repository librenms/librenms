	--
	-- fnFortiExtenderMib.fextInfo
	--
	-- fextInfoSystem   ::= {fextInfo 1}
	-- fextInfoExtender ::= {fextInfo 2}
	-- fextInfoModem	::= {fextInfo 3}
	-- fextInfoRouter	::= {fextInfo 4}
	-- fextInfoVWAN     ::= {fextInfo 5}
