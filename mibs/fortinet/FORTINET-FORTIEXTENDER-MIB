	--
	-- fextLTE.fextLTEPlan
	--

	fextLTEPlanEntry OBJECT-TYPE
		SYNTAX      FextLTEPlanEntry
		MAX-ACCESS  not-accessible
		STATUS      current
		DESCRIPTION "An entry containing LTE Plan information."
